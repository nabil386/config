<!-- 
Third party versions and configuration variables that impact Curam builds are listed in this file.
These values are read by CustomEnvironment.bat on the development environment and in the Azure Devops build pipelines 
 -->
<EnvironmentVariables>
	<!--Curam product version in this codebase -->
	<CURAM_VERSION>8.0.3.0</CURAM_VERSION>
	
	<!--Name of the ootb artifacts zip file on the Azure blob storage.
	use command 'bdm-initialize download.extract' to download the zip from blob storage, clean existing code and extract the files from zip-->
	<OOTB_ARTIFACTS_ZIP>CuramSPM8030_v1</OOTB_ARTIFACTS_ZIP>
	
	<!--BDC Foundations zip version -->
	<BDC_FOUNDATIONS_VERSION>Foundations2.0_8030</BDC_FOUNDATIONS_VERSION>


	<!--BDC Foundations zip version -->
	<BDC_FOUNDATIONS_ZIP>Foundations2.0_8030-dev-v2</BDC_FOUNDATIONS_ZIP>


	<!--Apache ANT version-->
	<APACHE_ANT_VERSION>apache-ant-1.10.6</APACHE_ANT_VERSION>
	
	<!--Values to be set in ANT_OPTS environment variable-->
	<ANT_OPT_VALUE>-Xms1024m -Xmx2048m -XX:MaxMetaspaceSize=2048m</ANT_OPT_VALUE>
	
	<!--Values to be set in ANT_ARGS environment variable-->
	<ANT_ARGS_VALUE>-Dcmp.maxmemory=4072m -Djava.maxmemory=4072m</ANT_ARGS_VALUE>
	
	<!-- Java version picked from dockerimage 22.0.0.6-full-java8-ibmjava used in helmcharts. jre - 8.0.7.20, java - 1.8.0_351-->
	<!-- download from https://esdcoasspmsourcesstg.blob.core.windows.net/spm/jdk8u351-b10.zip -->
	<JAVA_VERSION>jdk8u351-b10</JAVA_VERSION>
	
	<!-- Values to be set for JAVA_OPTS-->
	<JAVA_OPTS_VALUE>-XX:MaxMetaspaceSize=4096m -Xms2048m -Xmx4096m</JAVA_OPTS_VALUE>
	
	<!--Apache Tomcat version -->
	<APACHE_TOMCAT_VERSION>apache-tomcat-9.0.44</APACHE_TOMCAT_VERSION>

	<!-- Version of zip containing jars such as j2ee,jmockit, junit-tools etc. 
	Can be downloaded from: https://esdcoasspmsourcesstg.blob.core.windows.net/spm/dependency-jars1.1.zip-->
	<DEPENDENCY_JARS_VERSION>dependency-jars1.1</DEPENDENCY_JARS_VERSION>
	
	<!-- Client component order -->
	<CLIENT_COMPONENT_ORDER_VALUES>BDMOAS,BDM,custom,PlatformConfig,CommonIntake,Intake,ReferralsLite,PCR,CREOLEProgramRecommendation,SummaryViews,CitizenContextViewer,WorkspaceServices,CitizenWorkspaceAdmin,FundPM,Appeal,SupervisorAppeals,DecisionAssist,DynamicEvidence,CEFWidgets,CMIS,IntelligentEvidenceGathering,IEGAdmin,Datastore,Editors,SupervisorWorkspace,Verification,ServicePlans,sample,CTMInfrastructure,SamplePublicAccess,AdvancedEvidenceSharing,EvidenceBroker,CuramFinancialAdapter,CuramMDAdapter,Advisor,EvidenceSharing,PDC,ValidationManager,SmartNavigator
	</CLIENT_COMPONENT_ORDER_VALUES>
	
	<!-- Server component order -->
	<SERVER_COMPONENT_ORDER_VALUES>BDMOAS,BDM,custom,PlatformConfig,Appeal,CommonIntake,Intake,ReferralsLite,PCR,CREOLEProgramRecommendation,SummaryViews,CitizenWorkspace,CitizenContextViewer,WorkspaceServices,CitizenWorkspaceAdmin,FundPM,SupervisorAppeals,DecisionAssist,DynamicEvidence,CEFWidgets,CMISInfrastructure,CMIS,IntelligentEvidenceGathering,Datastore,Editors,SupervisorWorkspace,Verification,ServicePlans,sample,CTMInfrastructure,SamplePublicAccess,AdvancedEvidenceSharing,EvidenceBroker,CuramFinancialAdapter,CuramMDAdapter,CREOLEInfrastructure,Advisor,EvidenceSharing,PDC,PFMAppViews,Workflow,ValidationManager,SmartNavigator
	</SERVER_COMPONENT_ORDER_VALUES>
</EnvironmentVariables>