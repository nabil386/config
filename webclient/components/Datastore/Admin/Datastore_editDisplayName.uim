<PAGE PAGE_ID="Datastore_editDisplayName" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE
    CLASS="DatastoreSchema"
    NAME="DISPLAY"
    OPERATION="viewSchemaDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="DatastoreSchema"
    NAME="ACTION"
    OPERATION="editSchemaDisplayName"
    PHASE="ACTION"
  />
  
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
      <ACTION_CONTROL
       LABEL="ActionControl.Label.Save"
       TYPE="SUBMIT"
      />
      <ACTION_CONTROL
       LABEL="ActionControl.Label.Cancel"
      />
  </ACTION_SET>
	
  <PAGE_PARAMETER NAME="schemaName"/>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="schemaName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$schemaName"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$schemaName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$schemaName"
    />
  </CONNECT>

  <CLUSTER NUM_COLS="1" SHOW_LABELS="true" LABEL_WIDTH="25%">
    <FIELD
       LABEL="Field.Label.SchemaDisplayName"
       WIDTH="50">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$schemaDisplayName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$schemaDisplayName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER> 
  
</PAGE>
