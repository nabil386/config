<!-- ===========                                                            -->
<!-- Page used to confirm deleteing a Datastore schema.                     -->
<!--                                                                        -->
<!-- ====================================================================== -->
<PAGE PAGE_ID="Datastore_deleteSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
    WINDOW_OPTIONS="width=400">
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
    
    <PAGE_PARAMETER NAME="schemaName" />
    
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="schemaName" />
        <TARGET NAME="ACTION" PROPERTY="schemaName" />
    </CONNECT>

    <SERVER_INTERFACE CLASS="DatastoreSchema" NAME="ACTION" OPERATION="removeSchema" PHASE="ACTION" />
    
    <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
        <FIELD>
            <CONNECT>
                <SOURCE NAME="TEXT" PROPERTY="Field.Text.Delete" />
            </CONNECT>
        </FIELD>
    </CLUSTER>
    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
        <ACTION_CONTROL LABEL="ActionControl.YesButton.label" TYPE="SUBMIT">
            <LINK PAGE_ID="Datastore_clearIEGCache" />
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.NoButton.label" />
    </ACTION_SET>
</PAGE>
