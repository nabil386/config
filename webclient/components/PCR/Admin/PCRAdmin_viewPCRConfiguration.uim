<!-- Dispalys the details of a PCR Configuration and a tab to view all      -->
<!-- range mappings for the configuration.                                  -->
<PAGE
  PAGE_ID="PCRAdmin_viewPCRConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="pcrConfigurationID"/>


  <SERVER_INTERFACE
    CLASS="PCRAdmin"
    NAME="DISPLAY"
    OPERATION="viewPCRConfiguration"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="pcrConfigurationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$pcrConfigurationID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="PCRAdmin_viewPCRConfiguration"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.RangeMapping"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="PCRAdmin_viewPCRConfigurationRangeMappings"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="1"
    STYLE="outer-cluster-borderless"
  >


    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.EndDate"
      USE_BLANK="true"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RuleSet">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="ruleSetName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.CaseAndSubType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseTypeAndSubTypeForDisplay"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RunInDeferredProcessing">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runInDeferredProcessing"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RunManually">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runManually"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.PCRRunPoints"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runPointsForDisplay"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
</PAGE>
