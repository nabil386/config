<!-- Creates a new PCR Range Mapping                           		        -->
<PAGE
  PAGE_ID="PCRAdmin_newPCRRangeMapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="PCRAdmin"
    NAME="ACTION"
    OPERATION="createPCRRangeMapping"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="pcrConfigurationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="pcrConfigurationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="pcrConfigurationID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.RangeStart"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rangeStart"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Classification">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="classification"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.RangeEnd"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rangeEnd"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
