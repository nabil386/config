<!-- Modifies an existing PCR Range Mapping.                                -->
<PAGE PAGE_ID="PCRAdmin_modifyPCRRangeMapping" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="PCRAdmin" NAME="DISPLAY" OPERATION="viewPCRRangeMapping"/>


  <SERVER_INTERFACE CLASS="PCRAdmin" NAME="ACTION" OPERATION="modifyPCRRangeMapping" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="pcrRangeMappingID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pcrRangeMappingID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$pcrRangeMappingID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pcrRangeMappingID"/>
    <TARGET NAME="ACTION" PROPERTY="pcrRangeMappingID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="pcrConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="pcrConfigurationID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2">


    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="type"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RangeStart" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="rangeStart"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="rangeStart"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Classification">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="classification"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="classification"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RangeEnd" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="rangeEnd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="rangeEnd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>