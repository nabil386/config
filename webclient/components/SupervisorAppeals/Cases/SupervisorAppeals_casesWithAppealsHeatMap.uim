<!-- Reads a Task's home page details. -->
<PAGE
  PAGE_ID="SupervisorAppeals_casesWithAppealsHeatMap"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorAppealCase"
    NAME="DISPLAY"
    OPERATION="readCasesWithAppealsforHeatMap"
    PHASE="DISPLAY"
  />


  <!-- BEGIN, CR00405639, KRK -->
  <ACTION_SET>
    <ACTION_CONTROL
      APPEND_ELLIPSIS="false"
      LABEL="ActionControl.Label.ReassignCases"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="SupervisorAppeals_reassignCasesWithAppeals"
      />
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.ViewList">
      <LINK PAGE_ID="SupervisorAppeals_casesWithAppealsList"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- BEGIN, CR00405939, KRK -->
  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.CasesWithAppeals"
  >
    <!-- END, CR00405939 -->
    <!-- END, CR00405639 -->
    <FIELD CONFIG="CasesWithAppeals_HeatMap">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="heatMapXMLString"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
