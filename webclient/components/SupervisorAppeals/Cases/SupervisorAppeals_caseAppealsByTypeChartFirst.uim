  <INCLUDE FILE_NAME="SupervisorAppeals_caseAppealsByTypeChartView.vim"/>
  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="supervisor.appealOption"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$appealOption"
    />
  </CONNECT>


