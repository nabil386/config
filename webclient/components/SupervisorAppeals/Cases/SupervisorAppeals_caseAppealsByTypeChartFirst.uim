<PAGE
  PAGE_ID="SupervisorAppeals_caseAppealsByTypeChartFirst"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
