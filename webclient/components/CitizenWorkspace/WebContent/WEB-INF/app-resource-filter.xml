<app-resource-filter>
    <type name="PDF">
        <allow regex="*.pdf"/>
    </type>

    <type name="IMAGE">
        <allow regex="*.png"/>
        <allow regex="*.gif"/>
        <allow regex="title_*"/>
        <allow regex="icon_*"/>
        <allow regex="*action_control_*"/>
        <allow regex="*page_*"/>
        <allow regex="*_background"/>
        <allow name="ieg_print"/>
        <allow name="ieg_print_roll"/>
        <allow name="help_arrow"/>
        <allow name="help_roll"/>
        <allow name="back_arrow"/>
        <allow name="next_arrow"/>
        <allow name="nav_current"/>
        <allow name="nav_todo"/>
        <allow name="nav_done"/>
        <allow name="help_close"/>
        <allow name="help_close_roll"/>
        <allow name="help_arrow_selected"/>
        <allow name="help_nor_external"/>
        <allow name="help_roll_external"/>
        <allow name="help_click_external"/>
        <allow name="fail_image"/>
        <allow name="arrow_collapse"/>
        <allow name="arrow_expand"/>        
    </type>
    
    <type name="CSS">
        <allow regex="*.css"/>
    </type>    
</app-resource-filter>
