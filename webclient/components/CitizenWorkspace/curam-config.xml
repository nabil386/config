  <FILE_DOWNLOAD_CONFIG>
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.CitizenWorkspacePkg.CitizenWorkspace_getCompletedApplicationForm_TH"
      PAGE_ID="OpenApplicationForm"
    >
      <INPUT
        PAGE_PARAM="id"
        PROPERTY="key$playerID"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.CitizenWorkspacePkg.CitizenWorkspace_getScreeningProgramForm_TH"
      PAGE_ID="OpenScreeningProgramForm"
    >
      <INPUT
        PAGE_PARAM="id"
        PROPERTY="key$playerID"
      />
      <INPUT
        PAGE_PARAM="fileID"
        PROPERTY="key$pdfFormID"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.CitizenWorkspacePkg.CitizenWorkspace_getCompletedForm_TH"
      PAGE_ID="OpenSimpleApplicationForm"
    >
