<?xml version="1.0" encoding="UTF-8"?>
<PAGE
  PAGE_ID="BDMThirdPartyEvidence_createThirdPartyWizardStep1"
  WINDOW_OPTIONS="width=1045,height=700"
  SCRIPT_FILE="BDMDynamicCluster.js"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT"  PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>

	<SERVER_INTERFACE CLASS="IntegratedCase" NAME="LIST_CASE"  OPERATION="listCaseParticipantsWithAge"/>
   <SERVER_INTERFACE CLASS="BDMThirdPartyWizard"  NAME="DISPLAY" OPERATION="getThirdParytEvidenceWizardDetails" PHASE="DISPLAY"/>
   <SERVER_INTERFACE CLASS="BDMThirdPartyWizard"  NAME="PROGRAMCODE_DISPLAY" OPERATION="listBDMThirdPartyProgramCodes" PHASE="DISPLAY"/>
	<SERVER_INTERFACE CLASS="BDMThirdPartyWizard"  NAME="ACTION" OPERATION="saveThirdPartyDetails" PHASE="ACTION"/>
	
<MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="wizardProperties$wizardMenu"/>
    </CONNECT>
  </MENU> 
 
  
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="wizardStateID"/>

  <CONNECT>
    <SOURCE  NAME="PAGE" PROPERTY="caseID" />
    <TARGET  NAME="LIST_CASE" PROPERTY="caseID"/>
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID"/>
    <TARGET NAME="DISPLAY" PROPERTY="wizardKey$wizardStateID"/>
  </CONNECT>
  
   <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$wizardKey$wizardStateID"/>
    <TARGET NAME="ACTION" PROPERTY="wizardKey$wizardStateID"/>
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="ACTION" PROPERTY="caseID"/>
  </CONNECT>
  
  
   <ACTION_SET  BOTTOM="true" >
    <ACTION_CONTROL ALIGNMENT="LEFT" LABEL="ActionControl.Label.Cancel" />
    <ACTION_CONTROL LABEL="ActionControl.Label.Next"  TYPE="SUBMIT" >
      <LINK DISMISS_MODAL="false" PAGE_ID="BDMThirdPartyEvidence_createThirdPartyWizardOrgPageResolver" SAVE_LINK="false" >
      <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="roleType" />
          <TARGET NAME="PAGE" PROPERTY="roleType"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="wizardKey$wizardStateID" />
          <TARGET NAME="PAGE" PROPERTY="wizardStateID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE  NAME="PAGE" PROPERTY="caseID"   />
          <TARGET NAME="PAGE"  PROPERTY="caseID"   />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  
  <CLUSTER 
    LABEL_WIDTH="40"
    NUM_COLS="2" 
  >
  
  <FIELD LABEL="Cluster.Field.Label.ReceivedDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="receivedDate"
        />
      </CONNECT>
    </FIELD>
    
      <FIELD LABEL="Cluster.Field.Label.CaseParticipant" USE_BLANK="FALSE" USE_DEFAULT="FALSE" WIDTH="70">
        <CONNECT>
          <INITIAL HIDDEN_PROPERTY="participantRoleID" NAME="LIST_CASE" PROPERTY="name" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="caseParticipantRoleID"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION"  PROPERTY="participantRoleID"/>
        </CONNECT>
      </FIELD>
      
      <FIELD LABEL="Cluster.Field.Label.FromDate">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="fromDate"/>
        </CONNECT>       
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="fromDate"
        />
      </CONNECT>
      </FIELD>
      <FIELD CONTROL="CHECKBOXED_LIST" HEIGHT="2" USE_BLANK="true"
			USE_DEFAULT="false" LABEL="Cluster.Field.Label.Program" >
      <CONNECT>
				<INITIAL HIDDEN_PROPERTY="code"
					NAME="PROGRAMCODE_DISPLAY" PROPERTY="description" />
			</CONNECT>
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="program" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="program" />
			</CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD LABEL="Cluster.Field.Label.ToDate" USE_BLANK="TRUE"  USE_DEFAULT="FALSE">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="toDate"/>
        </CONNECT>     
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="toDate"
        />
      </CONNECT>
      </FIELD>
  </CLUSTER>
  
  <CLUSTER TITLE="Cluster.Title.Role"
    LABEL_WIDTH="40"
    NUM_COLS="2" 
  >
  
  <FIELD LABEL="Cluster.Field.Label.Role" USE_BLANK="true">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="role"/>
        </CONNECT>   
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="role"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.Field.Label.Role.POAClassification" USE_BLANK="true">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="poaClass"/>
        </CONNECT>     
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="poaClass"
        />
      </CONNECT>
      </FIELD>
    <FIELD LABEL="Cluster.Field.Label.Role.Type" USE_BLANK="true">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="roleType"/>
        </CONNECT>        
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="roleType"
        />
      </CONNECT>
      </FIELD>
      
      <FIELD LABEL="Cluster.Field.Label.Role.POAType" USE_BLANK="true">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="poaType"/>
        </CONNECT>          
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="poaType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <CLUSTER TITLE="Cluster.Title.NameRelDetails"
    LABEL_WIDTH="40"
  >
    <CLUSTER SHOW_LABELS="false">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="TEXT" PROPERTY="Third.Party.Contact.Static.Message1" />
          </CONNECT>
      </FIELD>
    </CLUSTER>
    <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Cluster.Field.Label.Reg.Participant">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="thirdPartyDetails$thirdPartyParticipantRoleID"/>
        </CONNECT>     
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="thirdPartyDetails$thirdPartyParticipantRoleID"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    </CLUSTER>
    <CLUSTER SHOW_LABELS="false">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="TEXT" PROPERTY="Third.Party.Contact.Static.Message2" />
          </CONNECT>
      </FIELD>
    </CLUSTER>
    <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Cluster.Field.Label.Person.Title" USE_BLANK="TRUE"  USE_DEFAULT="FALSE">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="title"/>
        </CONNECT> 
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="title"
        />
      </CONNECT>
      </FIELD>
    <FIELD LABEL="Cluster.Field.Label.Person.FirstName">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="firstName"/>
        </CONNECT>    
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="firstName"
        />
      </CONNECT>
      </FIELD>
      
    <FIELD LABEL="Cluster.Field.Label.Person.LastName">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="lastName"/>
        </CONNECT>    
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="lastName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.Field.Label.Person.RelToClient" USE_BLANK="TRUE">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="relToClient"/>
        </CONNECT>     
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="relToClient"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD LABEL="Cluster.Field.Label.Person.MiddleName">
            <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="middleName"/>
        </CONNECT> 
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="middleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.Field.Label.Person.LangPref" USE_BLANK="TRUE"  USE_DEFAULT="FALSE">
        <CONNECT>
          <SOURCE NAME="DISPLAY"  PROPERTY="languagePref"/>
        </CONNECT>     
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="languagePref"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    </CLUSTER>
  </CLUSTER>
  

</PAGE>
