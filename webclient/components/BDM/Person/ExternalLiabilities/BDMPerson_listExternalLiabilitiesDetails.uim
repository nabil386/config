<!-- Task 11053 - CK - page for viewing history of external liabilities -->
<PAGE
  PAGE_ID="BDMPerson_listExternalLiabilitiesDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  
  <PAGE_PARAMETER NAME="externalLiabilityID"/>
  

  <SERVER_INTERFACE
    CLASS="BDMExternalLiability"
    NAME="DISPLAY"
    OPERATION="listLiabilityHistory"
  />
    
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="externalLiabilityID"/>
    <TARGET
      NAME="DISPLAY"
      PROPERTY="externalLiabilityID"/>  
  </CONNECT>
  
  <LIST 
    TITLE="List.Title.HistoricalData">
    <FIELD
      LABEL="Field.Label.Date"
      WIDTH="9">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="modifyDate"        
        />
      </CONNECT>
    </FIELD> 
    <FIELD
      LABEL="Field.Label.OldBalance"
      WIDTH="18">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="originalLiabilityAmount"        
        />
      </CONNECT>
    </FIELD> 
    <FIELD
      LABEL="Field.Label.NewBalance"
      WIDTH="18">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="liabilityAmount"        
        />
      </CONNECT>
    </FIELD> 
    <FIELD
      LABEL="Field.Label.User"
      WIDTH="15">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="userFullName"        
        />
      </CONNECT>
    </FIELD> 
    <FIELD
      LABEL="Field.Label.Comments"
      WIDTH="40">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"        
        />
      </CONNECT>
    </FIELD> 
       
  </LIST>
</PAGE>