<?xml version="1.0" encoding="UTF-8"?>

<!-- This Page Manages a Preferred Name Evidence for to a particular participant -->
<PAGE PAGE_ID="BDMPerson_changeIdentificationInfo"
	WINDOW_OPTIONS="width=700,height=600"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
		</CONNECT>
	</PAGE_TITLE>


	<SERVER_INTERFACE CLASS="BDMPDCPerson" NAME="DISPLAY"
		OPERATION="readPersonIdentificationDetails" PHASE="DISPLAY" />


	<SERVER_INTERFACE CLASS="BDMPDCPerson" NAME="ACTION"
		OPERATION="changeIdentificationInfoDetails" PHASE="ACTION" />


	<ACTION_SET ALIGNMENT="CENTER">
		<ACTION_CONTROL IMAGE="SaveButton"
			LABEL="ActionControl.Label.Save" TYPE="SUBMIT" />
		<ACTION_CONTROL IMAGE="CancelButton"
			LABEL="ActionControl.Label.Cancel" />
	</ACTION_SET>

	<PAGE_PARAMETER NAME="concernRoleID" />

	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
		<TARGET NAME="DISPLAY" PROPERTY="key$dtls$concernRoleID" />
	</CONNECT>

	<CONNECT>
		<SOURCE NAME="DISPLAY" PROPERTY="result$dtls$concernRoleID" />
		<TARGET NAME="ACTION" PROPERTY="details$dtls$concernRoleID" />
	</CONNECT>

	<CONNECT>
		<SOURCE NAME="DISPLAY" PROPERTY="existingSINNumber" />
		<TARGET NAME="ACTION" PROPERTY="existingSINNumber" />
	</CONNECT>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40">
		<FIELD LABEL="Field.Label.ChangeReason">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="evidenceChangeReason" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="evidenceChangeReason" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40"
		TITLE="Cluster.CaseParticipant.Title">

		<FIELD LABEL="Field.Label.CaseParticipant">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="concernRoleName" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40"
		TITLE="Cluster.SocialInsuranceNumber.Title">

		<FIELD LABEL="Field.Label.ExistingSIN">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="existingSINNumber" />
			</CONNECT>
		</FIELD>
		
		<FIELD LABEL="Field.Label.StartDate">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="sinIdentificationExistingStartDate" />
			</CONNECT>
		</FIELD>
		<FIELD CONTROL="SKIP" />
		<FIELD LABEL="Field.Label.EndDate">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="sinIdentificationExistingEndDate" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION"
					PROPERTY="sinIdentificationExistingEndDate" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40" TITLE="Cluster.IdentificationDetails.Title">
		<FIELD LABEL="Field.Label.SocialInsuranceNumber">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="sinNumber" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="sinNumber" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.StartDate">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="sinIdentificationStartDate" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="sinIdentificationStartDate" />
			</CONNECT>
		</FIELD>
		<FIELD CONTROL="SKIP" />
		<FIELD LABEL="Field.Label.EndDate">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="sinIdentificationEndDate" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="sinIdentificationEndDate" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40"
		TITLE="Cluster.RegisteredName.Title">

		<FIELD LABEL="Field.Label.FirstName">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="firstForename" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="firstForename" />
			</CONNECT>
		</FIELD>

		<FIELD LABEL="Field.Label.LastName">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="lastName" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="lastName" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.MiddleName">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="middleName" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="middleName" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<CLUSTER NUM_COLS="2" LABEL_WIDTH="40"
		TITLE="Cluster.DateOfBirth.Title">

		<FIELD LABEL="Field.Label.DateOfBirth"> 
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="dateOfBirth" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="dateOfBirth" />
			</CONNECT>
		</FIELD>

		<FIELD LABEL="Field.Label.LastNameAtBirth">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="lastNameAtBirth" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="lastNameAtBirth" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.Field.Label.ParentLastNameAtBirth">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="parentLastNameAtBirth" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="parentLastNameAtBirth" />
			</CONNECT>
		</FIELD>


	</CLUSTER>

<CLUSTER NUM_COLS="1" SHOW_LABELS="false"
	TITLE="Cluster.Title.Attestation">
	<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text1" />
		</CONNECT>
	</FIELD>
</CLUSTER>
<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text2" />
		</CONNECT>
	</FIELD>
	</CLUSTER>
<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text3" />
		</CONNECT>
	</FIELD>
	</CLUSTER>
<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text4" />
		</CONNECT>
	</FIELD>
	</CLUSTER>
<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text5" />
		</CONNECT>
	</FIELD>
	</CLUSTER>
<CLUSTER>
	<FIELD>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Cluster.Field.Attestation.Text6" />
		</CONNECT>
	</FIELD>
	
	</CLUSTER>

</CLUSTER>
<CLUSTER NUM_COLS="2" LABEL_WIDTH="40">
<FIELD LABEL="Field.Label.bdmAttestationAgree" USE_BLANK="true" USE_DEFAULT="false">
		<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="attestationAgree" />
			</CONNECT>
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="attestationAgree" />
			</CONNECT>
	</FIELD>
	</CLUSTER>

<CLUSTER NUM_COLS="2" LABEL_WIDTH="40"
		TITLE="Attestee.Details.Cluster.Title">

		<FIELD LABEL="Attestee.Label">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="concernRoleName" />
			</CONNECT>
		</FIELD>
	</CLUSTER>
</PAGE>