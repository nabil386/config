<?xml version="1.0" encoding="UTF-8"?>
<!-- Description -->
<!-- =========== -->
<!-- Enter the Page ID of the Custom Page you are about to configure -->
<PAGE
  PAGE_ID="BDMManualOverPaymentWizard_selectBenefitCases"
  WINDOW_OPTIONS="width=1045,height=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  
  
    <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>
  



  <SERVER_INTERFACE
    CLASS="BDMManualOverPayment"
    NAME="ACTION"
    PHASE="ACTION"
    OPERATION="createBenefitOverpayment"
  />
  
  
  <SERVER_INTERFACE
    CLASS="BDMManualOverPayment"
    NAME="DISPLAY"
    OPERATION="listBenefitsForDeduction"
  />
  
    <SERVER_INTERFACE
    CLASS="BDMManualOverPayment"
    NAME="DISPLAYWIZARD"
    OPERATION="readWizardDetails"
  />
  



  <PAGE_PARAMETER NAME="wizardStateID"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="liabilityAmount"/>

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="wizardStateID"
    />
    <TARGET
      NAME="DISPLAYWIZARD"
      PROPERTY="key$wizardStateID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="wizardStateID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardStateID"
    />

  </CONNECT>
  
  
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$concernRoleID"
    />
  </CONNECT>
 
 <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$concernRoleID"
    />
  </CONNECT>
  
  <!-- <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="liabilityAmount"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$liabilityAmount"
    />
  </CONNECT> -->
 
 <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="liabilityAmount"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$liabilityAmount"
    />
  </CONNECT>

 
 <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Label.ProductDeliveryCases" SHOW_LABELS="false" 
  >
  
 
    <FIELD
        CONTROL="CHECKBOXED_LIST"
        LABEL="Field.Label.ProductDeliveryCases"
        USE_BLANK="false"
        HEIGHT="4"
      >
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="caseID"
            NAME="DISPLAY"
            PROPERTY="caseDescription"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="selCaseIDStr"
          />
        </CONNECT>
      </FIELD>
  
   <FIELD CONTROL="SKIP"/>

 </CLUSTER>
  
 <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Label.RecoveryRate" DESCRIPTION="Cluster.Label.RecoveryRate.Desc"
  >
    <FIELD LABEL="Field.Label.RecoveryRateFixed" USE_DEFAULT="false" >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="recoveryRateFixed"
        />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.Label.RecoveryRateVariable" USE_DEFAULT="false" >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="recoveryRateVariable"
        />
      </CONNECT>
    </FIELD>
    
   <FIELD LABEL="Field.Label.DeductionStartDate" >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="deductionStartDate"
        />
      </CONNECT>
    </FIELD>

 <FIELD CONTROL="SKIP"/>
  <FIELD CONTROL="SKIP"/>
   <FIELD CONTROL="SKIP"/>
 </CLUSTER>




















  <ACTION_SET
    BOTTOM="true"
    TOP="true"
  >


    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />


 <ACTION_CONTROL LABEL="ActionControl.Label.Back">
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="false"
        PAGE_ID="BDMManualOverPaymentWizard_enterBalance"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="wizardStateID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="wizardStateID"
          />
        </CONNECT>
        
         <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="Case_resolveCaseHome"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
  </ACTION_SET>



</PAGE>
