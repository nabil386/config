<?xml version="1.0" encoding="UTF-8"?>
<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to delete a foreign liaison. -->

<PAGE PAGE_ID="BDMFEC_deleteForeignLiaison"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

 
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE CLASS="BDMForeignLiaison"
    NAME="ACTION" OPERATION="deleteForeignLiaison" PHASE="ACTION"/>
  
  <PAGE_PARAMETER NAME="foreignLiaisonID" />
  <PAGE_PARAMETER NAME="versionNo" />

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="foreignLiaisonID" />
    <TARGET NAME="ACTION" PROPERTY="foreignLiaisonID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo" />
    <TARGET NAME="ACTION" PROPERTY="versionNo" />
  </CONNECT>

  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes" TYPE="SUBMIT" />
    <ACTION_CONTROL IMAGE="NoButton"
      LABEL="ActionControl.Label.No" />
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT"
          PROPERTY="Field.StaticText.DeleteForeignLiaison" />
      </CONNECT>
    </FIELD>
  </CLUSTER>

</PAGE>
