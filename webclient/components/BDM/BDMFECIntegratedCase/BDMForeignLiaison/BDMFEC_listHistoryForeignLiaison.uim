<?xml version="1.0" encoding="UTF-8"?>

<!-- BDM Foreign Application History List page. -->

<PAGE PAGE_ID="BDMFEC_listHistoryForeignLiaison"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
    <PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
		</CONNECT>
	</PAGE_TITLE>

  <SERVER_INTERFACE CLASS="BDMForeignLiaison"
    NAME="DISPLAY" OPERATION="listFoerignLiaisonHistory" />

  <PAGE_PARAMETER NAME="foreignLiaisonID" />

  <CONNECT>
    <SOURCE PROPERTY="foreignLiaisonID" NAME="PAGE" />
    <TARGET PROPERTY="key$foreignLiaisonID" NAME="DISPLAY" />
  </CONNECT>

  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL LABEL="Page.Title.FLDetails"
      STYLE="in-page-link" TYPE="ACTION">
      <LINK PAGE_ID="BDMFEC_detailsForeignLiaison">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="foreignLiaisonID" />
          <TARGET NAME="PAGE" PROPERTY="foreignLiaisonID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Page.Title.FLAttachments"
      STYLE="in-page-link" TYPE="ACTION">
      <LINK PAGE_ID="BDMFEC_listFLAttachment">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="foreignLiaisonID" />
          <TARGET NAME="PAGE" PROPERTY="foreignLiaisonID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Page.Title.FLHistory"
      STYLE="in-page-current-link" TYPE="ACTION">
      <LINK PAGE_ID="BDMFEC_listHistoryForeignLiaison">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="foreignLiaisonID" />
          <TARGET NAME="PAGE" PROPERTY="foreignLiaisonID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>

  <LIST TITLE="List.Title.ForeignLiaisons">

    <DETAILS_ROW>
      <INLINE_PAGE
        PAGE_ID="BDMFEC_historyDetailsForeignLiaison">
        <CONNECT>
          <SOURCE NAME="DISPLAY"
            PROPERTY="result$dtls$frgnLsnHstID" />
          <TARGET NAME="PAGE" PROPERTY="frgnLsnHstID" />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>

    <FIELD LABEL="Field.List.Label.Direction">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$dtls$liaisonRefDesc" />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.List.Label.Date">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$receiveDate" />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.List.Label.ForeignApplication">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$dtls$frgnAppRefDesc" />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.List.Label.ForeignIdentifier">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$dtls$foreignIdntifier" />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.List.Label.RecordStatus">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$recordStatus" />
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.List.Label.UpdatedBy">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$dtls$lastUpdatedByAndOnStr" />
      </CONNECT>
    </FIELD>

  </LIST>

</PAGE>
  