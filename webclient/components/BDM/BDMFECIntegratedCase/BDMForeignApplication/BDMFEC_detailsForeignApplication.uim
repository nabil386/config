<?xml version="1.0" encoding="UTF-8"?>

<!-- BDM Foreign Application Details page. -->

<PAGE PAGE_ID="BDMFEC_detailsForeignApplication"
  HIDE_CONDITIONAL_LINKS="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <SERVER_INTERFACE CLASS="BDMForeignEngagementCase"
    NAME="DISPLAY" OPERATION="viewForeignApplication" />

  <PAGE_PARAMETER NAME="fApplicationID" />

  <CONNECT>
    <SOURCE PROPERTY="fApplicationID" NAME="PAGE" />
    <TARGET PROPERTY="key$fApplicationID" NAME="DISPLAY" />
  </CONNECT>

  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL LABEL="Page.Title.FADetails"
      STYLE="in-page-current-link" TYPE="ACTION">
      <LINK PAGE_ID="BDMFEC_detailsForeignApplication">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="fApplicationID" />
          <TARGET NAME="PAGE" PROPERTY="fApplicationID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Page.Title.FAHistory"
      STYLE="in-page-link" TYPE="ACTION">
      <LINK PAGE_ID="BDMFEC_listHistoryForeignApplication">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="fApplicationID" />
          <TARGET NAME="PAGE" PROPERTY="fApplicationID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER NUM_COLS="2">
    <CLUSTER>
       <CONDITION>
           <IS_FALSE PROPERTY="result$isIOClientContact" NAME="DISPLAY"/>
        </CONDITION>
        <FIELD LABEL="Field.label.ForeignOffice">
          <CONNECT>
            <SOURCE NAME="DISPLAY"
              PROPERTY="result$foreignOfficeAddressData" />
          </CONNECT>
        </FIELD>
    </CLUSTER>

    <CLUSTER>
      <CONDITION>
         <IS_FALSE PROPERTY="result$isIOClientContact" NAME="DISPLAY"/>
      </CONDITION>
      <FIELD LABEL="Field.label.ForeignIdentifier">
          <CONNECT>
            <SOURCE NAME="DISPLAY"
              PROPERTY="result$fIdentifierAlternateID" />
          </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER>
      <CONDITION>
         <IS_FALSE PROPERTY="result$isIOClientContact" NAME="DISPLAY"/>
      </CONDITION>
      <FIELD LABEL="Field.label.BESS">
          <CONNECT>
            <SOURCE NAME="DISPLAY"
              PROPERTY="result$bessIndStr" />
          </CONNECT>
      </FIELD>
    </CLUSTER>

    <FIELD LABEL="Field.label.DeletionReason">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$faDeleteReason" />
      </CONNECT>
    </FIELD>

  </CLUSTER>
  
	<CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Checklist">
		<FIELD LABEL="Field.label.Checklist.Retirement">
		  <CONNECT>
		    <SOURCE NAME="DISPLAY" PROPERTY="result$chkRetirementStr" />
		  </CONNECT>
		</FIELD>		
		<FIELD LABEL="Field.label.Checklist.Disability">
		  <CONNECT>
		    <SOURCE NAME="DISPLAY" PROPERTY="result$chkDisabilityStr" />
		  </CONNECT>
		</FIELD>
		<FIELD LABEL="Field.label.Checklist.Survivor">
		  <CONNECT>
		    <SOURCE NAME="DISPLAY" PROPERTY="result$chkSurvivorStr" />
		  </CONNECT>
		</FIELD>
		<FIELD LABEL="Field.label.Checklist.Other">
		  <CONNECT>
		    <SOURCE NAME="DISPLAY" PROPERTY="result$chkOtherStr" />
		  </CONNECT>
		</FIELD>		
    </CLUSTER>
    
	<CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Consent">
		<FIELD LABEL="Field.label.Client.Consent" WIDTH="50">
		  <CONNECT>
		    <SOURCE NAME="DISPLAY" PROPERTY="result$consentStr" />
		  </CONNECT>
		</FIELD>
	</CLUSTER>  

  <CLUSTER NUM_COLS="1" TITLE="Cluster.Title.Comments">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$comments" />
      </CONNECT>
    </FIELD>

  </CLUSTER>

</PAGE>
  