<!-- This page allows a user to modify foreign engagement integrated case header details. -->

<PAGE PAGE_ID="BDMFEC_modifyIntegratedCase"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="BDMForeignEngagementCase"
    NAME="DISPLAY" OPERATION="readFEIntegratedCase" />

  <SERVER_INTERFACE CLASS="BDMForeignEngagementCase"
    NAME="ACTION" OPERATION="modifyFEIntegratedCase" PHASE="ACTION" />


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save" TYPE="SUBMIT" />


    <ACTION_CONTROL IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel" />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="caseID" />

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID" />
    <TARGET NAME="DISPLAY" PROPERTY="key$caseID" />
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID" />
    <TARGET NAME="ACTION" PROPERTY="caseID" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$concernRoleID" />
    <TARGET NAME="ACTION" PROPERTY="concernRoleID" />
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$orgObjectLinkID" />
    <TARGET NAME="ACTION" PROPERTY="ownerOrgObjectLinkID" />
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$versionNo" />
    <TARGET NAME="ACTION" PROPERTY="versionNo" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$registrationDate" />
    <TARGET NAME="ACTION" PROPERTY="registrationDate" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$objectiveCode" />
    <TARGET NAME="ACTION" PROPERTY="objectiveCode" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$expectedEndDate" />
    <TARGET NAME="ACTION" PROPERTY="expectedEndDate" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$priorityCode" />
    <TARGET NAME="ACTION" PROPERTY="priorityCode" />
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$outcomeCode" />
    <TARGET NAME="ACTION" PROPERTY="outcomeCode" />
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$receiptDate" />
    <TARGET NAME="ACTION" PROPERTY="receiptDate" />
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY"
      PROPERTY="result$readICDetails$details$classificationCode" />
    <TARGET NAME="ACTION" PROPERTY="classificationCode" />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="25" NUM_COLS="2">

    <FIELD LABEL="Field.Label.Country" USE_BLANK="true">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$countryCode" />
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="countryCode" />
      </CONNECT>
    </FIELD>

  </CLUSTER>

  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <CONNECT>
        <SOURCE NAME="DISPLAY"
          PROPERTY="result$readICDetails$details$comments" />
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="modifyICDetails$comments" />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
