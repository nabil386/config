<!-- This page allows the user to search for a task.                        -->
<PAGE
  PAGE_ID="Supervisor_searchTask"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="BDMInbox"
    NAME="ACTION"
    OPERATION="searchBDMForTasks"
    PHASE="ACTION"
  />
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <!-- BEGIN, CR00161962, BK -->
  <CLUSTER
    LABEL_WIDTH="25"
    TITLE="Cluster.Title.SearchCriteria"
  >


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.TaskID"
      USE_DEFAULT="false"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchTaskKey$taskID"
        />
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Container.Label.AssignedTo">
      <FIELD
        ALIGNMENT="LEFT"
        USE_BLANK="true"
        USE_DEFAULT="false"
        WIDTH="30"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchTaskKey$assigneeType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="40">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchTaskKey$assignedToID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <CONTAINER LABEL="Container.Label.BusinessObjType">
      <FIELD
        ALIGNMENT="LEFT"
        USE_BLANK="true"
        USE_DEFAULT="false"
        WIDTH="30"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchTaskKey$businessObjectType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="40">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchTaskKey$businessObjectID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.Status"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchTaskKey$status"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Priority"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchTaskKey$priority"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Deadline"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchTaskKey$deadlineTime"
        />
      </CONNECT>
    </FIELD>


    <!-- END, CR00161962 -->


    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.SearchButton.label"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.ResetButton.label"
      >
        <LINK PAGE_ID="Supervisor_searchTask"/>
      </ACTION_CONTROL>
    </ACTION_SET>


  </CLUSTER>


  <!-- BEGIN, CR00414427, MR -->
  <!-- BEGIN, CR00161962, BK -->
  <LIST TITLE="List.Title.SearchResults">


    <FIELD
      LABEL="Field.Title.TaskID"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$taskID"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_taskHome">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$taskDetailsList$taskID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskID"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00414427 -->
    </FIELD>
    <FIELD
      LABEL="Field.Title.Subject"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$subject"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.AssignedUrgentFlags"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$caseUrgentFlagStr"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.AssignedEscalationLevel"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$escalationLevelDesc"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Deadline"
      WIDTH="20"
    >
      <CONNECT>
        <!-- BEGIN, CR00220774, AMD -->
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$deadlineDateTime"
        />
        <!-- END, CR00220774 -->
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Priority"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$taskDetailsList$priority"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00161962 -->


  </LIST>
</PAGE>
