<!-- Reads a Task's home page details. -->
<PAGE
  PAGE_ID="Supervisor_reserveCaseTasks"
  WINDOW_OPTIONS="width=800"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
            
      
      <!--START CR00037210 NP -->
          
      
      <!--LINK PAGE_ID="THIS" /-->
            
      
      <!--ENDS CR00037210 NP -->
        </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
        </ACTION_CONTROL>


  </ACTION_SET>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorUsers"
    NAME="DISPLAY"
    OPERATION="listSupervisorUsers"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCase"
    NAME="ACTION"
    OPERATION="reserveCaseTasks"
    PHASE="ACTION"
  />


  <CLUSTER LABEL_WIDTH="25">


    <CONTAINER LABEL="Container.Label.ReserveToUser">
      <FIELD
        USE_BLANK="true"
        WIDTH="40"
      >
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="userName"
            NAME="DISPLAY"
            PROPERTY="fullName"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="supervisorUserName"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="42">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="userName"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="userFullName"/>


  <SERVER_INTERFACE
    CLASS="BDMMaintainSupervisorCase"
    NAME="DISPLAYCASES"
    OPERATION="listBDMCaseAssignedTasks"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAYCASES"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <LIST TITLE="List.Title.Cases">
    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.MultiSelect.Select"
      WIDTH="5"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAYCASES"
              PROPERTY="taskID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="TaskIDKeyList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.TaskID"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="result$taskDetails$dtls$caseTasksDetails$taskID"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Subject"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="result$taskDetails$dtls$caseTasksDetails$taskSubject"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD
      LABEL="Field.Label.UrgentFlags"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="caseUrgentFlagStr"
        />
      </CONNECT>
    </FIELD>

    <FIELD
      LABEL="Field.Label.EscalationLevel"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="escalationLevelDesc"
        />
      </CONNECT>
    </FIELD>

    <FIELD
      LABEL="Field.Label.Priority"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="result$taskDetails$dtls$caseTasksDetails$taskPriority"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Assigned"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="result$taskDetails$dtls$caseTasksDetails$taskAssignedDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Deadline"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="result$taskDetails$dtls$caseTasksDetails$taskDeadlineDateTime"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
