<PAGE PAGE_ID="Supervisor_reallocateTasksNotReservedByUser"
	WINDOW_OPTIONS="width=850"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


	<INFORMATIONAL>
		<CONNECT>
			<SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
		</CONNECT>
	</INFORMATIONAL>


	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText" />
		</CONNECT>
	</PAGE_TITLE>


	<SERVER_INTERFACE CLASS="BDMMaintainSupervisorUsers"
		NAME="DISPLAY" OPERATION="bdmListTasksAssignedToUser" PHASE="DISPLAY" />


	<SERVER_INTERFACE CLASS="MaintainSupervisorUsers"
		NAME="ACTION" OPERATION="reallocateTasksNotReservedByUser"
		PHASE="ACTION" />


	<PAGE_PARAMETER NAME="userName" />
	<PAGE_PARAMETER NAME="userFullName" />
	<PAGE_PARAMETER NAME="taskOptionCode" />


	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="userName" />
		<TARGET NAME="DISPLAY" PROPERTY="key$key$userName" />
	</CONNECT>


	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="userName" />
		<TARGET NAME="ACTION" PROPERTY="key$dtls$userName" />
	</CONNECT>


	<ACTION_SET ALIGNMENT="CENTER">
		<ACTION_CONTROL IMAGE="SaveButton"
			LABEL="ActionControl.Label.Save" TYPE="SUBMIT" />


		<ACTION_CONTROL IMAGE="CancelButton"
			LABEL="ActionControl.Label.Cancel" />
	</ACTION_SET>


	<LIST TITLE="List.Title.AssignedTasks">
		<CONTAINER ALIGNMENT="CENTER"
			LABEL="List.Multiselect.Header" WIDTH="5">
			<WIDGET TYPE="MULTISELECT">
				<WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
					<CONNECT>
						<SOURCE NAME="DISPLAY"
							PROPERTY="result$dtls$taskDetailsList$taskDetailsList$taskID" />
					</CONNECT>
				</WIDGET_PARAMETER>
				<WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
					<CONNECT>
						<TARGET NAME="ACTION"
							PROPERTY="key$dtls$assignedTaskIDList" />
					</CONNECT>
				</WIDGET_PARAMETER>
			</WIDGET>
		</CONTAINER>


		<FIELD LABEL="Field.Label.TaskID" WIDTH="10">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$taskID" />
			</CONNECT>
		</FIELD>


		<FIELD LABEL="Field.Label.Subject" WIDTH="35">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$subject" />
			</CONNECT>
		</FIELD>

		<!--START TASK 86598 : display urgent flag and escalation level -->

		<FIELD LABEL="Field.Label.UrgentFlag" WIDTH="15">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$caseUrgentFlagStr" />
			</CONNECT>
		</FIELD>


		<FIELD LABEL="Field.Label.EscalationLevel" WIDTH="15">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$escalationLevelDesc" />
			</CONNECT>
		</FIELD>

		<!--END TASK 86598 : display urgent flag and escalation level -->
		<FIELD LABEL="Field.Label.Priority" WIDTH="10">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$priority" />
			</CONNECT>
		</FIELD>


		<FIELD LABEL="Field.Label.Deadline" WIDTH="10">
			<CONNECT>
				<SOURCE NAME="DISPLAY"
					PROPERTY="result$dtls$taskDetailsList$taskDetailsList$deadlineDateTime" />
			</CONNECT>
		</FIELD>


	</LIST>


	<CLUSTER LABEL_WIDTH="25" SHOW_LABELS="false"
		TITLE="Cluster.Title.Comments">
		<!-- BEGIN, CR00417017, GK -->
		<FIELD HEIGHT="4" LABEL="Field.Label.Comments">
			<!-- END, CR00417017 -->
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="comments" />
			</CONNECT>
		</FIELD>
	</CLUSTER>


</PAGE>
