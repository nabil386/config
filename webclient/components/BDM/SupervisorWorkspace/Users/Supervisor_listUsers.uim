<?xml version="1.0" encoding="UTF-8"?>
<!-- This page lists user reporting to Supervisor.                          -->
<PAGE
  PAGE_ID="Supervisor_listUsers"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <!-- BEGIN - CKwong - Task 8196 - Skills and Languages View -->
  <SERVER_INTERFACE
    CLASS="BDMMaintainSupervisorUsers"
    NAME="DISPLAY"
    OPERATION="listSupervisorUsersWithTaskCountAndSkills"
    PHASE="DISPLAY"
  />
  <!-- END - CKwong - Task 8196 - Skills and Languages View -->


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Organization_viewUserDetailsFromList">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$fullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_userWorkspace">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    
    <!-- BEGIN - CKwong - Task 8196 - Skills and Languages View -->
    <FIELD
      LABEL="Field.Label.PrioritizedSkills"
      WIDTH="20">
      <CONNECT>
        <SOURCE 
          NAME="DISPLAY"
          PROPERTY="result$dtls$prioritizedSkills"
        />
      </CONNECT>     
    </FIELD>
   
    
    <FIELD
      LABEL="Field.Label.PTMSkills"
      WIDTH="20">
      <CONNECT>
        <SOURCE 
          NAME="DISPLAY"
          PROPERTY="result$dtls$ptmSkills"
        />
      </CONNECT>     
    </FIELD>
    
    <FIELD
      LABEL="Field.Label.Language"
      WIDTH="20">
      <CONNECT>
        <SOURCE 
          NAME="DISPLAY"
          PROPERTY="result$dtls$langList"
        />
      </CONNECT>     
    </FIELD>
    <!-- END - CKwong - Task 8196 - Skills and Languages View -->
    <FIELD
      LABEL="Field.Label.OpenCaseCount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$openCases"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_listUserCases">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ReservedTaskCount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$reservedCases"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_openTasks">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.AssignedTasks"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="assignedTasks"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_userAssignedTasks">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>
</PAGE>
