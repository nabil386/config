<!-- Configuration file for the web client application. -->
<APP_CONFIG>
  
  <ADDRESS_CONFIG>
    <LOCALE_MAPPING ADDRESS_FORMAT_NAME="BDMINTL" LOCALE="en" />
    <LOCALE_MAPPING ADDRESS_FORMAT_NAME="BDMINTL" LOCALE="fr" />
    <!-- include the xx_CA mapping. While this shouldn't be required if users are only
      configured to have en and fr default locales, it avoids confusion if the _CA
      format is used, otherwise the address will default to US format in some locations.
      -->
    <LOCALE_MAPPING ADDRESS_FORMAT_NAME="BDMINTL"
			LOCALE="en_CA" />
    <LOCALE_MAPPING ADDRESS_FORMAT_NAME="BDMINTL"
			LOCALE="fr_CA" />
    <!-- include en_US for OOTB users with en_US default locales -->
    <LOCALE_MAPPING ADDRESS_FORMAT_NAME="BDMINTL"
			LOCALE="en_US" />
  </ADDRESS_CONFIG>
  
   <!--NOTE: IF YOU ARE ADDING ELEMENTS/CHANGING THE ORDER OF ELEMENTS, YOU WILL NEED TO
       CHANGE SearchAddress.js AS THERE IS A DEPENDENCY ON THE ORDER OF CERTAIN ELEMENTS-->  
  <ADDRESS_FORMAT NAME="BDMINTL">
    
    <!-- BEGIN - CKwong - Task 5155 Address Evidence -->
    <ADDRESS_ELEMENT CODETABLE="Country" LABEL="Address.Label.Country" MANDATORY="TRUE"
      NAME="COUNTRY" />
    
    <!--CANADIAN POSTAL CODE-->
    <ADDRESS_ELEMENT LABEL="Address.Canada.Label.PostCode" NAME="POSTCODE"
      />
    
    <!--AMERICAN ZIP CODE-->
    <ADDRESS_ELEMENT LABEL="Address.USA.Label.Zip" 
      NAME="ZIP" />
      
    <!--INTERNATIONAL ZIP CODE -->
    <ADDRESS_ELEMENT LABEL="Address.International.Label.Zip" 
      NAME="BDMZIPX" />    
    
    <!--BEGIN STANDARD ELEMENTS-->
    <ADDRESS_ELEMENT LABEL="Address.Label.AptSuite"
			NAME="APT" />
    
    <ADDRESS_ELEMENT LABEL="Address.Label.POBox"
			NAME="POBOXNO" />
    
    
    <ADDRESS_ELEMENT LABEL="Address.Label.StreetNumber"
			 NAME="ADD1" />
    
    <ADDRESS_ELEMENT LABEL="Address.Label.Street.1"
       NAME="ADD2" />
			       
    <ADDRESS_ELEMENT LABEL="Address.Label.City" MANDATORY="TRUE" NAME="CITY" />
    <!--END STANDARD ELEMENTS-->    

    <!--CANADIAN PROVINCE-->    
    <ADDRESS_ELEMENT CODETABLE="ProvinceType" LABEL="Address.Canada.Label.Province" NAME="PROV"
      />
    <!--AMERICAN STATE-->
    <ADDRESS_ELEMENT CODETABLE="AddressState" LABEL="Address.USA.Label.State" NAME="STATEPROV" />
    
    <!--INTERNATIONAL STATE/PROVINCE-->
    <ADDRESS_ELEMENT LABEL="Address.International.Label.State" NAME="BDMSTPROVX" />
    <!-- END - CKwong - Task 5155 Address Evidence -->
	
    <!-- START: Task 93505: DEV: Address Format Updates to add BDMUNPARSE -->
    <ADDRESS_ELEMENT LABEL="Address.Label.Street.UnParsedInd" NAME="BDMUNPARSE" />
    <!-- END: Task 93505: DEV: Address Format Updates to add BDMUNPARSE -->
    
  </ADDRESS_FORMAT>
  
