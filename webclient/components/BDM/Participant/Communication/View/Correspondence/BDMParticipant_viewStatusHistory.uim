<!-- This page allows the user to view the status history of a              -->
<!-- correspondence communication                                           -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
PAGE_ID="BDMParticipant_viewStatusHistory" 
xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

   <PAGE_TITLE>
      <CONNECT>
         <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
      </CONNECT>
   </PAGE_TITLE>
   
   <PAGE_PARAMETER NAME="communicationID" />
   
   <MENU MODE="IN_PAGE_NAVIGATION">
      <ACTION_CONTROL LABEL="ActionControl.Label.Detail" STYLE="in-page-link">
         <LINK PAGE_ID="BDMParticipant_viewCorrespondence" SAVE_LINK="false">
            <CONNECT>
               <SOURCE NAME="PAGE" PROPERTY="communicationID" />
               <TARGET NAME="PAGE" PROPERTY="communicationID" />
            </CONNECT>
         </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.StatusHistory" STYLE="in-page-current-link">
         <LINK PAGE_ID="BDMParticipant_viewStatusHistory" SAVE_LINK="false">
            <CONNECT>
               <SOURCE NAME="PAGE" PROPERTY="communicationID" />
               <TARGET NAME="PAGE" PROPERTY="communicationID" />
            </CONNECT>
         </LINK>
      </ACTION_CONTROL>
   </MENU>
   
   <INCLUDE FILE_NAME="BDMCommunication_viewStatusHistoryView.vim"/>
</PAGE>