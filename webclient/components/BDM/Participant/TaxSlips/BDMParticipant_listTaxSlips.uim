<PAGE 
  PAGE_ID="BDMParticipant_listTaxSlips"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  HIDE_CONDITIONAL_LINKS="false">
  
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Static.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="concernRoleID"/>
  
  <SERVER_INTERFACE
    NAME="DISPLAY"  
    CLASS="BDMTaxSlips"
    OPERATION="listTaxSlips"  
  />
  

  <CONNECT>
    <SOURCE 
      NAME="PAGE" 
      PROPERTY="concernRoleID"/>
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"/> 
  </CONNECT>    
  
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Edit"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showEditInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_editTaxSlip"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Issue"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showIssueInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_issueTaxSlip"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Delete"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showDeleteInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_deleteTaxSlip"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Amend"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showAmendInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_amendTaxSlip"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
       <ACTION_CONTROL
        LABEL="ActionControl.Label.CreateCancelFromExisting"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showCancelInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_createCancelTaxSlipFromExisting"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.CreateNewFromExisting"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showCreateInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_createNewTaxSlipFromExisting"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
     
      <ACTION_CONTROL
        LABEL="ActionControl.Label.CreateDuplicate"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="showDuplicateInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_createDuplicateTaxSlip"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taxSlipDataID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taxSlipDataID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    
    
    <FIELD
      LABEL="Field.Label.TaxationYear">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="taxYear"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.TypeOfSlip">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="formTypeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Category">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="slipTypeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.IssuanceStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="taxSlipStatusDesc"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ProcessingStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="processingStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreationMethod">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationMethodType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ProcessingDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="processingDate"
        />
      </CONNECT>
    </FIELD>
    
    
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="BDMParticipant_viewTaxSlipInline">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taxSlipDataID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taxSlipDataID"
          />
        </CONNECT>
        
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>
      
</PAGE>