<!-- CK - TASK 23927 - Payment Destination -->
<PAGE 
  PAGE_ID="BDMParticipant_listEFTPaymentDestination"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Static.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="concernRoleID"/>
  
  <SERVER_INTERFACE 
    NAME="DISPLAY"
    CLASS="BDMPaymentDestination"
    OPERATION="listEFTDestinations"/>
    
  <CONNECT>
    <SOURCE 
      NAME="PAGE"
      PROPERTY="concernRoleID"/>
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"/> 
  </CONNECT>
  
  <ACTION_SET>
    <ACTION_CONTROL 
      LABEL="ActionControl.Label.Create">
      <LINK 
        OPEN_MODAL="true"
        PAGE_ID="BDMParticipant_addEFTPaymentDestination">
        <CONNECT>
          <SOURCE 
            NAME="PAGE" 
            PROPERTY="concernRoleID"/>
          <TARGET 
            NAME="PAGE" 
            PROPERTY="concernRoleID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL 
        LABEL="ActionControl.Label.Modify">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_modifyEFTPaymentDestination">
          <CONNECT>
            <SOURCE 
              NAME="DISPLAY" 
              PROPERTY="paymentDestinationID"/>
            <TARGET 
              NAME="PAGE" 
              PROPERTY="paymentDestinationID"/>            
          </CONNECT>
          <CONNECT>
            <SOURCE 
              NAME="PAGE" 
              PROPERTY="concernRoleID"/>
            <TARGET 
              NAME="PAGE" 
              PROPERTY="concernRoleID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL 
        LABEL="ActionControl.Label.Cancel">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="BDMParticipant_cancelEFTPaymentDestination">
          <CONNECT>
            <SOURCE 
              NAME="DISPLAY" 
              PROPERTY="paymentDestinationID"/>
            <TARGET 
              NAME="PAGE" 
              PROPERTY="paymentDestinationID"/>            
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.ProgramName">
      <CONNECT>
        <SOURCE 
			  NAME="DISPLAY" 
			  PROPERTY="programType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.BankName">
      <CONNECT>
        <SOURCE 
        NAME="DISPLAY" 
        PROPERTY="bankName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AccountNumber">
      <CONNECT>
        <SOURCE 
        NAME="DISPLAY" 
        PROPERTY="accountNumber"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE 
        NAME="DISPLAY" 
        PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE 
        NAME="DISPLAY" 
        PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>
  </LIST>
      
</PAGE>