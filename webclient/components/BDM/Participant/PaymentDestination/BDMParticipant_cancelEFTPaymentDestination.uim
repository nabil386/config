<!-- CK - TASK 23927 - Payment Destination -->
<PAGE 
  PAGE_ID="BDMParticipant_cancelEFTPaymentDestination"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Static.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="paymentDestinationID"/>
  
  <SERVER_INTERFACE
    NAME="DISPLAY"
    CLASS="BDMPaymentDestination"
    OPERATION="readEFTDestinationDetails"  
  />  
    
  <SERVER_INTERFACE
    NAME="ACTION"
    CLASS="BDMPaymentDestination"
    OPERATION="cancelEFTDestination"
    PHASE="ACTION"  
  />
  
  <CONNECT>
    <SOURCE 
      NAME="PAGE" 
      PROPERTY="paymentDestinationID"/>
    <TARGET 
      NAME="DISPLAY" 
      PROPERTY="paymentDestinationID"/> 
  </CONNECT>  
  <CONNECT>
    <SOURCE 
      NAME="PAGE" 
      PROPERTY="paymentDestinationID"/>
    <TARGET 
      NAME="ACTION" 
      PROPERTY="paymentDestinationID"/> 
  </CONNECT> 
  <CONNECT>
    <SOURCE 
      NAME="DISPLAY" 
      PROPERTY="versionNo"/>
    <TARGET 
      NAME="ACTION" 
      PROPERTY="key$versionNo"/> 
  </CONNECT>
  
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL 
      ALIGNMENT="LEFT" 
      LABEL="ActionControl.Label.Cancel" 
      TYPE="ACTION"/>
    
    <ACTION_CONTROL 
      LABEL="ActionControl.Label.Delete"
      TYPE="SUBMIT"
      />
  </ACTION_SET>  
  
  <CLUSTER>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="DeletePaymentDestination.Description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  

</PAGE>