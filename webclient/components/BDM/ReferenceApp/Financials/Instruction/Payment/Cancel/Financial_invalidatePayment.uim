<?xml version="1.0" encoding="UTF-8"?>
<!-- Description -->
<!-- =========== -->
<!-- This page is used to invalidate a cancelled payment.                   -->
<PAGE PAGE_ID="Financial_invalidatePayment" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <SERVER_INTERFACE CLASS="BDMFinancial" NAME="ACTION" OPERATION="invalidateCanceledPayment" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="finInstructionID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="finInstructionID"/>
    <TARGET NAME="ACTION" PROPERTY="finInstructionID"/>
  </CONNECT>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.InvalidatePaymentText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
