    <!-- BEGIN - TASK 10634 - CK - Liability Status  -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="28"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$caseType"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN - TASK 10634 - CK - Liability Status  -->

    <FIELD
