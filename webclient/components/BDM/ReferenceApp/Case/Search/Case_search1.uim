      WIDTH="28"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$caseClientsOpt"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Participants"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$participantCount"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Case_listParticipantRole"
