<?xml version="1.0" encoding="UTF-8"?>
<!-- Description -->
<!-- =========== -->
<!-- This page allows users to search for a Case.                           -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Case_search1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- BEGIN, CR00464654, EC -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Static"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- END, CR00464654 -->


  <INCLUDE FILE_NAME="Case_searchCriteriaView.vim"/>


  <CLUSTER>
    <ACTION_SET
      ALIGNMENT="CENTER"
      BOTTOM="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK PAGE_ID="Case_search1"/>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <LIST TITLE="List.Title.SearchResult">


	<CONDITION>
	  <IS_FALSE
	    NAME="DISPLAY"
	    PROPERTY="displayClientsOpt"
	  />
	</CONDITION>
    <!-- BEGIN, CR00440782, SP -->
    <DETAILS_ROW MINIMUM_EXPANDED_HEIGHT="195">
      <!-- END, CR00440782 -->


      <INLINE_PAGE
        URI_SOURCE_NAME="ACTION"
        URI_SOURCE_PROPERTY="contextPanelName"
      />


    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.CaseReference"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$caseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONDITION>
          <IS_FALSE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$restricted"
          />
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>

    <!-- BEGIN - TASK 10634 - CK - Liability Status  -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="28"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$caseType"
        />
      </CONNECT>
    </FIELD>
    <!-- END - TASK 10634 - CK - Liability Status  -->


    <FIELD
      LABEL="Field.Label.PrimaryClient"
      WIDTH="28"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$concernRoleName"
        />
      </CONNECT>
      <!--BEGIN, CR00139966, MC-->
      <LINK PAGE_ID="Participant_resolve">
        <CONDITION>
          <IS_FALSE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$restricted"
          />
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$concernRoleType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleType"
          />
        </CONNECT>
      </LINK>
      <!--END, CR00139966-->
    </FIELD>


    <FIELD
      LABEL="Field.Label.Participants"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$participantCount"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Case_listParticipantRole"
        SAVE_LINK="false"
      >
        <CONDITION>
          <IS_FALSE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$restricted"
          />
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchDtls$dtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$dtls$startDate"
        />
      </CONNECT>
    </FIELD>

    <!-- BEGIN - TASK 10634 - CK - Liability Status  -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$status"
        />
      </CONNECT>
    </FIELD>
    <!-- END - TASK 10634 - CK - Liability Status  -->


  </LIST>
  <LIST TITLE="List.Title.SearchResult">
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="displayClientsOpt"
        />
