<?xml version="1.0" encoding="UTF-8"?>
<!-- Description -->
<!-- =========== -->
<!-- Prospect Person search page                                            -->
<PAGE
  PAGE_ID="BDMIOAddProspectPerson_duplicateCheckForPDCWizard"
  HIDE_CONDITIONAL_LINKS="false"
  WINDOW_OPTIONS="width=975,height=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CLUSTER SHOW_LABELS="false">
      <FIELD>
          <CONNECT>
              <SOURCE NAME="TEXT" PROPERTY="Person.search.static.message" />
          </CONNECT>
      </FIELD>
  </CLUSTER>

  <!-- BEGIN, CR00282028, IBM -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00282028 -->
  
  <PAGE_PARAMETER NAME="concernRoleID"/>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


    <SERVER_INTERFACE
    	ACTION_ID_PROPERTY="actionIDProperty"
        CLASS="BDMParticipantRegistrationDetails"
    	NAME="ACTION"
    	OPERATION="setAddProspectPersonSearchCriteriaDetailsExt"
    	PHASE="ACTION"
  	/>


  <SERVER_INTERFACE
    CLASS="BDMParticipantRegistrationDetails"
    NAME="DISPLAYWIZARD"
    OPERATION="getBDMIOAddProspectPersonWizard"
  />


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAY"
    OPERATION="readHomePageDetails"
  />


  <ACTION_SET>
	<ACTION_CONTROL ALIGNMENT="LEFT"
		LABEL="ActionControl.Label.Cancel" />
	<ACTION_CONTROL LABEL="ActionControl.Label.Next">
		<CONDITION>
			<IS_TRUE NAME="ACTION" PROPERTY="result$isNextButtonEnabled" />
		</CONDITION>
		<LINK DISMISS_MODAL="false"
			PAGE_ID="BDMIOProspectPerson_registerAsPersonForPDCWizard"
			SAVE_LINK="false">
			<CONNECT>
				<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
				<TARGET NAME="PAGE" PROPERTY="concernRoleID" />
			</CONNECT>
		</LINK>
	</ACTION_CONTROL>
</ACTION_SET>

   <INCLUDE FILE_NAME="BDMIOAddProspectPerson_duplicateCheckForPDCWizardView.vim"/>


</PAGE>
