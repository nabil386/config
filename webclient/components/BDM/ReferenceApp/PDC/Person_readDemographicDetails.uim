<?xml version="1.0" encoding="UTF-8"?>

<!-- Reads a person&apos;s home page details and further details            -->
<PAGE
  PAGE_ID="Person_readDemographicDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="BDMPDCPerson"
    NAME="DISPLAY"
    OPERATION="readBDMNonPDCDemographicData"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
      <LINK
        OPEN_MODAL="TRUE"
        PAGE_ID="Person_modifyDemographicDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$concernRoleID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >
    
	<FIELD LABEL="Field.Label.IndigenousPersonType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="indigenousPersonType"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.EducationLevel">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="educationLevel"
        />
      </CONNECT>
    </FIELD>

	<!-- Begin Task-60434 remove marital status on demographics, it is created as a new evidence -->
    <FIELD CONTROL="SKIP"></FIELD>
    <!-- End Task-60434 -->
    
    <FIELD LABEL="Field.Label.MemberMinorityGrpType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="memberMinorityGrpType"
        />
      </CONNECT>
    </FIELD>

  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
