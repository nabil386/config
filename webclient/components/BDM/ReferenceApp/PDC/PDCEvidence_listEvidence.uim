<!-- Evidence view page for listing pdc evidence    -->

<PAGE PAGE_ID="PDCEvidence_listEvidence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


	<PAGE_PARAMETER NAME="concernRoleID" />
	<!-- <PAGE_PARAMETER NAME="wizardStateID" />  -->
	
	<!-- 
	<JSP_SCRIPTLET>
		<![CDATA[
            String wizardStateID="";
	    	wizardStateID = request.getParameter("wizardStateID");
		
			if (wizardStateID == null) {
				wizardStateID = "0";
			}
			
		]]>
	</JSP_SCRIPTLET>
		<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
		<TARGET NAME="DISPLAY" PROPERTY="wizardStateID" />
	</CONNECT>
	-->
	
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
		<TARGET NAME="DISPLAY" PROPERTY="concernRoleID" />
	</CONNECT>
	

	

	<SERVER_INTERFACE CLASS="BDMPDCPerson" NAME="DISPLAY"
		OPERATION="listEvidenceForParticipant" />


	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
	</PAGE_TITLE>
	
   
   <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="msgDtls$dtls$message" />
    </CONNECT>
  </INFORMATIONAL>
  
	
	<ACTION_SET>
		<ACTION_CONTROL LABEL="ActionControl.Label.NewEvidence">
			<LINK OPEN_MODAL="true" PAGE_ID="PDCEvidence_addNewEvidence"
				SAVE_LINK="true">
				<CONNECT>
					<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
					<TARGET NAME="PAGE" PROPERTY="concernRoleID" />
				</CONNECT>
			</LINK>
		</ACTION_CONTROL>

	</ACTION_SET>


	<!-- BEGIN, CR00463142, EC -->
	<LIST SUMMARY="Summary.Evidence.Details">
		<!-- END, CR00463142 -->
		<ACTION_SET TYPE="LIST_ROW_MENU">
			<ACTION_CONTROL LABEL="ActionControl.Label.Edit">
				<CONDITION>
					<IS_FALSE PROPERTY="readOnlyInd" NAME="DISPLAY"/>
				</CONDITION>
				<LINK OPEN_MODAL="TRUE"
					PAGE_ID="Evidence_resolveDynEvdModifyEvidencePage">
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceID" />
						<TARGET NAME="PAGE" PROPERTY="evidenceID" />
					</CONNECT>
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceType" />
						<TARGET NAME="PAGE" PROPERTY="evidenceType" />
					</CONNECT>
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="caseID"/>
						<TARGET NAME="PAGE" PROPERTY="caseID"/>
					</CONNECT>
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceDescriptorID" />
						<TARGET NAME="PAGE" PROPERTY="evidenceDescriptorID" />
					</CONNECT>
				</LINK>
			</ACTION_CONTROL>
			<ACTION_CONTROL LABEL="ActionControl.Label.Delete">
				<LINK OPEN_MODAL="TRUE" PAGE_ID="Evidence_removeActive">
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceID" />
						<TARGET NAME="PAGE" PROPERTY="evidenceID" />
					</CONNECT>
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceType" />
						<TARGET NAME="PAGE" PROPERTY="evidenceType" />
					</CONNECT>
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="evidenceDescriptorID" />
						<TARGET NAME="PAGE" PROPERTY="evidenceDescriptorID" />
					</CONNECT>
				</LINK>
			</ACTION_CONTROL>
		</ACTION_SET>


		<FIELD LABEL="List.Title.EvidenceType" WIDTH="20">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="evidenceType" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="List.Title.Description" WIDTH="30">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="description" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="List.Title.Source" WIDTH="15">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="source" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="List.Title.Period" WIDTH="15">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="period" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="List.Title.LatestActivity" WIDTH="20">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="latestActivity" />
			</CONNECT>
		</FIELD>
		<DETAILS_ROW>
			<INLINE_PAGE
				PAGE_ID="Evidence_resolveDynEvdViewEvidencePage">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="evidenceID" />
					<TARGET NAME="PAGE" PROPERTY="evidenceID" />
				</CONNECT>
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="evidenceType" />
					<TARGET NAME="PAGE" PROPERTY="evidenceType" />
				</CONNECT>
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="caseID" />
					<TARGET NAME="PAGE" PROPERTY="caseID" />
				</CONNECT>
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="evidenceDescriptorID" />
					<TARGET NAME="PAGE" PROPERTY="evidenceDescriptorID" />
				</CONNECT>
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="successionID" />
					<TARGET NAME="PAGE" PROPERTY="successionID" />
				</CONNECT>
			</INLINE_PAGE>
		</DETAILS_ROW>
	</LIST>

</PAGE>
