<!-- This page is used to display a list of decisions for the benefit       -->
<!-- overpayment.                                                           -->
<PAGE
  PAGE_ID="BDMBenefitOverpayment_listAllDecisions"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="LEFT">
    <ACTION_CONTROL
      IMAGE="ShowActive"
      LABEL="ActionControl.Label.ViewActiveDecisions"
    >


      <LINK PAGE_ID="BDMBenefitOverpayment_listDecision">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <INCLUDE FILE_NAME="ProductDelivery_listAllDecisionsView.vim"/>


</PAGE>
