<!-- List page for the existing clients page                     -->
<PAGE
  PAGE_ID="ProgramAuthorisation_listExistingCasesAnyClientMatch"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="intakeProgramApplicationID"/>
  
  <SERVER_INTERFACE 
    CLASS="ApplicationCase" 
    NAME="DISPLAY3" 
    OPERATION="checkAnyPendingCasesByProgram"  
    PHASE="DISPLAY"
  />
    
  <SERVER_INTERFACE 
    CLASS="ProgramAuthorisation" 
    NAME="DISPLAY" 
    OPERATION="listClientsExistingCases"
    PHASE="DISPLAY"
  />
  
  <SERVER_INTERFACE 
    CLASS="BDMProgramAuthorisation" 
    NAME="ACTION" 
    OPERATION="authorise"
    PHASE="ACTION"
  />  
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY3"
      PROPERTY="appCaseKey$caseID"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="intakeProgramApplicationID"
    />
    <TARGET
      NAME="DISPLAY3"
      PROPERTY="programKey$programID"
    />
  </CONNECT>
    
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseKey$applicationCaseID"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="intakeProgramApplicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$intakeProgramApplicationID"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationCaseID"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="intakeProgramApplicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$intakeProgramApplicationID"
    />
  </CONNECT>

  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY3" PROPERTY="result$informationalDtls$dtls$informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>
          
  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
    >
        
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
        
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
           
  </ACTION_SET>
  
  <CLUSTER LABEL_WIDTH="35">
    
    <FIELD LABEL="Field.Label.CreateNewIntegratedCase" >
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="createNewInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createNewInd" />
      </CONNECT>
    </FIELD> 
    
  </CLUSTER>   
  
  <CLUSTER>
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="displayClientsListInd"/>
    </CONDITION>
  
    <LIST TITLE="List.Label.Title"> 
      <CONTAINER>
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="dtls$caseID" />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="caseTabList" />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      
      <FIELD LABEL="Field.Label.CaseReference" WIDTH="23">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseReference"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Clients" WIDTH="40">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseClients"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.DateCreated" WIDTH="20">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseCreationDate"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Owner" WIDTH="17">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseOwner"/>
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Case_resolveOrgObjectTypeHome"
          WINDOW_OPTIONS="width=900"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseOwnerUsername"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="orgObjectReference"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="orgObjectReference"
            />
          </CONNECT>
          
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="orgObjectType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="orgObjectType"
            />
          </CONNECT>
        </LINK>                
      </FIELD>
    </LIST>
  </CLUSTER>  
  
  <CLUSTER TITLE="Cluster.Title.Comments" SHOW_LABELS="false" >   
    <FIELD HEIGHT="4">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="opComment"/>
      </CONNECT>
    </FIELD>  
       
  </CLUSTER>  
  
</PAGE>


