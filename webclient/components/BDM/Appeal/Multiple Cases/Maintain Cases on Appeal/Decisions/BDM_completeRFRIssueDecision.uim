<?xml version="1.0" encoding="UTF-8"?>
<!-- TASK 56872 Appeals Issue Edit Decision -->
<PAGE
  PAGE_ID="BDM_completeRFRIssueDecision"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
   <PAGE_TITLE>
	<CONNECT>
		<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
	</CONNECT>
   </PAGE_TITLE>

	<PAGE_PARAMETER NAME="bdmRFRIssueID" />

	<SERVER_INTERFACE CLASS="BDMMaintainRFRIssue"
		NAME="ACTION" OPERATION="completeBDMRFRIssue" PHASE="ACTION" />

	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="bdmRFRIssueID" />
		<TARGET NAME="ACTION" PROPERTY="bdmRFRIssueID" />
	</CONNECT>

	<ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD LABEL="Field.Label.Confirm">
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.Confirm"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

</PAGE>