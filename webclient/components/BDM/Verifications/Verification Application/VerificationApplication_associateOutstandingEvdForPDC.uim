<PAGE
	PAGE_ID="VerificationApplication_associateOutstandingEvdForPDC"
	WINDOW_OPTIONS="width=850,height=400"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
		</CONNECT>
	</PAGE_TITLE>
	<SERVER_INTERFACE CLASS="VerificationApplication"
		NAME="DISPLAY" OPERATION="listOutstandingVerificationDetailsForPDC"
		PHASE="DISPLAY" />
	<SERVER_INTERFACE
		ACTION_ID_PROPERTY="actionIDProperty"
		CLASS="BDMVerificationApplication" NAME="ACTION"
		OPERATION="newUserProvidedVerificationItemAndAssociatedEvidences"
		PHASE="ACTION" />


	<SERVER_INTERFACE CLASS="VerificationApplication"
		NAME="DISPLAYWIZARD"
		OPERATION="getAddItemsToOutstandingVerificationForPDCWizard" />
	<PAGE_PARAMETER NAME="concernRoleID" />
	<PAGE_PARAMETER NAME="wizardStateID" />
	<PAGE_PARAMETER NAME="verificationItemID" />


	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="verificationItemID" />
		<TARGET NAME="DISPLAY" PROPERTY="verificationItemID" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
		<TARGET NAME="DISPLAY" PROPERTY="participantID" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
		<TARGET NAME="ACTION" PROPERTY="wizardStateID" />
	</CONNECT>


	<MENU MODE="WIZARD_PROGRESS_BAR">
		<CONNECT>
			<SOURCE NAME="DISPLAYWIZARD" PROPERTY="wizardMenu" />
		</CONNECT>
	</MENU>


	<ACTION_SET>
		<ACTION_CONTROL LABEL="ActionControl.Label.Back">
			<LINK DISMISS_MODAL="false" OPEN_MODAL="false"
				PAGE_ID="VerificationApplication_verifyOutstandingEvdForPDC"
				SAVE_LINK="false">
				<CONNECT>
					<SOURCE NAME="PAGE" PROPERTY="concernRoleID" />
					<TARGET NAME="PAGE" PROPERTY="concernRoleID" />
				</CONNECT>


				<CONNECT>
					<SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
					<TARGET NAME="PAGE" PROPERTY="wizardStateID" />
				</CONNECT>
			</LINK>
		</ACTION_CONTROL>


		<ACTION_CONTROL ACTION_ID="SAVE"
			LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
		</ACTION_CONTROL>


		<ACTION_CONTROL ALIGNMENT="LEFT"
			LABEL="ActionControl.Label.Cancel">
		</ACTION_CONTROL>


	</ACTION_SET>


	<CLUSTER SHOW_LABELS="false">


		<LIST PAGINATED="false">


			<WIDGET LABEL="List.Title.Resolved" TYPE="MULTISELECT">
				<WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
					<CONNECT>
						<SOURCE NAME="DISPLAY" PROPERTY="vDIEDLinkID" />
					</CONNECT>
				</WIDGET_PARAMETER>
				<WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
					<CONNECT>
						<TARGET NAME="ACTION" PROPERTY="vDIEDLinkIDList" />
					</CONNECT>
				</WIDGET_PARAMETER>


			</WIDGET>


			<FIELD LABEL="Field.Label.Evidence" WIDTH="25">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="verifiableDataItemName" />
				</CONNECT>
			</FIELD>


			<FIELD LABEL="Field.Label.EvidenceType" WIDTH="25">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeOpt" />
				</CONNECT>
			</FIELD>


			<FIELD LABEL="Field.Label.Participant" WIDTH="25">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="primaryClient" />
				</CONNECT>


			</FIELD>


			<FIELD LABEL="Field.Label.DueDate" WIDTH="25">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="dueDate" />
				</CONNECT>
			</FIELD>
		</LIST>
	</CLUSTER>


	<CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


		<FIELD HEIGHT="4" LABEL="Field.Label.Comments">


			<CONNECT>
				<TARGET NAME="ACTION"
					PROPERTY="newUserProvidedVerificationInfo$comments" />
			</CONNECT>
		</FIELD>
	</CLUSTER>


</PAGE>
