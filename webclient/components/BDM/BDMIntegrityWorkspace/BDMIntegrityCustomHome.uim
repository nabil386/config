<?xml version="1.0" encoding="UTF-8"?>
<!-- Version  1.0 - Task 9082 by  Mohan 10/02/2022 -->
<PAGE
  COMPONENT_STYLE="page-omit-sidebar"
  PAGE_ID="BDMIntegrityCustomHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ANNOUNCEMENTDISPLAY"
    OPERATION="getAnnouncementsAsXML"
  />


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="announcements-cluster"
    WIDTH="100"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="ANNOUNCEMENTDISPLAY"
          PROPERTY="result$contentXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="BDMIntegrityCustomHome"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageID$pageID"
    />
  </CONNECT>


  <INCLUDE FILE_NAME="PodContainer.vim"/>


</PAGE>
