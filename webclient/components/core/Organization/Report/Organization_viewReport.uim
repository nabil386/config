<!-- This page allows a user to view the details for a report.          -->
<PAGE
  PAGE_ID="Organization_viewReport"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="readReport"
  />


  <PAGE_PARAMETER NAME="reportID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="reportID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$reportID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.FullTitle"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="title"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
