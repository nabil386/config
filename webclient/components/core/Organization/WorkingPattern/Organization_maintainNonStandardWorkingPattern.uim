<!-- This page allows the user to create an organization working pattern.   -->
<PAGE PAGE_ID="Organization_maintainNonStandardWorkingPattern" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="DISPLAY" OPERATION="readOrganizationNonStandardWorkingPattern" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Organization" NAME="ACTION" OPERATION="createModifyOrganizationNonStandardWorkingPattern" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="workingPatternID"/>
  <PAGE_PARAMETER NAME="dayNumber"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="dayNumber"/>
    <TARGET NAME="DISPLAY" PROPERTY="dayNumber"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workingPatternID"/>
    <TARGET NAME="DISPLAY" PROPERTY="workingPatternID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workingPatternID"/>
    <TARGET NAME="ACTION" PROPERTY="workingPatternID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="dayNumber"/>
    <TARGET NAME="ACTION" PROPERTY="dayNumber"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="24.5" NUM_COLS="1">


    <FIELD LABEL="Field.Label.Type" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="typeCode"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="typeCode"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="49" NUM_COLS="2">


    <FIELD LABEL="Field.Label.StartTime">


      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="startTime"/>
      </CONNECT>


      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="activePatternStartTime"/>
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.EndTime">


      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="endTime"/>
      </CONNECT>


      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="activePatternEndTime"/>
      </CONNECT>


    </FIELD>


  </CLUSTER>


</PAGE>