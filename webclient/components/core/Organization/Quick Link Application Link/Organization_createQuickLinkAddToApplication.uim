<!-- Create a new quick link from within an application.                    -->
<!-- BEGIN, CR00350581, PB -->
<PAGE PAGE_ID="Organization_createQuickLinkAddToApplication" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00350581 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="ACTION" OPERATION="createAndAddQuickLinkToApplicationCode" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="applicationCode"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationCode"/>
    <TARGET NAME="ACTION" PROPERTY="applicationCode"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="true">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">

    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="42">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CuramPageName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="webAddress"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OpenInModal">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="openInModalInd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>