<!-- Create a new quick link for an organization.                           -->
<!-- BEGIN, CR00357406, PB -->
<PAGE PAGE_ID="Organization_addQuickLinkToApplication" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00357406 -->


  <PAGE_TITLE DESCRIPTION="PageTitle.Description">
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="DISPLAY" OPERATION="listUnassociatedQuickLinks" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Organization" NAME="ACTION" OPERATION="addQuickLinkForApplicationCode" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="applicationCode"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationCode"/>
    <TARGET NAME="DISPLAY" PROPERTY="applicationCode"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationCode"/>
    <TARGET NAME="ACTION" PROPERTY="applicationCode"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" BOTTOM="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
     
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
     
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <CONTAINER ALIGNMENT="CENTER" WIDTH="6">
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$list$list$dtls$quickLinkID"/>
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="details$quickLinkList"/>
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="List.Title.Name" WIDTH="74">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$list$list$dtls$name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.Title.DateCreated" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$list$list$dtls$dateCreated"/>
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>