<!-- This page allows the user to modify a service plan approval check.     -->
<!-- BEGIN, CR00246725, MR -->
<PAGE PAGE_ID="Organization_modifySPGApprovalCheck" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00246725 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlanGroupApprovalCheck" NAME="DISPLAY" OPERATION="readServicePlanGroupApprovalCheck" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ServicePlanGroupApprovalCheck" NAME="ACTION" OPERATION="modifyServicePlanGroupApprovalCheck" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
