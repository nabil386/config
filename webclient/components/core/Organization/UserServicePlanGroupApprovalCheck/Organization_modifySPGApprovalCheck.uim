    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="servicePlanGroupApprovalChkID"/>
  <PAGE_PARAMETER NAME="servicePlanGroupID"/>
  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupApprovalChkID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$key$servicePlanGroupApprovalChkID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupApprovalChkID"/>
    <TARGET NAME="ACTION" PROPERTY="servicePlanGroupApprovalChkID"/>
  </CONNECT>


  <CONNECT>
