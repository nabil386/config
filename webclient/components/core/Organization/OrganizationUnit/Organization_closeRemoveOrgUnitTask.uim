<PAGE
  PAGE_ID="Organization_closeRemoveOrgUnitTask"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="orgUnitName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="orgUnitName"/>
  <PAGE_PARAMETER NAME="taskID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="taskID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="taskID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="TaskManagement"
    NAME="ACTION"
    OPERATION="close"
    PHASE="ACTION"
  />


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="ConfirmationText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="Action.Control.Yes"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Inbox_listReservedTasks">
  </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="Action.Control.No"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="Inbox_listReservedTasks">
  </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
