<!-- This process allows the user to view a list of locations               -->
<!-- for an organization unit.                                              -->
<PAGE PAGE_ID="Organization_listOrgUnitLocations" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="DISPLAY" OPERATION="listOrganisationUnitLocation"/>


  <PAGE_PARAMETER NAME="organisationUnitID"/>
  <PAGE_PARAMETER NAME="organisationStructureID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="organisationUnitID"/>
    <TARGET NAME="DISPLAY" PROPERTY="organisationUnitID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="organisationStructureID"/>
    <TARGET NAME="DISPLAY" PROPERTY="organisationStructureID"/>
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL IMAGE="CloseButton" LABEL="ActionControl.Label.Close"/>
  </ACTION_SET>


  <LIST DESCRIPTION="Cluster.Description.LocationDetails" TITLE="Cluster.Label.LocationDetails">


    <FIELD LABEL="Field.Label.LocationName" WIDTH="65">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dtls$locationName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.LocationStatus" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dtls$locationStatus"/>
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>