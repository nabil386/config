<!-- This page allows the user to view a resource home page.                -->
<PAGE
  PAGE_ID="Organization_resourceHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="readResource"
  />


  <PAGE_PARAMETER NAME="resourceID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="resourceID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="resourcesKeyStruct$resourceID"
    />
  </CONNECT>
  <CLUSTER LABEL_WIDTH="15">


    <FIELD LABEL="Field.Label.DataField">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dataField"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreationDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="getResourcesDetails$description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
