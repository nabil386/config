<!-- This page allows the user to create a currency exchange. -->
<PAGE
  PAGE_ID="Organization_createCurrencyExchange"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ACTION"
    OPERATION="createCurrencyExchange"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="organizationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organizationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="organisationID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="35">
    <FIELD LABEL="Field.Label.Currency">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="currencyTypeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Rate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.RateFromDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rateFromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.RateToDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rateToDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
