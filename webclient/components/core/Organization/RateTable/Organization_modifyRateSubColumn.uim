<!-- This process allows the user to modify rate column details.            -->
<PAGE
  PAGE_ID="Organization_modifyRateSubColumn"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="DISPLAY"
    OPERATION="readRateColumn"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="ACTION"
    OPERATION="modifySubColumn"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="RateSubColumnID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RateSubColumnID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readRateColumnKey$rateColumnID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RateSubColumnID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateColumnID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="parentColumnID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="parentColumnID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="1"
    SHOW_LABELS="true"
    TITLE="Cluster.Title.Details"
  >
    <FIELD LABEL="Field.Label.MinVal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateColumnKeyMin"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="minValue"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MaxVal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateColumnKeyMax"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="maxValue"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>