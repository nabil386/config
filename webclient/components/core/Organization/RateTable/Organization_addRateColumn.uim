<!-- This process allows the user to enter rate column details.             -->
<PAGE
  PAGE_ID="Organization_addRateColumn"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="ACTION"
    OPERATION="createRateColumn"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="rateHeaderID"/>
  <PAGE_PARAMETER NAME="ColumnIndex"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateHeaderID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateHeaderID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ColumnIndex"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateColumnIndex"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="1"
    SHOW_LABELS="true"
    TITLE="Cluster.Title.Details"
  >
    <!-- BEGIN, CR00080745, MM -->
    <FIELD
      LABEL="Field.Label.ColumnType"
      WIDTH="40"
    >
      <!-- END, CR00080745 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="columnType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
