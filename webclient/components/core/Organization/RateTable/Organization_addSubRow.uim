<!-- This page allows the user to a sub row to a rate table                 -->
<PAGE
  PAGE_ID="Organization_addSubRow"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="ACTION"
    OPERATION="createSubRow"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="rateHeaderID"/>
  <PAGE_PARAMETER NAME="RateRowID"/>
  <PAGE_PARAMETER NAME="SubRowIndex"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateHeaderID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateHeaderID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RateRowID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="parentRowID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="SubRowIndex"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateRowIndex"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    >
        
      <!-- <LINK PAGE_ID="Organization_modifyRateTable" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="" />
            <TARGET NAME="PAGE" PROPERTY="" />
          </CONNECT>
        </LINK> -->
      </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.AddRow"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
