<!-- This page allows the user manage rate table rows.                      -->
<PAGE
  PAGE_ID="Organization_manageRateRow"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="RateRowID"/>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Choice"
  >


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.Modify">
        <LINK
          DISMISS_MODAL="false"
          PAGE_ID="Organization_modifyRateRow"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="RateRowID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="RateRowID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.Delete">
        <LINK
          DISMISS_MODAL="false"
          PAGE_ID="Organization_deleteRow"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="RateRowID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="RateRowID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


  </CLUSTER>


</PAGE>
