<!-- This process allows the user to enter rate table details.              -->
<PAGE
  PAGE_ID="Organization_viewRateTable"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="DISPLAY"
    OPERATION="readRateTableReadOnly"
  />


  <PAGE_PARAMETER NAME="rateHeaderID"/>
  <PAGE_PARAMETER NAME="rateTableType"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateHeaderID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readRateTableKey$rateHeaderID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateTableType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readRateTableDetails$rateTableType"
    />
  </CONNECT>


  <CLUSTER WIDTH="50">
    <FIELD LABEL="Field.Label.EffectiveDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TableType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateTableType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.RateTable"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateTableDataString"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
