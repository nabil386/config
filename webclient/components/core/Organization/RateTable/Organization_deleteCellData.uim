<!-- This page allows the user to delete cell data from a rate cell         -->
<PAGE
  PAGE_ID="Organization_deleteCellData"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="DISPLAY"
    OPERATION="readRateCellData"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="ACTION"
    OPERATION="removeRateCellData"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="RateCellID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RateCellID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readRateCellKey$rateCellID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RateCellID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateCellID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    >
        
      <!-- <LINK PAGE_ID="Organization_modifyRateTable" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="" />
            <TARGET NAME="PAGE" PROPERTY="" />
          </CONNECT>
        </LINK> -->
      </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeleteCell"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
