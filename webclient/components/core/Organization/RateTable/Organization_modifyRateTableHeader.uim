<!-- This page allows the user to modify rate table header details.         -->
<PAGE
  PAGE_ID="Organization_modifyRateTableHeader"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="DISPLAY"
    OPERATION="readRateTableHeader"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="RateTable"
    NAME="ACTION"
    OPERATION="modifyRateTableHeader"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="rateHeaderID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateHeaderID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readRateTableKey$rateHeaderID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rateHeaderID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateHeaderID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="rateTableType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateTableType"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="rateStatus"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="rateStatus"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <!-- <LINK PAGE_ID="Organization_modifyRateTable" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="rateHeaderID"/>
          <TARGET NAME="PAGE" PROPERTY="rateHeaderID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL> -->


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
    <!-- <LINK PAGE_ID="Organization_modifyRateTable" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="rateHeaderID"/>
          <TARGET NAME="PAGE" PROPERTY="rateHeaderID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL> -->


  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    SHOW_LABELS="true"
    TITLE="Cluster.Title.Details"
  >
    <FIELD LABEL="Field.Label.EffectiveDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TableType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rateTableType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00408407, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00408407 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
