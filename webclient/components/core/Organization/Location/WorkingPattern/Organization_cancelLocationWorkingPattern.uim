<!-- This page allows the user to cancel a location working pattern.        -->
<PAGE PAGE_ID="Organization_cancelLocationWorkingPattern" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="ACTION" OPERATION="cancelLocationWorkingPattern" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="workingPatternID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workingPatternID"/>
    <TARGET NAME="ACTION" PROPERTY="workingPatternID"/>
  </CONNECT>


  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="locationID"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
     
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.DeleteWorkingPattern"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>