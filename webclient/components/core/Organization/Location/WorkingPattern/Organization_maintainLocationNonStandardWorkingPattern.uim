<!-- This page allows the user to create a location working pattern.        -->
<PAGE PAGE_ID="Organization_maintainLocationNonStandardWorkingPattern" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Organization" NAME="DISPLAY" OPERATION="readLocationNonStandardWorkingPattern" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Organization" NAME="ACTION" OPERATION="maintainLocationNonStandardWorkingPattern" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="description"/>


  <PAGE_PARAMETER NAME="workingPatternID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workingPatternID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$workingPatternID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workingPatternID"/>
    <TARGET NAME="ACTION" PROPERTY="workingPatternID"/>
  </CONNECT>


  <PAGE_PARAMETER NAME="dayNumber"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="dayNumber"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$dayNumber"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="dayNumber"/>
    <TARGET NAME="ACTION" PROPERTY="dayNumber"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="22.5" NUM_COLS="1">
    <FIELD LABEL="Field.Label.Type" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="typeCode"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="typeCode"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.Time">
    <FIELD LABEL="Field.Label.StartTime">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="activePatternStartTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="activePatternStartTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndTime">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="activePatternEndTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="activePatternEndTime"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>