<PAGE
  PAGE_ID="Organization_resolveLocationResource"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
      curam.omega3.request.RequestHandler
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
          
 
      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      
         String locStructureID = request.getParameter("locationStructureID");
         String locID = request.getParameter("locationID");
         
      String url = "";
      
    if (locStructureID == null) {
    // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "locationStructureID");
       System.out.println(e);
       throw e;
       // END, CR00110216
    }
      
         if (locID == null) {
    // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "locationID");
       System.out.println(e);
       throw e;
       
       }
         
         
      
    url = context + "Organization_createLocationResourcePage.do" + "?locationID="
    + curam.omega3.request.RequestUtils.escapeURL(locID) + "&amp;" + "locationStructureID=" + curam.omega3.request.RequestUtils.escapeURL(locStructureID) + "&amp;o3sfs=true&amp;norefresh=true";
      // END, CR00110216
      String resolve = request.getParameter("isResolve");
      if(resolve != null){
       url += "&amp;isResolve=true";  
      }
                 
      url += "&amp;" + rh.getSystemParameters();
      
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
