      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_createDailySchedule"
      >
        <!-- END, CR00100495  -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="locationDetails$locationID"
