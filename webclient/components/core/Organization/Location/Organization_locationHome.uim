<!-- Organization location home page. -->
<PAGE
  PAGE_ID="Organization_locationHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SHORTCUT_TITLE ICON="location.icon">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="locationContextDescriptionDetails$description"
      />
    </CONNECT>
  </SHORTCUT_TITLE>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="readLocation"
  />
  <PAGE_PARAMETER NAME="locationID"/>
  <PAGE_PARAMETER NAME="locationStructureID"/>


  <ACTION_SET>
    <ACTION_CONTROL
      IMAGE="LocationStructure"
      LABEL="ActionControl.Label.CreateSchedule"
    >
