<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Assigns a resource to an organization unit.                            -->
<PAGE
  PAGE_ID="Organization_assignResourceToOrganizationUnit"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ACTION"
    OPERATION="assignResourceToOrgUnit"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="organisationStructureID"/>
  <PAGE_PARAMETER NAME="organizationUnitID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organizationUnitID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="organisationUnitID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organisationStructureID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="organisationStructureID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="40">
    <FIELD LABEL="Field.Label.Resource">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="resourceID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Apply">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="applyIndicator"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
</PAGE>
