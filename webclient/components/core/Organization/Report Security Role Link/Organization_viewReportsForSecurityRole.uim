<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This process allows the user to view the reports associated with the   -->
<!-- the security role.                                                     -->
<PAGE
  PAGE_ID="Organization_viewReportsForSecurityRole"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="listReportForSecurityRole"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="roleName"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="roleName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$roleName"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL LABEL="ActionControl.Label.NewReport">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_addReportToSecurityRole"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$roleName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="roleName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >
      <LINK PAGE_ID="Organization_listSecurityRole"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="15"
    >


      <ACTION_CONTROL LABEL="ActionControl.Label.View">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewReport"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="reportID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="reportID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_removeReportFromSecurityRole"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="reportSecurityRoleLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="reportSecurityRoleLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="roleName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="roleName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </CONTAINER>
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.DateCreated"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateCreated"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
