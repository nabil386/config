<!-- This page allows the user to select the evidence solution that they    -->
<!-- would like to configure.                                               -->
<PAGE
  PAGE_ID="Organization_userEvidenceApproval"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE DESCRIPTION="PageTitle.Description">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="listTemporalEvidenceApprovalCheckByUser"
  />


  <PAGE_PARAMETER NAME="userName"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$userName"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">


    <CONTAINER ALIGNMENT="LEFT">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Temporal"
        TYPE="ACTION"
      >
        <LINK
          PAGE_ID="Organization_listUserTemporalEvidenceApproval"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER ALIGNMENT="LEFT">
      <ACTION_CONTROL LABEL="ActionControl.Label.Trees">
        <LINK
          PAGE_ID="Organization_listUserEvidenceApproval"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>


        </LINK>


      </ACTION_CONTROL>


    </CONTAINER>


  </CLUSTER>


</PAGE>
