<!-- This jsp redirects the user to the Organization Structure browser.-->
<PAGE
  PAGE_ID="Organization_resolveOrgStructureBrowserElement"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String organisationStructureID = request.getParameter("organisationStructureID");    
      String url = "";
  
      if (organisationStructureID == null) {
  
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "ID");
       System.out.println(e);
       throw e;
            
      
      } else {
      
        url = context + 
           "Organization_orgStructureBrowserPage.do?organisationStructureID=" 
     + curam.omega3.request.RequestUtils.escapeURL(organisationStructureID) + "&amp;elementID=" 
            + curam.omega3.request.RequestUtils.escapeURL(organisationStructureID) + "&amp;elementType=ELT4" ;
      
      }
      // END, CR00110216 
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
