<!-- This process allows the user to view a list of user skills for a user.                -->
<PAGE
  PAGE_ID="Organization_listUserSkillsForUser"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <!-- BEGIN, CR00322668, VT -->


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="getUserSkills"
    PHASE="DISPLAY"
  />
  <!-- END, CR00322668 -->


  <PAGE_PARAMETER NAME="userName"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="userSkillList$userName"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_createUserSkill1"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$description$description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <!-- BEGIN, CR00050298, MR -->
  <!-- BEGIN, HARP 64908, SP -->
  <LIST>
    <!-- END, HARP 64908 -->
    <!-- END, CR00050298 -->


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_modifyUserSkillFromList"
        >
          <CONNECT>
            <!-- BEGIN, CR00322668, VT -->
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$listUserSkillDetails$userSkillDtlsList$dtls$userSkillID"
            />
            <!-- END, CR00322668 -->


            <TARGET
              NAME="PAGE"
              PROPERTY="userSkillID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$description$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_cancelUserSkill"
        >
          <CONNECT>


            <!-- BEGIN, CR00322668, VT -->
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$listUserSkillDetails$userSkillDtlsList$dtls$userSkillID"
            />
            <!-- BEGIN, CR00322668 -->


            <TARGET
              NAME="PAGE"
              PROPERTY="userSkillID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$description$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <!-- BEGIN, CR00322668, VT -->
      <ACTION_CONTROL LABEL="ActionControl.Label.ManageLanguages">


        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="languageListIndicator"
          />
        </CONDITION>


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_modifyUserLanguageSkills"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$listUserSkillDetails$userSkillDtlsList$dtls$userSkillID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userSkillID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$description$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <!-- END, CR00322668 -->


    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.SkillType"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="skillType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Status"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Organization_viewUserSkill">
        <CONNECT>


          <!-- BEGIN, CR00322668, VT -->
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$listUserSkillDetails$userSkillDtlsList$dtls$userSkillID"
          />
          <!-- BEGIN, CR00322668, VT -->


          <TARGET
            NAME="PAGE"
            PROPERTY="userSkillID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$description$description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
