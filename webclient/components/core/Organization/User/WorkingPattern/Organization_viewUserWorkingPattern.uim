<!-- This page allows the user to create a working pattern.                 -->
<PAGE
  PAGE_ID="Organization_viewUserWorkingPattern"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="readUserWorkingPattern"
  />


  <PAGE_PARAMETER NAME="workingPatternID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="workingPatternID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="maintainWorkingPatternKey$workingPatternID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Label.Default"
  >


    <FIELD LABEL="Field.Label.StartTime">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activePatternStartTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndTime">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activePatternEndTime"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <LIST TITLE="List.Title.Days">
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.EditNonStandard">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_maintainUserNonStandardWorkingPattern"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="workingPatternID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="workingPatternID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dayNumber"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="dayNumber"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.DayNumber"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dayNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="45"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="typeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartTime"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="stringActivePatternStartTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndTime"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="stringActivePatternEndTime"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
