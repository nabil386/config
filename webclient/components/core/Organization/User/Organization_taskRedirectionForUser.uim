<!-- ===========                                                 -->
<!-- This page allows the user to create redirection period for tasks.        -->
<!-- BEGIN, CR00414190, VT -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Organization_taskRedirectionForUser"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00414190 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <!-- BEGIN, CR00216807 MN -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="getTaskRedirectionHistoryForUser"
    PHASE="DISPLAY"
  />
  <!-- END, CR00216807 -->


  <!-- BEGIN, CR00225492 MN -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ACTION"
    OPERATION="taskRedirectionsForUserDetails"
    PHASE="ACTION"
  />
  <!-- END, CR00225492 -->
  <!-- BEGIN, CR00414190, VT-->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ISUSERCLOSED"
    OPERATION="isUserClosed"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="ISUSERCLOSED"
      PROPERTY="key$userName"
    />
  </CONNECT>
  <!-- END, CR00414190-->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$userName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$fromUserName"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="userFullName"/>
  <PAGE_PARAMETER NAME="userName"/>


  <!-- BEGIN, CR00161962, KY -->
  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="1"
    TITLE="Cluster.Title.NewTaskRedirection"
  >
    <CONTAINER LABEL="Container.Label.RedirectTo">
      <FIELD
        USE_BLANK="true"
        USE_DEFAULT="false"
        WIDTH="30"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="key$toRedirectType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="60">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="key$toRedirectID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00161962 -->


    <FIELD
      LABEL="Field.Label.StartDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$startDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$endDateTime"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.ActiveAndPendingRedirections">


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- BEGIN, CR00414190, VT -->
      <CONDITION>
        <IS_FALSE
          NAME="ISUSERCLOSED"
          PROPERTY="closeUserInd"
        />
      </CONDITION>
      <!-- END, CR00414190 -->


      <ACTION_CONTROL LABEL="ActionControl.Label.Clear">
        <!-- BEGIN, CR00161962, KY -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_clearTaskRedirectionForUser"
          SAVE_LINK="true"
        >
          <!-- END, CR00161962 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$taskRedirectionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="redirectedTaskID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$startDateTime"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="startDateTime"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$endDateTime"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="endDateTime"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNum"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userFullName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userFullName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <!-- BEGIN, CR00161962, KY -->
    <FIELD
      LABEL="Field.Label.ActivePendingRedirectTo"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$targetObjectName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ActivePendingRedirectType"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$toRedirectType"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00161962 -->
    <FIELD
      LABEL="Field.Label.ActivePendingStartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$startDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ActivePendingEndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$endDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ActivePendingStatus"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activeAndPendingTaskRedirectionDetails$activeAndPendingDetails$redirectionStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SaveButton"
      LABEL="ActionControl.SaveButton.label"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.CancelButton.label"
    />
  </ACTION_SET>


</PAGE>
