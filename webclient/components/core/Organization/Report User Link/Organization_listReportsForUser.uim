<!-- This process allows the user to view his report list.                  -->
<PAGE
  PAGE_ID="Organization_listReportsForUser"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="listReportForUser"
    PHASE="DISPLAY"
  />


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL LABEL="ActionControl.Label.NewReport">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_addReportToUser"
      >
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_removeReportFromUser"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="reportUserLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="reportUserLinkID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.DateCreated"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateCreated"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Organization_viewReport">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reportID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="reportID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>
</PAGE>
