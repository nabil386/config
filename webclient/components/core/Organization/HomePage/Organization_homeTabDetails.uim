<!-- Reads a Organization tab display details.                              -->
<PAGE
  PAGE_ID="Organization_homeTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="name"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="name"
      />
    </CONNECT>
  </TAB_NAME>


  <SERVER_INTERFACE
    CLASS="AdminTab"
    NAME="DISPLAY"
    OPERATION="readOrganisationTabDetails"
    PHASE="DISPLAY"
  />


  <!-- BEGIN, CR00465109, EC -->
  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    SUMMARY="Summary.Organization.HomeTab"
  >
    <!-- END, CR00465109 -->


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PhoneNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="formattedPhoneNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TaxNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="taxNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Address">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="formattedAddressData"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EmailAddress">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="emailAddress"
        />
      </CONNECT>
      <LINK
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="emailAddressLink"
      />
    </FIELD>


    <FIELD LABEL="Field.Label.RegistrationNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="registrationNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.WebAddress">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="webAddress"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="webAddress"
      />
    </FIELD>


  </CLUSTER>


</PAGE>
