<PAGE PAGE_ID="DUIM_addDynamicPage">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DynamicUIMAdmin"
    NAME="ACTION"
    OPERATION="addDynamicUimPage"
    PHASE="ACTION"
  />


  <CLUSTER TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.PageID">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="pageID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Locale"
	  USE_DEFAULT="true">
	  <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="locale" />
	  </CONNECT>
    </FIELD>
    <WIDGET
      LABEL="Field.Label.UIM"
      TYPE="FILE_UPLOAD"
    >
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="uimContents"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
    <WIDGET
      LABEL="Field.Label.Properties"
      TYPE="FILE_UPLOAD"
    >
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="propertiesContents"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
  </CLUSTER>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="Control.Label.Cancel"/>
  </ACTION_SET>
</PAGE>
