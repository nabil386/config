<PAGE PAGE_ID="DUIM_validateDynamicPage">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DynamicUIMAdmin"
    NAME="DISPLAY"
    OPERATION="validateDynamicUimPage"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="resourceID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="resourceID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageKey$resourceID"
    />
  </CONNECT>
  <CONNECT>
  	<SOURCE
  	  NAME="TEXT"
  	  PROPERTY="Field.StaticText.Validation.ErrorMsg.1"
  	/>
  	<TARGET
	  NAME="DISPLAY"
	  PROPERTY="input1"
	/>
  </CONNECT>
  <CONNECT>
  	<SOURCE
  	  NAME="TEXT"
  	  PROPERTY="Field.StaticText.Validation.ErrorMsg.2"
  	/>
  	<TARGET
	  NAME="DISPLAY"
	  PROPERTY="input2"
	/>
  </CONNECT>
  <CONNECT>
  	<SOURCE
  	  NAME="TEXT"
  	  PROPERTY="Field.StaticText.Validation.SuccessMsg"
  	/>
  	<TARGET
	  NAME="DISPLAY"
	  PROPERTY="input3"
	/>
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD
      HEIGHT="8"
      LABEL="Field.results"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="details"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
