<!-- Description -->
<!-- =========== -->
<!-- This action allows the user to create a contact that is not registered -->
<!-- with the organization.                                                 -->
<!-- BEGIN, CR00335845, PB -->
<PAGE PAGE_ID="Participant_createUnregisteredContact" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00335845 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Participant" NAME="ACTION" OPERATION="createContactFromUnregisteredParticipant" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="true">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">


      <LINK PAGE_ID="Participant_listContact">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
          <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
        </CONNECT>


      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="createContactFromUnregisteredParticipant$concernRoleID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2" TITLE="Cluster.Title.Contact">


    <FIELD LABEL="Field.Label.ContactName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.FromDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createContactFromUnregisteredParticipant$startDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ContactType">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="contactTypeCode"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ToDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createContactFromUnregisteredParticipant$endDate"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2" TITLE="Cluster.Title.Address">


    <FIELD>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="addressData"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="25" NUM_COLS="1" TITLE="Cluster.Title.Phone">


    <CONTAINER ALIGNMENT="LEFT" LABEL="Field.Label.Phone">
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneCountryCode" WIDTH="10">
        <!-- END, CR00463142 -->
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneCountryCode"/>
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneAreaCode" WIDTH="16">
        <!-- END, CR00463142 -->
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneAreaCode"/>
        </CONNECT>
      </FIELD>


      <!-- BEGIN CR00086083, MR-->
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneNumber" WIDTH="8" WIDTH_UNITS="CHARS">
        <!-- END, CR00463142 -->
        <!--END CR00086083 -->


        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneNumber"/>
        </CONNECT>
      </FIELD>


    </CONTAINER>


    <FIELD LABEL="Field.Label.ContactPhoneExtension" WIDTH="10">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="phoneExtension"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


    <!-- BEGIN, CR00021339, CMB-->
    <!--BEGIN, HARP 64088, PAD-->
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createContactFromUnregisteredParticipant$comments"/>
      </CONNECT>
    </FIELD>
    <!--END, HARP 64088 -->
    <!--END, CR00021339 -->


  </CLUSTER>
</PAGE>