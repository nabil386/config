<!-- This Page allows users to view a Phone Number.                         -->
<PAGE
  PAGE_ID="Participant_viewPhoneNumber"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Participant"
    NAME="DISPLAY"
    OPERATION="readPhoneNumber"
  />


  <PAGE_PARAMETER NAME="concernRolePhoneNumberID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRolePhoneNumberID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readConcernRolePhoneKey$concernRolePhoneNumberID"
    />
  </CONNECT>


  <!-- BEGIN, CR00305982, IBM -->
  <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Field.Label.CountryCode">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="phoneCountryCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PhoneNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="phoneNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AreaCode">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="phoneAreaCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PhoneExtension">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="phoneExtension"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- END, CR00305982 -->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Comments"
  >


    <!-- BEGIN, CR00416277, VT -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
