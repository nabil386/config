<!-- This Page allows users to create a Phone number for a Participant.     -->
<!-- BEGIN, CR00334810, PB -->
<!-- BEGIN, CR00357432, SS -->
<PAGE PAGE_ID="Participant_createPhoneNumber" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00334810 -->
  <!-- END, CR00357432 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Participant" NAME="ACTION" OPERATION="createPhoneNumber" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="concernRoleID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="30" NUM_COLS="1">


    <CONTAINER ALIGNMENT="LEFT" LABEL="Field.Label.Phone">
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneCountryCode" WIDTH="15">
        <!-- END, CR00463142 -->
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneCountryCode"/>
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneAreaCode" WIDTH="20">
        <!-- END, CR00463142 -->
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneAreaCode"/>
        </CONNECT>
      </FIELD>


      <!-- BEGIN CR00086083, MR-->
      <!-- BEGIN, CR00463142, EC -->
      <FIELD LABEL="Field.Label.PhoneNumber" WIDTH="8" WIDTH_UNITS="CHARS">
        <!-- END, CR00463142 -->
        <!--END CR00086083 -->


        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneNumber"/>
        </CONNECT>
      </FIELD>


    </CONTAINER>


    <FIELD LABEL="Field.Label.Extension" WIDTH="24">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="phoneExtension"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Type" WIDTH="40">
      <!-- END, CR00141707 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="typeCode"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00141707, MC -->
    <FIELD LABEL="Field.Label.From">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.To" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Primary">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="primaryPhoneInd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>