<PAGE
  PAGE_ID="RegisterParticipant_resolveDuplicateCheck"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <JSP_SCRIPTLET>
      
      
      curam.omega3.request.RequestHandler rh = 
      curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(
      pageContext.getSession()).getLocale() + "/"; 
      
      String concernRoleType = request.getParameter("concernRoleType");
      String relatedConcernID = request.getParameter("relatedConcernID");
      
      if (concernRoleType == null) {
       
       core.ScriptletMissingParamException e = new
           core.ScriptletMissingParamException(-20002, "concernRoleType");
       System.out.println(e);
       throw e;       

      }

      if (relatedConcernID == null) {
       
       core.ScriptletMissingParamException e = new
           core.ScriptletMissingParamException(-20002, "relatedConcernID");
       System.out.println(e);
       throw e;       

      }

      String resolvedPageName = "";
      
      if (concernRoleType.equals("RL1")) {
        resolvedPageName = "Person_duplicateRegistrationCheck";
      } else if (concernRoleType.equals("RL2")) {
        resolvedPageName = "Employer_duplicateRegistrationCheck";
      } else if (concernRoleType.equals("RL3")) {
        resolvedPageName = "ServiceSupplier_duplicateRegistrationCheck";
      } else if (concernRoleType.equals("RL5")) {
        resolvedPageName = "InformationProvider_duplicateRegistrationCheck";
      } else if (concernRoleType.equals("RL17")) {
        resolvedPageName = "ExternalParty_duplicateRegistrationCheck";
      }
      
      String url = context + resolvedPageName + "Page.do?relatedConcernID="
        + curam.omega3.request.RequestUtils.escapeURL(relatedConcernID) + "&amp;" + rh.getSystemParameters();

      response.sendRedirect(response.encodeRedirectURL(url));

   
  </JSP_SCRIPTLET>


</PAGE>
