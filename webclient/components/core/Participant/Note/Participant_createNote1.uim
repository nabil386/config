<!-- This Page creates a Note for to a particular participant -->
<PAGE PAGE_ID="Participant_createNote1" WINDOW_OPTIONS="width=700,height=600" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Participant" NAME="ACTION" OPERATION="createNote" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="details$details$noteDetails$participantNoteDetails$participantID"/>
  </CONNECT>


    <CLUSTER LABEL_WIDTH="20">


      <FIELD LABEL="Field.Label.Subject">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="details$details$noteDetails$noteDetails$subjectText"/>
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <!-- BEGIN, CR00335793, SSK -->
    <CLUSTER NUM_COLS="2" LABEL_WIDTH="40">
      <CLUSTER>
        <FIELD LABEL="Field.Label.Priority">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="details$details$noteDetails$noteDetails$priorityCode"/>
          </CONNECT>
        </FIELD>
      </CLUSTER>
      <CLUSTER>
        <FIELD LABEL="Field.Label.Sensitivity">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="details$details$noteDetails$noteDetails$sensitivityCode"/>
          </CONNECT>
        </FIELD>
      </CLUSTER>
    </CLUSTER>
    <!-- END, CR00335793 -->

  <CLUSTER SHOW_LABELS="FALSE" STYLE="cluster-rte-no-padding">
    <FIELD HEIGHT="200" LABEL="Field.Label.Text">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$details$noteDetails$noteDetails$notesText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>