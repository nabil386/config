<!-- This page is used to display the details of a liability financial      -->
<!-- line item.                                                             -->
<?curam-deprecated Since Curam 6.0, replaced by Financial_viewLiabilityLineItem.uim?>
<PAGE
  PAGE_ID="Participant_viewLiabilityLineItem"
  WINDOW_OPTIONS="width=775"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL LABEL="ActionControl.Label.WriteOff">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_createWriteOff"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
        </CONNECT>
        <!-- BEGIN, CR00160310, MC -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>


      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Reverse">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_createLineItemReversal"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <!-- END, CR00160310 -->
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >
      <LINK
        PAGE_ID="Participant_viewLiabilityInstruction"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="finInstructionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="finInstructionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- BEGIN, CR00160135, MC -->
  <INCLUDE FILE_NAME="Participant_viewLineItemView.vim"/>
  <!-- END, CR00160135 -->


</PAGE>
