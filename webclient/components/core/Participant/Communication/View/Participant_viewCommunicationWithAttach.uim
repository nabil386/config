<!-- This page allows the user to view the details of a communication       -->
<!-- that does have an attached file.                                       -->
<!-- BEGIN, CR00237138, NS -->
<?curam-deprecated Since Curam 6.0, replaced with Participant_viewCommunicationWithAttach1.uim. See release note: CR00237138?>
<!-- END, CR00237138 -->
<PAGE
  PAGE_ID="Participant_viewCommunicationWithAttach"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="DeleteButton"
      LABEL="ActionControl.DeleteButton.label"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_deleteCommunicationWithAttach"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="communicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="communicationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="subject"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="subject"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>
  <INCLUDE FILE_NAME="Participant_viewCommunicationView.vim"/>
  <CLUSTER
    LABEL_WIDTH="25"
    TITLE="Cluster.Title.AttachedFile"
  >
    <CONTAINER LABEL="Field.Label.Name">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="newAttachmentName"
          />
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL
        APPEND_ELLIPSIS="false"
        LABEL="ActionControl.Label.OpenAttachment"
        TYPE="FILE_DOWNLOAD"
      >
        <LINK>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="attachmentID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="attachmentID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>
</PAGE>
