<!-- This page allows the user to create a new email communication.         -->
<PAGE
  PAGE_ID="Participant_getEmailCorrespondent"
  WINDOW_OPTIONS="width=650"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="pageDescription"/>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <!--BEGIN, CR00229271, SS-->
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <!--BEGIN CR00080805, GBA-->
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="true"
        PAGE_ID="Participant_createEmailCommunication"
        SAVE_LINK="false"
      >
        <!--END CR00080805-->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$details$correspondentConcernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="correspondentParticipantRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$details$correspondentType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="correspondentParticipantRoleType"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$details$correspondentName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="correspondentName"
          />
        </CONNECT>
        <!-- BEGIN CR00096148, DJ-->
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$details$caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
        <!--END CR00096148-->
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
    <!--END, CR00229271-->


  </ACTION_SET>


  <INCLUDE FILE_NAME="Participant_getCorrespondent.vim"/>


</PAGE>
