<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view a list of communication exceptions for both the     -->
<!-- original and duplicate Participants. The page allows a user merge      -->
<!-- selected duplicate participant communication exception records with the original       -->
<!-- participant                                                            -->
<PAGE
  PAGE_ID="Participant_mergeCommunicationExceptions"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="DISPLAY"
    OPERATION="listCommExceptionsForMerge"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="ACTION"
    OPERATION="mergeCommExceptions"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="concernRoleMergeID"/>
  <PAGE_PARAMETER NAME="originalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleMergeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleMergeID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST TITLE="List.Title.CommExceptions">


    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeCommunicationException">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtlsList$origList$dtls$commExceptionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="commExceptionID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->


    <FIELD
      LABEL="Field.Label.Method"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$origList$dtls$typeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Reason"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$origList$dtls$reasonCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.FromDate"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$origList$dtls$fromDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ToDate"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$origList$dtls$toDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$origList$dtls$status"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <LIST
    DESCRIPTION="List.MergeCommExceptions.Description"
    TITLE="List.Title.MergeCommExceptions"
  >


    <CONTAINER
      LABEL="Container.Label.Select"
      WIDTH="7"
    >


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dupList$dtls$commExceptionID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="mergeTabList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


      </WIDGET>


    </CONTAINER>


    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeCommunicationException">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dupList$dtls$commExceptionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="commExceptionID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->
    <FIELD
      LABEL="Field.Label.Method"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$dupList$dtls$typeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Reason"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$dupList$dtls$reasonCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.FromDate"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$dupList$dtls$fromDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ToDate"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$dupList$dtls$toDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$dupList$dtls$status"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
