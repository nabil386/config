<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view a list of alternative names for both -->
<!-- the original and duplicate Participants. The page allows a user merge  -->
<!-- selected duplicate participant alternative name records with the       -->
<!-- original participant                                                   -->
<PAGE
  PAGE_ID="Participant_mergeAlternativeNames"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="DISPLAY"
    OPERATION="listAlternativeNamesForMerge"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="ACTION"
    OPERATION="mergeAlternativeNames"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="originalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="concernRoleMergeID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleMergeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleMergeID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST TITLE="List.Title.AlternativeNames">


    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeAlternateName">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="origList$dtls$alternateNameID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="alternateNameID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->


    <FIELD
      LABEL="Field.Label.FirstName"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$dtls$firstForename"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.MiddleName"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$dtls$otherForename"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Surname"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$dtls$surname"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00332735, GA -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="20"
    >
      <!-- END, CR00332735 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$dtls$nameType"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00332735, GA -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <!-- END, CR00332735 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$dtls$nameStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <LIST
    DESCRIPTION="List.MergeAlternativeNames.Description"
    TITLE="List.Title.MergeAlternativeNames"
  >


    <CONTAINER WIDTH="7">


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dupList$dtls$alternateNameID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="mergeTabList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


      </WIDGET>


    </CONTAINER>


    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeAlternateName">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dupList$dtls$alternateNameID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="alternateNameID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->
    <FIELD
      LABEL="Field.Label.FirstName"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$dtls$firstForename"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.MiddleName"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$dtls$otherForename"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Surname"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$dtls$surname"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00332735, GA -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="20"
    >
      <!-- END, CR00332735 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$dtls$nameType"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00332735, GA -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <!-- END, CR00332735 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$dtls$nameStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
