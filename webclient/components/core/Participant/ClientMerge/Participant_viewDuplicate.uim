<!-- Description -->
<!-- =========== -->
<!-- Displays duplicate details for a specified concern role. -->
<PAGE
  PAGE_ID="Participant_viewDuplicate"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00281651, KRK -->
  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="DISPLAY"
    OPERATION="readDuplicateByConcernRole"
    PHASE="DISPLAY"
  />
  <!-- END, CR00281651 -->


  <PAGE_PARAMETER NAME="concernRoleDuplicateID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleDuplicateID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleDuplicateID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.ClientDetails"
  >


    <FIELD LABEL="Field.Label.Original">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="originalConcernRoleName"
        />
      </CONNECT>
      <LINK PAGE_ID="Participant_resolveConcernRoleTypeHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="originalConcernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Label.Duplicate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="duplicateConcernRoleName"
        />
      </CONNECT>
      <LINK PAGE_ID="Participant_resolveConcernRoleTypeHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="duplicateConcernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


  </CLUSTER>


  <CLUSTER TITLE="Cluster.Title.MarkDuplicateDetails">


    <FIELD LABEL="Field.Label.DuplicateDate">
      <CONNECT>
        <!-- BEGIN, CR00281651, KRK -->
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$duplicateDateTime"
        />
        <!-- END, CR00281651 -->
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DuplicateReason">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="duplicateReason"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DuplicateUser">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="duplicateUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="duplicateUser"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.MarkDuplicateComments"
  >
    <FIELD HEIGHT="3">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="duplicateComments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER TITLE="Cluster.Title.MergeDetails">


    <FIELD LABEL="Field.Label.MergeStartDate">
      <CONNECT>
        <!-- BEGIN, CR00281651, KRK -->
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$mergeStartDate"
        />
        <!-- END, CR00281651 -->
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MergeEndDate">
      <CONNECT>
        <!-- BEGIN, CR00281651, KRK -->
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$mergeEndDate"
        />
        <!-- END, CR00281651 -->
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MergeStartedBy">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="mergeStartedUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="mergeStartedBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Label.MergeCompletedBy">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="mergeCompletedUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="mergeCompletedBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


  </CLUSTER>


  <CLUSTER TITLE="Cluster.Title.UnmarkDetails">


    <FIELD LABEL="Field.Label.UnmarkDate">
      <CONNECT>
        <!-- BEGIN, CR00281651, KRK -->
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$unmarkDateTime"
        />
        <!-- END, CR00281651 -->
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.UnmarkReason">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="unmarkReason"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.UnmarkUser">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="unmarkUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="unmarkUser"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.UnmarkComments"
  >
    <FIELD HEIGHT="3">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="unmarkComments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
