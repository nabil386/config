<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view a list of addresses for both the     -->
<!-- original and duplicate Participants. The page allows a user merge      -->
<!-- selected duplicate participant email address records with the original -->
<!-- participant                                                            -->
<PAGE
  PAGE_ID="Participant_mergeEmailAddresses"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="DISPLAY"
    OPERATION="listEmailAddressesForMerge"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="ACTION"
    OPERATION="mergeEmailAddresses"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="originalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="concernRoleMergeID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleMergeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleMergeID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST TITLE="List.Title.EmailAddresses">
    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeEmailAddress">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="origList$details$dtls$concernRoleEmailAddressID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleEmailAddressID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Primary"
      WIDTH="20"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$dtls$primaryInd"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.EmailAddress"
      WIDTH="50"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$dtls$emailAddress"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="15"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$dtls$typeCode"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$dtls$statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <LIST
    DESCRIPTION="List.MergeEmailAddresses.Description"
    TITLE="List.Title.MergeEmailAddresses"
  >


    <CONTAINER WIDTH="7">


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dupList$details$dtls$concernRoleEmailAddressID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="mergeTabList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


      </WIDGET>


    </CONTAINER>
    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeEmailAddress">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dupList$details$dtls$concernRoleEmailAddressID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleEmailAddressID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->
    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Primary"
      WIDTH="20"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$dtls$primaryInd"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.EmailAddress"
      WIDTH="50"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$dtls$emailAddress"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="15"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$dtls$typeCode"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00359032, GA -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <!-- END, CR00359032 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$dtls$statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
