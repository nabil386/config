<!-- This Page lists all Notes belong to a particular participant           -->
<!-- and contains links to view edit and create note.                       -->
<PAGE
  PAGE_ID="Participant_mergeNotes1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="DISPLAY"
    OPERATION="listNotesForMerge1"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ClientMerge"
    NAME="ACTION"
    OPERATION="mergeNotes"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="originalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="concernRoleMergeID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleMergeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleMergeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST TITLE="List.Title.Notes">
    <!-- BEGIN, CR00223827 ,MC -->
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeNote">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="origList$details$participantNoteID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantNoteID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>
    <!-- END, CR00223827 -->


    <FIELD
      LABEL="Field.Label.Subject"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$subjectText"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Text"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$truncatedNoteText"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EnteredBy"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$fullname"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.CreatedModifiedDateTime"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$creationDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Priority"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$priorityCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="origList$details$recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <LIST
    DESCRIPTION="List.Label.Description"
    TITLE="List.Title.MergeNotes"
  >


    <CONTAINER
      LABEL="Container.Label.Select"
      WIDTH="7"
    >


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dupList$details$participantNoteID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="mergeTabList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


      </WIDGET>


    </CONTAINER>


    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewMergeNote">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dupList$details$participantNoteID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantNoteID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.Subject"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$subjectText"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Text"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$truncatedNoteText"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EnteredBy"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$fullname"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.CreatedModifiedDateTime"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$modifiedDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Priority"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$priorityCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dupList$details$recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
