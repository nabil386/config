<!-- This process allows the user to view alternate name details            -->
<!-- for a person. Examples of alternative name types include registered,   -->
<!-- preferred, alias and stage name.                                       -->
<PAGE
  PAGE_ID="Participant_viewMergeAlternateName"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="alternateNameID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="alternateNameID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readPersonNameKey$alternateNameID"
    />
  </CONNECT>
  <INCLUDE FILE_NAME="Participant_viewMergeAlternateNameView.vim"/>
</PAGE>
