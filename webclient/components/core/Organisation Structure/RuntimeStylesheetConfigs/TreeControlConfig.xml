      >
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="Organization_listChildOrgUnits">
              <nc:set-page-param
                from-key="organisationUnitID"
                name="organisationUnitID"
              />
              <nc:set-page-param
                from-key="organisationStructureID"
                name="organisationStructureID"
              />
              <nc:set-page-param
                from-key="nodeID"
                name="nodeID"
              />
            </nc:open-page-action>
          </nc:action>
