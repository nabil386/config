        icon="../Images/rules_set_16x16.gif"
        name="organisationStructure"
      >
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="Organization_orgStructureHome">
              <nc:set-page-param
                from-key="organisationStructureID"
                name="organisationStructureID"
              />
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>


      <nc:node-type
        icon="../Images/objective_group_16x16.gif"
