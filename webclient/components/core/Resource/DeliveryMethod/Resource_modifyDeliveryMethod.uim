<!-- This page allows the user to modify a Delivery Method -->
<PAGE
  PAGE_ID="Resource_modifyDeliveryMethod"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="readDeliveryMethod"
  />


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="ACTION"
    OPERATION="modifyDeliveryMethod"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="deliveryMethodID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deliveryMethodID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="adminDeliveryMethodIDKey$deliveryMethodID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="adminDeliveryMethodIDKey$deliveryMethodID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="deliveryMethodDetails$deliveryMethodID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="nameDesc"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="nameDesc"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.DeliveryMethod">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.BenefitIndicator">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appliesToBenefitInd"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="appliesToBenefitInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Offset"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="offset"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="offset"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.LiabilityIndicator">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appliesToLiabilityInd"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="appliesToLiabilityInd"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
