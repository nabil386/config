<!-- Lists the Delivery Methods for all Products -->
<PAGE
  PAGE_ID="Resource_listDeliveryMethod"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="listDeliveryMethod"
  />


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Resource_createDeliveryMethod"
      />
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_modifyDeliveryMethod"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="deliveryMethodID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="deliveryMethodID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.DeliveryMethod"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Offset"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="offset"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.BenefitIndicator"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appliesToBenefitInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LiabilityIndicator"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appliesToLiabilityInd"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
