<!-- This page allows the user to view a list of rules for an assessment.   -->
<PAGE
  PAGE_ID="Resource_listAssessmentRuleSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="listAssessmentRulesLink"
  />


  <PAGE_PARAMETER NAME="assessmentConfigurationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentConfigurationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="viewAssessmentConfigurationDetailsKey$assessmentConfigurationID"
    />
  </CONNECT>
  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.AssignRuleSet"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Resource_assignAssessmentRuleSet"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="assessmentContextDescription$description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="viewAssessmentConfigurationDetailsKey$assessmentConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="assessmentConfigurationID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_modifyAssessmentRuleSetFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="assessmentRulesLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="assessmentRulesLinkID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_cancelAssessmentRuleSet"
        >


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="assessmentRulesLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="assessmentRulesLinkID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>


        </LINK>


      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Rule"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesName"
        />
      </CONNECT>
      <LINK PAGE_ID="RulesEditor_treeWindow">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="ruleSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="ruleSetID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="statusCode"
        />
      </CONNECT>
    </FIELD>


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Resource_viewAssessmentRuleSet">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="assessmentRulesLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="assessmentRulesLinkID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
