<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page lists the Evidence types associated with the Person          -->
<!-- participant data case.                                                 -->
<PAGE
  PAGE_ID="PDCEvidenceLink_listPerson"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="PDCPerson"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="typeCode"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
    TOP="true"
  >


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.AddEvidence"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="PDCEvidenceLink_create"
      >


        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="PDCPerson"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="typeCode"
          />
        </CONNECT>


      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="PDCEvidenceLink_listView.vim"/>


</PAGE>
