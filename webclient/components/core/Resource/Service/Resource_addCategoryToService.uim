<!-- This page allows the user to add a category to a service.              -->
<PAGE
  PAGE_ID="Resource_addCategoryToService"
  WINDOW_OPTIONS="width=450"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="listServiceCategory"
  />


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="ACTION"
    OPERATION="addCategoriesToService"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="serviceID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="serviceID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="serviceID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="serviceID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="serviceID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">


    <FIELD HEIGHT="7">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="categoriesNotInService$dtls$categoryCode"
          NAME="DISPLAY"
          PROPERTY="categoriesNotInService$dtls$categoryCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="categoryTabList"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
