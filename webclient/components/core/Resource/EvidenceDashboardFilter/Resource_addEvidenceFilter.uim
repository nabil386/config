  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseTypeCode"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$caseType"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseTypeCode"/>
    <TARGET NAME="ACTION" PROPERTY="details$caseType"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseTypeCodeID"/>
    <TARGET NAME="ACTION" PROPERTY="details$caseTypeCodeID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$configList$evidenceType"/>
    <TARGET NAME="ACTION" PROPERTY="details$configList$evidenceType"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$configList$dashBoardFilterEvidenceTypeID"/>
