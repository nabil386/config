<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view a list of Temporal Evidence Approval -->
<!-- Checks                                                                 -->
<PAGE
  PAGE_ID="Resource_listTemporalEvidenceApprovalCheck"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="listTemporalEvidenceApprovalCheckByEvidenceType"
  />


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Resource_createTemporalEvidenceApprovalCheck"
      />
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Resource_viewTemporalEvidenceApprovalCheck">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="temporalEvApprovalCheckID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="temporalEvApprovalCheckID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_modifyTemporalEvidenceApprovalCheckFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="temporalEvApprovalCheckID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="temporalEvApprovalCheckID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_cancelTemporalEvidenceApprovalCheck"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="temporalEvApprovalCheckID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="temporalEvApprovalCheckID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.EvidenceType"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="evidenceType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Percentage"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="percentage"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
