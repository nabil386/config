<!-- This page allows the user to assign an assessment to a screening.      -->
<PAGE
  PAGE_ID="Resource_assignScreeningAssessment"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="ACTION"
    OPERATION="addAssessmentToScreeningConfiguration"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="screeningConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="screeningConfigID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="screeningConfigID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="30">
    <FIELD LABEL="Field.Label.Assessment">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="assessmentConfigurationID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
