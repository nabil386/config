<!-- This page allows the user to view a list of assessments for a screening-->
<PAGE
  PAGE_ID="Resource_listScreeningAssessment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="DISPLAY"
    OPERATION="listScreeningAssessmentWithVersionNo"
  />


  <PAGE_PARAMETER NAME="screeningConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="screeningConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="screeningConfigID"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.AssignAssessment"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Resource_assignScreeningAssessment"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="contextDescription$description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="screeningConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="screeningConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_modifyScreeningAssessmentFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="scrAssConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="scrAssConfigID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_cancelScreeningAssessment"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="scrAssConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="scrAssConfigID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Assessment"
      WIDTH="55"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="assessmentName"
        />
      </CONNECT>
      <LINK PAGE_ID="Resource_assessmentHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="assessmentConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="assessmentConfigurationID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
