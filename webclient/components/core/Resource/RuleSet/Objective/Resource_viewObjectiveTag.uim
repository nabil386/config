<!-- This page views a tags details for an objective.                       -->
<PAGE
  PAGE_ID="Resource_viewObjectiveTag"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="readRulesObjectiveTag"
  />


  <PAGE_PARAMETER NAME="rulesObjectiveTagID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rulesObjectiveTagID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$rulesObjectiveTagID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.Recurrence">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recurrencePattern"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Objective">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesObjectiveID"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
