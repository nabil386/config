<!-- This page allows the user to view a list of tags for an objective.     -->
<PAGE
  PAGE_ID="Resource_listObjectiveTag"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listRulesObjectiveTag"
  />


  <PAGE_PARAMETER NAME="rulesObjectiveID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rulesObjectiveID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="rulesObjectiveID"
    />
  </CONNECT>


  <LIST>


    <FIELD
      LABEL="Field.Label.Tag"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesObjectiveTagID"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Value"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="value"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Resource_viewObjectiveTag">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="rulesObjectiveTagID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="rulesObjectiveTagID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
