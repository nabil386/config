<!-- This page allows the user to create a new integrated case type.        -->
<!-- BEGIN, CR00238748, GD -->
<?curam-deprecated Since Curam 6.0, replaced by Resource_createIntegratedCaseType1.uim?>
<!-- END, CR00238748 -->
<!-- Resource_createIntegratedCase page should be used to access dynamic    -->
<!-- evidence.                                                              -->
<PAGE
  PAGE_ID="Resource_createIntegratedCaseType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ACTION"
    OPERATION="createAdminIntegratedCase"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="SaveAndNewButton"
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Resource_createIntegratedCaseType"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="35"
    TITLE="Cluster.Title.Details"
  >


    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="integratedCaseType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.HomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="homePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.MemberHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="memberHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.EmployerHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="employerHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.InfoProviderHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="infoProviderHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.PersonHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="personHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.ProspectPersonHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ppHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.ProductProviderHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="productProviderHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.RepresentativeHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="representativeHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.ServiceSupplierHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="serviceSupplierHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.UtilityHomePage"
      WIDTH="25"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="utilityHomePageName"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
