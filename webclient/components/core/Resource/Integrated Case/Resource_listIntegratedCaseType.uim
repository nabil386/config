<!-- This page lists the integrated case types available in the system.     -->
<?curam-deprecated Since Curam 6.0, replaced by Resource_listIntegratedCaseType1.uim?>
<PAGE
  PAGE_ID="Resource_listIntegratedCaseType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="listActiveAdminIntegratedCase"
  />


  <ACTION_SET BOTTOM="false">


    <!--BEGIN CR00096174, PN-->
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
    <!-- BEGIN CR00227516,MJ-->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Resource_createIntegratedCase"
      />
    <!-- END CR00227516,MJ-->
    
    </ACTION_CONTROL>
    <!--END CR00096174-->
  </ACTION_SET>

  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <!--BEGIN CR00096174, PN-->
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_modifyIntegratedCaseTypeFromList1"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adminIntegratedCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adminIntegratedCaseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!--END CR00096174-->


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Resource_cancelIntegratedCaseType"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adminIntegratedCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adminIntegratedCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="30"
    >
      <LINK PAGE_ID="Resource_viewIntegratedCaseType1">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="adminIntegratedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="adminIntegratedCaseID"
          />
        </CONNECT>
      </LINK>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="integratedCaseType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.HomePage"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="homePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.MemberHomePage"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="memberHomePageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
