<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to link Evidence Metadata with an Integrated -->
<!-- Case                                                                   -->
<PAGE
  PAGE_ID="Resource_addEvidenceTypesLink"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="EvidenceTypesLink"
    NAME="TARGET"
    OPERATION="createICEvidenceLink"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="EvidenceTypesLink"
    NAME="DISPLAY"
    OPERATION="listCurrentActiveICEvidenceMetadata"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="adminIntegratedCaseID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="adminIntegratedCaseID"
    />
    <TARGET
      NAME="TARGET"
      PROPERTY="associatedID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="adminIntegratedCaseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="adminIntegratedCaseID"
    />
  </CONNECT>
  <!-- BEGIN, CR00074941 MMC -->
  <CLUSTER LABEL_WIDTH="40">
    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.Type"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="evidenceTypeIDAndCode"
          NAME="DISPLAY"
          PROPERTY="evidenceTypeCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="TARGET"
          PROPERTY="evidenceTypeIDAndCode"
        />
      </CONNECT>
    </FIELD>
    
   <!-- BEGIN, CR00223623 EC -->
       
    <FIELD
      LABEL="Category.Field.Label.Type"
    >
      <CONNECT>
        <TARGET
          NAME="TARGET"
          PROPERTY="category"
        />
      </CONNECT>
    </FIELD>
    
    
     <FIELD
      LABEL="SortOrder.Field.Label.Type"
      USE_DEFAULT="false"
      WIDTH="10"
     >
      <CONNECT>
        <TARGET
          NAME="TARGET"
          PROPERTY="sortOrder"
        />
      </CONNECT>
    </FIELD>

    <FIELD
      LABEL="QuickLink.Field.Label.Type"
      WIDTH="10"
    >
      <CONNECT>
        <TARGET
          NAME="TARGET"
          PROPERTY="quickLinkInd"
        />
      </CONNECT>
    </FIELD>
          
   <!-- END, CR00223623 -->
      
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="false"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="AddButton"
      LABEL="ActionControl.Label.Add"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
  <!-- END, CR00074941  -->
</PAGE>
