<?curam-deprecated Since Curam 7.0, This functionality forms part of a feature that is no longer relevant to the product strategy and will not be replaced.?>
<PAGE
  PAGE_ID="IntegratedCase_timelineCalendarList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE
    CLASS="TimelineCalendar"
    NAME="DISPLAY"
    OPERATION="getYears"
  />


  <SERVER_INTERFACE
    CLASS="TimelineCalendar"
    NAME="ACTION"
    OPERATION="getCaseMembers"
    PHASE="ACTION"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="5">
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD CONTROL="SKIP"/>
    <FIELD CONTROL="SKIP"/>


    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.Year"
      WIDTH="4"
      WIDTH_UNITS="CHARS"
    >


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$currentYear"
        />
      </CONNECT>
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="result$dtls$year"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$year"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>


  <LIST TITLE="List.Title.CaseMembers">
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="IntegratedCase_timelineCalendarPerson">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$dtls$concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$year"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="year"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="concernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Age">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="age"
        />
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>
