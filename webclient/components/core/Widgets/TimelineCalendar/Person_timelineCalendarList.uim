<?curam-deprecated Since Curam 7.0, This functionality forms part of a feature that is no longer relevant to the product strategy and will not be replaced.?>
<PAGE
  PAGE_ID="Person_timelineCalendarList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <SERVER_INTERFACE
    CLASS="TimelineCalendar"
    NAME="DISPLAY"
    OPERATION="getYears"
  />


  <SERVER_INTERFACE
    CLASS="TimelineCalendar"
    NAME="ACTION"
    OPERATION="getConcernRoleListInformation"
    PHASE="ACTION"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="5">


    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD CONTROL="SKIP"/>
    <FIELD CONTROL="SKIP"/>


    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.Year"
      WIDTH="4"
      WIDTH_UNITS="CHARS"
    >


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$currentYear"
        />
      </CONNECT>
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="result$dtls$year"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$year"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>
    <FIELD CONTROL="SKIP"/>


  </CLUSTER>


  <LIST TITLE="List.Title.Eligibility">
    <FIELD LABEL="Field.Label.Benefits">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="value"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.From">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="from"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.To">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="to"
        />
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>
