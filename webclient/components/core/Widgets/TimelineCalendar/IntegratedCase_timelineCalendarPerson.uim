<?curam-deprecated Since Curam 7.0, This functionality forms part of a feature that is no longer relevant to the product strategy and will not be replaced.?>
<PAGE
  PAGE_ID="IntegratedCase_timelineCalendarPerson"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="year"/>


  <SERVER_INTERFACE
    CLASS="TimelineCalendar"
    NAME="DISPLAY"
    OPERATION="getConcernRoleListInformation"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="year"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$year"
    />
  </CONNECT>


  <LIST TITLE="List.Title.Eligibility">
    <FIELD LABEL="Field.Label.Benefits">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="value"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.From">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="from"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.To">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="to"
        />
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>
