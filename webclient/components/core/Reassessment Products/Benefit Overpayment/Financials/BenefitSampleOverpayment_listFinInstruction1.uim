<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="BenefitSampleOverpayment_listFinInstruction1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ProductDelivery"
    NAME="DISPLAY"
    OPERATION="listCaseFinancialInstruction"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$warnings$informationalMsgDtlsList$dtls$informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <!-- BEGIN, CR00184236, MC -->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuTransactions"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="BenefitSampleOverpayment_listFinInstruction1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuUnprocessed"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="BenefitSampleOverpayment_listUnprocessedILIs"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <!-- BEGIN, CR00289420, PMD -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuPaymentsReceived"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="BenefitSampleOverpayment_listPaymentsReceived"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00289420 -->    
    
  </MENU>
  <!-- END, CR00184236 -->


  <!-- BEGIN, CR00219579, MC -->
  <INCLUDE FILE_NAME="Financial_listFinInstructionView1.vim"/>
  <!-- END, CR00219579 -->


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="GenerateLiability"
      LABEL="ActionControl.Label.GenerateLiability"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ProductDelivery_generateLiability"
        WINDOW_OPTIONS="width=500"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
