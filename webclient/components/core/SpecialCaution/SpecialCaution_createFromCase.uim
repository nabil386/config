      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    > </ACTION_CONTROL>
  </ACTION_SET>
  <INCLUDE FILE_NAME="SpecialCaution_createViewFromCase.vim"/>
</PAGE>
