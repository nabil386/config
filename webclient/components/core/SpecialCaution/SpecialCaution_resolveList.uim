<!-- This jsp redirects the user to the special caution list page.          -->
<PAGE
  PAGE_ID="SpecialCaution_resolveList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
    
      String caseParticipantRoleID= request.getParameter("caseParticipantRoleID");
      
      String concernRoleID = "";
      
      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory
        .getUserPreferences(pageContext.getSession()).getLocale() + "/"; 
        
        // BEGIN, CR00279713, AC
        curam.interfaces.IntegratedCasePkg.IntegratedCase_readConcernRoleID_TH
        th = new curam.interfaces.IntegratedCasePkg.IntegratedCase_readConcernRoleID_TH();
        // BEGIN, CR00281699, AC
        // BEGIN, CR00282455, AC
        th.setFieldValue(th.key$dtls$caseParticipantRoleID_idx,caseParticipantRoleID);
        // END,  CR00282455
        // END, CR00281699
      th.callServer();
      
      concernRoleID = th.getFieldValue(th.result$concernRoleID_idx);  
      // END, CR00233772  
      // BEGIN, CR00372377, AC
      String url = context + "SpecialCaution_listFromCaseMemberPage.do?caseParticipantRoleID=" + 
       curam.omega3.request.RequestUtils.escapeURL(caseParticipantRoleID) + "&amp;" + "concernRoleID=" +  curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
     // END, CR00372377
     
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
    
    
  </JSP_SCRIPTLET>
</PAGE>
