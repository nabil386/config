<!-- This jsp redirects the action owner to a Home Page.                    -->
<PAGE
  PAGE_ID="Action_resolveOwnerHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);


      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String ownerConcernRoleID = request.getParameter("ownerConcernRoleID");
      String ownerName = request.getParameter("ownerName");
      String ownerType = request.getParameter("ownerType");
      String url = "";
  
      if(ownerType == null)
      {
        // BEGIN, CR00124523, SK
         core.ScriptletMissingParamException e = new
            core.ScriptletMissingParamException(-20002, "ownerType");
        System.out.println(e);
        throw e;
       // END, CR00124523
      }   
  
      if(ownerConcernRoleID == null) 
      {
        // BEGIN, CR00124523, SK
         core.ScriptletMissingParamException e = new
            core.ScriptletMissingParamException(-20002, "ownerConcernRoleID");
        System.out.println(e);
        throw e;
        // END, CR00124523
      } 

      if(ownerName == null) 
      {
        // BEGIN, CR00124523, SK
         core.ScriptletMissingParamException e = new
            core.ScriptletMissingParamException(-20002, "ownerName");
        System.out.println(e);
        throw e;
        // END, CR00124523
      } 

      if(ownerType.equals("AOT2000"))
      {
        url = context + "Participant_resolvePage.do?concernRoleID="
          + curam.omega3.request.RequestUtils.escapeURL(ownerConcernRoleID);
      }
      else
      {
        if(ownerType.equals("AOT2001"))
        {
          url = context + "Organization_viewUserDetailsPage.do?userName="
            + curam.omega3.request.RequestUtils.escapeURL(ownerName);
        }
      }
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
