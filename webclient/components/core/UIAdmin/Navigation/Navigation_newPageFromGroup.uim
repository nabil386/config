<PAGE PAGE_ID="Navigation_newPageFromGroup" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="navBarID"/>
  <PAGE_PARAMETER NAME="parentID"/>


  <SERVER_INTERFACE CLASS="Navigation" NAME="ACTION" OPERATION="addPage" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
    <TARGET NAME="ACTION" PROPERTY="details$navBarID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="parentID"/>
    <TARGET NAME="ACTION" PROPERTY="details$parentID"/>
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT">
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="30">
    <FIELD LABEL="Field.Label.ID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$id"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Title">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$title"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PageID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$pageID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$description"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.VisibleInd">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$isVisibleInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DynamicInd">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$isDynamicInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>