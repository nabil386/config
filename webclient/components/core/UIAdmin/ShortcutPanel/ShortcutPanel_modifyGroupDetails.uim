<PAGE
  PAGE_ID="ShortcutPanel_modifyGroupDetails"
  WINDOW_OPTIONS="width=500"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="parentID"/>
  <PAGE_PARAMETER NAME="shortcutPanelID"/>


  <SERVER_INTERFACE
    CLASS="ShortcutPanel"
    NAME="ACTION"
    OPERATION="updateGroupNodeDetails"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="ShortcutPanel"
    NAME="DISPLAY"
    OPERATION="readNodeDetails"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="parentID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="parentID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="parentID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$parentID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="shortcutPanelID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="shortcutPanelID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="id"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="id"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    >
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="20">
    <FIELD
      LABEL="Field.Label.Title"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="title"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="title"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
