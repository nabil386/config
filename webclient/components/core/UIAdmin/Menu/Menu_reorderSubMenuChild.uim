<PAGE
  PAGE_ID="Menu_reorderSubMenuChild"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="menuID"/>
  <PAGE_PARAMETER NAME="menuTitle"/>
  <PAGE_PARAMETER NAME="nodeID"/>
  <PAGE_PARAMETER NAME="parentID"/>
  <SERVER_INTERFACE
    CLASS="Menu"
    NAME="DISPLAY"
    OPERATION="listAllSubMenuChildrenToReorder"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Menu"
    NAME="ACTION"
    OPERATION="reorderChild"
    PHASE="ACTION"
  />
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- map to the display phase interface. -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="menuID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="nodeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="nodeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="parentID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="parentID"
    />
  </CONNECT>


  <!-- Map to the Action phase interface. -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="menuID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="nodeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="nodeID"
    />
  </CONNECT>
  <CLUSTER
    DESCRIPTION="Cluster.Description"
    LABEL_WIDTH="35"
  >
    <FIELD LABEL="Field.Label.MenuID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="nodeID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InsertBeforeParameter">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="id"
          NAME="DISPLAY"
          PROPERTY="id"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="reorderID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
