<PAGE PAGE_ID="Menu_deleteMenuItem" WINDOW_OPTIONS="width=400">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="menuID"/>
  <PAGE_PARAMETER NAME="configID"/>


  <SERVER_INTERFACE CLASS="Menu" NAME="ACTION" OPERATION="deleteMenuItem" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="menuID"/>
    <TARGET NAME="ACTION" PROPERTY="id"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="configID"/>
    <TARGET NAME="ACTION" PROPERTY="configID"/>
  </CONNECT>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="Control.Label.Yes" TYPE="SUBMIT">
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.No">
        </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER SHOW_LABELS="false">
    <FIELD LABEL="Field.Label.DeleteMenuItem">
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.DeleteMenuItem"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>