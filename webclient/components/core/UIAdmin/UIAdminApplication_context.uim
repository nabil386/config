<PAGE
  PAGE_ID="UIAdminApplication_context"
  TYPE="DETAILS"
>


  <PAGE_PARAMETER NAME="applicationID"/>


  <SERVER_INTERFACE
    CLASS="NewUIAdmin"
    NAME="DISPLAY"
    OPERATION="getApplicationDetails"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CLUSTER NUM_COLS="2">
    <CLUSTER TITLE="Cluster.Title.Banner">
      <FIELD LABEL="Field.Label.Title">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="title"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.SubTitle">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="subTitle"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Logo">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="logo"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Message">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="message"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <CLUSTER TITLE="Cluster.Title.SystemMenu">
      <FIELD LABEL="Field.Label.Preferences">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="prefsEnabled"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Help">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="helpEnabled"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Logout">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="logoutEnabled"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
</PAGE>
