<PAGE
  PAGE_ID="Section_context"
  TYPE="DETAILS"
>
  <PAGE_PARAMETER NAME="sectionID"/>


  <SERVER_INTERFACE
    CLASS="NewUIAdmin"
    NAME="DISPLAY"
    OPERATION="getSectionDetails"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sectionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$sectionID"
    />
  </CONNECT>
  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.SectionDetails"
  >
    <FIELD LABEL="Field.Label.SectionID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$sectionID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SectionTitle">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sectionTitle"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
