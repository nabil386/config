<PAGE PAGE_ID="External_addNavigationPage" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="navBarID"/>


  <SERVER_INTERFACE CLASS="Navigation" NAME="ACTION" OPERATION="addPage" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
    <TARGET NAME="ACTION" PROPERTY="details$navBarID"/>
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT">
      
      
      
      <!--LINK
        PAGE_ID="External_viewNavigationBar"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="id"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="navigationPageID"
          />
        </CONNECT>
      </LINK-->
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="40">
    <FIELD LABEL="Field.Label.ID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$id"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Title">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$title"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PageID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$pageID"/>
      </CONNECT>
    </FIELD>
    <!--FIELD
      HEIGHT="3"
      LABEL="Field.Label.Highlight.Page.ID"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="highlightPageIDList"
        />
      </CONNECT>
    </FIELD-->
    <WIDGET LABEL="Field.Label.Icon" TYPE="FILE_UPLOAD" WIDTH="100" WIDTH_UNITS="PERCENT">
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconContents"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="FILE_NAME">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconName"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="CONTENT_TYPE">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconContentType"/>
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
    <FIELD LABEL="Field.Label.VisibleInd">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$isVisibleInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DynamicInd">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$isDynamicInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>