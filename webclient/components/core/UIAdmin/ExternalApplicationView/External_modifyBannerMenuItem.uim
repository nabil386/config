<PAGE
  PAGE_ID="External_modifyBannerMenuItem"
  WINDOW_OPTIONS="width=780"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="menuItemID"/>
  <PAGE_PARAMETER NAME="applicationID"/>
  <PAGE_PARAMETER NAME="type"/>
  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="DISPLAY"
    OPERATION="readBannerMenuItemDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="ACTION"
    OPERATION="updateBannerMenuItemDetails"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="keyDetails$dtls$applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="keyDetails$dtls$menuItemID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="type"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="keyDetails$type"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="menuItemID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="type"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="type"
    />
  </CONNECT>
  <CLUSTER LABEL_WIDTH="45">
    <FIELD LABEL="Field.Label.Title">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="title"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="title"
        />
      </CONNECT>
    </FIELD>
    <WIDGET
      LABEL="Field.Label.Icon"
      TYPE="FILE_UPLOAD"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="iconContents"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="FILE_NAME">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="iconName"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="CONTENT_TYPE">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="iconContentType"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="45">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="needsDescription"
      />
    </CONDITION>
    <FIELD
      HEIGHT="5"
      LABEL="Field.Label.Text"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="text"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="45">
    <FIELD LABEL="Field.Label.Page.ID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="pageID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="pageID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="45">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="isPersonType"
      />
    </CONDITION>
    <FIELD LABEL="Field.Label.Hidden">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$hiddenForUserType"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$hiddenForUserType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
