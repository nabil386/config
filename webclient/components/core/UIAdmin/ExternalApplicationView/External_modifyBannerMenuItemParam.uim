<PAGE
  PAGE_ID="External_modifyBannerMenuItemParam"
  WINDOW_OPTIONS="width=500"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="menuItemID"/>
  <PAGE_PARAMETER NAME="applicationID"/>
  <PAGE_PARAMETER NAME="paramName"/>


  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="DISPLAY"
    OPERATION="readBannerMenuItemParamDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="ACTION"
    OPERATION="updateBannerMenuItemParamDetails"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="menuItemID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="paramName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="name"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="keyDetails$applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="paramName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="paramName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="menuItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="keyDetails$menuItemKey"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="55"
    TITLE="Cluster.Title"
  >
    <FIELD LABEL="Field.Label.Param.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      HEIGHT="1"
      LABEL="Field.Label.Param.Value"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="value"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="value"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      HEIGHT="1"
      LABEL="Field.Label.Param.Description"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
