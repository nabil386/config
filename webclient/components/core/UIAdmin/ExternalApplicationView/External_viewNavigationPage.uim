<PAGE PAGE_ID="External_viewNavigationPage" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="navigationPageID"/>
  <PAGE_PARAMETER NAME="navBarID"/>


  <SERVER_INTERFACE CLASS="Navigation" NAME="DISPLAY" OPERATION="readPageDetails"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navigationPageID"/>
    <TARGET NAME="DISPLAY" PROPERTY="navigationID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$navBarID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="30">
    <CONTAINER LABEL="Container.Label.Title">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="title"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Title.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="titlePropKey"/>
            <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
            <TARGET NAME="PAGE" PROPERTY="documentID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER LABEL="Container.Label.Description">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="description"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Description.TextTranslation">
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="showDescriptionTranslation"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="descriptionPropKey"/>
            <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
            <TARGET NAME="PAGE" PROPERTY="documentID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER LABEL="Field.Label.Icon">
      <FIELD LABEL="Field.Label.Icon">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$iconID"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
    <FIELD LABEL="Field.Label.VisibleInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="isVisibleInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DynamicInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="isDynamicInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>