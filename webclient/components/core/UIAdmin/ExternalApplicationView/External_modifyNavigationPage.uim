<PAGE PAGE_ID="External_modifyNavigationPage" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="navigationPageID"/>
  <PAGE_PARAMETER NAME="navBarID"/>


  <SERVER_INTERFACE CLASS="Navigation" NAME="DISPLAY" OPERATION="readPageDetails"/>
  <SERVER_INTERFACE CLASS="Navigation" NAME="ACTION" OPERATION="modifyPageDetails" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navigationPageID"/>
    <TARGET NAME="DISPLAY" PROPERTY="navigationID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$navBarID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navBarID"/>
    <TARGET NAME="ACTION" PROPERTY="navBarID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="navigationPageID"/>
    <TARGET NAME="ACTION" PROPERTY="id"/>
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT">
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="40">
    <FIELD LABEL="Field.Label.Title">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="title"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="title"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PageID">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="pageID"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="pageID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
    <FIELD HEIGHT="3" LABEL="Field.Label.Highlight.Page.ID">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="highlightPageIDList"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="highlightPageIDList"/>
      </CONNECT>
    </FIELD>
    <WIDGET LABEL="Field.Label.Icon" TYPE="FILE_UPLOAD" WIDTH="100" WIDTH_UNITS="PERCENT">
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconContents"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="FILE_NAME">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconName"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="CONTENT_TYPE">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="iconContentType"/>
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
    <FIELD LABEL="Field.Label.VisibleInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="isVisibleInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="isVisibleInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DynamicInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="isDynamicInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="isDynamicInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>