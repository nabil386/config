<PAGE PAGE_ID="External_viewBannerMenuItem" WINDOW_OPTIONS="width=500">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="menuItemID"/>
  <PAGE_PARAMETER NAME="applicationID"/>
  <PAGE_PARAMETER NAME="type"/>
  <SERVER_INTERFACE CLASS="ApplicationView" NAME="DISPLAY" OPERATION="readBannerMenuItemDetails" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="ApplicationView" NAME="listParams" OPERATION="listBannerMenuItemParams" PHASE="DISPLAY"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="keyDetails$dtls$applicationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="menuItemID"/>
    <TARGET NAME="DISPLAY" PROPERTY="keyDetails$dtls$menuItemID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="type"/>
    <TARGET NAME="DISPLAY" PROPERTY="keyDetails$type"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="listParams" PROPERTY="keyDetails$applicationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="menuItemID"/>
    <TARGET NAME="listParams" PROPERTY="keyDetails$menuItemID"/>
  </CONNECT>
  <CLUSTER LABEL_WIDTH="30">
    <CONTAINER LABEL="Container.Label.Title">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="title"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Title.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="titlePropKey"/>
            <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
            <TARGET NAME="PAGE" PROPERTY="documentID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER LABEL="Field.Label.Icon">
      <FIELD LABEL="Field.Label.Icon">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$iconID"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="30">
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="isPersonType"/>
    </CONDITION>
    <FIELD LABEL="Field.Label.Hidden">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$hiddenForUserType"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="30">
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="needsDescription"/>
    </CONDITION>
    <CLUSTER LABEL_WIDTH="30">
      <CONTAINER LABEL="Container.Label.Text">
        <FIELD>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="text"/>
          </CONNECT>
        </FIELD>
        <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Text.TextTranslation">
          <CONDITION>
            <IS_TRUE NAME="DISPLAY" PROPERTY="showTextTranslation"/>
          </CONDITION>
          <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="textPropKey"/>
              <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
              <TARGET NAME="PAGE" PROPERTY="documentID"/>
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>
    </CLUSTER>
  </CLUSTER>
  <LIST>
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="hasParams"/>
    </CONDITION>
    <FIELD LABEL="Field.Label.Param.Name">
      <CONNECT>
        <SOURCE NAME="listParams" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <CONTAINER LABEL="Container.Label.Param.Value">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="listParams" PROPERTY="value"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Param.Value.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="valuePropKey"/>
            <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
            <TARGET NAME="PAGE" PROPERTY="documentID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER LABEL="Container.Label.Param.Description">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="listParams" PROPERTY="description"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="AddTranslation" LABEL="ActionControl.Label.Param.Description.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="External_listLocalizableText">
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="descriptionPropKey"/>
            <TARGET NAME="PAGE" PROPERTY="localizableKey"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
            <TARGET NAME="PAGE" PROPERTY="documentID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="Control.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="External_modifyBannerMenuItemParam">
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="keyDetails$applicationID"/>
            <TARGET NAME="PAGE" PROPERTY="applicationID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="name"/>
            <TARGET NAME="PAGE" PROPERTY="paramName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="keyDetails$menuItemID"/>
            <TARGET NAME="PAGE" PROPERTY="menuItemID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Control.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="External_deleteBannerMenuItemParam">
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="keyDetails$applicationID"/>
            <TARGET NAME="PAGE" PROPERTY="applicationID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="name"/>
            <TARGET NAME="PAGE" PROPERTY="paramName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="listParams" PROPERTY="keyDetails$menuItemID"/>
            <TARGET NAME="PAGE" PROPERTY="menuItemID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </LIST>
</PAGE>