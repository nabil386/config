<PAGE PAGE_ID="UIAdminApplication_modifySearchDetails">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="applicationID"/>


  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="DISPLAY"
    OPERATION="readSearchDetails"
  />
  <SERVER_INTERFACE
    CLASS="ApplicationView"
    NAME="ACTION"
    OPERATION="modifySearchDetails"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    >
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel"> </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="30">
    <FIELD LABEL="Field.Label.DefaultSearchPage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="defaultSearchPage"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="defaultSearchPage"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InitialText">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$initialText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="initialText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="30"
    TITLE="Cluster.Title.FurtherDetails"
  >
    <FIELD LABEL="Field.Label.FurtherOptionsID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="furtherOptionsPageID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="furtherOptionsPageID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.FurtherOptionsDescription">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="furtherOptionsDescription"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="furtherOptionsDescription"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
