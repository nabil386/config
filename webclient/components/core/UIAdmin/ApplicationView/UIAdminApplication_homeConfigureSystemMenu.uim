<PAGE PAGE_ID="UIAdminApplication_homeConfigureSystemMenu" WINDOW_OPTIONS="width=700">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="applicationID"/>


  <SERVER_INTERFACE CLASS="ApplicationView" NAME="DISPLAY" OPERATION="readSystemMenuDetails"/>
  <SERVER_INTERFACE CLASS="ApplicationView" NAME="ACTION" OPERATION="modifySystemMenu" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$applicationID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="ACTION" PROPERTY="details$applicationID"/>
  </CONNECT>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT">
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Field.Label.Preferences">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="prefsEnabledInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="prefsEnabledInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Help">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="helpEnabledInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="helpEnabledInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Logout">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="logoutEnabledInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="logoutEnabledInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PrefsTitle">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="preferencesTitle"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="preferencesTitle"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.HelpTitle">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="helpTitle"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="helpTitle"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LogoutTitle">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="logoutTitle"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="logoutTitle"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>