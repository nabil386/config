<PAGE PAGE_ID="UIAdminApplication_modifySearchPageDetails" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ApplicationView" NAME="DISPLAY" OPERATION="readSearchPageDetails"/>
  <SERVER_INTERFACE CLASS="ApplicationView" NAME="ACTION" OPERATION="modifySearchPage" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="applicationID"/>
  <PAGE_PARAMETER NAME="pageID"/>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="defaultPageInd"/>
    <TARGET NAME="ACTION" PROPERTY="defaultPageInd"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="ACTION" PROPERTY="applicationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$applicationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pageID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$pageID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pageID"/>
    <TARGET NAME="ACTION" PROPERTY="details$pageID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="25">
    <FIELD LABEL="Field.Label.Description" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InitialText" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="initialText"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="initialText"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type" WIDTH="30">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="type"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT">
        </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel">
        </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>