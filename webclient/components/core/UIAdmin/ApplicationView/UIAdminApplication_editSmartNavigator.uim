<PAGE PAGE_ID="UIAdminApplication_editSmartNavigator" WINDOW_OPTIONS="width=500">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="applicationID"/>


  <SERVER_INTERFACE CLASS="ApplicationView" NAME="DISPLAY" OPERATION="readSearchDetails"/>
  <SERVER_INTERFACE CLASS="ApplicationView" NAME="ACTION" OPERATION="modifySmartNavigatorDetails" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$applicationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationID"/>
    <TARGET NAME="ACTION" PROPERTY="applicationID"/>
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Control.Label.Save" TYPE="SUBMIT"> </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Control.Label.Cancel"> </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="23">
	<FIELD LABEL="Field.Label.SmartNavigatorEnabled">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="smartNavigatorEnabled"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="smartNavigatorEnabled"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SmartNavigatorInitialText">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="smartNavigatorInitialText"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="smartNavigatorInitialText"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SmartNavigatorDescription">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="smartNavigatorDescription"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="smartNavigatorDescription"/>
      </CONNECT>
    </FIELD>
	<FIELD LABEL="Field.Label.SmartNavigatorDefault">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="smartNavigatorDefaultPage"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="smartNavigatorDefaultPage"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>