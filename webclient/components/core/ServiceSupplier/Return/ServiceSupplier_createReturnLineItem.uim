<!-- This page allows the user to create a return line item for a service   -->
<!-- supplier.                                                              -->
<PAGE PAGE_ID="ServiceSupplier_createReturnLineItem" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServiceSupplier" NAME="ACTION" OPERATION="createLineItem" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="returnID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="returnID"/>
    <TARGET NAME="ACTION" PROPERTY="supplierReturnID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="SaveAndNewButton" LABEL="ActionControl.Label.SaveAndNew" TYPE="SUBMIT">
      <LINK PAGE_ID="ServiceSupplier_createReturnLineItem"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="40" NUM_COLS="2" TITLE="Cluster.Title.Details">


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="clientID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="serviceType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Date" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dateOfService"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TotalCost" USE_DEFAULT="False">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="cost"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>