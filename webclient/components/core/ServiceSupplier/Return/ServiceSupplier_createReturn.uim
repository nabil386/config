<!-- This page allows the user to create a return -->
<PAGE
  PAGE_ID="ServiceSupplier_createReturn"
  WINDOW_OPTIONS="width=725"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServiceSupplier"
    NAME="ACTION"
    OPERATION="createReturn"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$supplierReturnHeaderDetails$supplierID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.ContactName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$contactName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.NumOfItems"
      USE_DEFAULT="False"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$numberOfItems"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.TotalAmount"
      USE_DEFAULT="False"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$totalAmount"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.SupplierInitials">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$supplierInitials"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ProductName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$productID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RefNumber">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$docReferenceNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReceiptDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$receiptDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00408407, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$supplierReturnHeaderDetails$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
