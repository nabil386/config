<!-- This page allows the user to view a service contract                   -->
<PAGE
  PAGE_ID="ServiceSupplier_viewContract"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServiceSupplier"
    NAME="DISPLAY"
    OPERATION="readContract"
  />


  <PAGE_PARAMETER NAME="contractID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="contractID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="contractDetailsKey$contractID"
    />
  </CONNECT>


  <!--BEGIN, CR00086212, SK-->


  <CLUSTER>


    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
    >


      <FIELD LABEL="Field.Label.Type">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="contractDetails$typeCode"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.SignedDate">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="signedDate"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


    <CLUSTER LABEL_WIDTH="22">


      <FIELD
        HEIGHT="3"
        LABEL="Field.Label.Summary"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="summary"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


  </CLUSTER>


  <!-- END, CR00086212 -->


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TAB_ORDER="COLUMN"
    TITLE="Cluster.Title.RenewalDetails"
  >


    <FIELD LABEL="Field.Label.RenewableContract">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="renewalInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RenewalLeadDays">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="renewalLeadDays"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.FileReferenceNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fileRefNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.FileLocation">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fileLocation"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <LIST TITLE="List.Title.SelectedServices">


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServiceSupplier_removeServiceVerify"
        >


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contractID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contractID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="serviceID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="serviceID"
            />
          </CONNECT>


        </LINK>


      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Service"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$typeCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
