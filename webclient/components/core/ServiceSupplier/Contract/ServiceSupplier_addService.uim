<!-- This page allows the user to add a service to a contract.              -->
<PAGE
  PAGE_ID="ServiceSupplier_addService"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServiceSupplier"
    NAME="DISPLAY"
    OPERATION="listServicesCovered"
  />


  <SERVER_INTERFACE
    CLASS="ServiceSupplier"
    NAME="ACTION"
    OPERATION="addServiceToContract"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="contractID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="contractID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="contractID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="contractID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="contractID"
    />
  </CONNECT>


  <LIST>


    <FIELD
      HEIGHT="5"
      LABEL="Field.Title.ServicesAdded"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="servicesCovered$typeCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.AddServices"
  >


    <FIELD
      HEIGHT="5"
      LABEL="Field.Label.Services"
      WIDTH="150"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="serviceNotCovered$serviceID"
          NAME="DISPLAY"
          PROPERTY="serviceNotCovered$typeCode"
        />
      </CONNECT>


      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="serviceIDLinkList"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
