<!-- This page allows a user to modify a target system service.             -->
<PAGE PAGE_ID="TargetSystem_modifyTargetSystemService"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="MaintainTargetSystemService" NAME="DISPLAY"
    OPERATION="viewTargetSystemService"/>

  <SERVER_INTERFACE CLASS="MaintainTargetSystemService" NAME="ACTION"
    OPERATION="modifyTargetSystemService" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="serviceID"/>
  <PAGE_PARAMETER NAME="systemID"/>

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="serviceID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$serviceID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="serviceID"/>
    <TARGET NAME="ACTION" PROPERTY="serviceID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="dtls$versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="systemID"/>
    <TARGET NAME="ACTION" PROPERTY="systemID"/>
  </CONNECT>

  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>

  <CLUSTER LABEL_WIDTH="30" TITLE="Cluster.Title.Details">
    <FIELD WIDTH="43" LABEL="Field.Label.ServiceName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>

    <FIELD WIDTH="80" LABEL="Field.Label.ExtensionURL">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="extensionURL"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="extensionURL"/>
      </CONNECT>
    </FIELD>
    <FIELD WIDTH="30" LABEL="Field.Label.UserName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="username"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="username"/>
      </CONNECT>
    </FIELD>
    <FIELD WIDTH="60" LABEL="Field.Label.Password">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="password"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
