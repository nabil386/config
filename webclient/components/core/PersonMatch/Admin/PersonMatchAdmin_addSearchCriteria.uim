<PAGE
  PAGE_ID="PersonMatchAdmin_addSearchCriteria"
  WINDOW_OPTIONS="width=520"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="PersonMatchSearchCriteria"
    NAME="ACTION"
    OPERATION="addSearchCriteria"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="PersonMatchSearchCriteria"
    NAME="DISPLAY"
    OPERATION="listSearchIndexCriteria"
    PHASE="DISPLAY"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="1"
    STYLE="cluster-no-bottom-margin-border"
  >


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Reference">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="reference"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Weight"
      WIDTH="5"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="weightString"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Enabled">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="enabledInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.SearchIndex"
      USE_BLANK="true"
    >
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchIndexField"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
