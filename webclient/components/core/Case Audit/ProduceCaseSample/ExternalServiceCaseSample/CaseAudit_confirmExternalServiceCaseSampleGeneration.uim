<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows a user to confirm the random case list generation  		-->
<!-- or to quit the case list generation process          									-->
<PAGE
  PAGE_ID="CaseAudit_confirmExternalServiceCaseSampleGeneration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AuditCaseSample"
    NAME="DISPLAYWIZARD"
    OPERATION="getExternalCaseAuditDataWizard"
  />


  <SERVER_INTERFACE
    CLASS="AuditCaseSample"
    NAME="ACTION"
    OPERATION="generateExternalCaseAuditData"
    PHASE="ACTION"
  />


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <PAGE_PARAMETER NAME="auditPlanID"/>
  <PAGE_PARAMETER NAME="externalCaseAuditDataID"/>
  <PAGE_PARAMETER NAME="numberOfCases"/>
  <PAGE_PARAMETER NAME="totalCases"/>
  <PAGE_PARAMETER NAME="percentageOfCases"/>
  <PAGE_PARAMETER NAME="startPoint"/>
  <PAGE_PARAMETER NAME="interval"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="auditPlanID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$auditPlanID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="externalCaseAuditDataID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$externalCaseAuditDataID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="numberOfCases"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$numberOfCases"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="percentageOfCases"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$percentageOfCases"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="startPoint"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$details$startPoint"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="interval"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$details$interval"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="totalCases"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$totalNumCases"
    />
  </CONNECT>


  <CLUSTER
    DESCRIPTION="Cluster.Description.Text"
    NUM_COLS="1"
    SHOW_LABELS="false"
  >
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />


    <ACTION_CONTROL
      IMAGE="BackButton"
      LABEL="ActionControl.Label.Back"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="CaseAudit_externalServiceConfigureAlgorithm"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="externalCaseAuditDataID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalCaseAuditDataID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="numberOfCases"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="numberOfCases"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="percentageOfCases"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="percentageOfCases"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="startPoint"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="startPoint"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interval"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interval"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="totalCases"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="totalCases"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="auditPlanID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="auditPlanID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="FinishButton"
      LABEL="ActionControl.Label.Finish"
      TYPE="SUBMIT"
    />


  </ACTION_SET>


</PAGE>
