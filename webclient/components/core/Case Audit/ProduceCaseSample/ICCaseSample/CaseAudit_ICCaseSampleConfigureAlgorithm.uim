        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="status"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="status"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="gender"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="gender"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="age"
