<!-- This jsp redirects the user to a Produce List of Cases for Audit Page              -->
<PAGE
  PAGE_ID="CaseAudit_resolveQueryType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        
        curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
        String context = request.getContextPath() + "/";
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
        String auditPlanID = request.getParameter("auditPlanID");
        String randomInd = request.getParameter("randomInd");
        String pageName="";        
        String url = "";
        
        if (auditPlanID == null) {
        core.ScriptletMissingParamException e = new
        core.ScriptletMissingParamException(-20002, "auditPlanID");
        throw e;        
        }
        if (randomInd == null) {
        core.ScriptletMissingParamException e = new
        core.ScriptletMissingParamException(-20002, "randomInd");
        throw e;        
        }
        
        curam.interfaces.AuditCaseSamplePkg.AuditCaseSample_resolveQuerySelection_TH
        th = new curam.interfaces.AuditCaseSamplePkg.AuditCaseSample_resolveQuerySelection_TH();
        
        th.setFieldValue(th.key$auditPlanID_idx,auditPlanID);
        th.setFieldValue(th.key$randomInd_idx,randomInd);
        
        th.callServer();
        
        pageName=th.getFieldValue(th.result$pageName_idx);       
        
        url = context + pageName;
        
        url += "&amp;" + rh.getSystemParameters();
        
        response.sendRedirect(response.encodeRedirectURL(url));
        
    </JSP_SCRIPTLET>
</PAGE>
