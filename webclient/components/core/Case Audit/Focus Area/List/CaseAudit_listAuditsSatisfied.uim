<!-- This page allows the user to view a list of case audits for the audit  -->
<!-- plan with a given focus area which has been satisfied.                 -->
<PAGE
  PAGE_ID="CaseAudit_listAuditsSatisfied"
  WINDOW_OPTIONS="width=850"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>


  </PAGE_TITLE>


  <!-- BEGIN, CR00290965, IBM -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00290965 -->


  <SERVER_INTERFACE
    CLASS="CaseAudit"
    NAME="DISPLAY"
    OPERATION="listAuditsOnPlanMeetingFocus"
  />


  <INCLUDE FILE_NAME="CaseAudit_listAuditsByFocusAreaFinding.vim"/>


</PAGE>
