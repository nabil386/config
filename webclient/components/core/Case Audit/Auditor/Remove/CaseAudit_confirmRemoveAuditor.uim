<!-- Asks user for confirmation before removing the auditor from the plan.  -->
<PAGE PAGE_ID="CaseAudit_confirmRemoveAuditor" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="type"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Auditor" NAME="DISPLAY" OPERATION="readAuditorForRemoval" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Auditor" NAME="ACTION" OPERATION="removeAuditorFromPlan" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="auditorID"/>
  <PAGE_PARAMETER NAME="auditPlanID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="auditorID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$auditorID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="auditorID"/>
    <TARGET NAME="ACTION" PROPERTY="details$auditorID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="auditPlanID"/>
    <TARGET NAME="ACTION" PROPERTY="auditPlanID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="auditorVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="auditorVersionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="auditTeamID"/>
    <TARGET NAME="ACTION" PROPERTY="auditTeamID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="teamVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="teamVersionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="confirmMessage"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>