<!-- This jsp redirects the user to the list selection criteria screen      -->
<PAGE
  PAGE_ID="CaseAudit_resolveListSelectionCriteriaPageName"
  WINDOW_OPTIONS="width=460"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        
        curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
        String context = request.getContextPath() + "/";
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
        String auditPlanID = request.getParameter("auditPlanID");
        String listSelectionCriteriaPageName="";
        String url = "";
        
        if (auditPlanID == null) {
          core.ScriptletMissingParamException e = new
          core.ScriptletMissingParamException(-20002, "auditPlanID");
          throw e;        
        }
        
        curam.interfaces.AuditPlanCriteriaPkg.AuditPlanCriteria_resolveListSelectionCriteriaPageName_TH
        th = new curam.interfaces.AuditPlanCriteriaPkg.AuditPlanCriteria_resolveListSelectionCriteriaPageName_TH();
        
        th.setFieldValue(th.key$auditPlanID_idx,auditPlanID);
        
        th.callServer();
        
        listSelectionCriteriaPageName=th.getFieldValue(th.result$pageName_idx);
        
        url = context + curam.omega3.request.RequestUtils.escapeURL(listSelectionCriteriaPageName) + "Page.do?auditPlanID=" 
        + curam.omega3.request.RequestUtils.escapeURL(auditPlanID);   
        
        url += "&amp;" + rh.getSystemParameters();
        response.sendRedirect(response.encodeRedirectURL(url));
        
    </JSP_SCRIPTLET>
</PAGE>
