<!-- This page allows a user to view details of the audit plan transaction  -->
<PAGE
  PAGE_ID="AuditPlan_viewTransactionLogRecord"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AuditPlanTransactionLog"
    NAME="DISPLAY"
    OPERATION="readAuditPlanTransaction"
  />


  <PAGE_PARAMETER NAME="transactionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="transactionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$transactionID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title"
  >
    <FIELD LABEL="Field.Label.EventType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transactionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EventDescription">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CreatedBy">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL LABEL="ActionControl.Label.Close"/>
  </ACTION_SET>
</PAGE>
