<!-- ===========                                                            -->
<!-- Asks user for confirmation before changing the audit coordinator       -->
<!-- Request.                                                               -->
<PAGE
  PAGE_ID="CaseAudit_confirmChangeCoord"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AuditPlan"
    NAME="DISPLAY"
    OPERATION="viewAuditPlan"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="AuditPlan"
    NAME="ACTION"
    OPERATION="modifyCoordinator"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="username"/>
  <PAGE_PARAMETER NAME="auditPlanID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="auditPlanID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$auditPlanID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$auditPlanDtls$versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$auditPlanDtls$versionNo"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="auditPlanID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="auditPlanDtls$auditPlanID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="username"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="coordinator"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.ConfirmApproval"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
