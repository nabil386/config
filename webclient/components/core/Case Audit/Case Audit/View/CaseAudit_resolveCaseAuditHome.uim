<!-- This jsp redirects the user to a Case Audit Home Page               		-->
<PAGE
  PAGE_ID="CaseAudit_resolveCaseAuditHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        
        curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
        String context = request.getContextPath() + "/";
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
        String caseAuditID = request.getParameter("caseAuditID");
        String caseAuditHomePageName="";        
        String url = "";
        
        if (caseAuditID == null) {
        core.ScriptletMissingParamException e = new
        core.ScriptletMissingParamException(-20002, "caseAuditID");
        throw e;        
        }
        
        curam.interfaces.CaseAuditPkg.CaseAudit_resolveCaseAuditHomePageName_TH
        th = new curam.interfaces.CaseAuditPkg.CaseAudit_resolveCaseAuditHomePageName_TH();
        
        th.setFieldValue(th.key$caseAuditID_idx,caseAuditID);
        
        th.callServer();
        
        caseAuditHomePageName=th.getFieldValue(th.result$name_idx);
        
        if ((caseAuditHomePageName==null) ||(caseAuditHomePageName.equals(""))) 
        {         
        caseAuditHomePageName = "CaseAudit_auditorViewCaseAudit";               
        } 
        
        url = context + curam.omega3.request.RequestUtils.escapeURL(caseAuditHomePageName) + "Page.do?caseAuditID=" 
        + curam.omega3.request.RequestUtils.escapeURL(caseAuditID);
        
        url += "&amp;" + rh.getSystemParameters();
        response.sendRedirect(response.encodeRedirectURL(url));
        
    </JSP_SCRIPTLET>
</PAGE>
