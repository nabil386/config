<!-- This page allows the user to view an activity.                         -->
<PAGE
  PAGE_ID="CalendarMeetingMinutes_resolveViewMeetingMinutes"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
      
    
       curam.omega3.request.RequestHandler 
         rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
 
       String context = request.getContextPath() + "/";
       context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/"; 
       String caseID = request.getParameter("caseID");
       String meetingID = request.getParameter("meetingID");
       String url = "";

       if (meetingID == null) { 
         throw new Exception("'meetingID' parameter does not exist"); 
       }            
       if (caseID == null) { 
         throw new Exception("'caseID' parameter does not exist"); 
       }     

       if (caseID.equals("0")) { // No associated case
       // BEGIN, CR00372377, AC
         url = context + "CalendarMeetingDetails_viewDetailsNotFromCasePage.do?meetingID=" 
          + curam.omega3.request.RequestUtils.escapeURL(meetingID) + "&amp;caseID=" +curam.omega3.request.RequestUtils.escapeURL(caseID);

       } else {
          url = context + "CalendarMeetingDetails_viewDetailsPage.do?caseID=" 
          + curam.omega3.request.RequestUtils.escapeURL(caseID) + "&amp;meetingID=" +curam.omega3.request.RequestUtils.escapeURL(meetingID);
       }
      // END, CR00372377
      url += "&amp;" + rh.getSystemParameters();
      response.sendRedirect(response.encodeRedirectURL(url));
      
    
  </JSP_SCRIPTLET>
</PAGE>
