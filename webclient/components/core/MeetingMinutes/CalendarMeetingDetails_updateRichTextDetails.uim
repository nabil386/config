<!-- Update the meeting minutes and decisions -->
<PAGE PAGE_ID="CalendarMeetingDetails_updateRichTextDetails" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="MeetingMinutesManagement" NAME="DISPLAY" OPERATION="readMeetingDetailsForCase"/>


  <SERVER_INTERFACE CLASS="MeetingMinutesManagement" NAME="DISPLAY_DECISIONS" OPERATION="readMeetingDecisions"/>


  <SERVER_INTERFACE CLASS="MeetingMinutesManagement" NAME="ACTION" OPERATION="updateMeetingDetailsForCase" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="MeetingMinutesManagement" NAME="UPDATE_DECISIONS" OPERATION="updateMeetingDecisions" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="meetingID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="meetingID"/>
    <TARGET NAME="DISPLAY" PROPERTY="meetingKey$meetingID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="caseKey$caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="meetingID"/>
    <TARGET NAME="DISPLAY_DECISIONS" PROPERTY="key$meetingID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="meetingID"/>
    <TARGET NAME="ACTION" PROPERTY="dtls$meetingID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="meetingDtls$activityID"/>
    <TARGET NAME="ACTION" PROPERTY="dtls$activityID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="meetingID"/>
    <TARGET NAME="UPDATE_DECISIONS" PROPERTY="dtls$meetingID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY_DECISIONS" PROPERTY="meetingDecisionsID"/>
    <TARGET NAME="UPDATE_DECISIONS" PROPERTY="dtls$meetingDecisionsID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY_DECISIONS" PROPERTY="versionNo"/>
    <TARGET NAME="UPDATE_DECISIONS" PROPERTY="dtls$versionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="organizer"/>
    <TARGET NAME="ACTION" PROPERTY="organizer"/>
  </CONNECT>

  <!-- BEGIN, RTC268286, DR -->
  <CONNECT>
     <SOURCE NAME="DISPLAY" PROPERTY="meetingAgenda"/>
     <TARGET NAME="ACTION" PROPERTY="dtls$richTextMeetingAgenda"/>
  </CONNECT>

  <CONNECT>
     <SOURCE NAME="DISPLAY_DECISIONS" PROPERTY="decisions"/>
     <TARGET NAME="UPDATE_DECISIONS" PROPERTY="dtls$richTextMeetingDecision"/>
  </CONNECT>
  <!-- END, RTC268286 -->

  <SCRIPT ACTION="hideTimeFieldsOnLoad()" EVENT="ONLOAD" SCRIPT_FILE="CalendarMeetingMinutes.js"/>


  <CLUSTER LABEL_WIDTH="15.5" NUM_COLS="1" STYLE="cluster-no-bottom-margin-border">
    <FIELD LABEL="Subject.Label">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dtls$subject"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Location.Label">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="location"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dtls$location"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00237816, ELG -->
    <FIELD LABEL="Field.Label.Sensitivity" WIDTH="8">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$meetingDtls$sensitivityCode"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="sensitivityCode"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00237816 -->


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="31" NUM_COLS="2">
    <FIELD LABEL="StartTime.Label" WIDTH="100">
      <SCRIPT ACTION="updateMeetingTime(this)" EVENT="ONCHANGE" SCRIPT_FILE="CalendarMeetingMinutes.js"/>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="startTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dtls$startTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="MeetingType.Label" USE_BLANK="true">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="meetingType"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dtls$meetingType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="EndTime.Label" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="endTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dtls$endTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AllDay">
      <SCRIPT ACTION="hideTimeFields()" EVENT="ONCLICK" SCRIPT_FILE="CalendarMeetingMinutes.js"/>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="allDayEvent"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="allDayEvent"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="41" NUM_COLS="2" TITLE="Cluster.Organizer.Details">
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="noAssociatedActivityInd"/>
    </CONDITION>
    <FIELD LABEL="Organized.By.Me.Label">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="byMeInd"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="organizedByOptions$byMeInd"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Organized.By.Other.User.Label">
      <CONNECT>
        <INITIAL NAME="DISPLAY" PROPERTY="result$organizedByOptions$otherUserDtls$fullName"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$organizedByOptions$otherUserDtls$userName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="organizedByOptions$otherUserDtls$userName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="41" NUM_COLS="2">
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="result$associatedActivityInd"/>
    </CONDITION>
    <FIELD LABEL="Organizer.FullName.Label">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="organizerFullName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="RIGHT" TOP="false">
    <ACTION_CONTROL DEFAULT="true" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


</PAGE>