<PAGE
  PAGE_ID="MeetingMinutesWizard_removeAttachment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="ACTION"
    OPERATION="removeAttachment"
    PHASE="ACTION"
  />
  
<PAGE_PARAMETER NAME="tempAttachmentID"/>
<PAGE_PARAMETER NAME="wizardStateID" />
  
   <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="tempAttachmentID" />
    <TARGET NAME="ACTION" PROPERTY="tempStoreID" />
   </CONNECT>

    <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="ACTION" PROPERTY="wizardStateID" />
  </CONNECT>

  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeleteText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >

     <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="MeetingMinutesWizard_attachments" SAVE_LINK="false"
        DISMISS_MODAL="true">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
          <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
        </CONNECT>
        
      </LINK>
      </ACTION_CONTROL>
    
    <ACTION_CONTROL LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="MeetingMinutesWizard_attachments" SAVE_LINK="false"
        DISMISS_MODAL="true">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
          <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
        </CONNECT>
        
      </LINK>
      </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
