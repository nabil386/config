<PAGE
  PAGE_ID="MeetingMinutesWizard_decisions"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAYWIZARD"
    OPERATION="getMeetingMinutesWizard"
  />
  
 <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAY"
    OPERATION="getDecisionsForWizard"
  />
  
  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="actionString"
    CLASS="MeetingMinutesWizard"
    NAME="ACTION"
    OPERATION="storeDecisionDetails"
    PHASE="ACTION"
  />

  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>
  
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="wizardStateID" />
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="DISPLAY" PROPERTY="arg1$wizardStateID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="ACTION" PROPERTY="wizardStateID" />
  </CONNECT>

  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Decisions"
  >
    <FIELD
      HEIGHT="221"
      LABEL="Cluster.Title.Decisions"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="decisions"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="decisions"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET>
      <ACTION_CONTROL
      ACTION_ID="Save"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />  
  
  	<ACTION_CONTROL LABEL="ActionControl.Label.Back">
     	<LINK 
      		PAGE_ID="MeetingMinutesWizard_notes"
      		SAVE_LINK="false"
      		DISMISS_MODAL="false"
    	>      
           <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
             <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
           </CONNECT>
           <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="caseID" />
             <TARGET NAME="PAGE" PROPERTY="caseID" />
           </CONNECT>
    	</LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Next" TYPE="SUBMIT" ACTION_ID="Next">
      <LINK PAGE_ID="MeetingMinutesWizard_attendance" SAVE_LINK="false"
        DISMISS_MODAL="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
          <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID" />
          <TARGET NAME="PAGE" PROPERTY="caseID" />
        </CONNECT>
        
      </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />
     
  </ACTION_SET>


</PAGE>
