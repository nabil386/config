<PAGE
  PAGE_ID="MeetingMinutesWizard_meetingDetails"
  SCRIPT_FILE="CalendarMeetingMinutes.js"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAY"
    OPERATION="getCaseMeetingDetailsForWizard"
  />

  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="actionString"
    CLASS="MeetingMinutesWizard"
    NAME="ACTION"
    OPERATION="storeCaseMeetingDetails"
    PHASE="ACTION"
  />
  
  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAYWIZARD"
    OPERATION="getMeetingMinutesWizard"
  />
  
  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <SCRIPT
    ACTION="hideTimeFieldsOnLoad()"
    EVENT="ONLOAD"
    SCRIPT_FILE="CalendarMeetingMinutes.js"
  />

  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="wizardStateID" />
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="DISPLAY" PROPERTY="wizardID$wizardStateID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="ACTION" PROPERTY="wizardStateID" />
  </CONNECT>
  
  <!-- BEGIN, 192763, JAF -->
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="organizerFullName" />
    <TARGET NAME="ACTION" PROPERTY="organizerFullName" />
  </CONNECT>
  <!-- END, 192763, JAF -->
  
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="organizer"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="organizer"
    />
  </CONNECT>

  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="20"
    NUM_COLS="1"
    STYLE="cluster-no-top-margin-border"
  >
    <FIELD LABEL="Subject.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="subject"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="subject"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Location.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="location"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="location"
        />
      </CONNECT>
    </FIELD>

    <FIELD
      LABEL="Field.Label.Sensitivity"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sensitivityCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="sensitivityCode"
        />
      </CONNECT>
    </FIELD>

  </CLUSTER>

  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="40"
    NUM_COLS="2"
    STYLE="cluster-no-top-margin-border"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$noAssociatedActivityInd"
      />
    </CONDITION>
    <FIELD LABEL="Organized.By.Me.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="byMeInd"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="byMeInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Organized.By.Other.User.Label">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="fullName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="userName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="userName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
    STYLE="cluster-no-top-margin-border"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="associatedActivityInd"
      />
    </CONDITION>
    <FIELD LABEL="Organizer.FullName.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="organizerFullName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    STYLE="cluster-no-top-margin-border cluster-smaller-date"
  >
    <FIELD LABEL="StartTime.Label">
      <SCRIPT
        ACTION="updateMeetingTime(this)"
        EVENT="ONCHANGE"
        SCRIPT_FILE="CalendarMeetingMinutes.js"
      />
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startTime"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="MeetingType.Label"
      USE_BLANK="true"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="meetingType"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="meetingType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="EndTime.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endTime"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="endTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AllDay">
      <SCRIPT
        ACTION="hideTimeFields()"
        EVENT="ONCLICK"
        SCRIPT_FILE="CalendarMeetingMinutes.js"
      />
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="allDayEvent"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="allDayEvent"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Agenda"
  >
    <FIELD
      HEIGHT="165"
      LABEL="Agenda.Label"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="meetingAgenda"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="meetingAgenda"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET>
  
    <ACTION_CONTROL
      ACTION_ID="Save"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    
    <ACTION_CONTROL LABEL="Control.Label.Next" TYPE="SUBMIT" ACTION_ID="Next">
      <LINK PAGE_ID="MeetingMinutesWizard_notes" SAVE_LINK="false"
        DISMISS_MODAL="false">
        <CONNECT>
           <SOURCE
            NAME="ACTION"
            PROPERTY="wizardStateID"
          />
           <TARGET NAME="PAGE" PROPERTY="wizardStateID"/>
        </CONNECT>
        <CONNECT>
           <SOURCE NAME="PAGE" PROPERTY="caseID"/>
           <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        
      </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />
      
  </ACTION_SET>
  
</PAGE>
