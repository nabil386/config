<PAGE
  PAGE_ID="MeetingMinutesWizard_attendance"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAYWIZARD"
    OPERATION="getMeetingMinutesWizard"
  />

  <SERVER_INTERFACE
    CLASS="MeetingMinutesWizard"
    NAME="DISPLAY"
    OPERATION="getAttendanceForWizard"
  />
  
  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="actionString"
    CLASS="MeetingMinutesWizard"
    NAME="ADD_ATTENDEE"
    OPERATION="storeAttendanceDetails"
    PHASE="ACTION"
  />
  
  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>



  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="wizardStateID" />
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="DISPLAY" PROPERTY="arg1$wizardStateID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
    <TARGET NAME="ADD_ATTENDEE" PROPERTY="wizardStateID" />
  </CONNECT>


  <CLUSTER
    DESCRIPTION="Cluster.CaseParticipantUser.Description"
    LABEL_WIDTH="23"
    TITLE="Cluster.Title.Invitation"
  >

    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="associatedCaseRefInd"
      />
    </CONDITION>
    <FIELD
      LABEL="Field.Label.CaseParticipantUser"
      USE_BLANK="true"
      WIDTH="70"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="identifier"
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="caseAttendeeIdentifier"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER
    DESCRIPTION="Cluster.Registered.Description"
    LABEL_WIDTH="25"
  >

    <CONTAINER LABEL="Container.Label.Registered">
      <FIELD WIDTH="35">
        <CONNECT>
          <TARGET
            NAME="ADD_ATTENDEE"
            PROPERTY="otherAttendeeType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="60">
        <CONNECT>
          <TARGET
            NAME="ADD_ATTENDEE"
            PROPERTY="otherAttendeeID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>

  <CLUSTER
    DESCRIPTION="Cluster.Email.Description"
    LABEL_WIDTH="25"
    NUM_COLS="1"
    STYLE="cluster-no-top-margin-border"
  >

    <FIELD LABEL="Field.Label.EmailAddress">
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="emailAddress"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
    STYLE="cluster-no-bottom-margin-border"
  >

    <FIELD
      LABEL="Field.Label.Acceptance"
      USE_DEFAULT="TRUE"
    >
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="invitationAcceptanceStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Attended">
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="attendedInd"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER
    LABEL_WIDTH="25"
    STYLE="cluster-no-top-margin-border"
  >

    <FIELD
      HEIGHT="5"
      LABEL="Field.Label.Comment"
    >
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER STYLE="outer-cluster-borderless-nospace">
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Add"
        TYPE="SUBMIT"
        ACTION_ID="Add"
      >
        <LINK
          PAGE_ID="THIS"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <LIST>
    <FIELD WIDTH="10">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="meetingOrganizerInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="attendeeFullName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Acceptance"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="invitationAcceptanceStatus"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Attended"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="attendedInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.Comment"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>

    <ACTION_SET TYPE="LIST_ROW_MENU">
	   <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="MeetingMinutesWizard_editAttendee"
          SAVE_LINK="false"
        >

          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="wizardKey$tempStoreID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="tempAttendeeID"
            />
          </CONNECT>
          <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
             <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
           </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="MeetingMinutesWizard_removeAttendee"
          SAVE_LINK="false"
        >

          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="wizardKey$tempStoreID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="tempAttendeeID"
            />
          </CONNECT>
          <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
             <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
           </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </LIST>

  <ACTION_SET>

      <ACTION_CONTROL
      ACTION_ID="Save"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />  
  
  	<ACTION_CONTROL LABEL="ActionControl.Label.Back">
     	<LINK 
      		PAGE_ID="MeetingMinutesWizard_decisions"
      		SAVE_LINK="false"
      		DISMISS_MODAL="false"
    	>      
           <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
             <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
           </CONNECT>
           <CONNECT>
             <SOURCE NAME="PAGE" PROPERTY="caseID" />
             <TARGET NAME="PAGE" PROPERTY="caseID" />
           </CONNECT>
    	</LINK>
    </ACTION_CONTROL>
   <ACTION_CONTROL LABEL="ActionControl.Label.Next">
      <LINK PAGE_ID="MeetingMinutesWizard_actions" SAVE_LINK="false"
        DISMISS_MODAL="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="wizardStateID" />
          <TARGET NAME="PAGE" PROPERTY="wizardStateID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID" />
          <TARGET NAME="PAGE" PROPERTY="caseID" />
        </CONNECT>
        
      </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />
     
  </ACTION_SET>

</PAGE>
