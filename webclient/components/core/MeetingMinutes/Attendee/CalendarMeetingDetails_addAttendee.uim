<!-- Add a new meeting attendee -->
<PAGE
  PAGE_ID="CalendarMeetingDetails_addAttendee"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MeetingMinutesManagement"
    NAME="ADD_ATTENDEE"
    OPERATION="addMeetingAttendee"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="MeetingMinutesManagement"
    NAME="DISPLAY_ATTENDEES"
    OPERATION="readMeetingAttendence"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="meetingID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="meetingID"
    />
    <TARGET
      NAME="ADD_ATTENDEE"
      PROPERTY="meetingID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ADD_ATTENDEE"
      PROPERTY="caseKey$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="meetingID"
    />
    <TARGET
      NAME="DISPLAY_ATTENDEES"
      PROPERTY="key$meetingID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY_ATTENDEES"
      PROPERTY="caseKey$caseID"
    />
  </CONNECT>


  <CLUSTER
    DESCRIPTION="Cluster.CaseParticipantUser.Description"
    LABEL_WIDTH="30"
    TITLE="Cluster.Title.Invitation"
  >
    <!-- BEGIN, CR00219000, CW -->
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY_ATTENDEES"
        PROPERTY="associatedCaseRefInd"
      />
    </CONDITION>
    <!-- END, CR00219000 -->
    <FIELD
      LABEL="Field.Label.CaseParticipantUser"
      USE_BLANK="true"
      WIDTH="70"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$meetingInviteeDetailsList$inviteeDetails$identifier"
          NAME="DISPLAY_ATTENDEES"
          PROPERTY="result$meetingInviteeDetailsList$inviteeDetails$description"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="attendanceDetails$caseAttendeeIdentifier"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    DESCRIPTION="Cluster.Registered.Description"
    LABEL_WIDTH="30"
  >
    <CONTAINER LABEL="Container.Label.Registered">
      <FIELD WIDTH="35">
        <CONNECT>
          <TARGET
            NAME="ADD_ATTENDEE"
            PROPERTY="attendanceDetails$otherAttendeeType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="60">
        <CONNECT>
          <TARGET
            NAME="ADD_ATTENDEE"
            PROPERTY="attendanceDetails$otherAttendeeID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER
    DESCRIPTION="Cluster.Email.Description"
    LABEL_WIDTH="30"
  >
    <FIELD LABEL="Field.Label.EmailAddress">
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="attendanceDetails$emailAddress"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="60"
    NUM_COLS="2"
    TITLE="Cluster.Title.Attendance"
  >
    <FIELD
      LABEL="Field.Label.Acceptance"
      USE_DEFAULT="TRUE"
      WIDTH="40"
    >
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="invitationAcceptanceStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Attended">
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="attendedInd"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="30"
    SHOW_LABELS="false"
    TITLE="Field.Label.Comment"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="5"
      LABEL="Field.Label.Comment"
    >
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET
          NAME="ADD_ATTENDEE"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


</PAGE>
