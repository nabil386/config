<!-- Lists all Product Period Display Categories on a sandbox CER Product.  -->
<?curam-deprecated Since Curam 6.0.5, replaced with CREOLEProductAdmin_listPublishedProductPeriodDispCat1.uim. See release note: CR00344739?>
<PAGE
  PAGE_ID="CREOLEProductAdmin_listPublishedProductPeriodDispCat"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="pagetitle.description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CREOLEProductAdmin"
    NAME="DISPLAY"
    OPERATION="listPublishedProductPeriodDisplayCategories"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="creoleProductPeriodID"/>
  <PAGE_PARAMETER NAME="productStructureClassSummary"/>
  <PAGE_PARAMETER NAME="otherKeyDataClassSummary"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductPeriodID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="creoleProductPeriodKey$creoleProductPeriodID"
    />
  </CONNECT>


  <!-- Product Structure and Key Factors -->
  <CLUSTER
    LABEL_WIDTH="20"
    STYLE="outer-cluster-borderless"
  >
    <FIELD LABEL="field.label.productStructureClassSummary">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="productStructureClassSummary"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="field.label.otherKeyDataClassSummary">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="otherKeyDataClassSummary"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    STYLE="outer-cluster-borderless"
    TITLE="Cluster.Title"
  >


    <LIST>


      <FIELD
        LABEL="field.label.result$dtls$decisionDisplayCatName"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$decisionDisplayCatName"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="field.label.decisionDetailsClassSummary"
        WIDTH="70"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="decisionDetailsClassSummary"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="field.label.result$dtls$displayOrder"
        WIDTH="10"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$displayOrder"
          />
        </CONNECT>
      </FIELD>


    </LIST>


  </CLUSTER>


</PAGE>
