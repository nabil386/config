<!-- Lists all Decision Display Categories on a Creole Product.             -->
<PAGE
  PAGE_ID="CREOLEProductAdmin_listPublishedDecisionDispCat"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="CREOLEProductAdmin"
    NAME="DISPLAY"
    OPERATION="listPublishedProductDecisionDisplayCategories"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="creoleProductPeriodID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DisplayCategories"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="CREOLEProductAdmin_listPublishedDecisionDispCat"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="creoleProductPeriodID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="creoleProductPeriodID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ProductPeriods"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CREOLEProductAdmin_listPublishedProductPeriod"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.EligibilityDetermination"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CREOLEProductAdmin_readPublishedEligibilityDeterminationDetails"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <INCLUDE FILE_NAME="CREOLEProductAdmin_sandboxCreationActionView.vim"/>
  <INCLUDE FILE_NAME="CREOLEProductAdmin_listDecisionDispCatView.vim"/>


</PAGE>
