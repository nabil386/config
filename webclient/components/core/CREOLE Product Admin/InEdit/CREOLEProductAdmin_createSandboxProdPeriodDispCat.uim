<!-- Creates a sandbox Product Period Display Category.                     -->
<?curam-deprecated Since Curam 6.0.5, replaced with CREOLEProductAdmin_createSandboxProdPeriodDispCat1.uim. See release note: CR00344739?>
<PAGE
  PAGE_ID="CREOLEProductAdmin_createSandboxProdPeriodDispCat"
  WINDOW_OPTIONS="width=800"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="page.title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="sandboxID"/>
  <PAGE_PARAMETER NAME="sandboxVersionNo"/>
  <PAGE_PARAMETER NAME="creoleProductPeriodID"/>
  <PAGE_PARAMETER NAME="productID"/>


  <SERVER_INTERFACE
    CLASS="CREOLEProductAdmin"
    NAME="DISPLAY"
    OPERATION="listProductDecisionDisplayCategories"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="CREOLEProductAdmin"
    NAME="ACTION"
    OPERATION="createProductPeriodDisplayCategory"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sandboxID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$sandboxID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sandboxID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="sandboxKey$sandboxID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sandboxVersionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="sandboxKey$sandboxVersionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductPeriodID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="creoleProductPeriodID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="productID"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="control.label.save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="control.label.cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="1"
  >


    <FIELD
      LABEL="Field.Label.ProductDecisionDispCat"
      USE_BLANK="false"
      WIDTH="50"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$dtls$dtls$creoleProductDecisionDispCatID"
          NAME="DISPLAY"
          PROPERTY="result$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$creoleProductDecisionDispCatID"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.RuleClass"
      WIDTH="100"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$decisionDetailsClassSummary"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
