<!-- Lists all Decision Display Categories in a sandbox Product.            -->
<PAGE
  PAGE_ID="CREOLEProductAdmin_listInEditDecisionDispCat"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="CREOLEProductAdmin"
    NAME="DISPLAY"
    OPERATION="listInEditProductDecisionDisplayCategories"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="productID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DisplayCategories"
      STYLE="in-page-current-link"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="actionsDisabledInd"
        />
      </CONDITION>
      <LINK
        PAGE_ID="CREOLEProductAdmin_listInEditDecisionDispCat"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ProductPeriods"
      STYLE="in-page-link"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="actionsDisabledInd"
        />
      </CONDITION>
      <LINK
        PAGE_ID="CREOLEProductAdmin_listInEditProductPeriod"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.EligibilityDetermination"
      STYLE="in-page-link"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="actionsDisabledInd"
        />
      </CONDITION>
      <LINK
        PAGE_ID="CREOLEProductAdmin_readInEditEligibilityDeterminationDetails"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <LIST>


    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="actionsDisabledInd"
      />
    </CONDITION>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEProductAdmin_modifyDecisionDispCat"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="creoleProductDecisionDispCatID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="creoleProductDecisionDispCatID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Rename">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEProductAdmin_confirmRenameDispCat"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="nameID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="nameID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEProductAdmin_confirmRemoveDecisionDispCat"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$dtls$creoleProductDecisionDispCatID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="creoleProductDecisionDispCatID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="field.label.result$dtls$name"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.result$dtls$dtls$categoryRef"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="categoryRef"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.result$dtls$dtls$displayPageName"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="displayPageName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.result$dtls$dtls$displayOrder"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="displayOrder"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <INCLUDE FILE_NAME="CREOLEProductAdmin_inEditActionsView.vim"/>


</PAGE>
