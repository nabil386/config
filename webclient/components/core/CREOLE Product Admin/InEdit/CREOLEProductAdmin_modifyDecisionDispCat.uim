<!-- Modifies a Decision Display Category in the sandbox.                   -->
<PAGE PAGE_ID="CREOLEProductAdmin_modifyDecisionDispCat" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <!-- The page title for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="pagetitle.description"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- Declare the 'display' server bean -->
  <SERVER_INTERFACE CLASS="CREOLEProductAdmin" NAME="DISPLAY" OPERATION="readDecisionDisplayCategory" PHASE="DISPLAY"/>


  <!-- Declare the 'action' server bean -->
  <SERVER_INTERFACE CLASS="CREOLEProductAdmin" NAME="ACTION" OPERATION="modifyDecisionDisplayCategory" PHASE="ACTION"/>


  <!-- Page parameters to this page -->
  <PAGE_PARAMETER NAME="creoleProductDecisionDispCatID"/>
  <PAGE_PARAMETER NAME="sandboxID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="creoleProductDecisionDispCatID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$recordID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sandboxID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$adminKey$sandboxID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sandboxID"/>
    <TARGET NAME="ACTION" PROPERTY="details$sandboxKey$sandboxID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="creoleProductDecisionDispCatID"/>
    <TARGET NAME="ACTION" PROPERTY="details$dtls$creoleProductDecisionDispCatID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="sandboxVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="details$sandboxKey$sandboxVersionNo"/>
  </CONNECT>


  <!-- Can't modify this for the moment so connect directly -->
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$nameID"/>
    <TARGET NAME="ACTION" PROPERTY="details$dtls$nameID"/>
  </CONNECT>


  <!-- Fields on this page -->
  <CLUSTER LABEL_WIDTH="40" NUM_COLS="1">


    <FIELD LABEL="field.label.result$name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$name"/>
      </CONNECT>
      <!-- Can't modify this for the moment 
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$name"
        />
      </CONNECT>
      -->
    </FIELD>


    <FIELD LABEL="field.label.result$dtls$displayOrder">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$displayOrder"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$dtls$displayOrder"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="field.label.result$dtls$displayPageName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$displayPageName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$dtls$displayPageName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="field.label.result$dtls$categoryRef">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryRef"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$dtls$categoryRef"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="control.label.save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="control.label.cancel"/>
  </ACTION_SET>


</PAGE>