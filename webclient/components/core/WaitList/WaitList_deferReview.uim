<!-- This page is for deferring the review for the wait list entry.  -->
<PAGE
  PAGE_ID="WaitList_deferReview"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="waitListEntryID"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="WaitList"
    NAME="ACTION"
    OPERATION="deferWaitListEntryReview"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="WaitList"
    NAME="DISPLAY"
    OPERATION="viewWaitListEntryToDeferReview"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="waitListEntryID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waitListEntryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$waitListEntryID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waitListEntryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="waitListEntryID"
    />
  </CONNECT>
  <CLUSTER>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeferReview"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.NewReviewDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reviewDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="reviewDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.NewExpiryDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="expiryDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expiryDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!--
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >

  </CLUSTER>
-->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


</PAGE>
