<!-- Description -->
<!-- This page is for removing the wait list entry.                                    -->
<PAGE
  PAGE_ID="WaitList_removeWaitListEntry"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="waitListEntryID"/>


  <SERVER_INTERFACE
    CLASS="WaitList"
    NAME="ACTION"
    OPERATION="removeWaitListEntry"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waitListEntryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="waitListEntryID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.RemoveText"
        />
      </CONNECT>
    </FIELD>
    <CLUSTER
      LABEL_WIDTH="45"
      TITLE="Cluster.Title"
    >
      <FIELD
        LABEL="Field.Label.WaitListEntry.RemovalReason"
        USE_BLANK="true"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="removalReason"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


</PAGE>
