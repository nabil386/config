<!-- Displays all the assigments of a task.                                 -->
<PAGE
  PAGE_ID="TaskManagement_viewAssignmentListInline"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewTask"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="TaskManagement_taskHomeInline"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taskKey$taskID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.TaskHistory"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="TaskManagement_viewHistoryTextInline">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taskKey$taskID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.AssignmentList"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="TaskManagement_viewAssignmentListInline">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taskKey$taskID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.GraphicalView"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="TaskManagement_visualiseProcessInstanceInline">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taskKey$taskID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="taskID"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="TaskManagement"
    NAME="DISPLAY"
    OPERATION="viewAssignmentsForTask"
  />
  <PAGE_PARAMETER NAME="taskID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="taskID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="taskKey$taskID"
    />
  </CONNECT>


  <LIST>
    <FIELD
      LABEL="Field.Title.AssignedTo"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$assignedTo"
        />
      </CONNECT>


    </FIELD>
    <FIELD
      LABEL="Field.Title.AssigneeType"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$assigneeType"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
