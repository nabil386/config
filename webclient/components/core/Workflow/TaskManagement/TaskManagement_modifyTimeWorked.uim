<!-- The included view for the modify work queue pages.                     -->
<PAGE PAGE_ID="TaskManagement_modifyTimeWorked" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="TaskManagement" NAME="DISPLAY" OPERATION="readTimeWorked" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="TaskManagement" NAME="ACTION" OPERATION="modifyTimeWorked" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="TaskManagement" NAME="ReadVersionNo" OPERATION="readTaskVersionNo" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="taskID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="DISPLAY" PROPERTY="taskID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="ReadVersionNo" PROPERTY="taskKey$taskID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="ACTION" PROPERTY="taskID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="ReadVersionNo" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="40">
    <FIELD LABEL="Container.Label.TimeWorked">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="timeWorked"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="timeWorked"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>
</PAGE>