<?xml version="1.0" encoding="UTF-8"?>
<PAGE
  PAGE_ID="WorkflowAdministration_resolveProcessInstanceErrorsSearchByProcessDetailsWithDates"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        curam.omega3.request.RequestHandler rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
        String context = request.getContextPath() + "/";
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
        
        String processCompletedFromDate = request.getParameter("processCompletedFromDate");
        String processCompletedToDate = request.getParameter("processCompletedToDate");
        String processInstanceErrorType = request.getParameter("processInstanceErrorType");
        String processDisplayName = request.getParameter("processDisplayName");
        String url = context;
        
        url = "WorkflowAdministration_processInstanceErrorsSearchByProcessDetailsWithDatesPage.do?";
        url += "processCompletedFromDate=" + "";
        url += "&amp;processCompletedToDate=" + "";
        url += "&amp;processInstanceErrorType=" + "";
        url += "&amp;processDisplayName=" + "";
        url += "&amp;autoSearchParam=" + "false";
        url += "&amp;" + rh.getSystemParameters();

        response.sendRedirect(response.encodeRedirectURL(url));
  </JSP_SCRIPTLET>
</PAGE>
