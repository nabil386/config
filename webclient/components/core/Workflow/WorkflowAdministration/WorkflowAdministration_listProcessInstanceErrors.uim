<!-- This page allows the administrator to view any Process Instance Errors -->
<PAGE PAGE_ID="WorkflowAdministration_listProcessInstanceErrors" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="WorkflowAdministration" NAME="DISPLAY" OPERATION="listProcessInstanceErrors" PHASE="DISPLAY"/>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Retry">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowAdministration_retryMessage" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$messageID"/>
            <TARGET NAME="PAGE" PROPERTY="messageID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processInstanceID"/>
            <TARGET NAME="PAGE" PROPERTY="processInstanceID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processName"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Abort">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowAdministration_abortMessage" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$messageID"/>
            <TARGET NAME="PAGE" PROPERTY="messageID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processInstanceID"/>
            <TARGET NAME="PAGE" PROPERTY="processInstanceID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processName"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL IMAGE="AbortAllButton" LABEL="Button.Label.AbortAll" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowAdministration_abortAllMessages" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processID"/>
            <TARGET NAME="PAGE" PROPERTY="processID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="processVersion"/>
            <TARGET NAME="PAGE" PROPERTY="processVersion"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="RetryAllButton" LABEL="Button.Label.RetryAll" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowAdministration_retryAllMessages" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processID"/>
            <TARGET NAME="PAGE" PROPERTY="processID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="processVersion"/>
            <TARGET NAME="PAGE" PROPERTY="processVersion"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>
    <FIELD LABEL="Field.Label.ProcessName" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processDisplayName"/>
      </CONNECT>
      <LINK OPEN_MODAL="true" PAGE_ID="WorkflowAdministration_viewErrorProcessInstance">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processInstanceID"/>
          <TARGET NAME="PAGE" PROPERTY="processInstanceID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processName"/>
          <TARGET NAME="PAGE" PROPERTY="processName"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.FailureDateTime" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$failureDateTime"/>
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="WorkflowAdministration_viewMessage">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$messageID"/>
          <TARGET NAME="PAGE" PROPERTY="messageID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$processInstanceErrorSummary$processVersion"/>
          <TARGET NAME="PAGE" PROPERTY="processVersion"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>
</PAGE>