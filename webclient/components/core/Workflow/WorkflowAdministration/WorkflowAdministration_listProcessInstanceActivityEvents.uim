<!-- Description -->
<!-- =========== -->
<!-- Lists all of the events that the specified activity instance is        -->
<!-- waiting for.                                                           -->
<PAGE
  PAGE_ID="WorkflowAdministration_listProcessInstanceActivityEvents"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="WorkflowAdministration"
    NAME="DISPLAY"
    OPERATION="listProcessInstanceActivityEvents"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="processName"/>
  <PAGE_PARAMETER NAME="processInstanceID"/>
  <PAGE_PARAMETER NAME="activityInstanceID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="activityInstanceID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="processInstanceActivityKey$activityInstanceID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="Button.Label.Close"
      TYPE="ACTION"
    >
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <FIELD WIDTH="10">
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Event.Label.Select"
        />
      </CONNECT>
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="true"
        PAGE_ID="WorkflowAdministration_overrideProcessInstanceActivityRedirect"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$eventClass"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eventClass"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$eventType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eventType"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$eventMatchData"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eventMatchData"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$eventIndex"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eventIndex"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EventClass"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$eventClass"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EventType"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$eventType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EventMatchData"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$eventMatchData"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
