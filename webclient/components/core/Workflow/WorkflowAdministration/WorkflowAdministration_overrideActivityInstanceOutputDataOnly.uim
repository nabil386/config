<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Displays the details of an event that may be overridden for the        -->
<!-- specified activity instance. In this instance, only the event output   -->
<!-- data has been specified as event output mappings.                      -->
<PAGE
  PAGE_ID="WorkflowAdministration_overrideActivityInstanceOutputDataOnly"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <INCLUDE FILE_NAME="WorkflowAdministration_viewActivityInstanceEventData.vim"/>
  <CLUSTER
    DESCRIPTION="Cluster.OverrideData.Description"
    LABEL_WIDTH="30"
    NUM_COLS="1"
    TITLE="Cluster.Title.OverrideData"
  >
    <CONTAINER LABEL="Container.Label.EventOutputData">
      <FIELD
        LABEL="Field.Label.EventOutputData"
        WIDTH="30"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="eventWaitOverrideDetails$eventOutputData"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.EventOutputDataWDOAttribute"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="eventOutputData"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      CONFIRM="Confirm.Text.Override"
      IMAGE="OverrideButton"
      LABEL="ActionControl.Label.Override"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="WorkflowAdministration_viewProcessInstanceActivityWithNoOverride">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="WorkflowAdministration_listProcessInstanceActivityEvents">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processInstanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityInstanceID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>