<!-- Searches for currently active Process Instances                        -->
<PAGE
  PAGE_ID="WorkflowAdministration_processInstanceSearchByEventWaitDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="WorkflowAdministration"
    NAME="ACTION"
    OPERATION="processInstanceSearchByEventWaitDetails"
    PHASE="ACTION"
  />


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="result$infoList$dtls$informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <CLUSTER
    LABEL_WIDTH="20"
    TITLE="Cluster.SearchByEventWaitDetails.Title"
  >
    <FIELD
      LABEL="Field.Label.Event"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="searchByEventWaitDetailsKey$eventClassAndType"
        />
      </CONNECT>
    </FIELD>
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="Button.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="Button.Label.Reset"
      >
        <LINK PAGE_ID="WorkflowAdministration_processInstanceSearchByEventWaitDetails"/>
      </ACTION_CONTROL>


    </ACTION_SET>
  </CLUSTER>


  <INCLUDE FILE_NAME="WorkflowAdministration_listProcessInstances.vim"/>


</PAGE>
