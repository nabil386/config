<!-- Display an alert's details.                                             -->
<PAGE
  PAGE_ID="Inbox_viewAlert_Inline"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Alert"
    NAME="DISPLAY"
    OPERATION="view"
  />
  <PAGE_PARAMETER NAME="alertID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="alertID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="alertKey$key$alertID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$message"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="15">
    <FIELD LABEL="Field.Label.DateCreated">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$dateCreated"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
  <LIST TITLE="Cluster.Label.RelatedPages">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$actionDetailsList$alertActionDetails$actionText"
        />
      </CONNECT>
      <LINK
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="result$details$actionDetailsList$alertActionDetails$alertLink"
      />
    </FIELD>
  </LIST>
</PAGE>
