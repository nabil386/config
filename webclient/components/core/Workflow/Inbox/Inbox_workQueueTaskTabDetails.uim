    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="TabTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Separator"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="queueName"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_PARAMETER NAME="queryID"/>
  <PAGE_PARAMETER NAME="queueName"/>


</PAGE>
