<!-- Description -->
<!-- =========== -->
<!-- Displays the summary details for the task redirection and allocation blocking status of the current user-->
<PAGE
  PAGE_ID="Inbox_viewInboxOptionsTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="TabTitle.StaticText1"
      />
    </CONNECT>
  </TAB_NAME>


  <SERVER_INTERFACE
    CLASS="Inbox"
    NAME="DISPLAY"
    OPERATION="viewInboxOptionsSummaryForUser"
    PHASE="DISPLAY"
  />


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.TaskRedirection">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$isRedirected"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.RedirectedTo">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$redirectedTo"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TaskAllocationBlocking">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$isBlocked"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
