<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page displays a list of historical versions of a specified        -->
<!-- Workflow Allocation rule set.                                          -->
<PAGE
  PAGE_ID="WorkflowAllocation_listRuleSetVersionHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CREOLERuleSetAdmin"
    NAME="DISPLAY"
    OPERATION="listRulesetVersionHistory"
  />


  <PAGE_PARAMETER NAME="creoleRuleSetID"/>
  <PAGE_PARAMETER NAME="ruleSetEditActionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleRuleSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$ruleSetID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ruleSetEditActionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$ruleSetEditActionID"
    />
  </CONNECT>


  <MENU>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewTask">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="TaskManagement_createTask"
      />
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewActivity">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Activity_createStandardUserActivity"
      />
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER
    LABEL_WIDTH="25"
    STYLE="outer-cluster-borderless"
  >
    <FIELD
      LABEL="Field.Label.RuleSetName"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$name"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST>


    <FIELD
      LABEL="Field.Label.RuleSetVersion"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$ruleSetVersion"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.UserName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$userName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ActionType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$actionType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ActionDateTime"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$actionDateTime"
        />
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>
