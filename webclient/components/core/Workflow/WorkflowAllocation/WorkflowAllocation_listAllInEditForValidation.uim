<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page lists all of the in edit Work Allocation rule set records    -->
<!-- to allow the user validate the changes made.                           -->
<PAGE PAGE_ID="WorkflowAllocation_listAllInEditForValidation" WINDOW_OPTIONS="width=1000" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="WorkflowCERAllocationAdmin" NAME="DISPLAY" OPERATION="listAllInEditVersions"/>


  <SERVER_INTERFACE CLASS="WorkflowCERAllocationAdmin" NAME="ACTION" OPERATION="validateRuleSetsFromAdmin" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.ValidateChanges" TYPE="SUBMIT">
      <!-- BEGIN, CR00290232, CW -->
      <LINK PAGE_ID="WorkflowAllocation_listCERRuleSets"/>
      <!-- END, CR00290232 -->


    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <LIST>


    <CONTAINER ALIGNMENT="CENTER" WIDTH="6">


      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="ruleSetEditActionID"/>
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="key$ruleSetEditActionList"/>
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="List.Title.RuleSetName" WIDTH="30">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="ruleSetDescription"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="List.Title.ActionType" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="actionType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="List.Title.ActionDateTime" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="actionDateTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="List.Title.UserName" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="userName"/>
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>