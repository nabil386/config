<!-- Creates a new Event Type for an Event Class                            -->
<!-- BEGIN, CR00334789, MR -->
<PAGE
  PAGE_ID="WorkflowEvents_createEventType"
  WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00334789 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="eventClassParam"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="eventClassParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dtls$eventClass"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="WorkflowEvents"
    NAME="ACTION"
    OPERATION="createEventType"
    PHASE="ACTION"
  />


  <CLUSTER LABEL_WIDTH="35">
    <FIELD LABEL="Field.Label.EventType">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dtls$eventType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      IMAGE="SaveAndNewButton"
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


</PAGE>
