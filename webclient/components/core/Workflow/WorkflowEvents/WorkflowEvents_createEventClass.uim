<!-- Creates a new Event Class                                              -->
<PAGE
  PAGE_ID="WorkflowEvents_createEventClass"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="WorkflowEvents"
    NAME="ACTION"
    OPERATION="createEventClass"
    PHASE="ACTION"
  />


  <CLUSTER LABEL_WIDTH="35">
    <FIELD LABEL="Field.Label.EventClass">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dtls$eventClass"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />


  </ACTION_SET>


</PAGE>
