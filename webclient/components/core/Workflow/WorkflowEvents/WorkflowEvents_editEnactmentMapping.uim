<!-- Edits a data mapping in a Workfow event enactment mapping              -->
<PAGE
  PAGE_ID="WorkflowEvents_editEnactmentMapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="WorkflowEvents"
    NAME="ACTION"
    OPERATION="modifyProcessEnactmentEventData"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="WorkflowEvents"
    NAME="DISPLAY"
    OPERATION="readProcessEnactmentEventData"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="procEvtMappingIDParam"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="procEvtMappingIDParam"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$procEvtMappingID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="procEvtMappingIDParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="data$procEvtMappingID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$procStartEventID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="data$procStartEventID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$wdoAttribute"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="data$wdoAttribute"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.EventMapping"
      WIDTH="45"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$eventField"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="data$eventField"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


</PAGE>
