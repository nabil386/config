<!-- Views an Event Type and its associated Workflow process enactments     -->
<PAGE PAGE_ID="WorkflowEvents_viewEventType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="WorkflowEvents" NAME="DISPLAY" OPERATION="getAllProcessEnactmentsForEvent" PHASE="DISPLAY"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="eventClassParam"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$eventClass"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="eventTypeParam"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$eventType"/>
  </CONNECT>


  <PAGE_PARAMETER NAME="eventClassParam"/>
  <PAGE_PARAMETER NAME="eventTypeParam"/>


  <LIST TITLE="Cluster.Title.ProcessesEnacted">
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Enable">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowEvents_enableEventMapping">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="eventTypeParam"/>
            <TARGET NAME="PAGE" PROPERTY="eventTypeParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processNameParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processStartEventID"/>
            <TARGET NAME="PAGE" PROPERTY="processStartEventIDParam"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Disable">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowEvents_disableEventMapping">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="eventTypeParam"/>
            <TARGET NAME="PAGE" PROPERTY="eventTypeParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processNameParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processStartEventID"/>
            <TARGET NAME="PAGE" PROPERTY="processStartEventIDParam"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkflowEvents_deleteProcessEnactmentMapping" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processStartEventID"/>
            <TARGET NAME="PAGE" PROPERTY="processStartEventIDParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processName"/>
            <TARGET NAME="PAGE" PROPERTY="processNameParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="eventClassParam"/>
            <TARGET NAME="PAGE" PROPERTY="eventClassParam"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="eventTypeParam"/>
            <TARGET NAME="PAGE" PROPERTY="eventTypeParam"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.WorkflowProcess" WIDTH="60">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processDisplayName"/>
      </CONNECT>
      <LINK PAGE_ID="ProcessDefinitionTool_viewProcess">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processID"/>
          <TARGET NAME="PAGE" PROPERTY="processID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processVersionNo"/>
          <TARGET NAME="PAGE" PROPERTY="processVersionNo"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Enabled" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$enabled"/>
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="WorkflowEvents_processEnactmentMappings">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$processStartEventID"/>
          <TARGET NAME="PAGE" PROPERTY="processStartEventIDParam"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>


</PAGE>