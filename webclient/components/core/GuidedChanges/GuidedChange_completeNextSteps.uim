<PAGE
  PAGE_ID="GuidedChange_completeNextSteps"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ManageGuidedChangeNextStep"
    NAME="ACTION"
    OPERATION="markGuidedChangeCOCComplete"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="guidedChangeCOCID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="guidedChangeCOCID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="guidedChangeCOCID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.CancelMarkComplete"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
