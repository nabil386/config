<PAGE
  PAGE_ID="GuidedChange_listCompletedNextStepsAction"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="guidedChangeCOCID"/>


  <SERVER_INTERFACE
    CLASS="ManageGuidedChangeNextStep"
    NAME="DISPLAY"
    OPERATION="listAddedChange"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="guidedChangeCOCID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$guidedChangeCOCID"
    />
  </CONNECT>


  <LIST>
    <FIELD
      LABEL="Field.Lable.Change"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$changeType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$status"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
