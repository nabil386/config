<!-- Evidence infrastructure view page to be included in the client page    -->
<!-- for applying evidence changes                                          -->
<?curam-deprecated Since Curam 6.0, replaced by Incident_closeIncident1.uim?>
<PAGE PAGE_ID="Incident_closeIncident" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>


    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="pageDescription"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Incidents" NAME="ACTION" OPERATION="closeIncident" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="incidentID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="incidentID"/>
    <TARGET NAME="ACTION" PROPERTY="incidentID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="FALSE">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="25" NUM_COLS="1" TITLE="Cluster.Details.Title">
    <FIELD LABEL="Field.Label.ClosureReason" USE_BLANK="TRUE" USE_DEFAULT="FALSE" WIDTH="30">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="closureReason"/>
      </CONNECT>
    </FIELD>


    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>