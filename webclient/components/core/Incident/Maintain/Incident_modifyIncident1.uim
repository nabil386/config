<!-- This page allows the user to modify an incident.                       -->
<PAGE
  PAGE_ID="Incident_modifyIncident1"
  WINDOW_OPTIONS="width=1200"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Incidents"
    NAME="DISPLAY"
    OPERATION="viewIncident1"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Incidents"
    NAME="ACTION"
    OPERATION="maintainIncident1"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="incidentID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="incidentID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$incidentID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="incidentID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="incidentID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="incidentDtls$versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$incidentDtls$versionNo"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$reporterVersionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$reporter$versionNo"
    />
  </CONNECT>


  <CLUSTER STYLE="cluster-cpr-no-internal-padding">
    <CLUSTER
      LABEL_WIDTH="30"
      NUM_COLS="2"
      STYLE="cluster-cpr-no-border"
    >
      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="60"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="type"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="type"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Sensitivity"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="sensitivityCode"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="sensitivityCode"
          />
        </CONNECT>
      </FIELD>


      <!-- BEGIN, CR00386722, MP -->
      <FIELD
        LABEL="Field.Label.Location"
        WIDTH="100"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="location"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="location"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Category"
        WIDTH="50"
      >
        <CONNECT>


          <SOURCE
            NAME="DISPLAY"
            PROPERTY="category"
          />
        </CONNECT>


        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="category"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Severity"
        WIDTH="50"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="severity"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="severity"
          />
        </CONNECT>
      </FIELD>
      <!-- END, CR00386722 -->


    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="17"
      STYLE="cluster-cpr-no-border"
    >
      <FIELD LABEL="Field.Label.Description">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="description"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="48"
    NUM_COLS="3"
    STYLE="cluster-cpr-no-internal-padding"
    TITLE="Cluster.Occurrence.Title"
  >
    <FIELD
      LABEL="Field.Label.IncidentDate"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="incidentDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="incidentDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.IncidentTime"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="incidentTime"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="incidentTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.TimeOfDay"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="timeOfDay"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="timeOfDay"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    STYLE="cluster-cpr-no-internal-padding"
    TITLE="Cluster.ReporterDetails.Title"
  >
    <CLUSTER
      LABEL_WIDTH="50"
      NUM_COLS="3"
      STYLE="cluster-cpr-no-border"
    >
      <FIELD LABEL="Field.Label.ReportedBy">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reporterFullName"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00281152, DJ -->
      <FIELD
        LABEL="Field.Label.ReportMethod"
        USE_BLANK="true"
      >
        <!-- END, CR00281152 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reportMethod"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="reportMethod"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.ReportedBy.Description"
      LABEL_WIDTH="50"
      NUM_COLS="3"
      STYLE="cluster-cpr-no-border"
    >
      <FIELD LABEL="Field.Label.AnonymousReport">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="anonymousReportInd"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="anonymousReportInd"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.ReportedByMe">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reportedByMeInd"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="reportedByMeInd"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <INCLUDE FILE_NAME="IncidentParticipant_incidentReporterParticipant_content.vim"/>


  </CLUSTER>
</PAGE>
