      <!--BEGIN, CR00140531, CL-->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="authorAndCreatedByFullName$createdByFullName"
        />
      </CONNECT>
      <!--END, CR00140531 -->
      <LINK
