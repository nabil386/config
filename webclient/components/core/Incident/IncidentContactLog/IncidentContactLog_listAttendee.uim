          <LINK
            OPEN_MODAL="true"
            PAGE_ID="IncidentContactLog_removeContactLogContact1"
            SAVE_LINK="true"
          >
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="incidentID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="incidentID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="contactLogID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contactLogID"
              />
