            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="userList$dtls$contactLogAttendeeID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contactLogAttendeeID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="userList$dtls$versionNo"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
          </LINK>
