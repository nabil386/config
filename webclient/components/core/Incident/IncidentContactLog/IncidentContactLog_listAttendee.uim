        <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="IncidentContactLog_removeContactLogContact1"
            SAVE_LINK="true"
          >
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="incidentID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="incidentID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="contactLogID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contactLogID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="concernRoleList$dtls$contactLogAttendeeID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contactLogAttendeeID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="concernRoleList$dtls$versionNo"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>


      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleList$dtls$concernRoleName"
          />
        </CONNECT>
        <LINK PAGE_ID="Participant_resolve">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="concernRoleList$dtls$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
        </LINK>
      </FIELD>


    </LIST>


    <LIST TITLE="List.Title.Users">


      <ACTION_SET TYPE="LIST_ROW_MENU">
