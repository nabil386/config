          <SOURCE
            NAME="PAGE"
            PROPERTY="incidentID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="incidentID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PREVIEW"
            PROPERTY="multiSelectStr"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="multiSelectStr"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
	<CONDITION>
      <IS_TRUE
        NAME="DISPLAY_SUBJECT"
        PROPERTY="result$contactLogSubjectEnabledInd"
      />
    </CONDITION>

    <!-- BEGIN, CR00407986, AC-->
    <CONTAINER>
      <WIDGET
