<!-- Evidence infrastructure view page to be included in the client page    -->
<!-- for applying evidence changes                                          -->
<PAGE PAGE_ID="IncidentParticipant_maintainIncidentParticipantUsername1" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Incidents" NAME="ACTION" OPERATION="maintainIncidentParticipantRole" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="Incidents" NAME="DISPLAY" OPERATION="viewIncidentParticipantRole" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="incidentParticipantRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="incidentParticipantRoleID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$incidentParticipantRoleID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="incidentParticipantRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="incidentParticipantRoleID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="40" NUM_COLS="2">


    <FIELD LABEL="Field.Label.Participant">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="fullName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RoleType" USE_DEFAULT="FALSE">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="incidentParticipantRoleType"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="incidentParticipantRoleType"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="FALSE" TITLE="Cluster.Comments.Title">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Comments.Title">
      <!-- END, CR00463142 -->
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>