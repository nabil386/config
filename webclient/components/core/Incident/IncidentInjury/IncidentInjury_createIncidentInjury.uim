<!-- This page allows the user to create an incident injury.            -->
<PAGE PAGE_ID="IncidentInjury_createIncidentInjury" WINDOW_OPTIONS="width=700,height=950" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="IncidentInjuries" NAME="ACTION" OPERATION="createIncidentInjury" PHASE="ACTION"/>
  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
  <SERVER_INTERFACE CLASS="Incidents" NAME="DISPLAY" OPERATION="listIncidentActiveParticipantRole"/>
  <PAGE_PARAMETER NAME="incidentID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="incidentID"/>
    <TARGET NAME="DISPLAY" PROPERTY="incidentID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="incidentID"/>
    <TARGET NAME="ACTION" PROPERTY="details$dtls$incidentID"/>
  </CONNECT>
  <CLUSTER LABEL_WIDTH="33" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Severity" USE_BLANK="true" USE_DEFAULT="false" WIDTH="47">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="injurySeverity"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Source" USE_BLANK="true" USE_DEFAULT="false" WIDTH="47">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="sourceOfInjury"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Injured" WIDTH="85">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="incidentParticipantRoleID" NAME="DISPLAY" PROPERTY="participantName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="injuredParticipant"/>
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="CHECKBOXED_LIST" HEIGHT="4" LABEL="Field.Label.Responsible">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="incidentParticipantRoleID" NAME="DISPLAY" PROPERTY="participantName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="responsibleParticipantTab"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER SHOW_LABELS="FALSE" STYLE="cluster-cpr-no-internal-padding" TITLE="Cluster.Description.Title">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD LABEL="Cluster.Description.Title">
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER SHOW_LABELS="FALSE" STYLE="cluster-cpr-no-internal-padding" TITLE="Cluster.ActionTaken.Title">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD LABEL="Cluster.ActionTaken.Title">
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="actionTaken"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>