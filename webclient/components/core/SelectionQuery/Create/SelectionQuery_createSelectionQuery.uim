<!-- This page allows a user to create a selection query                     -->
<PAGE PAGE_ID="SelectionQuery_createSelectionQuery" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="SelectionQuery" NAME="DISPLAY" OPERATION="readSelectionQuery" PHASE="DISPLAY"/>


  <SERVER_INTERFACE ACTION_ID_PROPERTY="details$actionIDProperty" CLASS="SelectionQuery" NAME="ACTION" OPERATION="createOrRemoveSelectionQuery" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="selectionQueryID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="selectionQueryID"/>
    <TARGET NAME="ACTION" PROPERTY="details$dtls$selectionQueryID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="selectionQueryID"/>
    <TARGET NAME="ACTION" PROPERTY="details$selectionQueryID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="selectionQueryID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$selectionQueryID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="details$versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="details$dtls$versionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL ACTION_ID="CREATE_AND_EXIT_SELECTION_QUERY" IMAGE="SaveAndExitButton" LABEL="ActionControl.Label.SaveAndExit" TYPE="SUBMIT">
      <LINK PAGE_ID="SelectionQuery_listSelectionQueries"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL ACTION_ID="CREATE_SELECTION_QUERY" IMAGE="NextButton" LABEL="ActionControl.Label.Next" TYPE="SUBMIT">
      <LINK DISMISS_MODAL="false" PAGE_ID="SelectionQuery_addCriteria">
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="result$selectionQueryID"/>
          <TARGET NAME="PAGE" PROPERTY="selectionQueryID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="37" NUM_COLS="2">


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Query" USE_BLANK="true" USE_DEFAULT="false" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="category"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="category"/>
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <FIELD LABEL="Field.Label.Type" USE_BLANK="true" USE_DEFAULT="false" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="type"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER DESCRIPTION="Cluster.Description.Details" LABEL_WIDTH="37" NUM_COLS="2" TITLE="Cluster.Title.PageNames">


    <FIELD LABEL="Field.Label.RandomPageName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="randPageName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="randPageName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ManualPageName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="manPageName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="manPageName"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="18.5">


    <FIELD HEIGHT="4" LABEL="Field.Label.SQL">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="sqlText"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="sqlText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>