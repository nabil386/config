<!-- This page lists all criteria for a selection query. 										                   -->
<PAGE
  PAGE_ID="SelectionQuery_listCriteria"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="SelectionQuery"
    NAME="DISPLAY"
    OPERATION="listSelectionQueryCriteria"
  />


  <PAGE_PARAMETER NAME="selectionQueryID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="selectionQueryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$selectionQueryID"
    />
  </CONNECT>


  <LIST>


    <FIELD LABEL="Field.Label.CriteriaName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Value">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$value"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DisplayName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$displayName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DisplayValue">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$displayValue"
        />
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>
