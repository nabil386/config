<!-- Page displaying the details of a task definition.                      -->
<PAGE PAGE_ID="WorkAllocation_viewTaskDefinition" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="name"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="WorkAllocation" NAME="DISPLAY" OPERATION="readTaskDefinitionDetails"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="DeleteButton" LABEL="ActionControl.Label.Delete">
      <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_cancelTaskDefinition" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="taskDefinitionID"/>
          <TARGET NAME="PAGE" PROPERTY="taskDefinitionID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
          <TARGET NAME="PAGE" PROPERTY="versionNo"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="name"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="taskDefinitionID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskDefinitionID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$dtls$dtls$taskDefinitionID"/>
  </CONNECT>


  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Details">


    <FIELD LABEL="Field.Label.TaskDefinitionID">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$dtls$taskDefinitionID"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ManualForwardAllowed">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="allowForwardInd"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AdministrationSID">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="administrationSID"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.AllocationStrategy">


    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="allocationStrategyCode"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AllocationName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="allocatorFunctionName"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.DeadlineStrategy">


    <FIELD LABEL="Field.Label.Function">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="deadlineFunctionName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TimeOutInterval">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="deadlineTimeoutString"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TimeOverrideAllowed">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="deadlineTimeOverrideInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="25" TITLE="Cluster.Title.ActionPage">


    <FIELD LABEL="Field.Label.PageName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="actionPageName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="actionPageNameCode"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CaseIDParameterName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseIDParameterName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ParticipantIDParameterName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="participantIDParameterName"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>