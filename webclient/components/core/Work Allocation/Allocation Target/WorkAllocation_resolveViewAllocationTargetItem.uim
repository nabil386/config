<!-- Resolve script used to determine the details page to open to view      -->
<!-- the details of an allocation target item.                              -->
<PAGE
  PAGE_ID="WorkAllocation_resolveViewAllocationTargetItem"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String type = request.getParameter("type");
      String relatedID = request.getParameter("relatedID");
      String relatedName = request.getParameter("relatedName");
      String organisationStructureID = request.getParameter("organisationStructureID");
      String organisationUnitID = request.getParameter("organisationUnitID");
      String url = "";
      
      if (type == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "type");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }
      if (relatedID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "relatedID");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }
      if (relatedName == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "relatedName");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }
      if (type.equals("RL10") &amp;&amp; organisationUnitID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "organisationUnitID");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }
      if (type.equals("RL21") &amp;&amp; organisationStructureID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "organisationStructureID");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }

      if (type.equals("RL9")) {
       
        url = context + "Organization_userHomePage.do?userName=" + curam.omega3.request.RequestUtils.escapeURL(relatedName);
      } else if (type.equals("RL21")) {
      //BEGIN, CR00147349, SS
        url = context + "Organization_viewPositionPage.do?positionID=" + curam.omega3.request.RequestUtils.escapeURL(relatedID) 
          + "&amp;organisationStructureID=" + curam.omega3.request.RequestUtils.escapeURL(organisationStructureID) 
          + "&amp;organisationUnitID=" + curam.omega3.request.RequestUtils.escapeURL(organisationUnitID);
      //END, CR00147349
      } else if (type.equals("RL10")) {
       //BEGIN, CR00147349, SS
        url = context + "Organization_orgUnitHomePage.do?&amp;organisationStructureID=" 
        + curam.omega3.request.RequestUtils.escapeURL(organisationStructureID) 
        + "&amp;organisationUnitID=" + curam.omega3.request.RequestUtils.escapeURL(organisationUnitID);
        //END, CR00147349
      } else if (type.equals("RL20")) {
        url = context + "Organization_viewJobPage.do?jobID=" + curam.omega3.request.RequestUtils.escapeURL(relatedID);
      } else if (type.equals("RL22")) {
        url = context + "Organization_orgUnitHomePage.do?organisationUnitID=" + curam.omega3.request.RequestUtils.escapeURL(relatedID);
      } else if (type.equals("RL23")) {
        url = context + "WorkAllocation_viewWorkQueuePage.do?workQueueID=" + curam.omega3.request.RequestUtils.escapeURL(relatedID);
      // BEGIN , CR130021 , DJ
      } else if (type.equals("RL50")) {
        url = context + "ExternalUser_userHomePage.do?userName=" + curam.omega3.request.RequestUtils.escapeURL(relatedName);
      // END, CR130021
      }

      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
