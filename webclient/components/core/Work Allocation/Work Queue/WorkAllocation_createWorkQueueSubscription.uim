<!-- This page allows the user or an org object to add a subscription to work queue.  -->
<PAGE
  PAGE_ID="WorkAllocation_createWorkQueueSubscription"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="WorkAllocation"
    NAME="ACTION"
    OPERATION="createWorkQueueSubscription"
    PHASE="ACTION"
  />
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.SaveButton.label"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.CancelButton.label"
    />
  </ACTION_SET>
  <PAGE_PARAMETER NAME="workQueueID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="workQueueID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$workQueueID"
    />
  </CONNECT>
  <!--
    Modified as part of new functionality for allowing org object to
    subscribe to a work queue in addition to user
  -->
  <!-- BEGIN, CR00161962, KY -->
  <CLUSTER LABEL_WIDTH="25">
    <CONTAINER LABEL="Container.Label.subscriber">
      <FIELD WIDTH="25">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$subscriberType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="65">
        <!-- BEGIN, CR00225579, LP -->
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$subscriberNameOrID"
          />
        </CONNECT>
        <!-- END, CR00225579 -->
      </FIELD>
    </CONTAINER>
    <!-- END, CR00161962 -->
    <!-- END -->
  </CLUSTER>
</PAGE>
