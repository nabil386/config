<!-- This page allows the user to view a task home page.                    -->
<PAGE PAGE_ID="WorkAllocation_viewTask" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="WorkAllocation" NAME="DISPLAY" OPERATION="readTaskSummaryDetails"/>
  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="EditButton" LABEL="ActionControl.EditButton.label">
      <LINK PAGE_ID="WorkAllocation_resolveTaskModify" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="taskID"/>
          <TARGET NAME="PAGE" PROPERTY="taskID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="userSetDeadlineInd"/>
          <TARGET NAME="PAGE" PROPERTY="userSetDeadlineInd"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER NUM_COLS="3" SHOW_LABELS="false" TITLE="Cluster.Title.Manage">
    <CONTAINER>
      <ACTION_CONTROL IMAGE="ReserveTaskIcon" LABEL="ActionControl.Label.Reserve">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_reserveTaskFromView" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Reserve">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_reserveTaskFromView" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL IMAGE="UnreserveTaskIcon" LABEL="ActionControl.Label.Unreserve">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_unreserveTask" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Unreserve">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_unreserveTask" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL IMAGE="ForwardTaskIcon" LABEL="ActionControl.Label.Forward">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_forwardTask" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Forward">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_forwardTask" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL IMAGE="ReallocateTaskIcon" LABEL="ActionControl.Label.Reallocate">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_reallocateTask" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Reallocate">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_reallocateTask" SAVE_LINK="false" WINDOW_OPTIONS="width=400">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL IMAGE="CloseTaskIcon" LABEL="ActionControl.Label.Close">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_closeTask" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Close">
        <LINK OPEN_MODAL="true" PAGE_ID="WorkAllocation_closeTask" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="taskID"/>
            <TARGET NAME="PAGE" PROPERTY="taskID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>
  <PAGE_PARAMETER NAME="taskID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$dtls$dtls$taskID"/>
  </CONNECT>
  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Subject">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="subject"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="30" NUM_COLS="2" TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.TaskID">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$taskID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="priority"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="status"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Deadline">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dueDateTime"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TimeWorked">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="totalTimeWorked"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Resolution">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="resolution"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="15" TITLE="Cluster.Title.AssignmentDetails">
    <FIELD LABEL="Field.Label.LastAssigned">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="assignedDateTime"/>
      </CONNECT>
    </FIELD>
    <CONTAINER LABEL="Field.Label.ReservedBy">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="reservedByUserFullName"/>
        </CONNECT>
        <LINK PAGE_ID="Organization_userHome">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="reservedBy"/>
            <TARGET NAME="PAGE" PROPERTY="userName"/>
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER LABEL_WIDTH="30" NUM_COLS="2" TITLE="Cluster.Title.Concerning">
    <FIELD LABEL="Field.Label.Action">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="actionDescription"/>
      </CONNECT>
      <LINK PAGE_ID="WorkAllocation_resolveTaskAction">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="taskID"/>
          <TARGET NAME="PAGE" PROPERTY="taskID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="participantReferenceID"/>
          <TARGET NAME="PAGE" PROPERTY="participantID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.CaseReference">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseReferenceNumber"/>
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Participant">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="participantName"/>
      </CONNECT>
      <LINK PAGE_ID="Participant_resolveRoleHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="participantReferenceID"/>
          <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="participantTypeCode"/>
          <TARGET NAME="PAGE" PROPERTY="participantType"/>
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>
</PAGE>