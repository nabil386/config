<!-- This page allows the user to close a task.                             -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="WorkAllocation_closeTask" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="pageDescription" />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="WorkAllocation" NAME="ACTION" OPERATION="closeTask" PHASE="ACTION" />
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" TYPE="SUBMIT" LABEL="ActionControl.SaveButton.label">
      <LINK PAGE_ID="WorkAllocation_viewTask" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="taskID" />
          <TARGET NAME="PAGE" PROPERTY="taskID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.CancelButton.label">
      <LINK PAGE_ID="WorkAllocation_viewTask" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="taskID" />
          <TARGET NAME="PAGE" PROPERTY="taskID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <PAGE_PARAMETER NAME="taskID" />
  <PAGE_PARAMETER NAME="pageDescription" />
  <PAGE_PARAMETER NAME="versionNo" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID" />
    <TARGET NAME="ACTION" PROPERTY="taskID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo" />
    <TARGET NAME="ACTION" PROPERTY="versionNo" />
  </CONNECT>
  <CLUSTER TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.Resolution">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="resolution" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>

