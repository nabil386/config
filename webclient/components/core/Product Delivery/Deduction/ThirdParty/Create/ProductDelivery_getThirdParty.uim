<!-- This page finds the third party to associate with a                    -->
<!-- third party deduction                                                  -->
<?curam-deprecated Since Curam 6.0. Replaced by ProductDelivery_createThirdPartyFixedDeduction1.uim and ProductDelivery_createThirdPartyVariableDeduction1.uim?>
<PAGE PAGE_ID="ProductDelivery_getThirdParty" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="category"/>
  <PAGE_PARAMETER NAME="deductionName"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="PreviousButton" LABEL="ActionControl.Label.Previous">
      <LINK PAGE_ID="ProductDelivery_selectThirdPartyName" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="pageDescription"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="NextButton" LABEL="ActionControl.Label.Next" TYPE="SUBMIT">
      <LINK PAGE_ID="ProductDelivery_selectThirdPartyDeductionType" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="pageDescription"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="category"/>
          <TARGET NAME="PAGE" PROPERTY="category"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="deductionName"/>
          <TARGET NAME="PAGE" PROPERTY="deductionName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="concernRoleID"/>
          <TARGET NAME="PAGE" PROPERTY="thirdPartyConcernRoleID"/>
        </CONNECT>


      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="ACTION" PROPERTY="caseID"/>
  </CONNECT>


  <SERVER_INTERFACE CLASS="ProductDelivery" NAME="ACTION" OPERATION="getDeductionClient" PHASE="ACTION"/>


  <CLUSTER DESCRIPTION="Cluster.ThirdPartySearch.Description" LABEL_WIDTH="30" NUM_COLS="1">
    <CONTAINER LABEL="Field.Label.Participant">
      <FIELD WIDTH="25">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="concernRoleType"/>
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="74">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="clientConcernRoleID"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


</PAGE>