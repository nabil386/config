<!-- Create a fixed applied deduction for a product delivery.               -->
<PAGE
  PAGE_ID="ProductDelivery_createAppliedFixedDeduction1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=775"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      ACTION_ID="ACTIVATE_NOW_FIXED"
      LABEL="ActionControl.Label.ActivateNow"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      ACTION_ID="ACTIVATE_LATER_FIXED"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="ProductDelivery_createAppliedFixedDeduction1Content.vim"/>


</PAGE>
