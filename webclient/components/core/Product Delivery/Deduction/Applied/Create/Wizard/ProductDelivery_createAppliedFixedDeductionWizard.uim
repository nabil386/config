<!-- Create a fixed applied deduction for a product delivery.               -->
<PAGE
  PAGE_ID="ProductDelivery_createAppliedFixedDeductionWizard"
  WINDOW_OPTIONS="width=800,height=420"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Financial"
    NAME="DISPLAYWIZARD"
    OPERATION="getAppliedFixedDeductionWizard"
  />


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      ALIGNMENT="LEFT"
    />
    <ACTION_CONTROL
      ACTION_ID="ACTIVATE_NOW_FIXED"
      LABEL="ActionControl.Label.ActivateNow"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="BackButton"
      LABEL="ActionControl.Label.Back"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="ProductDelivery_getParticipantAndLiabilityWizard"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      ACTION_ID="ACTIVATE_LATER_FIXED"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


  </ACTION_SET>
  
  <INCLUDE FILE_NAME="ProductDelivery_createAppliedFixedDeduction1Content.vim"/>


</PAGE>
