<?curam-deprecated Since Curam 6.0, replaced by ProductDelivery_createUnappliedVariableDeduction1.uim?>
<!-- END, CR00238748 -->
<PAGE PAGE_ID="ProductDelivery_createUnappliedVariableDeduction" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00021334, SK  -->
  <!-- BEGIN, HARP 61923, RCB -->
  <SERVER_INTERFACE CLASS="ProductDelivery" NAME="DISPLAY" OPERATION="readDefaultDeductionDetails"/>
  <!-- END, HARP 61923 -->
  <!-- END, CR00021334 -->
  <SERVER_INTERFACE CLASS="ProductDelivery" NAME="ACTION" OPERATION="createVariableDeduction" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="Case" NAME="DISPLAYNOM" OPERATION="listCaseNominee"/>


  <SERVER_INTERFACE CLASS="Case" NAME="DISPLAYCO" OPERATION="listCaseObjectiveDetails"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="deductionName"/>
  <PAGE_PARAMETER NAME="category"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="deductionName"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$deductionName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAYNOM" PROPERTY="caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAYCO" PROPERTY="caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="ACTION" PROPERTY="caseID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="category"/>
    <TARGET NAME="ACTION" PROPERTY="category"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="deductionName"/>
    <TARGET NAME="ACTION" PROPERTY="deductionName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="actionType"/>
    <TARGET NAME="ACTION" PROPERTY="actionType"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="PreviousButton" LABEL="ActionControl.Label.Previous">
      <LINK PAGE_ID="ProductDelivery_selectUnappliedDeductionType" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="pageDescription"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="deductionName"/>
          <TARGET NAME="PAGE" PROPERTY="deductionName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="category"/>
          <TARGET NAME="PAGE" PROPERTY="category"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.DeductionName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dtls$deductionName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>
    <!--BEGIN CR00097529, PN-->
    <FIELD LABEL="Field.Label.Percentage" WIDTH="4" WIDTH_UNITS="CHARS">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="defaultPercentage"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="rate"/>
      </CONNECT>
    </FIELD>
    <!--END CR00097529-->
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="priority"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="priority"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Component" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="rulesObjectiveIDPopup" NAME="DISPLAYCO" PROPERTY="objectiveCode"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="rulesObjectiveIDPopup"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Nominee" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="caseNomineeID" NAME="DISPLAYNOM" PROPERTY="concernRoleName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="nomineeIDPopup"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AssignNextPriority">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="applyNextPriorityInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>