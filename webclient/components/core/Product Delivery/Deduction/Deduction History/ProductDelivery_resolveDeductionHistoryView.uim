<!-- This jsp redirects the user to the appropriate page to display the     -->
<!-- details of a financial instruction, e.g. payment, bill, reversal etc.  -->
<?curam-deprecated Since Curam 6.0. Replaced by direct link to ICProductSample_listDeductionHistory.uim. ?>
<PAGE
  PAGE_ID="ProductDelivery_resolveDeductionHistoryView"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String caseDeductionItemID = request.getParameter("caseDeductionItemID");
      String pageDescription = request.getParameter("pageDescription");
      String relatedCaseID = "0";
      String thirdPartyConcernRoleID = "0";
      
      String url = "";
           
      curam.interfaces.ProductDeliveryPkg.ProductDelivery_readRelatedCaseIDAndThirdPartyID_TH th = new curam.interfaces.ProductDeliveryPkg.ProductDelivery_readRelatedCaseIDAndThirdPartyID_TH();
  
      th.setFieldValue(th.key$caseDeductionItemID_idx,caseDeductionItemID);
        
      th.callServer();  
  
      relatedCaseID = th.getFieldValue(th.result$dtls$relatedCaseID_idx);
      thirdPartyConcernRoleID = th.getFieldValue(th.result$dtls$thirdPartyConcernRoleID_idx);
  
  
      if (!relatedCaseID.equals("0")) {
      url = context + "ProductDelivery_listAppliedDeductionHistoryPage.do?caseDeductionItemID=" + curam.omega3.request.RequestUtils.escapeURL(caseDeductionItemID);
      } else if (!thirdPartyConcernRoleID.equals("0")){
      url = context + "ProductDelivery_listThirdPartyDeductionHistoryPage.do?caseDeductionItemID=" + curam.omega3.request.RequestUtils.escapeURL(caseDeductionItemID);
      } else {
      url = context + "ProductDelivery_listUnappliedDeductionHistoryPage.do?caseDeductionItemID=" + curam.omega3.request.RequestUtils.escapeURL(caseDeductionItemID);
      }
      
      if(pageDescription!=null) {
      url += "&amp;pageDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);
      }

      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
      
    
  </JSP_SCRIPTLET>
</PAGE>
