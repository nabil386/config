<!-- Description -->
<!-- =========== -->
<!-- This page is used to display a list of all determinations for the      -->
<!-- product delivery                                                       -->
<PAGE
  PAGE_ID="ProductDelivery_listDeterminations"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CaseDetermination"
    NAME="DISPLAY"
    OPERATION="listAssessmentHistory"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <LIST>


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="ProductDelivery_listCreoleDecisionsForDetermination">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseAssessmentDtls$determinationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>




    <CONTAINER
      LABEL="Field.Label.CreationDate"
      WIDTH="20"
    >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseAssessmentDtls$creationDateTime"
        />
      </CONNECT>
    </FIELD>
    <ACTION_CONTROL IMAGE="AppealsIcon" STYLE="in-page-list-icon">
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="determinationAppealIndOpt"
        />
      </CONDITION>
      <LINK
        PAGE_ID="Case_resolveCaseHome"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseAppealIDOpt"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
      </ACTION_CONTROL>
    </CONTAINER>



    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseAssessmentDtls$status"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Reason"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseAssessmentDtls$reason"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.CreatedBy"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseAssessmentDtls$createdByFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseAssessmentDtls$createdByUserName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>


    </FIELD>


  </LIST>


</PAGE>
