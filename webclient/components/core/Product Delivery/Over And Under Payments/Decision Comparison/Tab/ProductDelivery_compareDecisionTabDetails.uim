<!-- Decision comparison tab details.                                       -->
<PAGE
  PAGE_ID="ProductDelivery_compareDecisionTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.DecisionComparison"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Separator"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="productDeliveryType"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="productDeliveryRef"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.DecisionComparison"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Separator"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="productDeliveryType"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="productDeliveryRef"
      />
    </CONNECT>
  </TAB_NAME>


  <SERVER_INTERFACE
    CLASS="DecisionComparisonTab"
    NAME="DISPLAY"
    OPERATION="readDecisionComparisonTabDetail"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="oldDecisionID"/>
  <PAGE_PARAMETER NAME="reassessedDecisionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="oldDecisionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$oldCaseDecisionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="reassessedDecisionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$newCaseDecisionID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.CurrentDecision">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="currentDecisionResult"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CurrentDecisionPeriod">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="currentDecisionPeriod"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PreviousDecision">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="previousDecisionResult"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PreviousDecisionPeriod">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="previousDecisionPeriod"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
