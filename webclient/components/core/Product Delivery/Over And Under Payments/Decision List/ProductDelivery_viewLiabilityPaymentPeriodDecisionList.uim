<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows a user to display view the list of decisions (before  -->
<!-- and after reassessment) for a period for which an over or under        -->
<!-- payment has occurred.                                                  -->
<PAGE
  PAGE_ID="ProductDelivery_viewLiabilityPaymentPeriodDecisionList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="NAVIGATION">
    <ACTION_CONTROL LABEL="ActionControl.Label.Statement">
      <LINK
        PAGE_ID="ProductDelivery_viewLiabilityPaymentStatement"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="nomineeOverUnderPaymentID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="overUnderPaymentID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <LIST>


    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="12"
    >


      <ACTION_CONTROL LABEL="ActionControl.Label.Compare">
        <LINK PAGE_ID="ProductDelivery_resolveCompareDecision">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="oldDecisionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="oldDecisionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="reassessedDecisionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="reassessedDecisionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="reassessmentInfoID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="reassessmentInfoID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </CONTAINER>


    <FIELD
      LABEL="Field.Label.OriginalDecisionFromDate"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="oldDecisionFromDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.OriginalDecisionToDate"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="oldDecisionToDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.NewDecisionFromDate"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reassessedDecisionFromDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.NewDecisionToDate"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reassessedDecisionToDate"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <INCLUDE FILE_NAME="ProductDelivery_viewPaymentPeriodDecisionListView.vim"/>


</PAGE>