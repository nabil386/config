          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.Label.TotalDeduction"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.TotalDeduction"
        WIDTH="10"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="totalDeduction"
          />
        </CONNECT>
      </FIELD>
    </FOOTER_ROW>
    <FOOTER_ROW>
      <FIELD CONTROL="SKIP"/>
      <FIELD CONTROL="SKIP"/>
      <FIELD CONTROL="SKIP"/>
      <FIELD LABEL="Field.Label.TotalPayment">
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.Label.TotalPayment"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.TotalPayment"
        WIDTH="10"
