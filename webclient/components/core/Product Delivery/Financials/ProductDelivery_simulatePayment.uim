    >
      <!-- BEGIN, CR00086730, MM -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="ProductDelivery_saveSimulation"
        SAVE_LINK="false"
      >
        <!-- END, CR00086730 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="enteredDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="enteredDate"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.CloseButton.label"
    />
  </ACTION_SET>
  <CLUSTER
    DESCRIPTION="Cluster.Description"
    NUM_COLS="2"
