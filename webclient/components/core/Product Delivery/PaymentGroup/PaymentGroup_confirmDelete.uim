<!-- Logically deletes a payment group.                                     -->
<PAGE PAGE_ID="PaymentGroup_confirmDelete" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="PaymentGroupAdmin" NAME="ACTION" OPERATION="cancelPaymentGroup" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="paymentGroupID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
 
 
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="paymentGroupID"/>
    <TARGET NAME="ACTION" PROPERTY="paymentGroupID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>

  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Confirm.Message"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


</PAGE>