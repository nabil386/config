<!-- The page used to display the details of a decision made on             -->
<!-- a product delivery.                                                    -->
<PAGE
  PAGE_ID="ProductDelivery_viewDecisionViewOnly"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=650,height=500"
>


  <!-- BEGIN, CR00245037, MC -->

  <MENU MODE="IN_PAGE_NAVIGATION">

    <ACTION_CONTROL
      LABEL="ActionControl.Label.FullTreeButton"
      STYLE="in-page-current-link" 
    >
      <LINK
        PAGE_ID="ProductDelivery_viewDecisionViewOnly"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>

    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewStaticTree"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ProductDelivery_viewDecisionWithStaticRulesTreeViewOnly"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>

  </MENU>
  
  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Close"
    />
  </ACTION_SET>
    
  <INCLUDE FILE_NAME="ProductDelivery_viewDecisionView.vim"/>

  <FIELD CONTROL="DYNAMIC">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="resultText"
      />
    </CONNECT>
  </FIELD>


  <!-- END, CR00245037 -->

</PAGE>