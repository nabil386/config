<!--  Infrastructure page used for rejecting 1 or more evidence approval    -->
<!--  requests.                                                             -->
<PAGE PAGE_ID="Evidence_rejectApprovalRequests" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Evidence" NAME="DISPLAY" OPERATION="listAllForApproveReject" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Evidence" NAME="ACTION" OPERATION="rejectApprovalRequests" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="caseID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="RIGHT" BOTTOM="false">


    <ACTION_CONTROL IMAGE="RejectButton" LABEL="ActionControl.Label.RejectEvidence" TYPE="SUBMIT">
     </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
     </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="17">


    <!-- BEGIN, CR00291788 -->
    <FIELD LABEL="Field.Label.RejectionReason" WIDTH="45">
      <!-- END, CR00291788 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="rejectionReason"/>
      </CONNECT>
    </FIELD>


    <FIELD HEIGHT="4" LABEL="Field.Label.RejectionComments" WIDTH="88">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <LIST PAGINATED="false" TITLE="List.EvidenceList">
    <INCLUDE FILE_NAME="Evidence_listApprovalRequestedView.vim"/>
  </LIST>


</PAGE>