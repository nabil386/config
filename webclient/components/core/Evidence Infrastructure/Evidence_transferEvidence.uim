<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to select the case member whose evidence is  -->
<!-- being transferred to another case                                      -->
<?curam-deprecated Since Curam 6.0 SP1, replaced with Evidence_transferEvidence1?>
<PAGE
  PAGE_ID="Evidence_transferEvidence"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE DESCRIPTION="Page.Title.Description">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="key$actionIDProperty"
    CLASS="Evidence"
    NAME="ACTION"
    OPERATION="getEvidenceDetailsForTransfer"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="Evidence"
    NAME="DISPLAY"
    OPERATION="listCaseMembersAndEvidenceForTransfer"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="participantID"/>
  <PAGE_PARAMETER NAME="caseParticipantRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$caseID"
    />
  </CONNECT>
  <CLUSTER>
    <CLUSTER
      NUM_COLS="2"
      TITLE="Cluster.Title.Member"
    >
      <FIELD LABEL="Field.Label.Member">
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="caseParticipantRoleID"
            NAME="DISPLAY"
            PROPERTY="nameAndAgeOpt"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="filteredParticipantID"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="key$caseParticipantRoleID"
          />
        </CONNECT>
        <SCRIPT
          ACTION="enactHiddenActionControl_onChange()"
          EVENT="ONCHANGE"
          SCRIPT_FILE="evidenceTypeWorkspace.js"
        />
      </FIELD>
      <FIELD
        LABEL="Field.Label.Case"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$toCaseID"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <CONTAINER STYLE="inner-span-action-control-visibility-hidden">
      <ACTION_CONTROL
        ACTION_ID="NO"
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.List"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="this"/>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      ACTION_ID="YES"
      IMAGE="SearchButton"
      LABEL="ActionControl.Label.Transfer"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Evidence_transferEvidenceConfirmation"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$toCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="toCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="evidenceDetails$participantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="evidenceDetails$caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$datastoreEntityID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="datastoreEntityID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="Evidence_listEvidenceTransferView.vim"/>


</PAGE>
