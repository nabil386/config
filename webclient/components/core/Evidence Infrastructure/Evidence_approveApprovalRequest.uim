<!--  Infrastructure pages used for approving evidence                      -->
<PAGE PAGE_ID="Evidence_approveApprovalRequest" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Evidence" NAME="DISPLAY" OPERATION="listAllForApproveReject" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Evidence" NAME="ACTION" OPERATION="approveApprovalRequests" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="caseID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="RIGHT" BOTTOM="false">


    <ACTION_CONTROL IMAGE="ApproveButton" LABEL="ActionControl.Label.ApproveEvidence" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST PAGINATED="false">
    <INCLUDE FILE_NAME="Evidence_listApprovalRequestedView.vim"/>
  </LIST>


</PAGE>