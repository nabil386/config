<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Infrastructure page containing a selection of custom evidence which    -->
<!-- may be created.                                                        -->
<!-- BEGIN, CR00296357, ELG -->
<PAGE PAGE_ID="Evidence_addNewEvidence" SCRIPT_FILE="evidenceTypeWorkspace.js" WINDOW_OPTIONS="height=900,width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00296357 -->


  <SCRIPT ACTION="enactHiddenActionControl_onFirstLoad()" EVENT="ONLOAD"/>


  <PAGE_TITLE DESCRIPTION="Page.Title.Description">
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE CLASS="Evidence" NAME="CATEGORIES" OPERATION="getEvidenceCategoryList" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Evidence" NAME="LISTTYPES" OPERATION="listEditableEvidenceTypes" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="CATEGORIES" PROPERTY="caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="LISTTYPES" PROPERTY="key$caseID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" STYLE="evidence-hidden-submit">


    <FIELD LABEL="Field.Label.Category" USE_BLANK="false" USE_DEFAULT="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="categoryCode" NAME="CATEGORIES" PROPERTY="categoryDescription"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="LISTTYPES" PROPERTY="key$category"/>
      </CONNECT>
      <SCRIPT ACTION="enactHiddenActionControl_onChange()" EVENT="ONCHANGE"/>
    </FIELD>


    <CLUSTER STYLE="evidence-hidden-submit">
      <ACTION_SET ALIGNMENT="LEFT" TOP="false">
        <ACTION_CONTROL DEFAULT="true" IMAGE="SearchButton" LABEL="ActionControl.Label.List" TYPE="SUBMIT">
          <LINK PAGE_ID="THIS"/>
        </ACTION_CONTROL>
      </ACTION_SET>
    </CLUSTER>
  </CLUSTER>


  <!-- BEGIN, CR00296357, ELG -->
  <LIST PAGINATED="false">
    <!-- END, CR00296357 -->


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL APPEND_ELLIPSIS="true" LABEL="ActionControl.Label.Add">
        <LINK DISMISS_MODAL="false" PAGE_ID="Evidence_resolveCreateFromMetaData" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LISTTYPES" PROPERTY="result$evidenceTypes$dtls$evidenceType"/>
            <TARGET NAME="PAGE" PROPERTY="evidenceType"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <!-- BEGIN, CR00296357, ELG -->
    <FIELD LABEL="List.Title.EvidenceType" WIDTH="33">
      <!-- END, CR00296357 -->
      <CONNECT>
        <SOURCE NAME="LISTTYPES" PROPERTY="result$evidenceTypes$dtls$evidenceType"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00296357, ELG -->
    <FIELD LABEL="List.Title.Description" WIDTH="67">
      <!-- END, CR00296357 -->
      <CONNECT>
        <SOURCE NAME="LISTTYPES" PROPERTY="result$evidenceTypes$dtls$evidenceDesc"/>
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL LABEL="ActionControl.Label.Close"/>


  </ACTION_SET>


</PAGE>