<!-- Infrastructure page to list the evidence history.                      -->
<PAGE
  PAGE_ID="Evidence_trackEvidence1"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Evidence"
    NAME="DISPLAY"
    OPERATION="listEvidenceChangeHistory"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="evID"/>
  <PAGE_PARAMETER NAME="evType"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$evidenceID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$evidenceType"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="RIGHT">
    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >
      <LINK
        PAGE_ID="Evidence_resolveViewEvidencePage"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceType"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST
    PAGINATED="false"
    
  >


    <FIELD LABEL="List.Title.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="changeType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.Title.When">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="changeDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.Title.Who">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="changeUser"
        />
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>
