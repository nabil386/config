<!-- Creates a new meeting for a case.                                      -->
<PAGE PAGE_ID="CalendarMeeting_createMeeting" WINDOW_OPTIONS="width=1000" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="MeetingManagement" NAME="DISPLAY" OPERATION="listAttendeesForCaseCreate"/>
  <SERVER_INTERFACE CLASS="MeetingManagement" NAME="ACTION" OPERATION="create" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <SCRIPT ACTION="hideTimeFieldsOnLoad()" EVENT="ONLOAD" SCRIPT_FILE="CalendarMeetings.js"/>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$caseID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="ACTION" PROPERTY="caseID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="31" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Subject">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="subject"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Start" WIDTH="50">
      <SCRIPT ACTION="updateMeetingTime()" EVENT="ONCHANGE" SCRIPT_FILE="CalendarMeetings.js"/>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="defaultStartDateTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDateTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.End" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="defaultEndDateTime"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDateTime"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Location">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="locationName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AllDay">
      <SCRIPT ACTION="hideTimeFields()" EVENT="ONCLICK" SCRIPT_FILE="CalendarMeetings.js"/>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="allDayInd"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Priority" USE_DEFAULT="true" WIDTH="35">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="priorityCode"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Agenda">
    <FIELD HEIGHT="5" LABEL="Cluster.Title.Agenda">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="notes"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER NUM_COLS="2" SHOW_LABELS="false" STYLE="outer-cluster-borderless-nospace">
    <LIST TITLE="Cluster.Title.CaseClients">
      <CONTAINER ALIGNMENT="CENTER" LABEL="List.Title.Select" WIDTH="9">
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="clients$dtls$roleDtls$participantRoleID"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="selectedClients"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="clients$dtls$name"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Gender">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="clients$dtls$gender"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Age">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="clients$dtls$age"/>
        </CONNECT>
      </FIELD>
    </LIST>


    <LIST TITLE="Cluster.Title.CaseUsers">
      <CONTAINER ALIGNMENT="CENTER" LABEL="List.Title.Select" WIDTH="9">
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="users$dtls$dtls$ownerDetails$userName"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="selectedUsers"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      <FIELD LABEL="Field.Label.UserName">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="users$dtls$dtls$ownerDetails$userFullName"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Role">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="users$dtls$dtls$typeCode"/>
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>


  <CLUSTER NUM_COLS="2" SHOW_LABELS="false" STYLE="outer-cluster-borderless-nospace">
    <LIST TITLE="Cluster.Title.CaseParticipants">
      <CONTAINER ALIGNMENT="CENTER" LABEL="List.Title.Select" WIDTH="6">
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="participants$dtls$roleDtls$participantRoleID"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="selectedParticipants"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="participants$dtls$name"/>
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.Role">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="participants$dtls$roleDtls$typeCode"/>
        </CONNECT>
      </FIELD>


    </LIST>


    <LIST TITLE="Cluster.Title.CollaborationMembers">
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="collaborationEnabledInd"/>
      </CONDITION>
      <CONTAINER ALIGNMENT="CENTER" LABEL="List.Title.Select" WIDTH="3">
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="caseCollaborationMembers$memberDetails$memberID"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="selectedCaseCollaborationMembers"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseCollaborationMembers$memberDetails$memberNameAndRole"/>
        </CONNECT>
      </FIELD>
    </LIST>


  </CLUSTER>


</PAGE>