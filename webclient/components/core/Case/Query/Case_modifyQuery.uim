<!-- Description -->
<!-- =========== -->
<!-- This page allow a user to modify a previously defined case query.      -->
<!-- BEGIN, CR00238748, GD -->
<?curam-deprecated Since Curam 6.0, replaced by Case_modifyQuery1.uim?>
<!-- END, CR00238748 -->
<PAGE
  PAGE_ID="Case_modifyQuery"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SearchButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
	  </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="ResetButton"
      LABEL="ActionControl.Label.Cancel"
    >
	  </ACTION_CONTROL>
  </ACTION_SET>


  <SERVER_INTERFACE
    CLASS="Query"
    NAME="DISPLAY"
    OPERATION="runCaseQuery"
  />


  <SERVER_INTERFACE
    CLASS="Case"
    NAME="DISPLAY2"
    OPERATION="checkForAppealsInstallation"
  />


  <SERVER_INTERFACE
    CLASS="Query"
    NAME="ACTION"
    OPERATION="createModifyCaseQuery"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="queryID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="queryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$queryID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="queryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$queryID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="22.5"
    NUM_COLS="1"
    TITLE="Cluster.Title.QueryName"
  >
    <FIELD
      LABEL="Field.Label.MyQueryName"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="queryName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="queryName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <CLUSTER
      LABEL_WIDTH="40"
      NUM_COLS="2"
    >


      <FIELD
        LABEL="Field.Label.ClientName"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <INITIAL
            NAME="DISPLAY"
            PROPERTY="result$queryClientName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$concernRoleID"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.ClientReference"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$primaryAlternateID"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$primaryAlternateID"
          />
        </CONNECT>
      </FIELD>


      <CONTAINER LABEL="Field.Label.CategoryType">
        <FIELD
          USE_BLANK="true"
          USE_DEFAULT="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="category"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="category"
            />
          </CONNECT>
        </FIELD>
        <FIELD>
          <CONNECT>
            <INITIAL
              NAME="DISPLAY"
              PROPERTY="result$caseCategoryTypeName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="type"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="type"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Status"
        USE_BLANK="true"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="status"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="status"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="75"
      NUM_COLS="4"
    >


      <FIELD
        LABEL="Field.Label.DisplayCasesWithServicePlans"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="searchWithServicePlans"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchWithServicePlans"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.DisplayCasesWithIssues"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="searchWithIssues"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchWithIssues"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.DisplayCasesWithInvestigations"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="searchWithInvestigations"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="searchWithInvestigations"
          />
        </CONNECT>
      </FIELD>


      <CLUSTER STYLE="cluster-cpr-no-border">
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY2"
            PROPERTY="result$appealsInstalledInd"
          />
        </CONDITION>
        <FIELD
          LABEL="Field.Label.DisplayCasesWithAppeals"
          USE_DEFAULT="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="searchWithAppeals"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="searchWithAppeals"
            />
          </CONNECT>
        </FIELD>
      </CLUSTER>


    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="40"
      NUM_COLS="2"
    >
      <FIELD
        LABEL="Field.Label.StartDate"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$startDate"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$startDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.LastNumberofDays">
	      USE_DEFAULT="false"      
	    &gt;
	      <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="lastNumberOfDays"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="lastNumberOfDays"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.EndDate"
        USE_DEFAULT="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="endDate"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="endDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.LastNumberofWeeks">
	      USE_DEFAULT="false"      
	    &gt;
	      <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="lastNumberOfWeeks"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="lastNumberOfWeeks"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


  </CLUSTER>


</PAGE>
