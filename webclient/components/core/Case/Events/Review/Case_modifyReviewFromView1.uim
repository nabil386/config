<!-- Page used to modify a review event for a case.                         -->
<!-- This page is used when the edit link is selected on a view page.       -->
<PAGE
  PAGE_ID="Case_modifyReviewFromView1"
  WINDOW_OPTIONS="width=815"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="Case_viewReview"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="reviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="reviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK
        PAGE_ID="Case_viewReview"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="reviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="reviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <INCLUDE FILE_NAME="Case_modifyReviewView1.vim"/>


</PAGE>
