<PAGE
  PAGE_ID="Caseworker_cases"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Home"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Case"
    NAME="SI_CASE_SEARCH"
    OPERATION="getCaseID"
    PHASE="ACTION"
  />
  <CLUSTER
    NUM_COLS="2"
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.CaseShortcuts"
    >
      <CONTAINER>
        <ACTION_CONTROL LABEL="ActionControl.Label.CreateNewCase">
          <LINK PAGE_ID="ICSample_selectPersonType"/>
        </ACTION_CONTROL>
      </CONTAINER>
      <CONTAINER>
        <ACTION_CONTROL LABEL="ActionControl.Label.MyCases">
          <LINK PAGE_ID="Organization_userCases"/>
        </ACTION_CONTROL>
      </CONTAINER>
      <CONTAINER>
        <ACTION_CONTROL LABEL="ActionControl.Label.SearchCase">
          <LINK PAGE_ID="Case_search1"/>
        </ACTION_CONTROL>
      </CONTAINER>
    </CLUSTER>
    <CLUSTER TITLE="Cluster.Title.CaseSearchCriteria">
      <CONTAINER
        LABEL="Field.Label.CaseReference"
        WIDTH="90"
      >
        <FIELD
          LABEL="Field.Label.CaseReference"
          USE_DEFAULT="false"
        >
          <CONNECT>
            <TARGET
              NAME="SI_CASE_SEARCH"
              PROPERTY="key$caseReference"
            />
          </CONNECT>
        </FIELD>
        <!--BEGIN CR00088768, GM-->
        <ACTION_CONTROL
          IMAGE="SearchIcon"
          LABEL="Search.Icon.Text"
          TYPE="SUBMIT"
        >
          <!--END CR00088768-->
          <LINK PAGE_ID="Case_resolveCaseHome">
            <CONNECT>
              <SOURCE
                NAME="SI_CASE_SEARCH"
                PROPERTY="result$caseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>
    </CLUSTER>
  </CLUSTER>
</PAGE>
