      NAME="DISPLAY"
      PROPERTY="key$contactLogID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="true"
  >


    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
    >
      <!-- BEGIN, CR00119240, MC -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Case_modifyContactLogFromView"
        SAVE_LINK="false"
      >
        <!-- END, CR00119240 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contactLogID"
          />
