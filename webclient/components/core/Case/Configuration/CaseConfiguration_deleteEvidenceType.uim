<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to delete an evidence type from a cases.     -->
<PAGE
  PAGE_ID="CaseConfiguration_deleteEvidenceType"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="EvidenceTypesLink"
    NAME="ACTION"
    OPERATION="deleteEvidenceLink"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="evidenceTypeLinkID"/>
  <PAGE_PARAMETER NAME="associatedID"/>
  <PAGE_PARAMETER NAME="associatedCaseTypeOpt"/>
  <PAGE_PARAMETER NAME="evidenceTypeCode"/>
  <PAGE_PARAMETER NAME="versionNo"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceTypeLinkID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="evidenceTypeLinkID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="associatedID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="associatedID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="associatedCaseTypeOpt"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="associatedCaseTypeOpt"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceTypeCode"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="evidenceTypeCode"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.CancelEvidenceLink"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
