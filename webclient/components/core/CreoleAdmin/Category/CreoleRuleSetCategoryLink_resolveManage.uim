<!-- This jsp redirects the user to the rule set category management page.  -->
<!-- It identifies the appropriate ID to use on the rule set category link. -->
<PAGE
  PAGE_ID="CreoleRuleSetCategoryLink_resolveManage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>

    curam.omega3.request.RequestHandler 
      rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

    String context = request.getContextPath() + "/";
    context += curam.omega3.user.UserPreferencesFactory
      .getUserPreferences(pageContext.getSession()).getLocale() + "/";
      
    String url = new String("");
    String ruleSetID = new String("");
    
    String creoleRuleSetID = request.getParameter("creoleRuleSetID");
    String creoleRuleSetEditActionID = request.getParameter("creoleRuleSetEditActionID");
    
    if (!creoleRuleSetID.equals("0")) {
      ruleSetID = creoleRuleSetID;
    } else {
      ruleSetID = creoleRuleSetEditActionID;
    }
    
    url = context + "CreoleRuleSetCategoryLink_manage" + "Page.do?ruleSetID=" 
      + curam.omega3.request.RequestUtils.escapeURL(ruleSetID);

    url += "&amp;" + rh.getSystemParameters();
    
    response.sendRedirect(response.encodeRedirectURL(url));
        
  </JSP_SCRIPTLET>
</PAGE>
