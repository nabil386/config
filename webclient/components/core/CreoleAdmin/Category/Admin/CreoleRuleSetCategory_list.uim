<!-- This page lists all the creole rule set categories on the system.      -->
<PAGE PAGE_ID="CreoleRuleSetCategory_list" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

<PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>

  <!-- BEGIN, RTC 164467,COF-->
  <SERVER_INTERFACE CLASS="CREOLERuleSetAdmin" NAME="DISPLAY" OPERATION="listAllCategories"/>
  <!-- END,RTC 164467, COF -->


  <ACTION_SET ALIGNMENT="LEFT" BOTTOM="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <LINK OPEN_MODAL="TRUE" PAGE_ID="CreoleRuleSetCategory_create"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>


    <FIELD LABEL="Field.Label.RuleSetCategory" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="category"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RuleSetCategoryDescription" WIDTH="75">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="categoryDescription"/>
      </CONNECT>
    </FIELD>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Rename">
        <LINK OPEN_MODAL="TRUE" PAGE_ID="LocalizableText_viewLocalizableText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryID"/>
            <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="TRUE" PAGE_ID="LocalizableText_viewLocalizableText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryDescriptionID"/>
            <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="CreoleRuleSetCategory_confirmDelete">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$creoleRuleSetCategoryID"/>
            <TARGET NAME="PAGE" PROPERTY="creoleRuleSetCategoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryDescriptionID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryDescriptionID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
       <!-- BEGIN, RTC 164467,COF-->
      <ACTION_CONTROL LABEL="ActionControl.Label.Set">
        <CONDITION>
          <IS_FALSE NAME="DISPLAY" PROPERTY="filteredInd"/>
        </CONDITION>
        
        <LINK OPEN_MODAL="true" PAGE_ID="CreoleRuleSetCategory_setFiltered" WINDOW_OPTIONS="width=400">
        <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$creoleRuleSetCategoryID"/>
            <TARGET NAME="PAGE" PROPERTY="creoleRuleSetCategoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryDescriptionID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryDescriptionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
         </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Unset">
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="filteredInd"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="CreoleRuleSetCategory_unsetFiltered" WINDOW_OPTIONS="width=400">
         <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$creoleRuleSetCategoryID"/>
            <TARGET NAME="PAGE" PROPERTY="creoleRuleSetCategoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$categoryDescriptionID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryDescriptionID"/>
          </CONNECT>      
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- END,RTC 164467, COF -->
      


    </ACTION_SET>


  </LIST>


</PAGE>