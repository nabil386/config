<!-- This page is for creating a new CREOLE rule set category.              -->
<PAGE PAGE_ID="CreoleRuleSetCategory_create" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="CREOLERuleSetAdmin" NAME="ACTION" OPERATION="createCategory" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="30" NUM_COLS="1">


    <FIELD LABEL="Field.Label.RuleSetCategory" WIDTH="50">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="category$category"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RuleSetCategoryDescription" WIDTH="100">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="category$categoryDescription"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>