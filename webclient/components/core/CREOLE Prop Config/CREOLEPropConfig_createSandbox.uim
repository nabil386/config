    </FIELD>
    <FIELD HEIGHT="15" LABEL="field.label.configData">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="configXML"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


