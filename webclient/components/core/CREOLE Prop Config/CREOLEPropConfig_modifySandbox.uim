<!-- Modify a sandbox configuration.                                        -->
<PAGE PAGE_ID="CREOLEPropConfig_modifySandbox" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="page.title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="RuleObjectPropagatorConfiguration" NAME="DISPLAY" OPERATION="readConfiguration" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="sandboxID"/>
  <PAGE_PARAMETER NAME="sandboxVersionNo"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sandboxID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$sandboxID"/>
  </CONNECT>


  <SERVER_INTERFACE CLASS="RuleObjectPropagatorConfiguration" NAME="ACTION" OPERATION="modifyConfiguration" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sandboxID"/>
    <TARGET NAME="ACTION" PROPERTY="sandboxKey$sandboxID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sandboxVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="sandboxKey$sandboxVersionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$actionType"/>
    <TARGET NAME="ACTION" PROPERTY="dtls$actionType"/>
  </CONNECT>


  <!-- BEGIN, CR00232269, CW -->
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="configName"/>
    <TARGET NAME="ACTION" PROPERTY="details$configName"/>
  </CONNECT>
  <!-- END, CR00232269 -->


  <ACTION_SET>
    <ACTION_CONTROL LABEL="control.label.save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="control.label.cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="20">
    <FIELD LABEL="field.label.configName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="configName"/>
      </CONNECT>
    </FIELD>
    <FIELD HEIGHT="15" LABEL="field.label.configData">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="configXML"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$configXML"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>