<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Lists live configurations and sandbox configurations on the system.    -->
<PAGE
  PAGE_ID="CREOLEPropConfig_listLiveAndSandbox"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- The page title for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="page.title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- Declare the 'display' server bean -->
  <SERVER_INTERFACE
    CLASS="RuleObjectPropagatorConfiguration"
    NAME="DISPLAY"
    OPERATION="listLiveAndSandboxConfigurations"
    PHASE="DISPLAY"
  />


  <!-- List fields on this page -->
  <LIST TITLE="List.Title.Live">


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CREOLEPropConfig_listConfigurationHistory">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$liveDtls$dtls$dtls$ruleObjectPropagatorConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="configID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.View">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_viewLiveConfiguration"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$liveDtls$dtls$dtls$ruleObjectPropagatorConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="configID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_createSandboxFromConfig"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$liveDtls$dtls$dtls$ruleObjectPropagatorConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="configID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.CreateCopy">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_createSandboxFromSnapshot"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$liveDtls$dtls$dtls$snapshotID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="snapshotID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_confirmRemoveConfig"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$liveDtls$dtls$dtls$ruleObjectPropagatorConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="configID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="field.label.configName"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$liveDtls$dtls$configName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.actionType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$liveDtls$dtls$dtls$actionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.actionDateTime"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$liveDtls$dtls$dtls$actionDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.userName"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$liveDtls$dtls$userFullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_userHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$liveDtls$dtls$dtls$userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>


  <LIST TITLE="List.Title.Sandbox">


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.View">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_viewSandboxConfiguration"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$sandboxDtls$dtls$editDisabledInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_modifySandbox"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Rename">


        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$sandboxDtls$dtls$renameDisabledInd"
          />
        </CONDITION>


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_confirmRenameConfiguration"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$dtls$nameID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="nameID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Discard">
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$sandboxDtls$dtls$discardDisabledInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CREOLEPropConfig_confirmRemoveSandbox"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$sandboxDtls$dtls$sandboxKey$sandboxVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="sandboxVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="field.label.configName"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$sandboxDtls$dtls$configName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.actionType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$sandboxDtls$dtls$actionDescription"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.actionDateTime"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$sandboxDtls$dtls$dtls$actionDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="field.label.userName"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$sandboxDtls$dtls$userFullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_userHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$sandboxDtls$dtls$dtls$userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>
</PAGE>
