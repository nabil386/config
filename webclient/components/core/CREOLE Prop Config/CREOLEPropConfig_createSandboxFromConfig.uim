<!-- Creates a sandbox configuration.                                       -->
<PAGE
  PAGE_ID="CREOLEPropConfig_createSandboxFromConfig"
  WINDOW_OPTIONS="width=550,height=580"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="page.title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="configID"/>


  <SERVER_INTERFACE
    CLASS="RuleObjectPropagatorConfiguration"
    NAME="ACTION"
    OPERATION="createEditConfiguration"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="configID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$configID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$dtls$actionType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$actionType"
    />
  </CONNECT>


  <!-- BEGIN, CR00240803, KH -->
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="configName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="configName"
    />
  </CONNECT>
  <!-- END, CR00240803 -->


  <CLUSTER
    DESCRIPTION="cluster.descriptor"
    LABEL_WIDTH="20"
  >
    <FIELD LABEL="field.label.configName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="configName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      HEIGHT="15"
      LABEL="field.label.configData"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="configXML"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="configXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <INCLUDE FILE_NAME="CREOLEPropConfig_createSandboxFromView.vim"/>


</PAGE>
