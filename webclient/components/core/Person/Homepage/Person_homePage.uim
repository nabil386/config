    SCRIPT_FILE="PersonPhoto.js"
  />
  <ACTION_SET ALIGNMENT="CENTER">


    <!-- BEGIN,CR00101438, SB -->
    <ACTION_CONTROL
      IMAGE="AddPictureButton"
      LABEL="ActionControl.Label.AddPicture"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="concernRoleImagesEnabledAndImageNotExistsInd"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="TRUE"
        PAGE_ID="Person_addImage"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="RemovePictureButton"
      LABEL="ActionControl.Label.RemovePicture"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="concernRoleImagesEnabledAndImageExistsInd"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="TRUE"
        PAGE_ID="Person_removeImage"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleImageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleImageID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!-- END,CR00101438, SB -->
  </ACTION_SET>


