<!-- Lists the Change History of an Employment Working Hour record.         -->
<PAGE
  PAGE_ID="Person_listEmploymentWorkingHourHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewEmploymentWorkingHours"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Person_viewEmploymentWorkingHour"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="employmentWorkingHourID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="employmentWorkingHourID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.History"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Person_listEmploymentWorkingHourHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="employmentWorkingHourID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="employmentWorkingHourID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- BEGIN, CR00069651, PCAL -->
  <!-- END, CR00069651 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Person"
    NAME="DISPLAY"
    OPERATION="listEmploymentWorkingHourHistory"
  />
  <PAGE_PARAMETER NAME="employmentWorkingHourID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="employmentWorkingHourID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="historyKey$employmentWorkingHourID"
    />
  </CONNECT>


  <LIST>
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Person_viewEmploymentWorkingHourFromHistory">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="employWorkingHourSnapshotID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="employmentWorkingHourID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.HoursPerWeek"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="noHoursWorked"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ShiftWorker"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="shiftWorkInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.From"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Created"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationDateTime"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
