<!-- View the foreign residency details for a person.                       -->
<PAGE
  PAGE_ID="Person_viewForeignResidency"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- BEGIN, CR00151613, MC -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- END, CR00151613 -->


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewForgnRes"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Person_viewForeignResidency"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="foreignResidencyID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="foreignResidencyID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.History"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Person_listForeignResidencyHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="foreignResidencyID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="foreignResidencyID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- BEGIN, CR00069651, PCAL -->
  <!-- END, CR00069651 -->


  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="foreignResidencyID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="foreignResidencyID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readForeignResidencyKey$foreignResidencyID"
    />
  </CONNECT>
  <INCLUDE FILE_NAME="Person_viewForeignResidencyView.vim"/>
</PAGE>
