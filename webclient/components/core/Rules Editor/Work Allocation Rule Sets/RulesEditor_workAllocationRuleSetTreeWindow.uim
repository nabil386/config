<PAGE
  PAGE_ID="RulesEditor_workAllocationRuleSetTreeWindow"
  TYPE="SPLIT_WINDOW"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="ruleSetID"/>
  <SERVER_INTERFACE
    CLASS="WorkAllocationRuleSetTreeView"
    NAME="DISPLAY"
    OPERATION="buildWorkAllocationRuleSetTree"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ruleSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="ruleSetIDStruct$ruleSetID"
    />
  </CONNECT>
  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$XMLData"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
