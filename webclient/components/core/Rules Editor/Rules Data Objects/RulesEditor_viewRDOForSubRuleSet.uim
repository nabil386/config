<!-- Page displaying the details of a rules data object.                    -->
<PAGE
  PAGE_ID="RulesEditor_viewRDOForSubRuleSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="rulesDataObjectName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RulesEditor"
    NAME="DISPLAY"
    OPERATION="viewRulesDataObjectForSubRuleSet"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Close.label"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="rulesDataObjectID"/>


  <!-- BEGIN, CR00023238, LP -->
  <PAGE_PARAMETER NAME="RDOQualifier"/>
  <PAGE_PARAMETER NAME="RDOFinal"/>


  <!-- END, CR00023238 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="rulesDataObjectID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="rulesDataObjectID"
    />
  </CONNECT>
  <!-- BEGIN, CR00023238, LP -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RDOQualifier"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$rulesDataObjectQualifier"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="RDOFinal"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$rulesDataObjectFinal"
    />
  </CONNECT>
  <!-- END, CR00023238 -->


  <CLUSTER
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesDataObjectName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Final">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$nameAndTypeDtls$rulesDataObjectFinal"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesDataObjectType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Qualifier">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$nameAndTypeDtls$rulesDataObjectQualifier"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.Attributes">


    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="attributeName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="attributeType"
        />
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>
