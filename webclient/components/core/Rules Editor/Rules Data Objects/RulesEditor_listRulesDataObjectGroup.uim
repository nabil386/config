<!-- Lists all the rules data object groups in the system.                  -->
<PAGE
  PAGE_ID="RulesEditor_listRulesDataObjectGroup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="RulesEditor"
    NAME="DISPLAY"
    OPERATION="listRulesDataObjectGroups"
  />


  <LIST>


    <FIELD
      LABEL="Field.Title.Group"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rulesDataObjectGroupName"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="RulesEditor_viewRulesDataObjectGroup">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="rulesDataObjectGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="rulesDataObjectGroupID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
