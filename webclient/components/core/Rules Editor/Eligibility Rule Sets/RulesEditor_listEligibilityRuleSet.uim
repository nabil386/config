<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Lists all the eligibility rule sets in the system.                     -->
<PAGE
  PAGE_ID="RulesEditor_listEligibilityRuleSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="RulesEditor"
    NAME="DISPLAY"
    OPERATION="listEligibilityRuleSet"
    PHASE="DISPLAY"
  />
  
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <!-- BEGIN, CR00021749, KY -->
      <!-- BEGIN, CR00021602, KY -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Simulations">
        <LINK PAGE_ID="RulesEditor_listSimulation">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="ruleSetID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="ruleSetID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="RulesEditor_cancelEligibilityRuleSet"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="ruleSetID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="ruleSetID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="ruleSetName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- END, CR00021602 -->
      <!-- END, CR00021749 -->
    </ACTION_SET>
    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="ruleSetName"
        />
      </CONNECT>
      <LINK PAGE_ID="RulesEditor_treeWindow">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="ruleSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="ruleSetID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="ruleSetName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="ruleSetName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>
</PAGE>
