<!-- This page allows the user to cancel an objective group.                -->
<PAGE PAGE_ID="RulesEditor_cancelObjectiveGroup" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="RulesEditor" NAME="ACTION" OPERATION="deleteNode" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="ruleSetID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="nodeID"/>
  <PAGE_PARAMETER NAME="parentNodeID"/>
  <PAGE_PARAMETER NAME="parentNodeType"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="ruleSetID"/>
    <TARGET NAME="ACTION" PROPERTY="ruleSetID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="nodeID"/>
    <TARGET NAME="ACTION" PROPERTY="nodeID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.YesButton.label" TYPE="SUBMIT">
      <LINK PAGE_ID="RulesEditor_resolveViewRuleElementAfterDelete">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="ruleSetID"/>
          <TARGET NAME="PAGE" PROPERTY="ruleSetID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="parentNodeID"/>
          <TARGET NAME="PAGE" PROPERTY="nodeID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="parentNodeType"/>
          <TARGET NAME="PAGE" PROPERTY="nodeType"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
          <TARGET NAME="PAGE" PROPERTY="versionNo"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.NoButton.label"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelObjectiveGroup"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>