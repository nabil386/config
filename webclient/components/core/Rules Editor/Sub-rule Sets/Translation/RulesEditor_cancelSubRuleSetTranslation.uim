<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to remove a eligiblity ruleset translation   -->
<!--                                                                        -->
<PAGE PAGE_ID="RulesEditor_cancelSubRuleSetTranslation" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="RulesEditor" NAME="ACTION" OPERATION="deleteTranslation" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="ruleSetID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="languageCode"/>
  <PAGE_PARAMETER NAME="nodeID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="ruleSetID"/>
    <TARGET NAME="ACTION" PROPERTY="ruleSetID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="languageCode"/>
    <TARGET NAME="ACTION" PROPERTY="languageCode"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="nodeID"/>
    <TARGET NAME="ACTION" PROPERTY="nodeID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.YesButton.label" TYPE="SUBMIT">
      <LINK PAGE_ID="RulesEditor_viewSubRuleSet">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="ruleSetID"/>
          <TARGET NAME="PAGE" PROPERTY="ruleSetID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="nodeID"/>
          <TARGET NAME="PAGE" PROPERTY="nodeID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="RulesEditor_viewSubRuleSet">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="ruleSetID"/>
          <TARGET NAME="PAGE" PROPERTY="ruleSetID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="nodeID"/>
          <TARGET NAME="PAGE" PROPERTY="nodeID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.RemoveSubRuleSetTranslation"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>