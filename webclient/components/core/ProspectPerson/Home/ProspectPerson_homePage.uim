  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <INFORMATIONAL>
    <!-- BEGIN, CR00114322, PA -->
    <CONNECT>
      <SOURCE
        NAME="DISPLAYINFOMESSAGE"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
    <!-- END, CR00114322, PA -->
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$informationalMsgDtlsList$dtls$informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <!--BEGIN CR00086044, GBA-->
  <SHORTCUT_TITLE ICON="prospectperson.icon">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </SHORTCUT_TITLE>
  <!--END CR00086044-->


  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAY"
    OPERATION="readHomePageDetails"
  />
  <!-- BEGIN, CR00114322, PA -->
  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAYINFOMESSAGE"
    OPERATION="readInformationalMessage"
    PHASE="DISPLAY"
  />
  <!-- END, CR00114322, PA -->


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleHomePageKey$concernRoleID"
    />
  </CONNECT>


  <!-- BEGIN, CR00075582, RF -->
  <SERVER_INTERFACE
