<!-- This page allows the user to register a prospect employer as employer. -->
<PAGE
  PAGE_ID="ProspectPerson_registerAsPerson"
  WINDOW_OPTIONS="width=975"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <INCLUDE FILE_NAME="ProspectPerson_registerAsPersonView.vim"/>
  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="ACTION"
    OPERATION="registerProspectPersonAsPerson"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAY"
    OPERATION="autoPopulateRegistrationFields"
  />


  <!-- BEGIN, CR00108252 PA -->


  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAYPROSPECTPERSON"
    OPERATION="readProspectPerson"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ProspectPerson"
    NAME="DISPLAYRACE"
    OPERATION="listRace"
    PHASE="DISPLAY"
  />


  <!-- END, CR00108252 - PA -->


  <!-- BEGIN, CR00078486, POH -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="msg"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00078486 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="maintainConcernRoleKey$concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="maintainConcernRoleKey$concernRoleID"
    />
  </CONNECT>


  <!-- BEGIN, CR00108252 PA -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAYPROSPECTPERSON"
      PROPERTY="maintainConcernRoleKey$concernRoleID"
    />
  </CONNECT>


  <!-- END, CR00108252 - PA -->


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="RegisterButton"
      LABEL="ActionControl.Label.Register"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="false"
        PAGE_ID="ProspectPerson_registrationAsPersonComplete"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$registrationIDDetails$concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$registrationIDDetails$alternateID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="alternateID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
