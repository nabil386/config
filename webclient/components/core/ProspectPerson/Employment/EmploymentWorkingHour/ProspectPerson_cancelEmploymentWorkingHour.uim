<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Sets prospect person employment working hour record status from        -->
<!-- "Active" to "Canceled"                                                 -->
<PAGE PAGE_ID="ProspectPerson_cancelEmploymentWorkingHour" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Person" NAME="ACTION" OPERATION="cancelEmploymentWorkingHour" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="employmentWorkingHourID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="employmentWorkingHourID"/>
    <TARGET NAME="ACTION" PROPERTY="employmentWorkingHourID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelWorkingHour"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>