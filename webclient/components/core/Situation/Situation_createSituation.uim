<!-- This page is used to create a new Situation.                           -->
<!-- BEGIN, CR00200707, AK -->
<!-- BEGIN, CR00213311, AK -->
<PAGE
  PAGE_ID="Situation_createSituation"
  WINDOW_OPTIONS="width=825"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00213311 -->
  <!-- END, CR00200707 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Situation"
    NAME="ACTION"
    OPERATION="createSituation"
    PHASE="ACTION"
  />
  <!-- BEGIN, CR00179295, AK -->
  <SERVER_INTERFACE
    CLASS="IntegratedCase"
    NAME="DISPLAYCASEPARTICIPANT"
    OPERATION="getCaseParticipantsAgeConcernRoleID"
  />


  <SERVER_INTERFACE
    CLASS="InvestigationDelivery"
    NAME="DISPLAYALLEGATIONS"
    OPERATION="listAllegation"
  />
  <!-- END, CR00179295 -->


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="actionPlanID"/>
  <PAGE_PARAMETER NAME="description"/>


  <!-- BEGIN, CR00407868, SG -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>
  <!-- END, CR00407868 -->


  <!-- BEGIN, CR00179295, AK -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAYALLEGATIONS"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="actionPlanID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="actionPlanID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAYCASEPARTICIPANT"
      PROPERTY="caseID"
    />
  </CONNECT>


  <!-- END, CR00179295 -->
  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="true"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <!-- BEGIN, CR00179295, AK -->
  <INCLUDE FILE_NAME="Situation_createSituationView.vim"/>
  <!-- END, CR00179295 -->
</PAGE>
