<PAGE
  PAGE_ID="ProcessDefinitionTool_viewLoopBeginActivity"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="message"
      />
    </CONNECT>
  </INFORMATIONAL>
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="activityID"/>
  <SERVER_INTERFACE
    CLASS="LoopActivityAdmin"
    NAME="DISPLAY"
    OPERATION="readBeginLoopActivity"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$processID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processVersionNo"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$processVersionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="activityID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$activityID"
    />
  </CONNECT>
  <CLUSTER
    LABEL_WIDTH="40%"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.ActivityName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SplitType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$splitType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Category">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$category"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoopEndActivity">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="loopEndActivityName"
        />
      </CONNECT>
      <LINK PAGE_ID="ProcessDefinitionTool_viewLoopEndActivity">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="processVersionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processVersionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="loopEndActivityReference"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.ActivityType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.JoinType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$joinType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoopType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="loopType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <INCLUDE FILE_NAME="ProcessDefinitionTool_Activity.vim"/>
</PAGE>
