<PAGE
  PAGE_ID="ProcessDefinitionTool_searchProcesses"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ProcessAdmin"
    NAME="ACTION"
    OPERATION="searchProcesses"
    PHASE="ACTION"
  />


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Search"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="THIS"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Reset"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="ProcessDefinitionTool_searchProcesses"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="30%"
    NUM_COLS="2"
    TITLE="Cluster.SearchCriteria.Title"
  >

    <FIELD LABEL="Cluster.SearchCriteria.WorkflowName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.SearchCriteria.WorkflowDisplayName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$displayName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Cluster.SearchCriteria.VersionNumber"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$processVersionNo"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Cluster.SearchCriteria.Category"
      USE_BLANK="true"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$category"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.SearchCriteria.Released">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$releasedInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Cluster.SearchCriteria.CreatedBy">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$createdBy"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Cluster.SearchCriteria.DateCreated"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="processSearchDetails$dateCreated"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <LIST TITLE="List.SearchResults.Title">
    <FIELD LABEL="List.SearchResults.WorkflowDisplayName">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$dtls$displayName"
        />
      </CONNECT>
      <LINK PAGE_ID="ProcessDefinitionTool_viewProcessRedirect">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$dtls$processID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$dtls$processVersionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processVersionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$dtls$name"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="processName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="List.SearchResults.Category">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$dtls$category"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.SearchResults.Version">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$dtls$processVersionNo"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.SearchResults.Released">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$dtls$releasedInd"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
