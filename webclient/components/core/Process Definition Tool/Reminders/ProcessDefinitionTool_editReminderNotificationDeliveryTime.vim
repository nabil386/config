    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    DESCRIPTION="Cluster.DeliveryTime.Description"
    NUM_COLS="3"
    TITLE="Cluster.DeadlineTimeout.Title"
  >
    <FIELD
      LABEL="Field.Label.Days"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$days"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="days"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Hours"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$hours"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
