<PAGE
  PAGE_ID="ProcessDefinitionTool_selectStructField"
  POPUP_PAGE="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="message"
      />
    </CONNECT>
  </INFORMATIONAL>
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="structName"/>
  <PAGE_PARAMETER NAME="workflowDataObjectAttributeType"/>
  <PAGE_PARAMETER NAME="isListStructAttributeRequired"/>
  <SERVER_INTERFACE
    CLASS="ProcessAdmin"
    NAME="ACTION"
    OPERATION="listStructFields"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="workflowDataObjectAttributeType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="workflowDataObjectAttributeType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="isListStructAttributeRequired"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="isListStructAttributeRequired"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="control.CloseButton.label"
      TYPE="DISMISS"
    />
  </ACTION_SET>


  <CLUSTER
    DESCRIPTION="Cluster.Description"
    LABEL_WIDTH="30"
    TITLE="Cluster.Title.SearchCriteria"
  >


    <FIELD LABEL="Field.Label.StructName">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="qualifiedStructName"
          NAME="PAGE"
          PROPERTY="qualifiedStructName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="qualifiedStructName"
        />
      </CONNECT>
    </FIELD>
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="control.ResetButton.label">
        <LINK PAGE_ID="ProcessDefinitionTool_selectStructField">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="processID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="processID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="processVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="processVersionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="structName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="structName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>
  <LIST
    
    TITLE="List.Title.SearchResults"
  >
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="10"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Select"
        TYPE="DISMISS"
      >
        <LINK>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="qualifiedNameToParse"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="value"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="qualifiedName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Label.FieldName">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
