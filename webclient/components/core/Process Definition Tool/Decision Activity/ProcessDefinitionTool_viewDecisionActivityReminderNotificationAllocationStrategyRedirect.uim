<!-- This jsp redirects the user to the appropriate allocation strategy     -->
<!-- page for a notification based on the specified allocation type. The    -->
<!-- types include function, rule or direct.                                -->
<PAGE
  PAGE_ID="ProcessDefinitionTool_viewDecisionActivityReminderNotificationAllocationStrategyRedirect"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="activityID"/>
  <PAGE_PARAMETER NAME="activityType"/>
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="reminderID"/>


  <INCLUDE FILE_NAME="ProcessDefinitionTool_viewReminderNotificationAllocationStrategyRedirect.vim"/>
</PAGE>
