<PAGE
  PAGE_ID="ProcessDefinitionTool_selectEventType"
  POPUP_PAGE="true"
  WINDOW_OPTIONS="width=450"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="EventWaitActivityAdmin"
    NAME="DISPLAY"
    OPERATION="listEventClasses"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="EventWaitActivityAdmin"
    NAME="ACTION"
    OPERATION="listEventTypesByClass"
    PHASE="ACTION"
  />


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="control.CloseButton.label"
      TYPE="DISMISS"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="30"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="control.ResetButton.label">
        <LINK PAGE_ID="ProcessDefinitionTool_selectEventType"/>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="Field.Label.EventClass">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="dtls$eventClass"
          NAME="DISPLAY"
          PROPERTY="dtls$eventClass"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$eventClass"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <LIST
    
    TITLE="List.Title.SearchResults"
  >
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="20"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Select"
        TYPE="DISMISS"
      >
        <LINK>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="eventClassAndType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="value"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="eventClassAndType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Label.EventType">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="eventType"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
