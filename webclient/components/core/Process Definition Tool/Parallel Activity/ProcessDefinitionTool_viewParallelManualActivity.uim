<PAGE
  PAGE_ID="ProcessDefinitionTool_viewParallelManualActivity"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ParallelActivityAdmin"
    NAME="DISPLAY"
    OPERATION="readParallelManualActivity"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="activityID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$processID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processVersionNo"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$processVersionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="activityID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="activityKey$activityID"
    />
  </CONNECT>
  <CLUSTER
    LABEL_WIDTH="40%"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.ActivityName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Category">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$category"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SplitType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$splitType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActivityType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ParallelActivityType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$parallelActivityType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.JoinType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$joinType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="20"
    NUM_COLS="1"
    TITLE="Cluster.ParallelActivityListWDO"
  >
    <CONTAINER LABEL="Field.Label.ListWDOToUse">
      <FIELD LABEL="Field.Label.ListWDOToUse">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$parallelActivityListWDOName"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.ParallelActivityListWDOIsValid">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$parallelActivityListWDOIsValid"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="20"
    NUM_COLS="1"
    TITLE="Cluster.SubjectText"
  >
    <FIELD LABEL="Field.Label.SubjectText">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$activitySubject"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$dtls$dtls$message"
      />
    </CONNECT>
  </INFORMATIONAL>
  <CLUSTER
    LABEL_WIDTH="20"
    NUM_COLS="2"
    TITLE="Cluster.AllocationStrategy"
  >
    <FIELD LABEL="Field.Label.AllocationType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$allocationStrategyType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AllocationName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$allocationName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <INCLUDE FILE_NAME="ProcessDefinitionTool_Activity.vim"/>
</PAGE>
