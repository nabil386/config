<!-- This jsp redirects the user to the appropriate UIM pages based on      -->
<!-- whether the method being processed is overloaded or not.               -->
<PAGE
  PAGE_ID="ProcessDefinitionTool_checkForOverloadedMethodToUseRedirect"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <JSP_SCRIPTLET>
    
          
      String processID = request.getParameter("processID");
      String processVersionNo = request.getParameter("processVersionNo");
      String activityID = request.getParameter("activityID");
      String methodName = request.getParameter("methodName");
      String overloaded = request.getParameter("overloaded");
      String pageIdentifier = request.getParameter("pageIdentifier");
      
      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory
        .getUserPreferences(pageContext.getSession()).getLocale() + "/";
       
      String pageID = null;
      String url = null;
      
      boolean isOverloaded = Boolean.valueOf(overloaded).booleanValue();
     // BEGIN, CR00372377, AC
      if (!isOverloaded) {
        pageID = "ProcessDefinitionTool_closeModalAutomaticallyPage.do";
        url = context + pageID + "?processID=" + curam.omega3.request.RequestUtils.escapeURL(processID) 
        + "&amp;processVersionNo=" + curam.omega3.request.RequestUtils.escapeURL(processVersionNo) 
        + "&amp;activityID=" + curam.omega3.request.RequestUtils.escapeURL(activityID);
           
      } else {
        pageID = "ProcessDefinitionTool_selectOverloadedMethodToUsePage.do";
          url = context + pageID + "?processID=" +curam.omega3.request.RequestUtils.escapeURL(processID) 
            + "&amp;processVersionNo=" + curam.omega3.request.RequestUtils.escapeURL(processVersionNo) 
            + "&amp;activityID=" + curam.omega3.request.RequestUtils.escapeURL(activityID)
            + "&amp;methodName=" + curam.omega3.request.RequestUtils.escapeURL(methodName);
      }
      // END, CR00372377
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
      
    
  </JSP_SCRIPTLET>
</PAGE>
