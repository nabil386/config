<!-- Select the primary rule class and other workflow rule classes. -->
<PAGE PAGE_ID="ProcessDefinitionTool_selectRuleClasses" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <!-- BEGIN, CR00345422, EC -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="result$message$dtls$message"/>
    </CONNECT>
  </INFORMATIONAL>  
  
  <SERVER_INTERFACE CLASS="AllocationStrategyAdmin" NAME="DISPLAY" OPERATION="readCERSetClassesForModify" PHASE="DISPLAY"/> 
  <SERVER_INTERFACE CLASS="AllocationStrategyAdmin" NAME="ACTION" OPERATION="createCERInputMappingList" PHASE="ACTION"/>

  <PAGE_PARAMETER NAME="cerSetName"/>
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="activityID"/>
  <PAGE_PARAMETER NAME="strategyParent"/>
  <PAGE_PARAMETER NAME="reminderID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="cerSetName"/>
    <TARGET NAME="ACTION" PROPERTY="cerSetName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processID"/>
    <TARGET NAME="DISPLAY" PROPERTY="processID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processVersionNo"/>
    <TARGET NAME="DISPLAY" PROPERTY="processVersionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="activityID"/>
    <TARGET NAME="DISPLAY" PROPERTY="activityID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="strategyParent"/>
    <TARGET NAME="DISPLAY" PROPERTY="strategyParent"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="reminderID"/>
    <TARGET NAME="DISPLAY" PROPERTY="reminderID"/>
  </CONNECT>  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="cerSetName"/>
    <TARGET NAME="DISPLAY" PROPERTY="cerSetName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processID"/>
    <TARGET NAME="ACTION" PROPERTY="processID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="processVersionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="activityID"/>
    <TARGET NAME="ACTION" PROPERTY="activityID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="strategyParent"/>
    <TARGET NAME="ACTION" PROPERTY="strategyParent"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="reminderID"/>
    <TARGET NAME="ACTION" PROPERTY="reminderID"/>
  </CONNECT>  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="selectCERRuleClassesDetails$versionNo"/>
  </CONNECT>  

  <CLUSTER LABEL_WIDTH="35">
    <FIELD WIDTH="70" USE_BLANK="true" LABEL="Field.Label.PrimayRuleClass">
      <CONNECT>
        <INITIAL NAME="DISPLAY" PROPERTY="result$primary$primaryList$name"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$primary$name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="cerPrimaryClassName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <LIST TITLE="List.Label.OtherRuleClasses">
      <CONTAINER ALIGNMENT="CENTER" LABEL="Container.Label.Action" WIDTH="7">
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="result$secondary$secondaryList$name"/>
            </CONNECT>           
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="listOfNonPrimaryClassNames"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="result$name"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>

      <FIELD LABEL="Field.Label.ClassName" WIDTH="70">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$secondary$secondaryList$name"/>
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>

  <ACTION_SET>
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
</PAGE>