<PAGE PAGE_ID="ProcessDefinitionTool_createTransition" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="result$dtls$dtls$message"/>
    </CONNECT>
  </INFORMATIONAL>
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.SaveAndNew" TYPE="SUBMIT">
      <LINK PAGE_ID="ProcessDefinitionTool_createTransition"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
  <SERVER_INTERFACE CLASS="TransitionAdmin" NAME="DISPLAY" OPERATION="listActivityNamesAndIDs" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="TransitionAdmin" NAME="ACTION" OPERATION="createTransition" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processID"/>
    <TARGET NAME="DISPLAY" PROPERTY="processID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processVersionNo"/>
    <TARGET NAME="DISPLAY" PROPERTY="processVersionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processID"/>
    <TARGET NAME="ACTION" PROPERTY="processID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="processVersionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>
  <CLUSTER LABEL_WIDTH="35">
    <FIELD LABEL="Field.Label.FromActivity" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="result$from$dtls$activityID" NAME="DISPLAY" PROPERTY="result$from$dtls$activityName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="fromActivityID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ToActivity" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="result$to$dtls$activityID" NAME="DISPLAY" PROPERTY="result$to$dtls$activityName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="toActivityID"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>