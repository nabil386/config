    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="Prospect" NAME="ACTION" OPERATION="cancelEmployment" PHASE="ACTION"/>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
