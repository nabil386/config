<!-- This page allows a user to create a new Microsoft Word template.              -->
<!-- BEGIN, CR00357406, PB -->
<!-- BEGIN, CR00351676, PS -->
<PAGE PAGE_ID="System_createDocumentTemplate1" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00351676 -->
  <!-- END, CR00357406 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="System" NAME="ACTION" OPERATION="createDocumentTemplate1" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="35" NUM_COLS="2">


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TemplateDocumentID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="documentTemplateID"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00145315, SK -->
    <FIELD LABEL="Field.Label.Locale">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="localeIdentifier"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00145315 -->


    <FIELD CONTROL="SKIP"/>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="17.5" NUM_COLS="1">
    <WIDGET LABEL="Field.Label.File" TYPE="FILE_UPLOAD">
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="contents"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <!-- BEGIN, CR00229063, PB -->
      <WIDGET_PARAMETER NAME="FILE_NAME">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="filename"/>
        </CONNECT>
      </WIDGET_PARAMETER>
      <!-- END, CR00229063 -->


    </WIDGET>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Category">
    <FIELD CONTROL="CT_HIERARCHY_HORIZONTAL" USE_BLANK="true" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$categoryCode"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>