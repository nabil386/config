<!-- This page allows the user to view a Microsoft Word template.                  -->
<PAGE
  PAGE_ID="System_viewDocumentTemplateDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="readDocumentTemplate1"
  />


  <PAGE_PARAMETER NAME="documentTemplateID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="documentTemplateID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="documentTemplateID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.TemplateDocumentID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="documentTemplateID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Comments"
  >
    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
