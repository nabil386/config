<!-- This page allows the user to check in a template.                      -->
<PAGE
  PAGE_ID="System_checkInTemplate"
  WINDOW_OPTIONS="width=850"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE DESCRIPTION="PageTitle.StaticText2">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="System"
    NAME="ACTION"
    OPERATION="checkInTemplate"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.CheckIn"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="System_listTemplate"/>


    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="templateID"/>
  <PAGE_PARAMETER NAME="templateName"/>
  <!-- BEGIN, CR00145315, SK -->
  <PAGE_PARAMETER NAME="localeIdentifier"/>
  <!-- END, CR00145315 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="templateID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$templateID"
    />
  </CONNECT>
  <!-- BEGIN, CR00145315, SK -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="localeIdentifier"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$localeIdentifier"
    />
  </CONNECT>
  <!-- END, CR00145315 -->


  <!-- BEGIN, CR00350333, SS -->
  <CLUSTER LABEL_WIDTH="50">
    <!-- END, CR00350333 -->


    <WIDGET
      LABEL="Field.Label.File"
      TYPE="FILE_UPLOAD"
    >
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="document"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>


  </CLUSTER>


  <!-- BEGIN, CR00350333, SS -->
  <CLUSTER LABEL_WIDTH="50">
    <!-- END, CR00350333 -->
    <FIELD
      LABEL="Field.Label.CheckInTemplate"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="override"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Comments"
  >


    <!-- BEGIN, CR00417389, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00417389 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="checkInComment"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
