<!-- This page allows the user to view Report Viewer configuration details. -->
<PAGE
  PAGE_ID="System_viewReportViewerConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ReportViewerConfiguration"
    NAME="DISPLAY"
    OPERATION="readReportViewerConfigurationViewDetails"
  />


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="System_modifyReportViewerConfigurationFromView"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$biViewerConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="biViewerConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.AddParameter">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="System_addConfigurationParameter"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$biViewerConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="biViewerConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.ReportViewerName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$viewerName"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.ReportRoot">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$reportRoot"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReportContext">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$reportContext"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReportServlet">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$reportServlet"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReportNameParameter">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$reportNameParameter"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.ReportParameters">


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.EditParameter"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="System_modifyConfigurationParameter"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="viewerParamDtlsList$biReportParameterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="biReportParameterID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.DeleteParameter">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="System_deleteConfigurationParameter"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="viewerParamDtlsList$biReportParameterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="biReportParameterID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="34"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="viewerParamDtlsList$name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Value"
      WIDTH="66"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="viewerParamDtlsList$value"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
