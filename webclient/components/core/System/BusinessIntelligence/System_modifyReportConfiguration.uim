<!-- This page allows the user to modify report configuration.              -->
<PAGE PAGE_ID="System_modifyReportConfiguration" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ReportConfiguration" NAME="DISPLAY" OPERATION="readReportConfigurationDetails" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ReportConfiguration" NAME="ACTION" OPERATION="modifyReportConfigurationDetails" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="biReportConfigurationID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="biReportConfigurationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$biReportConfigurationID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="biReportConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="biReportConfigurationID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="recordStatus"/>
    <TARGET NAME="ACTION" PROPERTY="recordStatus"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2">
    <FIELD LABEL="Field.Title.ReportName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reportName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reportName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.ReportCategory">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reportCategory"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reportCategory"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.ReportFileName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reportFileName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reportFileName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.ReportServlet">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reportServlet"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reportServlet"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER DESCRIPTION="Cluster.Title.Parameters.Description" LABEL_WIDTH="50" NUM_COLS="2" TITLE="Cluster.Title.Parameters">
    <FIELD LABEL="Field.Title.Width" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="width"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="width"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Scrolling" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="scrolling"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="scrolling"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Height" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="height"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="height"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.ReportFrameborder">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reportFrameborder"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reportFrameborder"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>