<!-- Reads a codetable details for display on the codetable tab             -->
<!-- details panel.                                                         -->
<PAGE
  PAGE_ID="System_reportTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="reportName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="reportName"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_PARAMETER NAME="reportName"/>


</PAGE>
