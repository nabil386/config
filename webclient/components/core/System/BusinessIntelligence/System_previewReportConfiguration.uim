<!-- This is the BI report configuration preview screen.                    -->
<PAGE
  PAGE_ID="System_previewReportConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Home"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ReportConfiguration"
    NAME="DISPLAY"
    OPERATION="readReportDataForPreview"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="biReportConfigurationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="biReportConfigurationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="biReportConfigurationID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >


    <CLUSTER
      NUM_COLS="1"
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >


      <FIELD ALIGNMENT="CENTER">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reportDataXML"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


  </CLUSTER>


</PAGE>
