<!-- List the batch error codes for the system -->
<PAGE
  PAGE_ID="System_listBatchErrorCode"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="listBatchErrorCode"
  />


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="System_createBatchErrorCode"
      />
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="System_modifyBatchErrorCodeFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="batchErrorCodeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="batchErrorCodeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="RemoveButton"
        LABEL="ActionControl.Label.Remove"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="System_removeBatchErrorCode"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="batchErrorCodeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="batchErrorCodeID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="batchErrorCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="batchErrorCode"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.BatchErrorCodeID"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="batchErrorCodeID"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.BatchErrorCode"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="batchErrorCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
