<!-- Remove a batch process from a batch group. -->
<PAGE PAGE_ID="System_removeProcessFromGroup" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="System" NAME="ACTION" OPERATION="removeProcessFromGroup" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="groupID"/>
  <PAGE_PARAMETER NAME="processDefName"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="groupID"/>
    <TARGET NAME="ACTION" PROPERTY="groupID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="processDefName"/>
    <TARGET NAME="ACTION" PROPERTY="processDefName"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.RemoveProcess"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>