<!-- Execute a batch process. -->
<PAGE
  PAGE_ID="System_confirmExecuteBatchProcess"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="readBatchParameters"
  />
  <SERVER_INTERFACE
    CLASS="System"
    NAME="ACTION"
    OPERATION="executeBatchProcess"
    PHASE="ACTION"
  />
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="ExecuteButton"
      LABEL="ActionControl.Label.Execute"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="System_resolveRefreshSearch" DISMISS_MODAL="false"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>
  <PAGE_PARAMETER NAME="processDefName"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processDefName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="processDefName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processDefName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="processDefName"
    />
  </CONNECT>
  <CLUSTER
    LABEL_WIDTH="35"
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.ExecuteConfirm"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
