<!-- This pop-up modifies the parent code.                                  -->
<PAGE
  PAGE_ID="System_modifyParentCodePopup"
  POPUP_PAGE="true"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="listPotentialParentCodes"
  />
  <SERVER_INTERFACE
    CLASS="System"
    NAME="readParentCodeBean"
    OPERATION="readParentCode"
  />
  <SERVER_INTERFACE
    CLASS="System"
    NAME="SAVE"
    OPERATION="modifyParentCode"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="hierarchyName"/>
  <PAGE_PARAMETER NAME="codetableName"/>
  <PAGE_PARAMETER NAME="code"/>
  <PAGE_PARAMETER NAME="parentCodetable"/>
  <PAGE_PARAMETER NAME="parentCode"/>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="control.SaveButton.label"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="control.CancelButton.label"
      TYPE="DISMISS"
    />
  </ACTION_SET>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="parentCodetable"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="name"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="codetableName"
    />
    <TARGET
      NAME="SAVE"
      PROPERTY="key$name"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="code"
    />
    <TARGET
      NAME="SAVE"
      PROPERTY="key$code"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="codetableName"
    />
    <TARGET
      NAME="readParentCodeBean"
      PROPERTY="key$name"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="code"
    />
    <TARGET
      NAME="readParentCodeBean"
      PROPERTY="key$code"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hierarchyName"
    />
    <TARGET
      NAME="SAVE"
      PROPERTY="popupParams$hierarchyName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="codetableName"
    />
    <TARGET
      NAME="SAVE"
      PROPERTY="popupParams$codetableName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="code"
    />
    <TARGET
      NAME="SAVE"
      PROPERTY="popupParams$code"
    />
  </CONNECT>
  <CLUSTER LABEL_WIDTH="30">
    <FIELD LABEL="Field.Label.ParentCode">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="code"
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="readParentCodeBean"
          PROPERTY="result$code"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SAVE"
          PROPERTY="newParentCode$code"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
