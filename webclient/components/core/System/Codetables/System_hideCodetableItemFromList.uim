<!-- Modify the details for a codetable item when linked from a list page   -->
<PAGE PAGE_ID="System_hideCodetableItemFromList" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="System" NAME="ACTION" OPERATION="toggleCodeTableItem" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="codetableName"/>
  <PAGE_PARAMETER NAME="code"/>
  <PAGE_PARAMETER NAME="languageCode"/>
  <PAGE_PARAMETER NAME="enabled"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="codetableName"/>
    <TARGET NAME="ACTION" PROPERTY="key$name"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="code"/>
    <TARGET NAME="ACTION" PROPERTY="key$code"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="languageCode"/>
    <TARGET NAME="ACTION" PROPERTY="key$languageCode"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="enabled"/>
    <TARGET NAME="ACTION" PROPERTY="enabled"/>
  </CONNECT>
  <CLUSTER SHOW_LABELS="FALSE" STYLE="outer-cluster-borderless">
    <CONDITION>
      <IS_FALSE NAME="PAGE" PROPERTY="enabled"/>
    </CONDITION>
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.Hide.Item"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <CONDITION>
      <IS_TRUE NAME="PAGE" PROPERTY="enabled"/>
    </CONDITION>
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.Show.Item"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>