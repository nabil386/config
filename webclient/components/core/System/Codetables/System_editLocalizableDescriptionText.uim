<!--This page allows you to view the list of the text translations               -->
<!-- for Organization description.                                                   -->
<PAGE
  PAGE_ID="System_editLocalizableDescriptionText"
  POPUP_PAGE="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="System"
    NAME="ACTION"
    OPERATION="modifyTextTranslation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="searchByKeyAndLocale"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="codetableName"/>
  <PAGE_PARAMETER NAME="displayNameId"/>
  <PAGE_PARAMETER NAME="LocaleCode"/>
  <PAGE_PARAMETER NAME="descText"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="displayNameId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="searchInput$displayNameId"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="LocaleCode"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="searchInput$LocaleCode"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="LocaleCode"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="localeCode"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="displayNameId"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="displayNameId"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="25">
    <FIELD LABEL="Field.Title.Language">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$LocaleCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    SHOW_LABELS="false"
    TITLE="Field.Title.Text"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Title.Text"
    >
      <!-- END, CR00463142 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$translationText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="descText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
