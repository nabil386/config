<!-- Reads a security identifier's details                                  -->
<PAGE
  PAGE_ID="System_viewSecurityIdentifier"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="System"
    NAME="DISPLAY"
    OPERATION="readSecurityIdentifier"
  />


  <PAGE_PARAMETER NAME="securityIdentifierID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="securityIdentifierID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$securityIdentifierID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="15">
    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sidDescription"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
