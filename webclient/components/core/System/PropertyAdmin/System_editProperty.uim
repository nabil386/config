<PAGE PAGE_ID="System_editProperty" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="PropertyAdmin" NAME="DISPLAY" OPERATION="readProperty"/>


  <SERVER_INTERFACE CLASS="PropertyAdmin" NAME="ACTION" OPERATION="modifyProperty" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="propertyID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="propertyID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$propertyID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="key$propertyID"/>
    <TARGET NAME="ACTION" PROPERTY="propertyID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$displayName"/>
    <TARGET NAME="ACTION" PROPERTY="displayName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$category"/>
    <TARGET NAME="ACTION" PROPERTY="category"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$type"/>
    <TARGET NAME="ACTION" PROPERTY="type"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$dynamic"/>
    <TARGET NAME="ACTION" PROPERTY="dynamic"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$defaultValue"/>
    <TARGET NAME="ACTION" PROPERTY="defaultValue"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="System_resolveRefreshSearch" DISMISS_MODAL="false"/>
    </ACTION_CONTROL>
    <!-- BEGIN CR00021324, SG-->
    <!-- BEGIN, HARP 64091, NK-->
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>
    <!--END, HARP 64091-->
    <!--END CR00021324-->
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="20">
    <FIELD LABEL="Field.Label.Value" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$value"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="value"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER BEHAVIOR="NONE" SHOW_LABELS="false" TITLE="Cluster.Label.Description">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" STYLE="property-admin-dynamic-message">
    <CONDITION>
      <IS_FALSE NAME="DISPLAY" PROPERTY="result$dynamic"/>
    </CONDITION>
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.Label.DynamicFalse"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>