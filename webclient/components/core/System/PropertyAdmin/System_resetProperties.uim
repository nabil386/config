<PAGE PAGE_ID="System_resetProperties" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="PropertyAdmin" NAME="ACTION" OPERATION="resetPropertyValue" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="PropertyAdmin" NAME="DISPLAY" OPERATION="readProperty" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="propertyID"/>
  <PAGE_PARAMETER NAME="defaultValue"/>
  <PAGE_PARAMETER NAME="dynamic"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="propertyID"/>
    <TARGET NAME="ACTION" PROPERTY="propertyID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="propertyID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$propertyID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL DEFAULT="true" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="System_resolveRefreshSearch" DISMISS_MODAL="false"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER BEHAVIOR="NONE" SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.Text.WarningMessage"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER BEHAVIOR="NONE" LABEL_WIDTH="35" STYLE="outer-cluster-borderless">
    <FIELD LABEL="Field.Label.DefaultValue">
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="defaultValue"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER BEHAVIOR="NONE" SHOW_LABELS="false" STYLE="property-admin-dynamic-message">
    <CONDITION>
      <IS_FALSE NAME="DISPLAY" PROPERTY="result$dynamic"/>
    </CONDITION>
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.Label.DynamicFalse"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>