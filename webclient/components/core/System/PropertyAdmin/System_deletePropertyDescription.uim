<PAGE
  PAGE_ID="System_deletePropertyDescription"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="PropertyAdmin"
    NAME="ACTION"
    OPERATION="removePropertyDescription"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="propertyID"/>
  <PAGE_PARAMETER NAME="languageCode"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="propertyID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="propertyID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="languageCode"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="languageCode"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.YesButton.label"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.NoButton.label"
    />
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Text.Delete"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
