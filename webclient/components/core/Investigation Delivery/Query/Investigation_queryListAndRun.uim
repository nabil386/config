<!-- This page displays a list of queries for a user.                       -->
<PAGE
  PAGE_ID="Investigation_queryListAndRun"
  HIDE_CONDITIONAL_LINKS="false"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <!-- BEGIN, CR00276230, ELG -->
  <SERVER_INTERFACE
    CLASS="InvestigationQuery"
    NAME="DISPLAY"
    OPERATION="listAndRunWithMessages"
  />
  
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00276230 -->

  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="FALSE"
  >
    <ACTION_CONTROL LABEL="ActionControl.Label.CreateQuery">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Investigation_createQuery"
      />
    </ACTION_CONTROL>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="queryID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="queryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$queryID"
    />
  </CONNECT>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Run">
        <LINK
          PAGE_ID="Investigation_queryListAndRun"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$queryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="queryID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Investigation_modifyQuery"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$queryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="queryID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Investigation_cancelQuery"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$queryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="queryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$queryName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="queryName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Clone">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Investigation_cloneQuery"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="queryListDtls$dtls$dtls$queryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="queryID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.QueryName"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="queryListDtls$dtls$dtls$queryName"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <LIST TITLE="List.Title.QueryResult">

    <DETAILS_ROW>


      <INLINE_PAGE
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="contextPanelName"
      />


    </DETAILS_ROW>

    <FIELD
      LABEL="Field.Label.Reference"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$caseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$runResult$list$searchDtls$restricted"
          />
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="runResult$list$searchDtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$investigationType"
        />
      </CONNECT>


    </FIELD>


    <FIELD
      LABEL="Field.Label.Subtype"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$subtype"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Client"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$concernRoleName"
        />
      </CONNECT>
      <!--BEGIN, CR00139966, MC-->
      <LINK PAGE_ID="Participant_resolve">
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$runResult$list$searchDtls$restricted"
          />
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="runResult$list$searchDtls$concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="runResult$list$searchDtls$concernRoleType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleType"
          />
        </CONNECT>
      </LINK>
      <!--END, CR00139966-->
    </FIELD>


    <FIELD
      LABEL="Field.Label.Resolution"
      WIDTH="16"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$resolution"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="runResult$list$searchDtls$statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
