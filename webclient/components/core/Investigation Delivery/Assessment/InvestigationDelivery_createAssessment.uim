<!-- The page allows the user to create an assessment.                      -->
<PAGE
  PAGE_ID="InvestigationDelivery_createAssessment"
  WINDOW_OPTIONS="width=450"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IntegratedCase"
    NAME="LIST_CASE_MEMBERS"
    OPERATION="listCaseParticipantsDetails"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Resource"
    NAME="LIST_ASSESSMENT_TYPES"
    OPERATION="listDistinctInvCAssessmentConfigurationsByCaseID"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="InvestigationDelivery"
    NAME="CREATE_ASSESSMENT"
    OPERATION="createAssessment"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="LIST_CASE_MEMBERS"
      PROPERTY="key$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="LIST_ASSESSMENT_TYPES"
      PROPERTY="key$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="CREATE_ASSESSMENT"
      PROPERTY="key$dtls$integratedCaseID"
    />
  </CONNECT>
  
  <CLUSTER
    NUM_COLS="1"
    LABEL_WIDTH="35"
  >
    <FIELD LABEL="Field.Label.ClientName">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$memberDetailsList$dtls$participantRoleID"
          NAME="LIST_CASE_MEMBERS"
          PROPERTY="result$memberDetailsList$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="CREATE_ASSESSMENT"
          PROPERTY="key$dtls$clientID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Type"
      USE_BLANK="TRUE"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$list$list$dtls$assessmentConfigurationID"
          NAME="LIST_ASSESSMENT_TYPES"
          PROPERTY="result$list$list$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="CREATE_ASSESSMENT"
          PROPERTY="key$dtls$assessmentConfigurationID"
        />
      </CONNECT>
    </FIELD>
    <FIELD 
      LABEL="Field.Label.AssessmentDate"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="CREATE_ASSESSMENT"
          PROPERTY="key$dtls$expectedStartDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
