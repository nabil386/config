<!-- This page allows the user to select the type of integrated case being  -->
<!-- created.                                                               -->
<PAGE
  PAGE_ID="InvestigationSample_selectType"
  WINDOW_OPTIONS="width=775"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
  
    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        OPEN_MODAL="false"
        DISMISS_MODAL="false"
        SAVE_LINK="false"
        PAGE_ID="InvestigationSample_searchPerson"
      />
    </ACTION_CONTROL>
    
    <ACTION_CONTROL
      IMAGE="CreateCase"
      LABEL="ActionControl.Label.CreateCase"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="InvestigationSample_home">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    >
    </ACTION_CONTROL>
    
  </ACTION_SET>
  
  
  <INCLUDE FILE_NAME="InvestigationSample_selectTypeView.vim" />

</PAGE>
