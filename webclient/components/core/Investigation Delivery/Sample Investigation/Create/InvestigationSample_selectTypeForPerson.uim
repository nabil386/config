<!-- This page allows the user to select the type of integrated case being  -->
<!-- created.                                                               -->
<PAGE
  PAGE_ID="InvestigationSample_selectTypeForPerson"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="CreateCase"
      LABEL="ActionControl.Label.CreateCase"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00438956, AC-->
      <!-- BEGIN, CR00239384, MC -->
      <LINK PAGE_ID="Investigation_resolveHome">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00239384 -->
      <!-- END, CR00438956-->
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    >
    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="InvestigationSample_selectTypeView.vim"/>


</PAGE>
