<!-- EducationalInstitution_duplicateRegistrationCheck page -->
<PAGE PAGE_ID="EducationalInstitute_duplicateRegistrationCheck1" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="relatedConcernID"/>


  <INCLUDE FILE_NAME="EducationalInstitute_searchCriteriaView.vim"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="PreviousButton" LABEL="ActionControl.Label.Previous">
      <LINK DISMISS_MODAL="false" OPEN_MODAL="false" PAGE_ID="EducationalInstitute_previousRegistrationCheck" SAVE_LINK="false"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="ContinueButton" LABEL="ActionControl.Label.ContinueRegistration">
      <LINK DISMISS_MODAL="false" OPEN_MODAL="false" PAGE_ID="EducationalInstitute_register" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="relatedConcernID"/>
          <TARGET NAME="PAGE" PROPERTY="relatedConcernID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER>
    <ACTION_SET ALIGNMENT="LEFT" BOTTOM="false">
      <ACTION_CONTROL DEFAULT="true" IMAGE="SearchButton" LABEL="ActionControl.Label.Search" TYPE="SUBMIT">
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="ResetButton" LABEL="ActionControl.Label.Reset">
        <LINK PAGE_ID="EducationalInstitute_duplicateRegistrationCheck1"/>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <LIST TITLE="List.Title.SearchResults">


    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="EducationalInstitute_homeTabDetails">
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="dtlsList$concernRoleID"/>
          <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>


    <FIELD LABEL="Field.Title.Name" WIDTH="34">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$participantDetails"/>
      </CONNECT>
      <LINK PAGE_ID="EducationalInstitute_home">
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="dtlsList$concernRoleID"/>
          <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Title.Address" WIDTH="44">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$formattedAddress"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Type" WIDTH="18">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$subtypeDescription"/>
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>