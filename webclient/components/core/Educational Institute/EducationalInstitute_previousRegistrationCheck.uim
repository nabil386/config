<!-- Description 																														-->
<!-- =========== 																														-->
<!-- This page allows the user to enter details to check if the 						-->
<!-- external party has already been registered.   													-->
<PAGE
  PAGE_ID="EducationalInstitute_previousRegistrationCheck"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE DESCRIPTION="PageTitle.Description">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.PreviousRegistration"
  >


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.No">
        <LINK
          DISMISS_MODAL="false"
          OPEN_MODAL="false"
          PAGE_ID="EducationalInstitute_duplicateRegistrationCheck1"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.ProductProvider">
        <LINK
          DISMISS_MODAL="false"
          OPEN_MODAL="false"
          PAGE_ID="EducationalInstitute_previousProductProviderRegistrationSearch1"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.Employer">
        <LINK
          DISMISS_MODAL="false"
          OPEN_MODAL="false"
          PAGE_ID="EducationalInstitute_previousEmployerRegistrationSearch1"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.ServiceSupplier">
        <LINK
          DISMISS_MODAL="false"
          OPEN_MODAL="false"
          PAGE_ID="EducationalInstitute_previousServiceSupplierRegistrationSearch1"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </CONTAINER>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false">
    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.Exit">
        
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


</PAGE>
