<!-- Creates an action plan                                                 -->
<!-- BEGIN, CR00186503, GYH -->
<?curam-deprecated Since Curam 6.0, replaced with ActionPlan_createActionPlan1.
  In tabbed UI, the Investigation Home tab defines caseID as page-param, but
  this page requires two page parameters, linkID and pageDescription. So this
  page doesn't support when it accessed from Investigation Home tab. Hence this
  page has been deprecated. See release note: CR00186503 ?>
<!-- END, CR00186503 -->
<PAGE
  PAGE_ID="ActionPlan_createActionPlan"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="pageDescription"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="InvestigationDelivery"
    NAME="ACTION"
    OPERATION="createActionPlan"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="linkID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$linkID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="linkID"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <!-- BEGIN, CR00179293, AK -->


  <INCLUDE FILE_NAME="ActionPlan_createActionPlanView.vim"/>


  <!-- END, CR00179293 -->


</PAGE>
