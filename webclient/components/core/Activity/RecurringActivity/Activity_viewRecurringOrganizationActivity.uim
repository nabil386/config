<!-- This page allows the user to view an organization's recurring activity -->
<!-- when coming directly from the organization calendar.                   -->
<PAGE
  PAGE_ID="Activity_viewRecurringOrganizationActivity"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
    >
      <!-- BEGIN CR00438746, HS -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Activity_modifyRecurringOrganizationActivity"
        SAVE_LINK="false"
      >
        <!-- END, CR00438746 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="activityID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="DeleteButton"
      LABEL="ActionControl.Label.Delete"
    >
      <!-- BEGIN CR00438746, HS -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Activity_cancelRecurringOrganizationActivity"
        SAVE_LINK="false"
      >
        <!-- END, CR00438746 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="activityID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="activityID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <INCLUDE FILE_NAME="Activity_viewRecurringOrganizationActivityView.vim"/>


</PAGE>
