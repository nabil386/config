<!-- This jsp redirects the user to a Home Page.-->
<!-- Each assignee type value is matched to a Home Page name.-->
<PAGE
  PAGE_ID="Activity_resolveClientHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);


      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String activityClientType = request.getParameter("activityClientType");
      String activityClientID = request.getParameter("activityClientID");
      String url = "";
  
      if(activityClientType == null)
      {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "activityClientType");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }   
  
      if(activityClientID == null) 
      {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "activityClientID");
       System.out.println(e);
       throw e;
       
      } 
  
      if(activityClientType.equals("RL1"))
      {
        url = context + "Person_homePagePage.do?concernRoleID="
          + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
      }
        else
        {
          if(activityClientType.equals("RL2"))
          {
            url = context + "Employer_homePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
        else
        {
          if(activityClientType.equals("RL3"))
          {
            url = context + "ServiceSupplier_homePagePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
        else
        {
          if(activityClientType.equals("RL4"))
          {
            url = context + "ProductProvider_homePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
        else
        {
          if(activityClientType.equals("RL5"))
          {
            url = context + "InformationProvider_homePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
        else
        {
          if(activityClientType.equals("RL6"))
          {
            url = context + "Utility_homePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
         else
        {
          if(activityClientType.equals("RL13"))
          {
            url = context + "Representative_homePage.do?concernRoleID="
              + curam.omega3.request.RequestUtils.escapeURL(activityClientID);
          }
        }
        }
        }
        }
        }
        }
      // END, CR00110216
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
