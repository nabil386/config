<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to add a facility to a Product Provider      -->
<!-- location.                                                              -->
<PAGE
  PAGE_ID="ProductProvider_addFacilityToLocation"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ProductProvider"
    NAME="DISPLAY"
    OPERATION="listProviderLocationFacility"
  />


  <SERVER_INTERFACE
    CLASS="ProductProvider"
    NAME="ACTION"
    OPERATION="addFacilitiesToProviderLocation"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="providerLocationID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="providerLocationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="providerLocationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="providerLocationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="providerLocationID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD HEIGHT="5">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="facilitiesNotInLocation$dtls$typeCode"
          NAME="DISPLAY"
          PROPERTY="facilitiesNotInLocation$dtls$typeCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="locationTabList"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
