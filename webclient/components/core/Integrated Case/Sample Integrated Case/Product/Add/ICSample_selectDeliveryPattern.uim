<!-- This page allows the user to select the delivery pattern for the case. -->
<PAGE
  PAGE_ID="ICSample_selectDeliveryPattern"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!--BEGIN, CR00146808, CD-->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!--END, CR00146808-->


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listDeliveryPatternPreferredMethodOfPayment1"
  />


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="productProviderID"/>
  <PAGE_PARAMETER NAME="locationID"/>
  <PAGE_PARAMETER NAME="integratedCaseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$concernRoleID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="25">
    <FIELD LABEL="Field.Label.PreferredPaymentMethod">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="preferredDeliveryMethod"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        PAGE_ID="ICSample_selectProviderAndLocation"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    />


  </ACTION_SET>


  <CLUSTER
    SHOW_LABELS="false"
    WIDTH="100%"
  >


    <LIST TITLE="List.Title.AvailableDeliveryPatterns">


      <CONTAINER
        LABEL="Container.Label.Action"
        SEPARATOR="Container.Separator"
        WIDTH="20"
      >
        <ACTION_CONTROL LABEL="ActionControl.Label.View">
          <LINK PAGE_ID="Product_viewDeliveryPattern">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="productDeliveryPatternInfoID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="productDeliveryPatternInfoID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>


        <ACTION_CONTROL LABEL="ActionControl.Label.Select">
          <!--BEGIN, CR00146808, CD-->
          <CONDITION>
            <IS_TRUE
              NAME="DISPLAY"
              PROPERTY="selectLinkInd"
            />
          </CONDITION>
          <!--END, CR00146808-->
          <!--BEGIN CR00085879, GBA-->
          <LINK
            PAGE_ID="ICSample_createProductDelivery"
            SAVE_LINK="false"
          >
            <!--END CR00085879-->
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="concernRoleID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="concernRoleID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="productID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="productID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="productProviderID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="productProviderID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="locationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="locationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="productDeliveryPatternID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="productDeliveryPatternID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="integratedCaseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="integratedCaseID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="70"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="name"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.PreferredMethod"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernPrefDeliveryMthdInd"
          />
        </CONNECT>
      </FIELD>


    </LIST>


  </CLUSTER>
</PAGE>
