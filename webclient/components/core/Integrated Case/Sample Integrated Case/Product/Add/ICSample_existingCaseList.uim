<!-- The page displys a list of existing cases that a client is already     -->
<!-- receiving. The user can exit the process or continue.                  -->
<!-- BEGIN, CR00335698, PB -->
<PAGE
  PAGE_ID="ICSample_existingCaseList"
  WINDOW_OPTIONS="width=900"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00335698 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        OPEN_MODAL="false"
        DISMISS_MODAL="false"
        SAVE_LINK="false"
        PAGE_ID="ICSample_selectMember"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
    >
      <LINK
        OPEN_MODAL="false"
        DISMISS_MODAL="false"
        SAVE_LINK="false"
        PAGE_ID="ICSample_createProductDelivery"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    />
  </ACTION_SET>
  
  <PAGE_PARAMETER NAME="integratedCaseID"/>

  <!-- BEGIN, CR00211698, MC -->
  <INCLUDE FILE_NAME="ICSample_existingCaseListContent.vim"/>
  <!-- END, CR00211698 -->


</PAGE>
