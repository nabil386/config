<!-- This page is used to display a list of events for the income           -->
<!-- assistance benefit sample case.                                        -->
<PAGE
  HIDE_CONDITIONAL_LINKS="true"
  PAGE_ID="DefaultICProduct_listEvent"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


    

  <ACTION_SET BOTTOM="false">
  
  <ACTION_CONTROL LABEL="ActionControl.Label.NewMeeting">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="CalendarMeeting_createMeeting"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
  <SEPARATOR/>
  
  <ACTION_CONTROL
        IMAGE="NewActivityButton"
        LABEL="ActionControl.Label.NewActivity"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DefaultICProduct_createActivity"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$contextDescription$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
  
  
      <ACTION_CONTROL
        IMAGE="NewRecurringActivityButton"
        LABEL="ActionControl.Label.CreateRecurringActivity"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DefaultICProduct_createRecurringActivity"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$contextDescription$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
    </ACTION_CONTROL>
    <SEPARATOR/>
     <ACTION_CONTROL
          IMAGE="CreateReview"
          LABEL="ActionControl.Label.CreateReview"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="Case_createReview"
            WINDOW_OPTIONS="width=875,height=540"
          >
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="caseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="description"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pageDescription"
              />
            </CONNECT>
          </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL
      IMAGE="CreateReferral"
      LABEL="ActionControl.Label.CreateReferral"
    >
      <!-- BEGIN, 277131, BD -->
      <CONDITION>
        <IS_FALSE
          NAME="DISPLAY"
          PROPERTY="isCPMInstalledInd"
        />
      </CONDITION>
      <!-- END, 277131 -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Case_createReferral"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>

