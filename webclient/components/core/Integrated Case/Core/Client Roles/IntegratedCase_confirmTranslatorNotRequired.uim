<!-- Asks user for confirmation for  translator is required for case member  -->
<PAGE PAGE_ID="IntegratedCase_confirmTranslatorNotRequired" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="Case" NAME="ACTION" OPERATION="modifyTranslatorRequiredDetails" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="Case" NAME="DISPLAY" OPERATION="readCaseMember1"/>
  <PAGE_PARAMETER NAME="concernCaseRoleID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernCaseRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="caseParticipantRoleID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernCaseRoleID"/>
    <TARGET NAME="DISPLAY" PROPERTY="concernCaseRoleID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$translationRequiredInd"/>
    <TARGET NAME="ACTION" PROPERTY="translationRequiredInd"/>
  </CONNECT>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="confirmMessage"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>