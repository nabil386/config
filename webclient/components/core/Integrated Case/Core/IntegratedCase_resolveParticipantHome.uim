<!-- This jsp redirects the user to a member home page within an integrated case. -->
<PAGE
  PAGE_ID="IntegratedCase_resolveParticipantHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
       curam.omega3.request.RequestHandler
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String caseID = request.getParameter("caseID");
      String caseParticipantRoleID  = request.getParameter("caseParticipantRoleID");
      String homePageName="";
      String url = "";

      if (caseParticipantRoleID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "caseParticipantRoleID");
       throw e;
       // END, CR00110216
      }
            
      curam.interfaces.IntegratedCasePkg.IntegratedCase_resolveParticipantHome_TH 
         th = new curam.interfaces.IntegratedCasePkg.IntegratedCase_resolveParticipantHome_TH();
    
      th.setFieldValue(th.key$caseParticipantRoleID_idx,caseParticipantRoleID);
   
      th.callServer();
    
      homePageName=th.getFieldValue(th.result$participantHomePageName_idx);
      
      // BEGIN, CR00243612, EC
      if (caseID == null) {      
       curam.interfaces.IntegratedCasePkg.IntegratedCase_readCaseIDandParticipantRoleIDDetails_TH 
                  th2 = new curam.interfaces.IntegratedCasePkg.IntegratedCase_readCaseIDandParticipantRoleIDDetails_TH();     
          
       th2.setFieldValue(th2.key$dtls$caseParticipantRoleID_idx,caseParticipantRoleID);          
       th2.callServer();
   
       caseID=th2.getFieldValue(th2.result$caseID_idx);
      }
      // END, CR00243612
      
    // BEGIN, CROOO53132, GSP
    // BEGIN, HARP 71578, MM
    // BEGIN, CR00110216, SK
    if ((homePageName==null) ||(homePageName.equals(""))) 
    { 
    url = context + "Case_resolveCaseNoHomePage.do?caseParticipantRoleID="+
     curam.omega3.request.RequestUtils.escapeURL(caseParticipantRoleID) + "&amp;caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID);        
    } 
    else 
    { 
     
     url = context + curam.omega3.request.RequestUtils.escapeURL(homePageName) + "Page.do?caseParticipantRoleID=" + curam.omega3.request.RequestUtils.escapeURL(caseParticipantRoleID) + "&amp;caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID);
    } 
    // END, CR00110216
    // END, HARP 71578 
    // END, CROOO53132
    url += "&amp;" + rh.getSystemParameters(); 
      response.sendRedirect(response.encodeRedirectURL(url));
       
  </JSP_SCRIPTLET>
</PAGE>
