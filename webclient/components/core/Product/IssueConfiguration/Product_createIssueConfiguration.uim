<!-- This page allows a user to create an issue configuration record. -->
<PAGE PAGE_ID="Product_createIssueConfiguration" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="createIssueConfiguration" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="47" NUM_COLS="2">


    <FIELD LABEL="Field.Label.Type" USE_BLANK="false" USE_DEFAULT="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Priority" USE_BLANK="false" USE_DEFAULT="true" WIDTH="50">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="priority"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.HomePageIdentifier">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="homePageIdentifier"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00187598, SS -->
    <FIELD LABEL="Field.Title.OwnershipStrategy">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="ownershipStrategyName"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00187598 -->
    <!-- BEGIN, CR00283741, KOH -->
    <FIELD LABEL="Field.Label.Appealable">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="notAppealableInd"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00283741 -->
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="23.5" TITLE="Cluster.Title.EventDetails">


    <FIELD LABEL="Field.Label.CreateEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="eventClassAndType"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>