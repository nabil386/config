<!-- This page allows a user to change the status of an existing  issue configuration record to Canceled. -->
<PAGE PAGE_ID="Product_cancelIssueConfiguration" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="cancelIssueConfiguration" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="Product" NAME="DISPLAY" OPERATION="readIssueConfiguration"/>
  <PAGE_PARAMETER NAME="issueConfigurationID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="issueConfigurationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$issueConfigurationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="issueConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="issueConfigurationID"/>
  </CONNECT>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="Product_listIssueConfiguration" SAVE_LINK="false"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelIssueConfiguration"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>