<!-- This page lists all the resolutions associated with a product          -->
<PAGE
  PAGE_ID="Product_listResolution"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listResolutionConfiguration"
  />
  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Product_createResolution"
      />


    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_modifyResolution"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="resolutionConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="resolutionConfigurationID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_removeResolution"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="resolutionConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="resolutionConfigurationID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Resolution"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="resolution"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ResolutionEvent"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="eventClassAndType"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
