<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to create a delivery pattern for a Product.  -->
<PAGE
  PAGE_ID="Product_createDeliveryPatternWizard"
  WINDOW_OPTIONS="width=950,height=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="wizardDataForDeliveryDtls$wizardMenu"
      />
    </CONNECT>
  </MENU>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="productCategoryID"/>
  <PAGE_PARAMETER NAME="deliveryPatternID"/>
  <PAGE_PARAMETER NAME="productProvisionID"/>
  <PAGE_PARAMETER NAME="provisionLocationID"/>
  <PAGE_PARAMETER NAME="creoleProductID"/>


  <SERVER_INTERFACE
    CLASS="ProductWizard"
    NAME="DISPLAY"
    OPERATION="readDeliveryPattern"
  />
  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="wizardDataForDeliveryDtls$wizardAction"
    CLASS="ProductWizard"
    NAME="ACTION"
    OPERATION="storeDeliveryPattern"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productCategoryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productCategoryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deliveryPatternID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$deliveryPatternID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productProvisionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productProvisionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="provisionLocationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$provisionLocationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$creoleProductID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$productID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productCategoryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$productCategoryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deliveryPatternID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$deliveryPatternID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productProvisionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$productProvisionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="provisionLocationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$provisionLocationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$wizardDataForDeliveryDtls$creoleProductID"
    />
  </CONNECT>


  <!-- START,CR00227081,MJ -->
  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      LABEL="ActionControl.Label.Back"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Product_addCategoryToBenefitWizard"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$deliveryPatternID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      ACTION_ID="next"
      ALIGNMENT="RIGHT"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Product_createProductProvisionForBenefitWizard"
        SAVE_LINK="false"
      >


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$deliveryPatternID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <!-- END,CR00227081,MJ -->


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TAB_ORDER="ROW"
  >
    <FIELD LABEL="Field.Label.From">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fromDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Max">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="maximumAmount"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="maximumAmount"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Default">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="defaultPatternInd"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="defaultPatternInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Cover">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="coverPattern"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="coverPattern"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DeliveryMethod">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="deliveryMethodTypeCode"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deliveryMethodID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="deliveryMethodID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DeliveryFreq">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deliveryFrequency"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="deliveryFrequency"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.Offset">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="offset"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="offset"
        />


      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00408407, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00408407 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
