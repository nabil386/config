<!-- This page is used to create a new Provision Location. -->
<PAGE
  PAGE_ID="Product_createProvisionLocationForBenefitWizard"
  WINDOW_OPTIONS="width=950,height=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="wizardDataForProvisionLocationDtls$wizardMenu"
      />
    </CONNECT>
  </MENU>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ProductWizard"
    NAME="DISPLAY"
    OPERATION="readProvisionLocation"
  />
  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="wizardDataForProvisionLocationDtls$wizardAction"
    CLASS="ProductWizard"
    NAME="ACTION"
    OPERATION="storeProvisionLocation"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="productCategoryID"/>
  <PAGE_PARAMETER NAME="deliveryPatternID"/>
  <PAGE_PARAMETER NAME="productProvisionID"/>
  <PAGE_PARAMETER NAME="provisionLocationID"/>
  <PAGE_PARAMETER NAME="creoleProductID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$productID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productCategoryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$productCategoryID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deliveryPatternID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$deliveryPatternID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productProvisionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$productProvisionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="provisionLocationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$provisionLocationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardDataForProvisionLocationDtls$creoleProductID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productCategoryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productCategoryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deliveryPatternID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$deliveryPatternID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productProvisionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$productProvisionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="provisionLocationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$provisionLocationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProductID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$creoleProductID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Back">
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Product_createProductProvisionForBenefitWizard"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$provisionLocationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      ACTION_ID="next"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00295809, CW -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Product_addEligibilityDeterminationToBenefitWizard1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$provisionLocationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="provisionLocationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="creoleProductID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="deliveryPatternID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productCategoryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$eligibilityEntitlementRuleSetNameOpt"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eligibilityEntitlementRuleSetNameOpt"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$eligibilityEntitlementRuleSetDescOpt"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="eligibilityEntitlementRuleSetDescOpt"
          />
        </CONNECT>
        <!-- END, CR00295809 -->
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- Start Provision Location. -->


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.Location">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="providerLocationName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerLocationID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$providerLocationID"
        />
      </CONNECT>
      <LINK>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productProvisionID"
          />
        </CONNECT>
      </LINK>


    </FIELD>
    <FIELD LABEL="Field.Label.StartDate1">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$provisionLocationPageDtls$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Cost"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="estimateCost"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$provisionLocationPageDtls$estimateCost"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate1">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$provisionLocationPageDtls$endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00408407, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00408407 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$provisionLocationPageDtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- End Provision Location. -->


</PAGE>
