<!-- This page allows a user to cancel an investigation resolution link.    -->
<PAGE PAGE_ID="Product_cancelInvestigationResolutionLink" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="cancelInvestigationResolutionLink" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="resolutionLinkID"/>
  <PAGE_PARAMETER NAME="investigationConfigID"/>
  <PAGE_PARAMETER NAME="description"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="resolutionLinkID"/>
    <TARGET NAME="ACTION" PROPERTY="investigationResolutionLinkID"/>
  </CONNECT>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="Product_listResolutionsForInvestigation" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="investigationConfigID"/>
          <TARGET NAME="PAGE" PROPERTY="investigationConfigID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelInvestigationResolutionLink"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>