<!-- This page allows the user to create a resolution configuration.                            -->
<PAGE
  PAGE_ID="Product_addExistingResolutionForInvestigation"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="ACTION"
    OPERATION="createResolutionForInvestigation"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listUnassignedResolutionDetailsForInvestigation"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="investigationConfigID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="investigationConfigID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="investigationConfigID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="investigationConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="investigationConfigID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="AddButton"
      LABEL="ActionControl.Label.Add"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="25">
    <FIELD LABEL="Field.Label.Resolution">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="resolutionConfigurationID"
          NAME="DISPLAY"
          PROPERTY="resolution"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="resolutionConfigurationID"
        />
      </CONNECT>


    </FIELD>


  </CLUSTER>


</PAGE>
