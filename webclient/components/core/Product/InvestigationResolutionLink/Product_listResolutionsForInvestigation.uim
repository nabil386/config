<!-- This page lists all the resolutions associated with a product.         -->
<PAGE
  PAGE_ID="Product_listResolutionsForInvestigation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listResolutionsForInvestigation"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="investigationConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="investigationConfigID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="investigationConfigID"/>
  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
    TOP="true"
  >
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Product_createResolutionForInvestigation"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="investigationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="investigationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="AddExisting"
      LABEL="ActionControl.Label.AddExisting"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Product_addExistingResolutionForInvestigation"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="investigationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="investigationConfigID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_modifyResolution"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="resolutionConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="resolutionConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_cancelInvestigationResolutionLink"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="investigationResolutionLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="resolutionLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="investigationConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="investigationConfigID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Resolution"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="resolution"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
