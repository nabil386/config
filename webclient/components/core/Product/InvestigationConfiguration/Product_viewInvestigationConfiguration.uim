<!-- ===========                                                            -->
<!-- This page allows a user to view the details for an existing            -->
<!-- investigation configuration record. -->
<PAGE
  PAGE_ID="Product_viewInvestigationConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="readInvestigationConfiguration"
  />


  <PAGE_PARAMETER NAME="investigationConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="investigationConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$investigationConfigID"
    />
  </CONNECT>


  <!--BEGIN, CR00225878, PB-->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >
    <!--END, CR00225878-->


    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.HomePageIdentifier">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="homePageIdentifier"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00305740, PB -->
    <FIELD LABEL="Field.Title.ContactLog">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="contactLogInd"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00305740 -->
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreationDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00205192, SS -->
    <FIELD LABEL="Field.Title.OwnershipStrategy">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="ownershipStrategyName"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00205192 -->
    <!--BEGIN, CR00205591, PB-->
    <!--BEGIN, CR00225878, PB-->
    <FIELD
      LABEL="Field.Label.TranslationRequired"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="adminTranslationRequiredInd"
        />
      </CONNECT>
    </FIELD>
    <!--END, CR00225878-->
    <!--END, CR00205591-->
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.MDTSharingDetails"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="displayCollaborationSharingDetailsInd"
      />
    </CONDITION>
    <FIELD
      LABEL="Field.Title.MDTType"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="collaborationType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.MDTPortal"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="collaborationPortalHomePage"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.MDTSharing">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="collaborationSharingInd"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.EventDetails"
  >


    <FIELD LABEL="Field.Label.CreateEvent">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createEvent"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CloseEvent">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="closeEvent"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Label.SecurityRestrictions"
  >
    <FIELD LABEL="Field.Label.CreateRights">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="securityDtls$createSecurity"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ReadRights">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="securityDtls$readSecurity"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ApproveRights">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="securityDtls$approveSecurity"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MaintainRights">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="securityDtls$maintainSecurity"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
