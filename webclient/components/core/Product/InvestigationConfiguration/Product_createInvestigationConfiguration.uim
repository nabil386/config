<!-- This page allows a user to create an investigation configuration       -->
<!-- record. -->
<!--BEGIN, CR00225878, PB-->
<PAGE PAGE_ID="Product_createInvestigationConfiguration" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!--END, CR00225878-->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="createInvestigationConfiguration" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="Collaboration" NAME="DISPLAY" OPERATION="readCollaborationConfiguration" PHASE="DISPLAY"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN, CR00372416, VT -->
  <CLUSTER LABEL_WIDTH="64" NUM_COLS="2">
    <!-- END, CR00372416 -->


    <!--BEGIN, CR00143688, SK  -->
    <FIELD LABEL="Field.Label.Type" USE_BLANK="true" USE_DEFAULT="false" WIDTH="72">
      <!--END, CR00143688  -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00205192, SS -->
    <FIELD LABEL="Field.Title.OwnershipStrategy">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="ownershipStrategyName"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00205192 -->
    <!-- BEGIN, CR00305740, PB -->
    <FIELD LABEL="Field.Title.ContactLog">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="contactLogInd"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00305740 -->
    <FIELD LABEL="Field.Label.HomePageIdentifier">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="homePageIdentifier"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>


    <!--BEGIN, CR00205591, PB-->
    <FIELD LABEL="Field.Label.TranslationRequired" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="adminTranslationRequiredInd"/>
      </CONNECT>
    </FIELD>
    <!--END, CR00205591-->
  </CLUSTER>


  <!-- BEGIN, CR00372416, VT-->
  <CLUSTER LABEL_WIDTH="64" NUM_COLS="2" TITLE="Cluster.Title.MDTSharingDetails">
    <!-- END, CR00372416 -->


    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="collaborationEnabledInd"/>
    </CONDITION>


    <FIELD LABEL="Field.Title.MDTType" USE_BLANK="true" WIDTH="60">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="collaborationType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.MDTPortal" WIDTH="100">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="collaborationPortalHomePage"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.MDTSharing">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="collaborationSharingInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- BEGIN, CR00372416, VT -->
  <CLUSTER LABEL_WIDTH="32" TITLE="Cluster.Title.EventDetails">
    <!-- END, CR00372416 -->


    <FIELD LABEL="Field.Label.CreateEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createEvent"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CloseEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="closeEvent"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <!-- BEGIN, CR00372416, VT -->
  <CLUSTER LABEL_WIDTH="32" TITLE="Cluster.Label.SecurityRestrictions">
    <!-- END, CR00372416 -->


    <FIELD LABEL="Field.Label.CreateRights">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="securityDtls$createSecurity"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReadRights">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="securityDtls$readSecurity"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ApproveRights">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="securityDtls$approveSecurity"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MaintainRights">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="securityDtls$maintainSecurity"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">


    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>