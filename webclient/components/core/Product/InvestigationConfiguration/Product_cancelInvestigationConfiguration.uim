<!-- This page allows a user to change the status of an existing            -->
<!-- investigation configuration record to Canceled. -->
<PAGE PAGE_ID="Product_cancelInvestigationConfiguration" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="cancelInvestigationConfiguration" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="Product" NAME="DISPLAY" OPERATION="readInvestigationConfiguration"/>


  <PAGE_PARAMETER NAME="investigationConfigID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="investigationConfigID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$investigationConfigID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="investigationConfigID"/>
    <TARGET NAME="ACTION" PROPERTY="investigationConfigID"/>
  </CONNECT>
  >
    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="Product_listInvestigationConfiguration" SAVE_LINK="false"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelInvestigationConfiguration"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>