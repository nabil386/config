<!-- This page lists all the milestones associated with a Product Delivery  -->
<PAGE
  PAGE_ID="Product_listMilestoneForProductDelivery"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>


  </PAGE_TITLE>


  <!-- BEGIN, CR00236426, GP -->
  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="listMilestoneConfigurationsForCaseType"
  />
  <!-- END, CR00236426 -->


  <PAGE_PARAMETER NAME="caseTypeID"/>
  <PAGE_PARAMETER NAME="caseType"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseTypeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseTypeID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseType"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Product_createMilestoneForProductDelivery"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseTypeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseTypeID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseType"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>


      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="AddExisting"
      LABEL="ActionControl.AddExisting.label"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Product_addExistingMilestoneToProductDelivery"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseTypeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseTypeID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseType"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL
        IMAGE="EditButton"
        LABEL="ActionControl.Label.Edit"
      >


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_modifyMilestoneConfigAssociation"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneLinkID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_removeMilestoneFromProductDelivery"
        >


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneLinkID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseTypeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseTypeID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseType"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <!-- BEGIN, CR00236426, GP -->
    <CONTAINER LABEL="Field.Label.Name">
      <!-- END, CR00236426  -->
      <FIELD WIDTH="30">
        <LINK PAGE_ID="Product_listMilestoneConfiguration">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneConfigurationID"
            />
          </CONNECT>
        </LINK>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="name"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00236426, GP -->
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_listLocalizableMilestoneNameText"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="nameTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneConfigurationID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END,   -->


    <!-- BEGIN, CR00236426, GP -->
    <CONTAINER LABEL="Field.Label.Type">
      <!-- END, CR00236426 -->
      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="type"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00236426, GP -->
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Product_listLocalizableMilestoneTypeText"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="typeTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneConfigurationID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00236426  -->


    <FIELD
      LABEL="Field.Label.DateCreated"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Product_viewMilestoneConfigAssociation">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="milestoneLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="milestoneLinkID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>


      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
