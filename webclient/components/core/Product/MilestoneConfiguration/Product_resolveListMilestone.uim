<!-- This jsp redirects the user to a Case Home Page.-->
<!-- Each case type value is matched to a Home Page name.-->
<PAGE
  PAGE_ID="Product_resolveListMilestone"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
  
        curam.omega3.request.RequestHandler 
          rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
  
        String context = request.getContextPath() + "/";
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
        
        String productID = request.getParameter("productID");
        String adminIntegratedCaseID = request.getParameter("adminIntegratedCaseID");
        String screeningConfigID = request.getParameter("screeningConfigID");
        String caseTypeID = "";
        String caseType = "";
        String url = "";
        String pageName = "";
        
        
        if(productID != null &amp;&amp; !(productID.equals(""))){          
            caseTypeID = productID;
            caseType = "CT2";     
            pageName = "Product_listMilestoneForProductDelivery";             
        } else if(adminIntegratedCaseID != null &amp;&amp; !(adminIntegratedCaseID.equals(""))){
            caseTypeID = adminIntegratedCaseID;
            caseType = "CT5"; 
            pageName = "Product_listMilestoneForIntegratedCase";   
        } else if(screeningConfigID != null &amp;&amp; !(screeningConfigID.equals(""))){
            caseTypeID = screeningConfigID;
            caseType = "CT6"; 
            pageName = "Product_listMilestoneForScreeningCase";
        }  
        
        url = context + pageName + "Page.do?caseTypeID=" + curam.omega3.request.RequestUtils.escapeURL(caseTypeID) + "&amp;caseType=" + curam.omega3.request.RequestUtils.escapeURL(caseType);
        url += "&amp;" + rh.getSystemParameters();
          
        response.sendRedirect(response.encodeRedirectURL(url));
      
  </JSP_SCRIPTLET>
</PAGE>
