<!-- This page allows a user to modify the details for an existing milestone-->
<!-- configuration link record.                       -->
<!-- BEGIN, CR00235147, SS -->
<PAGE PAGE_ID="Product_modifyMilestoneConfigAssociation" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00235147 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Product" NAME="DISPLAY" OPERATION="readMilestoneConfigurationLink" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="Product" NAME="ACTION" OPERATION="modifyMilestoneLinkAssociation" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="milestoneLinkID"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
 
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
    
    </ACTION_CONTROL>


  </ACTION_SET>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="milestoneLinkID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$milestoneLinkID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="milestoneLinkID"/>
    <TARGET NAME="ACTION" PROPERTY="milestoneLinkID"/>
  </CONNECT>


  <CLUSTER>
    <CLUSTER SHOW_LABELS="FALSE" STYLE="cluster-cpr-no-border">
      <FIELD CONFIG="DynamicDropDown.MilestoneComponents">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="existingComponents"/>
        </CONNECT>


        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="components"/>
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER LABEL_WIDTH="50" NUM_COLS="2" STYLE="cluster-cpr-no-border">


      <FIELD LABEL="Field.Label.CreationEvent">
        <CONNECT>
          <INITIAL NAME="DISPLAY" PROPERTY="creationEvent"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="creationEvent"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="creationEvent"/>
        </CONNECT>
      </FIELD>


      <!-- BEGIN, CR00235147, SS -->
      <FIELD LABEL="Field.Label.CompletionEvent" WIDTH="15" WIDTH_UNITS="CHARS">
        <!-- END, CR00235147 -->
        <CONNECT>
          <INITIAL NAME="DISPLAY" PROPERTY="completionEvent"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="completionEvent"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="completionEvent"/>
        </CONNECT>
      </FIELD>
    </CLUSTER>


  </CLUSTER>


</PAGE>