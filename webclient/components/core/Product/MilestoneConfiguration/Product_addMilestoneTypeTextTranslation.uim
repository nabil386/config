<!-- This page allows the user to  add a localizable text translation to the       -->
<!-- Milestone attribute Type..                                                       -->
<PAGE
  PAGE_ID="Product_addMilestoneTypeTextTranslation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Product"
    NAME="ACTION"
    OPERATION="addMilestoneTypeTextTranslation"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="milestoneConfigurationID"/>
  <PAGE_PARAMETER NAME="localizableTextID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="localizableTextID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$localizableTextID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneConfigurationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$localizableTextParentID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$versionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN, CR00249121, GP -->
  <CLUSTER LABEL_WIDTH="35">
    <!-- END, CR00249121 -->
    <FIELD
      LABEL="Field.Title.Language"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="12"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$localeCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Field.Title.Text"
  >
    <FIELD HEIGHT="4">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
