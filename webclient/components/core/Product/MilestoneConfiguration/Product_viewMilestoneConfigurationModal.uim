<!-- This page allows a user to view the details for an existing milestone  -->
<!-- configuration record.                                                  -->
<PAGE PAGE_ID="Product_viewMilestoneConfigurationModal" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Product" NAME="DISPLAY" OPERATION="readMilestoneConfiguration"/>


  <!-- BEGIN, CR00145379, MC-->
  <PAGE_PARAMETER NAME="milestoneConfigurationID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="milestoneConfigurationID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$milestoneConfigurationID"/>
  </CONNECT>
  <!-- END, CR00145379-->


  <CLUSTER LABEL_WIDTH="60" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!--BEGIN, CR00142282, MC-->
  <INCLUDE FILE_NAME="Product_viewMilestoneConfigurationView.vim"/>
  <!--END, CR00142282-->


  <ACTION_SET>
    <ACTION_CONTROL IMAGE="CloseButton" LABEL="ActionControl.Label.Close">
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>