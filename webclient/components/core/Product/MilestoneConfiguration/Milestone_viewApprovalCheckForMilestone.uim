<!-- This method allows an administrator to view approval check for a           -->
<!-- particular milestone.                                                      -->
<PAGE
  PAGE_ID="Milestone_viewApprovalCheckForMilestone"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainMilestoneWaiverApproval"
    NAME="DISPLAY"
    OPERATION="viewUserMWApproval"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="milestoneWaiverApprovalID"/>
  <PAGE_PARAMETER NAME="name"/>
  <PAGE_PARAMETER NAME="milestoneConfigurationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneWaiverApprovalID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$dtls$milestoneWaiverApprovalCheckID"
    />
  </CONNECT>


  <CLUSTER>
    <FIELD LABEL="Field.Title.Percentage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="percentage"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00416277, VT -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
