<!-- This jsp redirects the user to the appropriate product home page       -->
<PAGE
  PAGE_ID="Product_resolveProductHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String productID = request.getParameter("productID");
      String url = "";
      
      //boolean benefitInd = false;
      
      if (productID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "productID");
       System.out.println(e);
       throw e;
       // END, CR00110216
      }
      
      curam.interfaces.ProductPkg.Product_readProductTypeByProductID_TH 
         th = new curam.interfaces.ProductPkg.Product_readProductTypeByProductID_TH();
    
      th.setFieldValue(th.key$productID_idx, productID);
         
      th.callServer();
    
      //benefitInd=th.getFieldValue(th.result$benefitInd_idx);
      
      // BEGIN, CR00110216, SK
      if(th.getFieldValue(th.result$benefitInd_idx).equals("true")) {
            
        url = context + "Product_benefitHome1Page.do?productID=" + curam.omega3.request.RequestUtils.escapeURL(productID);
      }
      else {
          
        url = context + "Product_liabilityHome1Page.do?productID=" + curam.omega3.request.RequestUtils.escapeURL(productID);
      }
      // END, CR00110216
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
      
    
  </JSP_SCRIPTLET>
</PAGE>
