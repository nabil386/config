<!-- This page allows users to create an Evidence Metadata entry            -->
<!-- BEGIN, CR00349172, PB -->
<PAGE
  PAGE_ID="Product_createEvidenceMetadata"
  WINDOW_OPTIONS="width=800"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00349172 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="ACTION"
    OPERATION="createEvidenceMetadata"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="40">


    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="evidenceType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ViewPage">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="viewPageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ViewSnapshotPage">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="viewSnapshotPageName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EffectiveDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="effectiveFrom"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ModifyPage">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="modifyPageName"
        />
      </CONNECT>
    </FIELD>

<!-- BEGIN , 184517, CC-->
    <FIELD LABEL="Field.Label.AutoEndDateIndOpt">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="autoEndDateIndOpt"
        />
      </CONNECT>
    </FIELD>
        
    <FIELD LABEL="Field.Label.WizardCreatePageNameOpt">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="wizardCreatePageNameOpt"
        />
      </CONNECT>
    </FIELD>
<!-- END , 184517, CC -->    

    <FIELD LABEL="Field.Label.MaintenancePattern">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="maintenancePattern"
        />
      </CONNECT>
    </FIELD>  

  </CLUSTER>

</PAGE>