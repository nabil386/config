<!-- This page allows the user to view a list of Temporal Evidence Approval -->
<!-- Checks for a specified user                                            -->
<PAGE
  PAGE_ID="Product_viewTemporalEvidenceApprovalCheckForProduct"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY"
    OPERATION="readTemporalEvidenceApprovalCheck"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Product"
    NAME="DISPLAY2"
    OPERATION="listProductsSharingEvidence"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="productID"/>
  <PAGE_PARAMETER NAME="temporalEvApprovalCheckID"/>
  <PAGE_PARAMETER NAME="evidenceType"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="temporalEvApprovalCheckID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$key$temporalEvApprovalCheckID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY2"
      PROPERTY="key$productID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceType"
    />
    <TARGET
      NAME="DISPLAY2"
      PROPERTY="key$evidenceType"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="11"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="Cluster.Title.Shared">
    <FIELD
      LABEL="Field.Label.ProductName"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY2"
          PROPERTY="name"
        />
      </CONNECT>
      <LINK PAGE_ID="Product_resolveProductHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY2"
            PROPERTY="list$productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>


</PAGE>
