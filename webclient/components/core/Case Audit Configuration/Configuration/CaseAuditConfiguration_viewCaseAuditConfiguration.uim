<!-- This page allows a user to view audit case configuration details.      -->
<PAGE
  PAGE_ID="CaseAuditConfiguration_viewCaseAuditConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CaseAuditConfiguration"
    NAME="DISPLAY"
    OPERATION="viewAuditCaseConfiguration"
    PHASE="DISPLAY"
  />


  <MENU>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewTask">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="TaskManagement_createTask"
      />
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewActivity">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Activity_createStandardUserActivity"
      />
    </ACTION_CONTROL>
  </MENU>


  <PAGE_PARAMETER NAME="auditCaseConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="auditCaseConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$auditCaseConfigID"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="1">


    <CLUSTER STYLE="cluster-cpr-no-border">
      <FIELD LABEL="Field.Label.AllowManSelectInd">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$configDtls$dtls$manCaseSelectInd"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


  </CLUSTER>


  <CLUSTER TITLE="Cluster.Title.FixedQueries">


    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$displayFixedQueries"
      />
    </CONDITION>


    <LIST>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$fixedQueries$dtls$queryName"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>


  <CLUSTER TITLE="Cluster.Title.SelectedFocusAreas">


    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$displayFocusAreas"
      />
    </CONDITION>


    <LIST>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$focusAreas$focusAreas$focusAreaCode"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>


</PAGE>
