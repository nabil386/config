<!-- =========== -->
<!-- This page allows the user to search for an employer and prospect       -->
<!-- employer.                                                              -->
<!-- BEGIN, CR00232757, GD -->
<?curam-deprecated Since Curam 6.0, replaced by Employer_search1.uim?>
<!-- END, CR00232757 -->
<PAGE
  PAGE_ID="EmployerProspectEmployer_search"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ProspectEmployer"
    NAME="ACTION"
    OPERATION="searchEmployersAndProspectEmployers"
    PHASE="ACTION"
  />


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD LABEL="Field.Label.ReferenceNumber">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prospectEmployerSearchKey$referenceNumber"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TradingName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prospectEmployerSearchKey$tradingName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AddressLineOne">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prospectEmployerSearchKey$businessAddress"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <FIELD LABEL="Field.Label.RegisteredName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prospectEmployerSearchKey$registeredName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.City">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prospectEmployerSearchKey$businessCity"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER>
    <ACTION_SET
      ALIGNMENT="LEFT"
      BOTTOM="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK PAGE_ID="EmployerProspectEmployer_search"/>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <!-- List to display Employer records-->
  <LIST>
    <TITLE>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="List.Title.Employers"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$employerIndicator"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Label.SearchResultsClosingParanthesis"
        />
      </CONNECT>
    </TITLE>


    <CONTAINER
      LABEL="Container.Label.ReferenceNumber"
      WIDTH="8"
    >
      <FIELD WIDTH="8">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="eDetails$dtls$referenceNumber"
          />
        </CONNECT>
        <LINK PAGE_ID="ProspectEmployer_resolveDuplicateRegistrationCheck">
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="eDetails$dtls$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="eDetails$dtls$concernRoleType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleType"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="eDetails$dtls$altRegistrationIndicator"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="altRegistrationIndicator"
            />
          </CONNECT>


        </LINK>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.TradingName"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="eDetails$dtls$tradingName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.RegisteredName"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="eDetails$dtls$registeredName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.AddressLineOne"
      WIDTH="14"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="eDetails$dtls$businessAddress"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.City"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="eDetails$dtls$businessCity"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <!-- List to display Prospect Employers records-->
  <LIST>
    <TITLE>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="List.Title.ProspectEmployers"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$prospectEmployerIndicator"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Label.SearchResultsClosingParanthesis"
        />
      </CONNECT>
    </TITLE>


    <CONTAINER
      LABEL="Container.Label.AlternativeRegistration"
      WIDTH="4"
    >
      <FIELD WIDTH="4">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="peDetails$dtls$altRegistrationIndicator"
          />
        </CONNECT>
        <LINK PAGE_ID="Employer_home">
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="peDetails$dtls$employerConcernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>


    <CONTAINER
      LABEL="Container.Label.ReferenceNumber"
      WIDTH="5"
    >
      <FIELD WIDTH="5">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="peDetails$dtls$referenceNumber"
          />
        </CONNECT>
        <LINK PAGE_ID="ProspectEmployer_resolveDuplicateRegistrationCheck">
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="peDetails$dtls$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="peDetails$dtls$concernRoleType"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleType"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="peDetails$dtls$altRegistrationIndicator"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="altRegistrationIndicator"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.TradingName"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="peDetails$dtls$tradingName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.RegisteredName"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="peDetails$dtls$registeredName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.AddressLineOne"
      WIDTH="14"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="peDetails$dtls$businessAddress"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.City"
      WIDTH="9"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="peDetails$dtls$businessCity"
        />
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>
