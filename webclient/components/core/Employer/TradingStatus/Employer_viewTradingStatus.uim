<!-- Views the details of an existing trading status of an employer. -->
<PAGE
  PAGE_ID="Employer_viewTradingStatus"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Employer"
    NAME="DISPLAY"
    OPERATION="readTradingStatus"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="tradingStatusID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="tradingStatusID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="readEmployerTradingStatusKey$tradingStatusID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    SHOW_LABELS="true"
    TAB_ORDER="ROW"
  >
    <FIELD LABEL="Field.Label.TradingStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="tradingStatusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="statusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.From">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.To">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
