<!-- This is the home page for the payments in the financial application.   -->
<PAGE
  PAGE_ID="Payments_home"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE DESCRIPTION="Field.StaticText.Welcome">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Home"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.PmntOption"
  >
    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.Label.ReceivePayment">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Financial_selectPaymentReceivedClientType"
        />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.Label.CaptureManualPayment">
        <LINK PAGE_ID="Financial_manualPaymentSearchPersonWizard"/>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


</PAGE>
