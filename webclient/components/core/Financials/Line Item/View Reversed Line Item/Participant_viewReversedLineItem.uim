<!-- This page is used to display the details of a reversed financial      -->
<!-- line item.                                                            -->
<?curam-deprecated Since Curam 6.0. Replaced by Financial_cancelledPaymentInstruction.uim. ?>
<PAGE
  PAGE_ID="Participant_viewReversedLineItem"
  WINDOW_OPTIONS="width=775"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="AllocateButton"
      LABEL="ActionControl.Label.Allocate"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_allocatePaymentReceivedSelectLiability"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="primaryClientID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="finInstructionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="finInstructionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="WriteOffButton"
      LABEL="ActionControl.Label.WriteOff"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_createWriteOff"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="lineItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!-- BEGIN, CR00149932, MC -->
    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >
      <LINK PAGE_ID="Participant_viewReversalInstruction">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="finInstructionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="finInstructionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00149932 -->


  </ACTION_SET>


  <!-- BEGIN, CR00160135, MC -->
  <INCLUDE FILE_NAME="Participant_viewLineItemView.vim"/>
  <!-- END, CR00160135 -->


</PAGE>
