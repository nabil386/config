<!-- This page is used to read a saved payment simulation.                  -->
<PAGE
  PAGE_ID="Financial_readSimulatedPayment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ProductDelivery"
    NAME="DISPLAY"
    OPERATION="readSimulationDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="simulationHeaderID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="pageDescription"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="description"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="simulationHeaderID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="simulationHeaderID"
    />
  </CONNECT>
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <LIST TITLE="List.Title.Payments">
    <FIELD
      LABEL="Field.Label.Component"
      WIDTH="24"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="paymentILIList$dtls$iliDescription"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Nominee"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="paymentILIList$dtls$caseNomineeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CoversPeriod"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="paymentILIList$dtls$coversPeriod"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.Amount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="paymentILIList$dtls$amount"
        />
      </CONNECT>
    </FIELD>
    <FOOTER_ROW>
      <FIELD CONTROL="SKIP"/>


      <FIELD CONTROL="SKIP"/>
      <FIELD
        ALIGNMENT="RIGHT"
        LABEL="Field.Label.TotalEntitlement"
      >
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.Label.TotalEntitlement"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        ALIGNMENT="RIGHT"
        LABEL="Field.Label.TotalEntitlement"
        WIDTH="10"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="totalEntitlement"
          />
        </CONNECT>
      </FIELD>
    </FOOTER_ROW>
  </LIST>
  <LIST TITLE="List.Title.Deductions">
    <FIELD
      LABEL="Field.Label.Component"
      WIDTH="24"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deductionILIList$dtls$iliDescription"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Nominee"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deductionILIList$dtls$caseNomineeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CoversPeriod"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deductionILIList$dtls$coversPeriod"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.Amount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="deductionILIList$dtls$amount"
        />
      </CONNECT>
    </FIELD>
    <FOOTER_ROW>
      <FIELD CONTROL="SKIP"/>


      <FIELD CONTROL="SKIP"/>
      <FIELD
        ALIGNMENT="RIGHT"
        LABEL="Field.Label.TotalDeduction"
      >
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.Label.TotalDeduction"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        ALIGNMENT="RIGHT"
        LABEL="Field.Label.TotalDeduction"
        WIDTH="10"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="totalDeduction"
          />
        </CONNECT>
      </FIELD>
    </FOOTER_ROW>
  </LIST>
  <CLUSTER
    LABEL_WIDTH="60"
    NUM_COLS="2"
    TITLE="Cluster.Title.ExpectedTotal"
  >
    <FIELD CONTROL="SKIP"/>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.TotalPayment"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="totalPayment"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
