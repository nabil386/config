<!-- =========== -->
<!-- This page allows the user to search for the client for whom the benfit -->
<!-- is being created.                                                      -->
<PAGE HIDE_CONDITIONAL_LINKS="false" PAGE_ID="Financial_refundPaymentSearchPerson" WINDOW_OPTIONS="width=700,height=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <SERVER_INTERFACE CLASS="Person" NAME="ACTION" OPERATION="searchPerson" PHASE="ACTION"/>


  <ACTION_SET>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <INCLUDE FILE_NAME="Person_searchCriteriaView.vim"/>


  <CLUSTER>
    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
      <ACTION_CONTROL DEFAULT="true" IMAGE="SearchButton" LABEL="ActionControl.Label.Search" TYPE="SUBMIT">
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="ResetButton" LABEL="ActionControl.Label.Reset">
        <LINK PAGE_ID="Financial_refundPaymentSearchPerson"/>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <LIST PAGINATED="false" TITLE="List.Title.SearchResults">
    <CONTAINER LABEL="Container.Label.Action" WIDTH="10">
      <ACTION_CONTROL LABEL="ActionControl.Label.Select">
        <CONDITION>
          <IS_FALSE NAME="ACTION" PROPERTY="result$personSearchResult$dtlsList$restrictedIndOpt"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="Financial_refundPayments" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="ACTION" PROPERTY="concernRoleID"/>
            <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Label.ConcernRoleName" WIDTH="35">
      <!-- BEGIN, CR00387470, AC-->
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$xmlPersonData"/>
      </CONNECT>
      <!-- END, CR00387470-->
    </FIELD>


    <FIELD LABEL="Field.Title.AddressLineOne" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$formattedAddress"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.DateOfBirth" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$dateOfBirth"/>
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>