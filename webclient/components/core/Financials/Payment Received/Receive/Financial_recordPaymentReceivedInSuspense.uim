<!-- This page allows the user to capture the details of a payment received -->
<!-- that is being recorded in the suspense account.                        -->
<PAGE
  PAGE_ID="Financial_recordPaymentReceivedInSuspense"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Financial"
    NAME="ACTION"
    OPERATION="receivePayment"
    PHASE="ACTION"
  />


  <ACTION_SET>
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="Financial_paymentReceivedDetailsClusterView.vim"/>
  <INCLUDE FILE_NAME="Financial_unknownIssuerDetailsClusterView.vim"/>
</PAGE>
