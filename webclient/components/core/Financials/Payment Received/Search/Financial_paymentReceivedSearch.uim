<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to search for a payment received.            -->
<!-- BEGIN, CR00295793, ELG -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Financial_paymentReceivedSearch"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="Financial"
    NAME="ACTION"
    OPERATION="searchForPaymentReceivedWithIndicators"
    PHASE="ACTION"
  />
  <!-- END, CR00295793 -->


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD
      LABEL="Field.Label.ReferenceNumber"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="alternateID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Amount"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$amount"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ReceivedDateFrom"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$receivedDateFrom"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EffectiveDate"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD
      LABEL="Field.Label.ReceiptMethod"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$receiptMethodCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ReceivedDateTo"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$receivedDateTo"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK PAGE_ID="Financial_paymentReceivedSearch"/>
      </ACTION_CONTROL>
    </ACTION_SET>


  </CLUSTER>


  <LIST TITLE="List.Title.SearchResults">


    <!-- BEGIN, CR00232221, MC -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Financial_viewPaymentReceivedInstruction1">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="finInstructionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="finInstructionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="instructionLineItemType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="instructionLineItemType"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00232221 -->

    <FIELD
      LABEL="Field.Title.IssuerName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$issuerName"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00324347, KH -->
    <FIELD
      LABEL="Field.Title.ClientName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$clientName"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00324347 -->
    <FIELD
      LABEL="Field.Title.ReceiptMethod"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$receiptMethodCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ReceivedDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$receivedDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Title.Amount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$amount"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00324347, KH -->
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Title.UnallocatedAmount"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$unallocatedAmount"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00324347 -->

    <!-- BEGIN, CR00295793, ELG -->
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <!-- BEGIN, CR00324347, KH -->      
      <ACTION_CONTROL LABEL="ActionControl.Label.Allocate">
        <CONDITION>
          <IS_TRUE
            NAME="ACTION"
            PROPERTY="dtls$allocateIndicator"
          />
        </CONDITION>

        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Financial_allocatePaymentReceivedSelectLiability1"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="iliID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="lineItemID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="finInstructionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="finInstructionID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.Refund">
        <CONDITION>
          <IS_TRUE
            NAME="ACTION"
            PROPERTY="dtls$allocateIndicator"
          />
        </CONDITION>

        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Financial_createRefund"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="finInstructionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="finInstructionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="dtls$unallocatedAmount"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="unallocatedAmount"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- END, CR00324347 -->

      <ACTION_CONTROL LABEL="ActionControl.Label.Reverse">
        <!-- BEGIN, CR00295967, ELG -->
        <CONDITION>
          <IS_TRUE
            NAME="ACTION"
            PROPERTY="dtls$reverseIndicator"
          />
        </CONDITION>
        <!-- END, CR00295967 -->

        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Financial_createInstructionReversal"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="finInstructionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="finInstructionID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>

    </ACTION_SET>
    <!-- END, CR00295793 -->


  </LIST>
</PAGE>
