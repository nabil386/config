<!-- This page allows a user to search for an employer as part of the transfer    -->
<!-- payment from suspense account process.                                       -->
<PAGE HIDE_CONDITIONAL_LINKS="false" PAGE_ID="Financial_suspenseTransferSearchEmployer" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00282028, IBM -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <SERVER_INTERFACE CLASS="Employer" NAME="ACTION" OPERATION="searchEmployer" PHASE="ACTION"/>
  <!-- END, CR00282028 -->
  <PAGE_PARAMETER NAME="suspenseAccountID"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER BEHAVIOR="NONE" DESCRIPTION="Cluster.Description">
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>


  <INCLUDE FILE_NAME="Employer_searchCriteriaView.vim"/>


  <CLUSTER>
    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
      <ACTION_CONTROL DEFAULT="true" IMAGE="SearchButton" LABEL="ActionControl.Label.Search" TYPE="SUBMIT">
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="ResetButton" LABEL="ActionControl.Label.Reset">
        <LINK PAGE_ID="Financial_suspenseTransferSearchEmployer" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="suspenseAccountID"/>
            <TARGET NAME="PAGE" PROPERTY="suspenseAccountID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <LIST PAGINATED="false" TITLE="List.Title.SearchResults">


    <DETAILS_ROW>
      <INLINE_PAGE URI_SOURCE_NAME="ACTION" URI_SOURCE_PROPERTY="dtlsList$employerTabDetailsURL"/>
    </DETAILS_ROW>


    <CONTAINER LABEL="Container.Label.Action" WIDTH="10">
      <ACTION_CONTROL LABEL="ActionControl.Label.Select">
        <CONDITION>
          <IS_FALSE NAME="ACTION" PROPERTY="result$dtls$dtlsList$restrictedIndOpt"/>
        </CONDITION>
        <LINK DISMISS_MODAL="false" PAGE_ID="Financial_suspenseTransferToEmployerDetails" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="ACTION" PROPERTY="concernRoleID"/>
            <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="suspenseAccountID"/>
            <TARGET NAME="PAGE" PROPERTY="suspenseAccountID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Title.TradingName" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$tradingName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.RegName" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$xmlRegisteredNameData"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.BusinessAdd1" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="ACTION" PROPERTY="dtlsList$formattedBusinessAddress"/>
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>