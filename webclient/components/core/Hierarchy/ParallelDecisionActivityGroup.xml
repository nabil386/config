  <PAGE_REF
    DEFAULT="true"
    PAGE_ID="ProcessDefinitionTool_viewParallelDecisionActivity"
    TITLE="Page.Title.ParallelDecisionActivity"
  />
  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_viewParallelDecisionActivitySubject"
    TITLE="Page.Title.ParallelDecisionActivitySubject"
