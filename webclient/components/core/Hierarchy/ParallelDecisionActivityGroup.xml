  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_viewParallelDecisionActivityAllocationStrategyRedirect"
    TITLE="Page.Title.ParallelDecisionActivityAllocationStrategy"
