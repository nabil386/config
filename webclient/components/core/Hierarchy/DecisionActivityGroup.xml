<!-- The page hierarchy for the Decision Activity group.                    -->
<PAGE_GROUP
  APPLICATION="Curam"
  GROUP_ID="DecisionActivity"
  TITLE="Group.Title.DecisionActivity"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="CuramUIMSchema"
>


  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="activityID"/>


  <PAGE_REF
    DEFAULT="true"
    PAGE_ID="ProcessDefinitionTool_viewDecisionActivity"
    TITLE="Page.Title.DecisionActivity"
  />


  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivitySubject"
    TITLE="Page.Title.ActivitySubject"
  />


  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_viewDecisionActivityAllocationStrategyRedirect"
    TITLE="Page.Title.AllocationStrategy"
  />


  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewDecisionActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivityRuleAllocationStrategy"
    VISIBLE="false"
  />


  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewDecisionActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivityTargetAllocationStrategy"
    VISIBLE="false"
  />


  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewDecisionActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivityFunctionAllocationStrategy"
    VISIBLE="false"
  />


  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewDecisionActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_editDecisionActivityAllocationStrategy"
    VISIBLE="false"
  />


  <PAGE_GROUP_REF
    FILE_NAME="DecisionActivityActionsGroup.xml"
    VISIBLE="true"
  />


  <PAGE_GROUP_REF
    FILE_NAME="DecisionDeadlineDetailsGroup.xml"
    VISIBLE="true"
  />


  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_viewQuestionRedirect"
    TITLE="Page.Title.Question"
  />


  <PAGE_REF
    DEFAULT="false"
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewQuestionRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivityFreeFormQuestion"
    VISIBLE="false"
  />


  <PAGE_REF
    DEFAULT="false"
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewQuestionRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleDecisionActivityMultipleChoiceQuestion"
    VISIBLE="false"
  />


  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_visualizeProcessFromDecisionActivity"
    TITLE="Page.Title.GraphicalView"
  />


</PAGE_GROUP>
