<!-- The page hierarchy for the manual activity group.                      -->
<PAGE_GROUP
  APPLICATION="Curam"
  GROUP_ID="ManualActivity"
  TITLE="Group.Title.ManualActivity"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="processID"/>
  <PAGE_PARAMETER NAME="processVersionNo"/>
  <PAGE_PARAMETER NAME="activityID"/>


  <PAGE_REF
    DEFAULT="true"
    PAGE_ID="ProcessDefinitionTool_viewManualActivity"
    TITLE="Page.Title.ManualActivity"
  />
  <PAGE_GROUP_REF FILE_NAME="TaskDetailsGroup.xml"/>


  <PAGE_GROUP_REF FILE_NAME="ManualDeadlineDetailsGroup.xml"/>
  <PAGE_REF
    PAGE_ID="ProcessDefinitionTool_viewManualActivityAllocationStrategyRedirect"
    TITLE="Page.Title.ActivityAllocationStrategy"
  />


  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewManualActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleManualActivityRuleAllocationStrategy"
    TITLE="Page.Title.ActivityRuleAllocationStrategy"
    VISIBLE="false"
  />
  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewManualActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleManualActivityTargetAllocationStrategy"
    TITLE="Page.Title.ActivityTargetAllocationStrategy"
    VISIBLE="false"
  />
  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewManualActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_viewSingleManualActivityFunctionAllocationStrategy"
    TITLE="Page.Title.ActivityFunctionAllocationStrategy"
    VISIBLE="false"
  />
  <PAGE_REF
    HIGHLIGHT_PAGE_ID="ProcessDefinitionTool_viewManualActivityAllocationStrategyRedirect"
    PAGE_ID="ProcessDefinitionTool_editManualActivityAllocationStrategy"
    VISIBLE="false"
  />
  <PAGE_REF
    PAGE_ID="ProcessDefinitionTool_listSingleManualActivityBusinessObjectAssociations"
    TITLE="Page.Title.BusinessObjectAssociations"
  />


  <PAGE_REF
    DEFAULT="false"
    PAGE_ID="ProcessDefinitionTool_visualizeProcessFromManualActivity"
    TITLE="Page.Title.GraphicalView"
  />


</PAGE_GROUP>
