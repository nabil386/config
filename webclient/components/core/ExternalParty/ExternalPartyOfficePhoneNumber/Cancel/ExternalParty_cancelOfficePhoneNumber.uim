<!-- This page allows users to view an external party office phone number   -->
<PAGE PAGE_ID="ExternalParty_cancelOfficePhoneNumber" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ExternalParty" NAME="ACTION" OPERATION="cancelOfficePhoneNumber" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="officePhoneNumberID"/>
  <PAGE_PARAMETER NAME="officeName"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="officePhoneNumberID"/>
    <TARGET NAME="ACTION" PROPERTY="officePhoneNumberID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <FIELD LABEL="Field.Label.CancelOfficePhoneNumber">
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelOfficePhoneNumber"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>