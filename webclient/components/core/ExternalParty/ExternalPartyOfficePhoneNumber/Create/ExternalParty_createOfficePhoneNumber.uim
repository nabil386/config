<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to enter phone number details for an         -->
<!-- external party office.                                                 -->
<PAGE PAGE_ID="ExternalParty_createOfficePhoneNumber" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ExternalParty" NAME="ACTION" OPERATION="createOfficePhoneNumber" PHASE="ACTION"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
     
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
     
    </ACTION_CONTROL>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="externalPartyOfficeID"/>
  <PAGE_PARAMETER NAME="officeName"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="externalPartyOfficeID"/>
    <TARGET NAME="ACTION" PROPERTY="externalPartyOfficeID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="35" NUM_COLS="1" TITLE="Cluster.Title.PhoneNumber">


    <CONTAINER LABEL="Field.Label.Phone">


      <FIELD LABEL="Field.Label.CountryCode" WIDTH="40">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneCountryCode"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.AreaCode" WIDTH="40">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneAreaCode"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Phone" WIDTH="40">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="phoneNumber"/>
        </CONNECT>
      </FIELD>
      <FIELD CONTROL="SKIP"/>


    </CONTAINER>


    <FIELD LABEL="Field.Label.Extension" WIDTH="5" WIDTH_UNITS="CHARS">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="phoneExtension"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PhoneType" USE_BLANK="true" USE_DEFAULT="false" WIDTH="50">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="phoneType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Primary">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="primaryPhoneInd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>