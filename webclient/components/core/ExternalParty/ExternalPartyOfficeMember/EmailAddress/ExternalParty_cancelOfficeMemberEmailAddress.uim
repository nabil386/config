<!-- This page allows users to cancel an external party office members      -->
<!-- email address                                                          -->
<PAGE PAGE_ID="ExternalParty_cancelOfficeMemberEmailAddress" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="Participant" NAME="ACTION" OPERATION="cancelEmailAddress" PHASE="ACTION"/>


  <!-- Parameters used to cancel an Email Address -->
  <PAGE_PARAMETER NAME="concernRoleEmailAddressID"/>
  <PAGE_PARAMETER NAME="emailAddressVersionNo"/>
  <PAGE_PARAMETER NAME="concernRoleEmailAddressVersionNo"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <!-- Map page parameters to cancelEmailAddress action-phase bean -->
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleEmailAddressID"/>
    <TARGET NAME="ACTION" PROPERTY="concernRoleEmailAddressID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="emailAddressVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="emailAddressVersionNo"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleEmailAddressVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="concernRoleEmailAddressVersionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelEmailAddress"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>