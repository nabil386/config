<!-- This action allows the user to view an external party office.          -->
<PAGE
  PAGE_ID="ExternalParty_viewExternalPartyOffice"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ExternalParty"
    NAME="DISPLAY"
    OPERATION="readExternalPartyOffice"
  />


  <PAGE_PARAMETER NAME="externalPartyOfficeID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="externalPartyOfficeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="officeKey$externalPartyOfficeID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.OfficeHome"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="ExternalParty_viewExternalPartyOffice"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Addresses"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ExternalParty_listOfficeAddress"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.OfficeMembers"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ExternalParty_listOfficeMember"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.PhoneNumbers"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ExternalParty_listOfficePhoneNumber"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ServiceOffered">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="serviceOffered"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
