<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows users to view a list of all the offices associated    -->
<!-- with the specified external party.     				    -->
<PAGE
  PAGE_ID="ExternalParty_listExternalPartyOffice"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ExternalParty"
    NAME="DISPLAY"
    OPERATION="listExternalPartyOffice"
  />


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="externalPartyKey$concernRoleID"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ExternalParty_createExternalPartyOffice"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>


    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="ExternalParty_viewExternalPartyOffice">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="externalPartyOfficeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="externalPartyOfficeID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ExternalParty_modifyOfficeFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalPartyOfficeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="externalPartyOfficeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.NewAddress">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ExternalParty_createOfficeAddress"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalPartyOfficeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="externalPartyOfficeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.NewMember">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ExternalParty_createOfficeMember"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalPartyOfficeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="externalPartyOfficeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.NewPhone">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ExternalParty_createOfficePhoneNumber"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalPartyOfficeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="externalPartyOfficeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ExternalParty_cancelExternalPartyOffice"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalPartyOfficeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="externalPartyOfficeID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.OfficeName"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="externalPartyOfficeDtls$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Address"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="addressLine1"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.City"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="city"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="externalPartyOfficeDtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
