<!-- This page allows the user to modify note details for an external party -->
<!-- when opened from the note list page                                    -->
<PAGE
  PAGE_ID="ExternalParty_modifyNoteFromList"
  WINDOW_OPTIONS="width=705,height=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="concernRoleID"/>
  
  <SERVER_INTERFACE
    CLASS="Participant"
    NAME="DISPLAY_STATUS"
    OPERATION="readNoteLockedStatus"
  />

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantNoteID"
    />
    <TARGET
      NAME="DISPLAY_STATUS"
      PROPERTY="key$key$participantNoteID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <CONDITION>
        <IS_FALSE NAME="DISPLAY_STATUS" PROPERTY="noteLockedInd" />
      </CONDITION>
      <LINK
        PAGE_ID="ExternalParty_listNote1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK
        PAGE_ID="ExternalParty_listNote1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="Participant_modifyNoteView1.vim"/>


</PAGE>
