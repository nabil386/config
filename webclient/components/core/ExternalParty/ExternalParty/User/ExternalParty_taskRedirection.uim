<!-- This page allows the user to modify a Dependant Data Item record.      -->
<!-- This page is accessed from the external party navigation hierarchy.    -->
<PAGE
  PAGE_ID="ExternalParty_taskRedirection"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- BEGIN, CR00216807 MN -->
  <SERVER_INTERFACE
    CLASS="ExternalParty"
    NAME="DISPLAY"
    OPERATION="getExternalPartyTaskRedirectionHistoryForUser"
    PHASE="DISPLAY"
  />
  <!-- END, CR00216807 -->
  <!-- BEGIN, CR00225492 MN -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ACTION"
    OPERATION="taskRedirectionsForUserDetails"
    PHASE="ACTION"
  />
  <!-- END, CR00225492 -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="userName"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$fromUserName"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="userFullName"/>
  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
    TITLE="Cluster.Title.NewTaskRedirection"
  >
    <CONTAINER LABEL="Container.Label.RedirectToUser">
      <FIELD>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="key$toUserNameInSupervisorGroup"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
    <FIELD
      LABEL="Field.Label.StartDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$startDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$endDateTime"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.ActiveAndPendingRedirections">


    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="10"
    >
      <ACTION_CONTROL LABEL="ActionControl.Label.Clear">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Supervisor_clearTaskRedirection"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activeAndPendingDetails$taskRedirectionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="redirectedTaskID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activeAndPendingDetails$startDateTime"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="startDateTime"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activeAndPendingDetails$endDateTime"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="endDateTime"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activeAndPendingDetails$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNum"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userFullName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userFullName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="userName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Label.ActivePendingRedirectTo">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activeAndPendingDetails$toUserFullName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActivePendingStartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activeAndPendingDetails$startDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActivePendingEndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activeAndPendingDetails$endDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActivePendingStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="activeAndPendingDetails$redirectionStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      IMAGE="SaveButton"
      LABEL="ActionControl.SaveButton.label"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.CancelButton.label"
    />
  </ACTION_SET>


</PAGE>
