<!-- This page allows the user to enter external party registration details. -->
<PAGE
  PAGE_ID="ExternalParty_register"
  WINDOW_OPTIONS="width=975"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="false"
        PAGE_ID="ExternalParty_duplicateRegistrationCheck"
        SAVE_LINK="false"
      />
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="RegisterButton"
      LABEL="ActionControl.Label.Register"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="ExternalParty_registerView.vim"/>


</PAGE>
