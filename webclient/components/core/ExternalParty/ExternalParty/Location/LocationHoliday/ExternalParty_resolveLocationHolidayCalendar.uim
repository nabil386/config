<!-- This script opens the location holiday Calendar page for               -->
<!-- for a particular location.                                             -->
<PAGE
  PAGE_ID="ExternalParty_resolveLocationHolidayCalendar"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
    
      curam.omega3.request.RequestHandler 
       rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String locationID = request.getParameter("locationID");
      String description = request.getParameter("description");
      String locationStructureID = request.getParameter("locationStructureID");
      String concernRoleID = request.getParameter("concernRoleID");
      
       // BEGIN, CR00372377, AC
      String url = context + 
        "ExternalParty_locationHolidayCalendarPage.do?locationID="
        + curam.omega3.request.RequestUtils.escapeURL(locationID) 
          + "&amp;startDate="
            + "&amp;calendarViewType=CVT3"
              + "&amp;description=" + curam.omega3.request.RequestUtils.escapeURL(description)
              + "&amp;concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(concernRoleID)
              +"&amp;locationStructureID=" + curam.omega3.request.RequestUtils.escapeURL(locationStructureID);
     // END, CR00372377
      url += "&amp;" + rh.getSystemParameters();
      response.sendRedirect(response.encodeRedirectURL(url));
    
    
  </JSP_SCRIPTLET>
</PAGE>
