<PAGE
  PAGE_ID="SummaryView_cancelGroup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="groupName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="summaryViewGroupDefinitionID"/>
  <PAGE_PARAMETER NAME="summaryViewDefinitionID"/>
  <PAGE_PARAMETER NAME="groupName"/>
  <PAGE_PARAMETER NAME="productID"/>


  <SERVER_INTERFACE
    CLASS="SummaryViewConfiguration"
    NAME="ACTION"
    OPERATION="removeGroup"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="summaryViewGroupDefinitionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$summaryViewGroupDefinitionID"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        IMAGE="YesButton"
        LABEL="ActionControl.Label.Yes"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="SummaryView_viewConfiguration">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="summaryViewDefinitionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="summaryViewDefinitionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="productID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="productID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="NoButton"
        LABEL="ActionControl.Label.No"
      >                
            </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.delete"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>