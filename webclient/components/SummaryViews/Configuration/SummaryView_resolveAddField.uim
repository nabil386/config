<!-- Resolver to find the request to add fields page     -->
<PAGE
  PAGE_ID="SummaryView_resolveAddField"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
    curam.omega3.request.RequestHandler 
    rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
    String context = request.getContextPath() + "/";
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    
    String summaryViewGroupDefinitionID = request.getParameter("summaryViewGroupDefinitionID"); 
	summaryViewGroupDefinitionID = curam.omega3.request.RequestUtils.escapeURL(summaryViewGroupDefinitionID);	
    String ruleSetID = request.getParameter("ruleSetID");   
	ruleSetID = curam.omega3.request.RequestUtils.escapeURL(ruleSetID);
    String productID = request.getParameter("productID"); 
	productID = curam.omega3.request.RequestUtils.escapeURL(productID);	
    String groupType = request.getParameter("groupType");   
	groupType = curam.omega3.request.RequestUtils.escapeURL(groupType);		
    
    String url = "";
    
    if (groupType.equals("SVGT4001")) {       
    url = context + "SummaryView_addFieldPage.do?summaryViewGroupDefinitionID=" 
    + summaryViewGroupDefinitionID
    + "&amp;ruleSetID=" + ruleSetID
    + "&amp;groupType=" + groupType       
    + "&amp;productID=" + productID;    
    } 
    else if (groupType.equals("SVGT4002")) {
    url = context + "SummaryView_addFieldForListGroupPage.do?summaryViewGroupDefinitionID=" 
    + summaryViewGroupDefinitionID
    + "&amp;ruleSetID=" + ruleSetID 
    + "&amp;groupType=" + groupType      
    + "&amp;productID=" + productID;        
    }
    else if (groupType.equals("SVGT4003")) {
    url = context + "SummaryView_addFieldToLinkedGroupPage.do?summaryViewGroupDefinitionID=" 
    + summaryViewGroupDefinitionID
    + "&amp;ruleSetID=" + ruleSetID 
    + "&amp;groupType=" + groupType       
    + "&amp;productID=" + productID;    
    } 
    url += "&amp;" + rh.getSystemParameters();  
    
    response.sendRedirect(response.encodeRedirectURL(url));   
    
  </JSP_SCRIPTLET>
</PAGE>