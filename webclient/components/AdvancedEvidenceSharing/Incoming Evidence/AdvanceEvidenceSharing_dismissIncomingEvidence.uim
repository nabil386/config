<!-- This page allows a user to dismiss an evidence on the incoming list  -->
<PAGE
  PAGE_ID="AdvanceEvidenceSharing_dismissIncomingEvidence"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="evidenceDescriptorID" />
  
  <SERVER_INTERFACE
    CLASS="IncomingEvidence"
    NAME="ACTION"
    OPERATION="dismiss"
    PHASE="ACTION"
  />
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceDescriptorID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$evidenceDescriptorID"
    />
  </CONNECT>
  
  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.InformText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.ConfirmText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false" >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT">
      <LINK PAGE_ID="AdvancedEvidenceSearch_afterActionResolve" />
    </ACTION_CONTROL>
      
    <ACTION_CONTROL 
      LABEL="ActionControl.Label.No"
      />
  </ACTION_SET>
  
</PAGE>