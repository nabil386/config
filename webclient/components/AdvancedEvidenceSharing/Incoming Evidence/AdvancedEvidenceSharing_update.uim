<PAGE
  PAGE_ID="AdvancedEvidenceSharing_update"
  WINDOW_OPTIONS="width=500"
  POPUP_PAGE="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="existingEvidenceDescriptorIDs" />
  <PAGE_PARAMETER NAME="incomingEvidenceDescriptorIDs" />
  <PAGE_PARAMETER NAME="useExistingInd" />
  <PAGE_PARAMETER NAME="useIncomingInd" />
  
  <SERVER_INTERFACE
    CLASS="IncomingEvidence"
    NAME="ACTION"
    OPERATION="updateBusinessObject"
    PHASE="ACTION"
  />
  
  <SERVER_INTERFACE
    CLASS="IncomingEvidence"
    NAME="DISPLAY"
    OPERATION="getMessagesForDisplay"
    PHASE="DISPLAY"
  />
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="incomingEvidenceDescriptorIDs"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="incomingEvidenceDescriptorIDs"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="existingEvidenceDescriptorIDs"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="existingEvidenceDescriptorIDs"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="incomingEvidenceDescriptorIDs"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="incomingEvidenceDescriptorIDs"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="existingEvidenceDescriptorIDs"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="existingEvidenceDescriptorIDs"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="useExistingInd"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="useExistingInd"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="useIncomingInd"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="useIncomingInd"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="useExistingInd"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="useExistingInd"
    />
  </CONNECT>

  <LIST>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="confirmationMessage"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  
  <ACTION_SET TOP="false" ALIGNMENT="RIGHT">
  
    <ACTION_CONTROL
      LABEL="ActionControl.Label.OK"
      TYPE="SUBMIT">
      <LINK PAGE_ID="AdvancedEvidenceSearch_afterActionResolve" />
    </ACTION_CONTROL>
     
     <ACTION_CONTROL 
      LABEL="ActionControl.Label.Cancel" 
      TYPE="DISMISS" />

  </ACTION_SET>
    
</PAGE>