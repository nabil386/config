<!-- This page adds Identical shared evidence configurations for shared configuration -->
<PAGE PAGE_ID="AES_addIdenticalEvidence" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="EvidenceSharingAdmin" NAME="DISPLAY" OPERATION="listIdenticalEvidenceTypesAvailableForSharing" PHASE="DISPLAY"/>

  <SERVER_INTERFACE CLASS="EvidenceSharingAdmin" NAME="DISPLAY1" OPERATION="getSourceAndTargetDesc" PHASE="DISPLAY"/>

  <SERVER_INTERFACE CLASS="EvidenceSharingAdmin" NAME="ACTION" OPERATION="createIdenticalEvidenceSharingConfig" PHASE="ACTION"/>

  <PAGE_PARAMETER NAME="evidenceBrokerConfigID"/>
  <PAGE_PARAMETER NAME="sourceID"/>
  <PAGE_PARAMETER NAME="sourceType"/>
  <PAGE_PARAMETER NAME="sourceName"/>
  <PAGE_PARAMETER NAME="targetID"/>
  <PAGE_PARAMETER NAME="targetType"/>
  <PAGE_PARAMETER NAME="targetName"/>

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceID"/>
    <TARGET NAME="ACTION" PROPERTY="sourceID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceType"/>
    <TARGET NAME="ACTION" PROPERTY="sourceType"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetID"/>
    <TARGET NAME="ACTION" PROPERTY="targetID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetType"/>
    <TARGET NAME="ACTION" PROPERTY="targetType"/>
  </CONNECT>

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceID"/>
    <TARGET NAME="DISPLAY" PROPERTY="sourceID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceType"/>
    <TARGET NAME="DISPLAY" PROPERTY="sourceType"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetID"/>
    <TARGET NAME="DISPLAY" PROPERTY="targetID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetType"/>
    <TARGET NAME="DISPLAY" PROPERTY="targetType"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceName"/>
    <TARGET NAME="DISPLAY1" PROPERTY="sourcePartyName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetName"/>
    <TARGET NAME="DISPLAY1" PROPERTY="targetPartyName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="sourceType"/>
    <TARGET NAME="DISPLAY1" PROPERTY="sourceCaseType"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetType"/>
    <TARGET NAME="DISPLAY1" PROPERTY="targetCaseType"/>
  </CONNECT>

  <CLUSTER LABEL_WIDTH="35" NUM_COLS="2" TITLE="Cluster.Title.SharingSettings">

   <FIELD LABEL="Field.Label.Source">
      <CONNECT>
        <SOURCE NAME="DISPLAY1" PROPERTY="sourceName"/>
      </CONNECT>
   </FIELD>

    <FIELD LABEL="Field.Label.ShareVerifications">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="shareVerificationsOption"/>
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.Target">
      <CONNECT>
        <SOURCE NAME="DISPLAY1" PROPERTY="targetName"/>
      </CONNECT>
    </FIELD>

      <FIELD LABEL="Field.Label.TrustedSource">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="trustedSource"/>
      </CONNECT>
    </FIELD>
    
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <LIST TITLE="List.Label.EvidenceSelection">


      <CONTAINER ALIGNMENT="CENTER" LABEL="Container.Label.Action" WIDTH="7">


        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="result$listDetails$evidenceType"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="result$listDetails$evidenceDescription"/>
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="evidenceList"/>
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD LABEL="Field.Label.EvidenceType">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$listDetails$evidenceDescription"/>
        </CONNECT>
      </FIELD>
    </LIST>


  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="AES_listEvidenceSharingConfig" SAVE_LINK="false">
        </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
      <LINK PAGE_ID="AES_listEvidenceSharingConfig" SAVE_LINK="false">
        </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  


</PAGE>