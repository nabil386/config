<PAGE
  PAGE_ID="AES_sharedInstancesGraph"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

 <SERVER_INTERFACE
    CLASS="SearchSharedInstances"
    NAME="DISPLAY"
    OPERATION="getAESGraphData"
    PHASE="DISPLAY"
  />
  
  <PAGE_PARAMETER NAME="shareSetID"/>
  
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.GraphView"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="AES_sharedInstancesGraph">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="shareSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="shareSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
	<ACTION_CONTROL
      LABEL="ActionControl.Label.ListView"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="AES_sharedInstancesListView">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="shareSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="shareSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="shareSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$shareSetID"
    />
  </CONNECT>
   
	<FIELD ALIGNMENT="CENTER">
		<CONNECT>
			<SOURCE
				NAME="DISPLAY"
				PROPERTY="result$jsonData"
			/>
		</CONNECT>
	</FIELD>
</PAGE>
