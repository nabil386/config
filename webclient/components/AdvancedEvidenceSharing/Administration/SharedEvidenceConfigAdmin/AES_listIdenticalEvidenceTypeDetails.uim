<!-- Displays evidence sharing configuration details for identical evidence -->
<!-- sharing.                                                               -->
<PAGE
  PAGE_ID="AES_listIdenticalEvidenceTypeDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="EvidenceSharingAdmin"
    NAME="DISPLAY"
    OPERATION="listIdenticalEvidenceTypeDetails"
    PHASE="DISPLAY"
  />
  
  <PAGE_PARAMETER NAME="evidenceBrokerConfigID"/>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceBrokerConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$evidenceBrokerConfigID"
    />
  </CONNECT>
 
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Identical"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="AES_listIdenticalEvidenceTypeDetails">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceBrokerConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceBrokerConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
	<ACTION_CONTROL
      LABEL="ActionControl.Label.LogicallyEquivalent"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="AES_listLogicallyEquivalentEvidenceTypes">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceBrokerConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceBrokerConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
   
 	<CLUSTER>
	  <DESCRIPTION>
	  	<CONNECT>
	  		<SOURCE
	  			NAME="TEXT"
	  			PROPERTY="Cluster.Description.Identical"
	  		/>
	  	</CONNECT>
	  </DESCRIPTION>
	  
	  <LIST>
	    <ACTION_SET TYPE="LIST_ROW_MENU">
	    	<ACTION_CONTROL
		        LABEL="ActionControl.Label.Edit"
		        TYPE="ACTION"
		      >
		        <LINK
		          OPEN_MODAL="true"
		          PAGE_ID="AES_modifyIdenticalEvidenceConfig"
		        >
		          	<CONNECT>
		              <SOURCE
		                NAME="DISPLAY"
		                PROPERTY="result$dtls$details$sharingConfigDetails$evidenceBrokerConfigID"
		              />
		              <TARGET
		                NAME="PAGE"
		                PROPERTY="evidenceBrokerConfigID"
		            />
		          	</CONNECT>
		          	<CONNECT>
					  <SOURCE
					    NAME="DISPLAY"
					    PROPERTY="sourceType"
					  />
					  <TARGET
					    NAME="PAGE"
					    PROPERTY="sourceType"
					  />
					</CONNECT>
					<CONNECT>
					  <SOURCE
					    NAME="DISPLAY"
					    PROPERTY="sourceName"
					  />
					  <TARGET
					    NAME="PAGE"
					    PROPERTY="sourceName"
					  />
					</CONNECT>
					<CONNECT>
					  <SOURCE
					    NAME="DISPLAY"
					    PROPERTY="targetType"
					  />
					  <TARGET
					    NAME="PAGE"
					    PROPERTY="targetType"
					  />
					</CONNECT>
					<CONNECT>
					  <SOURCE
					    NAME="DISPLAY"
					    PROPERTY="targetName"
					  />
					  <TARGET
					    NAME="PAGE"
					    PROPERTY="targetName"
					  />
					</CONNECT>
		        </LINK>
			</ACTION_CONTROL>	
	    	<ACTION_CONTROL
		        LABEL="ActionControl.Label.Delete"
		        TYPE="ACTION"
		      >
		        <LINK
		          OPEN_MODAL="true"
		          PAGE_ID="AES_cancelIdenticalSharedEvidenceConfig"
		        >
		          <CONNECT>
		            <SOURCE
		              NAME="DISPLAY"
		              PROPERTY="result$dtls$details$sharingConfigDetails$evidenceBrokerConfigID"
		            />
		            <TARGET
		              NAME="PAGE"
		              PROPERTY="evidenceBrokerConfigID"
		            />
		          </CONNECT>
		          <CONNECT>
		            <SOURCE
		              NAME="DISPLAY"
		              PROPERTY="versionNo"
		            />
		            <TARGET
		              NAME="PAGE"
		              PROPERTY="versionNo"
		            />
		          </CONNECT>
		        </LINK>
			</ACTION_CONTROL>
		</ACTION_SET>
	 
		<FIELD LABEL="Field.Label.EvidenceType">
	      <CONNECT>
	        <SOURCE
	          NAME="DISPLAY"
	          PROPERTY="result$dtls$details$sharingConfigDetails$sourceEvidenceType"
	        />
	      </CONNECT>
	    </FIELD>
	
	    <FIELD LABEL="Field.Label.TrustedSource">
	      <CONNECT>
	        <SOURCE
	          NAME="DISPLAY"
	          PROPERTY="result$dtls$details$sharingConfigDetails$trustedSource"
	        />
	      </CONNECT>
	    </FIELD>
	    
	    <FIELD LABEL="Field.Label.ShareVerifications">
	      <CONNECT>
	        <SOURCE
	          NAME="DISPLAY"
	          PROPERTY="result$dtls$details$sharingConfigDetails$shareVerificationsOption"
	        />
	      </CONNECT>
	    </FIELD>
	    
	  </LIST>
	</CLUSTER>
</PAGE>