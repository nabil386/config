<!-- This page lists all the active rule sets in the system                 -->
<PAGE
  PAGE_ID="System_datastoreEditorDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <SERVER_INTERFACE
    CLASS="DatastoreSchema"
    NAME="DISPLAY"
    OPERATION="viewSchemaDetails"
    PHASE="DISPLAY"
  />

  <PAGE_PARAMETER NAME="schemaName"/>

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="schemaName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$schemaName"
    />
  </CONNECT> 

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.EmptyTitle"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$schemaDisplayName"
      />
    </CONNECT>
  </TAB_NAME>


</PAGE>
