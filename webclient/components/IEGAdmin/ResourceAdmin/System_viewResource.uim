<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="System_viewResource" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
    <SERVER_INTERFACE CLASS="ResourceAdmin" NAME="DISPLAY" OPERATION="readResource" />
    <PAGE_PARAMETER NAME="resourceID" />
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="resourceID" />
        <TARGET NAME="DISPLAY" PROPERTY="key$resourceID" />
    </CONNECT>
    <CLUSTER NUM_COLS="2" SHOW_LABELS="true">
        <FIELD LABEL="Field.Title.Name">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="name" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ContentType">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="contentType" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ContentDisposition">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="contentDisposition" />
            </CONNECT>
        </FIELD>
        <WIDGET LABEL="Field.Title.Content" TYPE="FILE_DOWNLOAD">
          <WIDGET_PARAMETER NAME="LINK_TEXT">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="name" />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="PARAMS">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="name" />
              <TARGET NAME="PAGE" PROPERTY="name" />
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="locale" />
              <TARGET NAME="PAGE" PROPERTY="locale" />
            </CONNECT>            
          </WIDGET_PARAMETER>
        </WIDGET>        
        <FIELD LABEL="Field.Title.Locale">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="locale" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.Category">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="category" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.Internal">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="internal" />
            </CONNECT>
        </FIELD>  		      
    </CLUSTER>
    
    <CLUSTER SHOW_LABELS="false" TITLE="Field.Title.Description">
        <FIELD HEIGHT="5">
            <CONNECT>
                <SOURCE
                    NAME="DISPLAY"
                    PROPERTY="description"
                />
            </CONNECT>
        </FIELD>
    </CLUSTER>    
</PAGE>
