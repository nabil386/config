<APP_CONFIG>
    
  <FILE_DOWNLOAD_CONFIG>

    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IEGScriptAdminPkg.IEGScriptAdmin_exportScriptForDownload_TH"
      PAGE_ID="IEG2_listAllIEG2Scripts"
    >
      <INPUT
        PAGE_PARAM="scriptID"
        PROPERTY="key$scriptID"
      />
      <INPUT
        PAGE_PARAM="scriptType"
        PROPERTY="key$scriptType"
      />
      <INPUT
        PAGE_PARAM="scriptVersion"
        PROPERTY="key$scriptVersion"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>
    
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IEGScriptAdminPkg.IEGScriptAdmin_exportScript_TH"
      PAGE_ID="CitizenWorkspaceAdmin_listAllIEG2Scripts"
    >
      <INPUT
        PAGE_PARAM="scriptID"
        PROPERTY="key$scriptID"
      />
      <INPUT
        PAGE_PARAM="scriptType"
        PROPERTY="key$scriptType"
      />
      <INPUT
        PAGE_PARAM="scriptVersion"
        PROPERTY="key$scriptVersion"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
	  <CONTENT_TYPE PROPERTY="result$contentType"/>
    </FILE_DOWNLOAD>

    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ResourceAdminPkg.ResourceAdmin_exportResource_TH"
      PAGE_ID="IEG2_listAllResourcesByCategory"
    >
      <INPUT
        PAGE_PARAM="name"
        PROPERTY="key$name"
      />
      <INPUT
        PAGE_PARAM="locale"
        PROPERTY="key$locale"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>
    
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ResourceAdminPkg.ResourceAdmin_exportResource_TH"
      PAGE_ID="CitizenWorkspaceAdmin_listAllResourcesByCategory"
    >
      <INPUT
        PAGE_PARAM="name"
        PROPERTY="key$name"
      />
      <INPUT
        PAGE_PARAM="locale"
        PROPERTY="key$locale"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>

    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ResourceAdminPkg.ResourceAdmin_exportResource_TH"
      PAGE_ID="System_viewResource"
    >
      <INPUT
        PAGE_PARAM="name"
        PROPERTY="key$name"
      />
      <INPUT
        PAGE_PARAM="locale"
        PROPERTY="key$locale"
      />
      <FILE_DATA PROPERTY="result$content"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>    
  
  </FILE_DOWNLOAD_CONFIG>
    
</APP_CONFIG>
