<PAGE PAGE_ID="CitizenWorkspace_viewStatusOfEvidenceOnVerificationsTask" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" SCRIPT_FILE="EvidenceStatusForVerificationsTask.js">

    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
    
    <PAGE_PARAMETER NAME="caseID"/>
    <PAGE_PARAMETER NAME="participantID"/>
    <PAGE_PARAMETER NAME="isCaseEvidence"/>


   <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="result$dtls$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participantID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="isCaseEvidence"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="isCaseEvidence"
    />
  </CONNECT>
    
    
     <SERVER_INTERFACE
        CLASS="EvidenceStatusForVerificationsTask"
        NAME="DISPLAY"
        OPERATION="listEvidencesOnTask"
        PHASE="DISPLAY"
    />
    
    <LIST>
       <FIELD LABEL="Field.Label.VerificatioinStatus">
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="verificationReadyStatus"
           />
        </CONNECT>
       </FIELD>
       <!--CONTAINER LABEL="Field.Label.EvidenceID" STYLE="container-evidence-id">
         <FIELD>
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="evidenceID"
           />
        </CONNECT>
       </FIELD>
       </CONTAINER-->
        <FIELD LABEL="Field.Label.CaseMember">
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseMember"
           />
        </CONNECT>
        <LINK PAGE_ID="VerificationApplication_PersonVerificationList">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
       </LINK>
       </FIELD>
       <FIELD
      LABEL="Field.Title.CaseReference"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="CitizenWorkspace_listVerificationsForCaseworkerTask">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
       <FIELD LABEL="Field.Label.EvidenceType">
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="evidenceType"
           />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.VerifiableDataItemName">
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="verifiableDataItemName"
           />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.SubmittedDocuments">
         <CONNECT>
           <SOURCE
              NAME="DISPLAY"
              PROPERTY="verificationItem"
           />
        </CONNECT>
      </FIELD>
    </LIST>
</PAGE>