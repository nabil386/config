<PAGE PAGE_ID="CitizenWorkspaceAdmin_listResultsPageActions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="ResultsPageActionAdmin" NAME="DISPLAY" OPERATION="getActiveResultsPageActions"/>

  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.CreateNew">
      <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_createResultsPageAction"/>
    </ACTION_CONTROL>
  </ACTION_SET>

  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_editResultsPageAction">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="resultsPageActionID"/>
            <TARGET NAME="PAGE" PROPERTY="resultsPageActionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_deleteResultsPageAction">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="resultsPageActionID"/>
            <TARGET NAME="PAGE" PROPERTY="resultsPageActionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>

    <FIELD LABEL="Field.Label.Name" WIDTH="15">
     <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.State" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="state"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="type"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Uri" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="uri"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ParameterReference" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="parameterReference"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
    </FIELD>

  </LIST>
</PAGE>
