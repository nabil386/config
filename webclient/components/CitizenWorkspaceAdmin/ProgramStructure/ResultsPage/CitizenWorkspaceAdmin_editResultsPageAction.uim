<PAGE PAGE_ID="CitizenWorkspaceAdmin_editResultsPageAction" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=800,height=310">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="resultsPageActionID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  
  <SERVER_INTERFACE PHASE="DISPLAY" CLASS="ResultsPageActionAdmin" NAME="GET_DETAILS" OPERATION="getResultsPageActionData"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="resultsPageActionID"/>
    <TARGET NAME="GET_DETAILS" PROPERTY="key$resultsPageActionID"/>
  </CONNECT>

  <SERVER_INTERFACE PHASE="ACTION" CLASS="ResultsPageActionAdmin" NAME="MODIFY_DETAILS" OPERATION="modifyResultsPageAction"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="resultsPageActionID"/>
    <TARGET NAME="MODIFY_DETAILS" PROPERTY="resultsPageActionID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="MODIFY_DETAILS" PROPERTY="versionNo"/>
  </CONNECT>

  <CLUSTER LABEL_WIDTH="50" NUM_COLS="1">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.State" USE_BLANK="true">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="stateList$state" HIDDEN_PROPERTY="stateList$state"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$dtls$state"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="state"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="type"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="type"/>
      </CONNECT>
    </FIELD>    
    <FIELD LABEL="Field.Label.Uri">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="uri"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="uri"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ParameterReference" USE_BLANK="true">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="parameterReferenceName" HIDDEN_PROPERTY="parameterReferenceList$parameterReference"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="dtls$parameterReference"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="parameterReference"/>
      </CONNECT>
    </FIELD>

  </CLUSTER>

  <CLUSTER TITLE="Field.Label.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="9">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET >
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
</PAGE>
