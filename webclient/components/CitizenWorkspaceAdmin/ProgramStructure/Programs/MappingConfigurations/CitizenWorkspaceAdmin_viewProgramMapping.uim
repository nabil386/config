<PAGE PAGE_ID="CitizenWorkspaceAdmin_viewProgramMapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
      <CONNECT> 
          <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$name" />
      </CONNECT>      
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="MappingConfiguration" NAME="DISPLAY" OPERATION="readProgramMapping" PHASE="DISPLAY"/>
  <PAGE_PARAMETER NAME="mappingConfigurationID" />  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="mappingConfigurationID" />
    <TARGET NAME="DISPLAY" PROPERTY="key$mappingConfigurationID" />
  </CONNECT>    
  
  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$name" />
      </CONNECT>
    </FIELD>
      <CONTAINER LABEL="Field.Title.ConfigXML">
        <WIDGET TYPE="FILE_DOWNLOAD">
          <WIDGET_PARAMETER NAME="LINK_TEXT">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="mappingConfigurationXML" />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="PARAMS">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="mappingConfigurationXML" />
              <TARGET NAME="PAGE" PROPERTY="name" />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="mappingConfigurationType" />
      </CONNECT>
    </FIELD>
    <CONTAINER LABEL="Field.Title.MappingXML">
        <WIDGET TYPE="FILE_DOWNLOAD">
          <WIDGET_PARAMETER NAME="LINK_TEXT">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="mappingXML" />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="PARAMS">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="mappingXML" />
              <TARGET NAME="PAGE" PROPERTY="name" />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
    </CONTAINER>   
    <!--<FIELD LABEL="Field.Title.MappingXML">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="mappingXML" />
      </CONNECT>
    </FIELD>-->
  </CLUSTER>
  <CLUSTER TITLE="Cluster.Title.Description" SHOW_LABELS="false">
    <FIELD >
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>