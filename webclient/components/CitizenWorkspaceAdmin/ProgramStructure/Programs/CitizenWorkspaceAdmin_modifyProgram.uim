<PAGE PAGE_ID="CitizenWorkspaceAdmin_modifyProgram"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" WINDOW_OPTIONS="width=1075,height=680">
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
        </CONNECT>
    </PAGE_TITLE>
    <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="DISPLAY" OPERATION="viewProgram"/>
    <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="ACTION" OPERATION="modifyProgram" PHASE="ACTION"/>
    <PAGE_PARAMETER NAME="programTypeID"/>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="programTypeID"/>
        <TARGET NAME="DISPLAY" PROPERTY="key$programTypeID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="programTypeID"/>
        <TARGET NAME="ACTION" PROPERTY="programTypeID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="details$dtls$versionNo"/>
        <TARGET NAME="ACTION" PROPERTY="versionNo"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$nameTextID"/>
        <TARGET NAME="ACTION" PROPERTY="nameTextID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="summaryTextID"/>
        <TARGET NAME="ACTION" PROPERTY="summaryTextID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="details$dtls$descriptionTextID"/>
        <TARGET NAME="ACTION" PROPERTY="descriptionTextID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$pdfFormProgramLinkVersionNo"/>
        <TARGET NAME="ACTION" PROPERTY="details$pdfFormProgramLinkVersionNo"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="url"/>
        <TARGET NAME="ACTION" PROPERTY="url"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="multipleAppIndicator"/>
        <TARGET NAME="ACTION" PROPERTY="multipleAppIndicator"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$pdfFormID"/>
        <TARGET NAME="ACTION" PROPERTY="details$pdfFormID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="summary"/>
        <TARGET NAME="ACTION" PROPERTY="summary"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="details$description"/>
        <TARGET NAME="ACTION" PROPERTY="description"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="localOfficeInd"/>
        <TARGET NAME="ACTION" PROPERTY="localOfficeInd"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="localOfficeDescription"/>
        <TARGET NAME="ACTION" PROPERTY="localOfficeDescription"/>
    </CONNECT>
    
    <ACTION_SET>
        <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
        <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
    </ACTION_SET>
    
    <!-- Details -->
    <CLUSTER NUM_COLS="2" LABEL_WIDTH="50">
        <FIELD LABEL="Field.Title.Name">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="details$name"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="name"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.Reference">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="programTypeReference"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="programTypeReference"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>
    
    <!-- Intake Processing Details -->
    <CLUSTER TITLE="Cluster.Title.IntakeProcessingDetails" NUM_COLS="2" LABEL_WIDTH="50">
        <!-- Col 1 -->
        <FIELD USE_BLANK="true" LABEL="Field.Title.IntakeSystem">
            <CONNECT>
                <INITIAL NAME="DISPLAY" 
                    HIDDEN_PROPERTY="systemID"
                    PROPERTY="systemName"/>
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="intakeSystemID"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="intakeSystemID"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ReopenAvailable">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="reopenAvailableInd"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="reopenAvailableInd"/>
            </CONNECT>
        </FIELD>
        <FIELD USE_BLANK="true" LABEL="Field.Title.CoverageType">
            <CONNECT>
                <INITIAL NAME="DISPLAY" 
                    PROPERTY="coverageType"/>
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="coverageType"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="coverageType"/>
            </CONNECT>
        </FIELD>
        <!-- Col 2 -->
        <FIELD USE_BLANK="true" LABEL="Field.Title.ApplicationCase">
            <CONNECT>
                <INITIAL NAME="DISPLAY" 
                    HIDDEN_PROPERTY="caseConfigurationList$caseConfigurationID"
                    PROPERTY="caseConfigurationList$caseConfigurationName"/>
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="dtls$caseConfigurationID"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="caseConfigurationID"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ProgramReopened">
            <CONNECT>
                <INITIAL NAME="DISPLAY" PROPERTY="programReopenedWorkflow" />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="programReopenedWorkflow"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="programReopenedWorkflow"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>
    
    <!-- Case Processing System -->
    <CLUSTER TITLE="Cluster.Title.CaseProcessingDetails" NUM_COLS="2" LABEL_WIDTH="65">
        <!-- Col 1 -->
        <FIELD LABEL="Field.Title.CaseProcessingSystem" USE_BLANK="true">
            <CONNECT>
                <INITIAL NAME="DISPLAY" 
                    HIDDEN_PROPERTY="systemID"
                    PROPERTY="systemName"/>
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="details$targetSystemID"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="details$targetSystemID"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.IntegratedCaseStrategy" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="integratedCaseStrategy"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="integratedCaseStrategy"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ClientSelectionRuleset" USE_BLANK="true">
            <CONNECT>
                <INITIAL
                    HIDDEN_PROPERTY="result$initialDtls$ruleSetList$ruleSetID"
                    NAME="DISPLAY"
                    PROPERTY="result$initialDtls$ruleSetList$name" 
                />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="clientSelectionRulesetID"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="clientSelectionRulesetID"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.ProductDeliveryType" USE_BLANK="true">
            <CONNECT>
                <INITIAL
                    NAME="DISPLAY"
                    HIDDEN_PROPERTY="productList$productID"
                    PROPERTY="productList$name" 
                />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="dtls$productID"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="productID"/>
            </CONNECT>
        </FIELD>
        <!-- Col 2 -->
        <FIELD LABEL="Field.Title.CuramCaseType" USE_BLANK="true">
            <CONNECT>
                <INITIAL
                    HIDDEN_PROPERTY="listAdminIntegratedCaseDetails$dtls$integratedCaseType"
                    NAME="DISPLAY"
                    PROPERTY="listAdminIntegratedCaseDetails$dtls$integratedCaseType" 
                />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="details$dtls$integratedCaseType"/>
            </CONNECT>
            <CONNECT>
                <TARGET
                    NAME="ACTION"
                    PROPERTY="integratedCaseType"
                />
            </CONNECT>
        </FIELD>  
        <FIELD LABEL="Field.Title.ClientSelectionStrategy" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="clientSelectionStrategy"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="clientSelectionStrategy"/>
            </CONNECT>
        </FIELD>
        
        <FIELD LABEL="Field.Title.SubmitProductDelivery">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="submitProductDeliveryInd"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="submitProductDeliveryInd"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>
       <!-- Outcome Plan Configuration -->
    <CLUSTER TITLE="Cluster.Title.OutcomePlanDetails" NUM_COLS="2" LABEL_WIDTH="65">
    	<FIELD LABEL="Field.Title.OutcomePlanType" USE_BLANK="true">
    	    <CONNECT>
                <INITIAL
                    HIDDEN_PROPERTY="result$initialDtls$outcomePlanTypes$dtls$outcomePlanAdminID"
                    NAME="DISPLAY"
                    PROPERTY="result$initialDtls$outcomePlanTypes$dtls$outcomePlanName" 
                />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$outcomePlanType"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="outcomePlanType" />                
            </CONNECT>
        </FIELD> 
    	<FIELD LABEL="Field.Title.ClientSelectionOPStrategy" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="clientSelectionOPStrategy"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="clientSelectionOPStrategy"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.OutcomePlanStrategy" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="outcomePlanStrategy"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="outcomePlanStrategy"/>
            </CONNECT>
        </FIELD>
    	<FIELD LABEL="Field.Title.ClientSelectionOPRuleset" USE_BLANK="true">
    	    <CONNECT>
                <INITIAL
                    HIDDEN_PROPERTY="result$initialDtls$ruleSetTypes$ruleSetID"
                    NAME="DISPLAY"
                    PROPERTY="result$initialDtls$ruleSetTypes$name" 
                />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="clientSelectionOPRuleset"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="clientSelectionOPRuleset"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>
    
    <CLUSTER TITLE="Cluster.Title.Timer" NUM_COLS="2" LABEL_WIDTH="65">
      <!-- Col 1 -->
      <FIELD LABEL="Field.Title.DispositionPeriod" WIDTH_UNITS="CHARS" WIDTH="4">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="numDispositionDays"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="numDispositionDays"/>
            </CONNECT>
        </FIELD>
      <FIELD LABEL="Field.Title.WarningDays" WIDTH_UNITS="CHARS" WIDTH="4">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="numWarningDays"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="numWarningDays"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.ExtensionApproval">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="timerApprovalRequiredInd"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="timerApprovalRequiredInd"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.ResumeTimer">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="resumeTimerInd"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="resumeTimerInd"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.TimerStart">
            <CONNECT>
                <INITIAL NAME="DISPLAY" PROPERTY="timerStartWorkflow" />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="timerStartWorkflow"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="timerStartWorkflow"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.DeadlineNotAchieved">
            <CONNECT>
                <INITIAL NAME="DISPLAY" PROPERTY="deadlineNotAchievedWorkflow" />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="deadlineNotAchievedWorkflow"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="deadlineNotAchievedWorkflow"/>
          </CONNECT>
      </FIELD>
      <!-- Col 2 -->
      <FIELD LABEL="Field.Title.StartOn" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="timerStartOn"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="timerStartOn"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.ExtensionAllowed">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="timerExtensionAllowedInd"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="timerExtensionAllowedInd"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.UseBusinessDays">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="useBusinessDaysInd"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="useBusinessDaysInd"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.ResumeFrom" USE_BLANK="true">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="timerResumeFrom"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="timerResumeFrom"/>
          </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Title.WarningReached">
            <CONNECT>
                <INITIAL NAME="DISPLAY" PROPERTY="warningReachedWorkflow" />
            </CONNECT>
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="warningReachedWorkflow"/>
            </CONNECT>
          <CONNECT>
              <TARGET NAME="ACTION" PROPERTY="warningReachedWorkflow"/>
          </CONNECT>
      </FIELD>
    </CLUSTER>
    
    <!-- Description -->
    <CLUSTER TITLE="Cluster.Title.Description" SHOW_LABELS="false">
        <FIELD HEIGHT="100">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="internalDescription"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="internalDescription"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>
    
</PAGE>
