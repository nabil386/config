<PAGE PAGE_ID="CitizenWorkspaceAdmin_listProgramsPopup" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  POPUP_PAGE="true" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="DISPLAY" OPERATION="listPrograms" />
  <LIST TITLE="List.Title.Programs">
    <CONTAINER LABEL="Container.Label.Action" WIDTH="20">
      <ACTION_CONTROL LABEL="ActionControl.Label.Select" TYPE="DISMISS">
        <LINK>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="programTypeID" />
            <TARGET NAME="PAGE" PROPERTY="value" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="name" />
            <TARGET NAME="PAGE" PROPERTY="description" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Title.Name" WIDTH="80">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name" />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>

