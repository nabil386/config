<PAGE PAGE_ID="CitizenWorkspaceAdmin_addLocalOffice" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
   WINDOW_OPTIONS="width=550">
    <PAGE_TITLE>
        <CONNECT> 
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ProgramOfficeServiceAreaLink" NAME="DISPLAY" OPERATION="listAddLocalOfficeDetails" />
  <SERVER_INTERFACE CLASS="ProgramOfficeServiceAreaLink" NAME="ACTION" OPERATION="createProgramLinkDetails" PHASE="ACTION" />
  <PAGE_PARAMETER NAME="programTypeID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="programTypeID" />
    <TARGET NAME="DISPLAY" PROPERTY="programTypeID" />
  </CONNECT>    
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="programTypeID" />
    <TARGET NAME="ACTION" PROPERTY="programTypeID" />
  </CONNECT>    

  <ACTION_SET   >
    <ACTION_CONTROL
        IMAGE="SaveButton"
        LABEL="ActionControl.Label.Save"
        TYPE="SUBMIT"
      >
    </ACTION_CONTROL>
    <ACTION_CONTROL
        IMAGE="CancelButton"
        LABEL="ActionControl.Label.Cancel"
      >
    </ACTION_CONTROL>
  </ACTION_SET>

   <CLUSTER LABEL_WIDTH="45">
        <FIELD LABEL="Field.Title.LocalOffice">
            <CONNECT>
                <INITIAL NAME="DISPLAY" PROPERTY="orgPublicOfficeList$dtls$name" HIDDEN_PROPERTY="locationID" />
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="locationID" />
            </CONNECT>                
        </FIELD>   
   </CLUSTER>      
       
  <LIST TITLE="List.ServiceAreas" >


    <CONTAINER>


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="serviceAreaID" />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="serviceAreaTabList" />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>

    <FIELD LABEL="List.Title.Name" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dtlsList$dtls$name" />
      </CONNECT>
    </FIELD>

  </LIST>  
      
      
  

</PAGE>
