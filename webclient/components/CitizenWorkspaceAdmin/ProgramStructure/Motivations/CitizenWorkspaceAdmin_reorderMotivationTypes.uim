<PAGE PAGE_ID="CitizenWorkspaceAdmin_reorderMotivationTypes" SCRIPT_FILE="LifeEventReorder.js"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
 WINDOW_OPTIONS="width=550">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="MotivationTypeAdmin" NAME="DISPLAY" OPERATION="getMotivationTypeDisplayOrder" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="MotivationTypeAdmin" NAME="ACTION" OPERATION="setMotivationTypeDisplayOrder" PHASE="ACTION"/>

  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="data"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="data"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" />
  </ACTION_SET>
</PAGE>
