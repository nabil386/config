<PAGE PAGE_ID="CitizenWorkspaceAdmin_editMotivationType" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=825,height=350">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="motivationTypeID"/>
  <PAGE_PARAMETER NAME="versionNo"/>

  <SERVER_INTERFACE PHASE="DISPLAY" CLASS="MotivationTypeAdmin" NAME="GET_DETAILS" OPERATION="getMotivationTypeEditDetails"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="motivationTypeID"/>
    <TARGET NAME="GET_DETAILS" PROPERTY="key$motivationTypeID"/>
  </CONNECT>

  <SERVER_INTERFACE PHASE="ACTION" CLASS="MotivationTypeAdmin" NAME="MODIFY_DETAILS" OPERATION="modifyMotivationType"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="motivationTypeID"/>
    <TARGET NAME="MODIFY_DETAILS" PROPERTY="motivationTypeID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="MODIFY_DETAILS" PROPERTY="versionNo"/>
  </CONNECT>

  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Script">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="scriptList$dtls$scriptName" HIDDEN_PROPERTY="scriptList$dtls$scriptCSV"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$scriptCSV"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="scriptCSV"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Ruleset" USE_BLANK="true">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="rulesetList$dtls$name"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$rulesetName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="rulesetName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.RequiresLogin">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$requiresLogin"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="requiresLogin"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Reference">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$reference"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="reference"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Schema">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="schemaList$dtls$schemaName"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$schemaName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="schemaName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultSchema" USE_BLANK="true">
      <CONNECT>
        <INITIAL NAME="GET_DETAILS" PROPERTY="schemaList$dtls$schemaName"/>
      </CONNECT>
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$resultSchemaName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="resultSchemaName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER TITLE="Field.Label.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="9">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="details$description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="MODIFY_DETAILS" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET >
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>

</PAGE>
