<PAGE PAGE_ID="CitizenWorkspaceAdmin_viewMotivationType" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <PAGE_PARAMETER NAME="motivationTypeID"/>
  
  <SERVER_INTERFACE CLASS="MotivationTypeAdmin" NAME="GET_DETAILS" OPERATION="getMotivationTypeDetails"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="motivationTypeID"/>
    <TARGET NAME="GET_DETAILS" PROPERTY="key$motivationTypeID"/>
  </CONNECT>

  <CLUSTER NUM_COLS="2" LABEL_WIDTH="45">
    <FIELD LABEL="Field.Label.Reference">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$reference"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Schema">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$schemaName"/>
      </CONNECT>
      <LINK PAGE_ID="System_scriptEditor">
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptID" />
          <TARGET NAME="PAGE" PROPERTY="scriptID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptType" />
          <TARGET NAME="PAGE" PROPERTY="scriptType" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptVersion" />
          <TARGET NAME="PAGE" PROPERTY="scriptVersion" />
        </CONNECT>                    
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$schemaName" />
          <TARGET NAME="PAGE" PROPERTY="schemaName" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$name" />
          <TARGET NAME="PAGE" PROPERTY="name" />
        </CONNECT>                    
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultSchema">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$resultSchemaName"/>
      </CONNECT>
      <LINK PAGE_ID="System_scriptEditor">
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptID" />
          <TARGET NAME="PAGE" PROPERTY="scriptID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptType" />
          <TARGET NAME="PAGE" PROPERTY="scriptType" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptVersion" />
          <TARGET NAME="PAGE" PROPERTY="scriptVersion" />
        </CONNECT>                    
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$resultSchemaName" />
          <TARGET NAME="PAGE" PROPERTY="schemaName" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$name" />
          <TARGET NAME="PAGE" PROPERTY="name" />
        </CONNECT>                    
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Script">
     <CONNECT>
       <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptID"/>
     </CONNECT>
     <LINK PAGE_ID="System_scriptEditor">
       <CONNECT>
         <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptID"/>
         <TARGET NAME="PAGE" PROPERTY="scriptID"/>
       </CONNECT>
       <CONNECT>
         <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptType"/>
         <TARGET NAME="PAGE" PROPERTY="scriptType"/>
       </CONNECT>
       <CONNECT>
         <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptVersion"/>
         <TARGET NAME="PAGE" PROPERTY="scriptVersion"/>
       </CONNECT>
       <CONNECT>
         <SOURCE NAME="GET_DETAILS" PROPERTY="result$schemaName"/>
         <TARGET NAME="PAGE" PROPERTY="schemaName"/>
       </CONNECT>
       <CONNECT>
         <SOURCE NAME="GET_DETAILS" PROPERTY="result$scriptName"/>
         <TARGET NAME="PAGE" PROPERTY="name"/>
       </CONNECT>
     </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.RuleSet">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$rulesetName"/>
      </CONNECT>
      <LINK PAGE_ID="CreoleAdmin_resolveOpenEditor">
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$rulesetSnapshotID"/>
          <TARGET NAME="PAGE" PROPERTY="snapShotID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="GET_DETAILS" PROPERTY="result$rulesetName"/>
          <TARGET NAME="PAGE" PROPERTY="name"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.RequiresLogin">
      <CONNECT>
        <SOURCE NAME="GET_DETAILS" PROPERTY="result$requiresLogin"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
