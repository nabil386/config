<PAGE PAGE_ID="CitizenWorkspaceAdmin_createMotivationType" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=800,height=480">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE PHASE="ACTION" CLASS="MotivationTypeAdmin" NAME="CREATE_MOTIVATION_TYPE" OPERATION="createMotivationType"/>

  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Script">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$scriptID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Ruleset">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$rulesetName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.RequiresLogin">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$requiresLogin"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Reference">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$reference"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Schema" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$schemaName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultSchema" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$resultSchemaName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER TITLE="Field.Label.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="9">
      <CONNECT>
        <TARGET NAME="CREATE_MOTIVATION_TYPE" PROPERTY="details$description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET >
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>

</PAGE>
