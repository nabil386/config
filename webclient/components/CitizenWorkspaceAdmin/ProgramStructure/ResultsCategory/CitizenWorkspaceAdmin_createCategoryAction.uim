<PAGE PAGE_ID="CitizenWorkspaceAdmin_createCategoryAction" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=800,height=315">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="resultsCategoryID"/> <!-- Required so we know which ResultsCategory to link the new action to. -->

  <SERVER_INTERFACE PHASE="ACTION" CLASS="ResultsCategoryAdmin" NAME="CREATE_CATEGORY_ACTION" OPERATION="createCategoryAction"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="resultsCategoryID"/>
    <TARGET NAME="CREATE_CATEGORY_ACTION" PROPERTY="resultsCategoryID"/>
  </CONNECT>

  <CLUSTER LABEL_WIDTH="50">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="CREATE_CATEGORY_ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DisplayState" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="CREATE_CATEGORY_ACTION" PROPERTY="displayState"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.URI">
      <CONNECT>
        <TARGET NAME="CREATE_CATEGORY_ACTION" PROPERTY="uri"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER TITLE="Field.Label.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="9">
      <CONNECT>
        <TARGET NAME="CREATE_CATEGORY_ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET >
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
</PAGE>
