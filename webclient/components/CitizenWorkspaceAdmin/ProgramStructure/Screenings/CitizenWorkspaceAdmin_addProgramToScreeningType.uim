<PAGE PAGE_ID="CitizenWorkspaceAdmin_addProgramToScreeningType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
   WINDOW_OPTIONS="width=550">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="ProgramItem" NAME="DISPLAY" OPERATION="listProgramTypesAndGroups" />
  <SERVER_INTERFACE CLASS="ScreeningType" NAME="ACTION" OPERATION="addProgramItemToScreening" PHASE="ACTION" />
  <PAGE_PARAMETER NAME="screeningTypeID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="screeningTypeID" />
    <TARGET NAME="ACTION" PROPERTY="screeningTypeID" />
  </CONNECT>    

  <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
    <ACTION_CONTROL
        IMAGE="SaveButton"
        LABEL="ActionControl.Label.Save"
        TYPE="SUBMIT"
      >
    </ACTION_CONTROL>
    <ACTION_CONTROL
        IMAGE="CancelButton"
        LABEL="ActionControl.Label.Cancel"
      >
    </ACTION_CONTROL>
  </ACTION_SET>

  <CLUSTER LABEL_WIDTH="25">
    <FIELD LABEL="Field.Label.Program">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="programItemIDAndType"
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="programItemIDAndType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Order" WIDTH="5" WIDTH_UNITS="CHARS">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="displayOrder" />
      </CONNECT>
    </FIELD>    
  </CLUSTER>
</PAGE>
