<PAGE PAGE_ID="CitizenWorkspaceAdmin_modifyScreeningProgramDisplayOrder" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
   WINDOW_OPTIONS="width=550">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ScreeningType" NAME="DISPLAY" OPERATION="readProgramDisplayOrder" />
  <SERVER_INTERFACE CLASS="ScreeningType" NAME="ACTION" OPERATION="modifyProgramDisplayOrder" PHASE="ACTION" />
  <PAGE_PARAMETER NAME="screeningTypeID" />
  <PAGE_PARAMETER NAME="programItemID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="programItemID" />
    <TARGET NAME="DISPLAY" PROPERTY="onlineProgramItemID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="programItemID" />
    <TARGET NAME="ACTION" PROPERTY="programItemID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="screeningTypeID" />
    <TARGET NAME="ACTION" PROPERTY="screeningTypeID" />
  </CONNECT>
  <ACTION_SET >
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT" />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" />
  </ACTION_SET>
  <CLUSTER LABEL_WIDTH="30" >
    <FIELD LABEL="Field.Title.DisplayOrder" WIDTH="5" WIDTH_UNITS="CHARS">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$displayOrder" />
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="displayOrder" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
