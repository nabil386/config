<PAGE PAGE_ID="CitizenWorkspaceAdmin_listScreenings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
  <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="DISPLAY" OPERATION="listScreenings" />
  <ACTION_SET >
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_newScreeningType" OPEN_MODAL="true" />
    </ACTION_CONTROL>
  </ACTION_SET>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_modifyScreeningTypeFromList" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="screeningTypeID" />
            <TARGET NAME="PAGE" PROPERTY="screeningTypeID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL  LABEL="ActionControl.Label.AddProgram">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_addProgramToScreeningType" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="screeningTypeID" />
            <TARGET NAME="PAGE" PROPERTY="screeningTypeID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <SEPARATOR/>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_deleteScreeningType" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="screeningTypeID" />
            <TARGET NAME="PAGE" PROPERTY="screeningTypeID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_viewScreeningType">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="screeningTypeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="screeningTypeID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>    
    
    <CONTAINER LABEL="Field.Title.Name">
	    <FIELD WIDTH="35">
	      <CONNECT>
	        <SOURCE NAME="DISPLAY" PROPERTY="name" />
	      </CONNECT>
	    </FIELD>
      <ACTION_CONTROL IMAGE="CALocalizableTextTranslation" LABEL="ActionControl.Label.Translations">
        <LINK PAGE_ID="LocalizableText_viewLocalizableText" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="nameTextID" /> <!-- was details$dtls$nameTextID -->
            <TARGET NAME="PAGE" PROPERTY="localizableTextID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
	  </CONTAINER>
	  <CONTAINER LABEL="Field.Title.Summary">
	    <FIELD WIDTH="65">
	      <CONNECT>
	        <SOURCE NAME="DISPLAY" PROPERTY="summary" />
	      </CONNECT>
	    </FIELD>
      <ACTION_CONTROL IMAGE="CALocalizableTextTranslation" LABEL="ActionControl.Label.Translations">
        <LINK PAGE_ID="LocalizableText_viewLocalizableText" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="summaryTextID" />
            <TARGET NAME="PAGE" PROPERTY="localizableTextID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
	  </CONTAINER>
  </LIST>
</PAGE>
