<PAGE PAGE_ID="CitizenWorkspaceAdmin_modifyPDFFormOffice" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
   WINDOW_OPTIONS="width=550">
    <PAGE_TITLE>
        <CONNECT> 
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="PDFForm" NAME="DISPLAY" OPERATION="listPDFOfficeDetailsForModify" />
  <SERVER_INTERFACE CLASS="PDFForm" NAME="ACTION" OPERATION="modifyOfficeForPDFForm" PHASE="ACTION" />
  <PAGE_PARAMETER NAME="pdfFormID" />
  <PAGE_PARAMETER NAME="locationID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pdfFormID" />
    <TARGET NAME="DISPLAY" PROPERTY="key$pdfFormID" />
  </CONNECT>
    <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="locationID" />
    <TARGET NAME="DISPLAY" PROPERTY="key$locationID" />
  </CONNECT>    
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="pdfFormID" />
    <TARGET NAME="ACTION" PROPERTY="pdfFormID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="locationID" />
    <TARGET NAME="ACTION" PROPERTY="locationID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="pdfFormOfficeServiceAreaLinkTabList" />
    <TARGET NAME="ACTION" PROPERTY="pdfFormOfficeServiceAreaLinkTabList" />
  </CONNECT>       

  <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
    <ACTION_CONTROL
        IMAGE="SaveButton"
        LABEL="ActionControl.Label.Save"
        TYPE="SUBMIT"
      >
    </ACTION_CONTROL>
    <ACTION_CONTROL
        IMAGE="CancelButton"
        LABEL="ActionControl.Label.Cancel"
      >
    </ACTION_CONTROL>
  </ACTION_SET>


                
  <LIST TITLE="List.ServiceAreas" >


    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.Title.Select"
      WIDTH="10"
    >


      <WIDGET TYPE="MULTISELECT">


        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="serviceAreaID" />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="serviceAreaTabList" />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="serviceAreaTabList" />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>

    <FIELD LABEL="List.Title.Name" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dtlsList$dtls$name" />
      </CONNECT>
    </FIELD>

  </LIST>  
       
</PAGE>
