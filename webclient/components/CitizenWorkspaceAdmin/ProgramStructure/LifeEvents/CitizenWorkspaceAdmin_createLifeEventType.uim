<PAGE PAGE_ID="CitizenWorkspaceAdmin_createLifeEventType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" WINDOW_OPTIONS="width=925,height=650">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE PHASE="ACTION" CLASS="LifeEventAdmin" NAME="CREATE_LIFE_EVENT" OPERATION="createLifeEventType"/>
  <SERVER_INTERFACE PHASE="DISPLAY" CLASS="LifeEventAdmin" NAME="GET_ALL_CATEGORIES" OPERATION="getAllCategories"/>
  <SERVER_INTERFACE PHASE="DISPLAY" CLASS="LifeEventAdmin" NAME="GET_ALL_CHANNELS" OPERATION="getAllChannels"/>
  <SERVER_INTERFACE PHASE="DISPLAY" NAME="GET_DATA_HUB_CONTEXTS" CLASS="LifeEventAdmin" OPERATION="getDataHubContexts"/>

  <CLUSTER LABEL_WIDTH="65" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="details$name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionScript">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$questionScript"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Schema" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="schema$name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DataRuleSet">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$dataRulesetName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CitizenConsentEnabled">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$citizenConsentEnabledInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ChannelType" USE_BLANK="TRUE">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="channelType" NAME="GET_ALL_CHANNELS" PROPERTY="channelType"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$channelType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ChannelName">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$channelName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AnswerScript">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$answerScript"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DisplayRuleSet">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$ruleSet"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LaunchDescription">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$launchDescription"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CitizenConsentOneChoice">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$citizenConsentOneChoiceInd"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Popular">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$isPopular"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER TITLE="Field.Label.SubmissionText" SHOW_LABELS="false">
    <FIELD HEIGHT="150" LABEL="Dummy.Label.SubmissionText">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$submissionText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER TITLE="Field.Label.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="150" LABEL="Dummy.Label.Description">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER TITLE="Field.Label.AdditionalInfo" SHOW_LABELS="false">
    <FIELD HEIGHT="150" LABEL="Dummy.Label.AdditionalInfo">
      <CONNECT>
        <TARGET NAME="CREATE_LIFE_EVENT" PROPERTY="channel$additionalInfo"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <!-- Save and Cancel buttons -->
  <ACTION_SET>
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>

</PAGE>
