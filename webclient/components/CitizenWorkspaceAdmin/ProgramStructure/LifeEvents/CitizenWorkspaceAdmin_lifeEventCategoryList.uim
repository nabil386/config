<PAGE PAGE_ID="CitizenWorkspaceAdmin_lifeEventCategoryList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE CLASS="LifeEventAdmin" NAME="DISPLAY" OPERATION="getAllCategories" />
  
  <!-- New (Life Event) button -->
  <ACTION_SET >
    <ACTION_CONTROL LABEL="ActionControl.Label.CreateNew">
      <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_createLifeEventCategory"/>
    </ACTION_CONTROL>
  </ACTION_SET>
  
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
    
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_editLifeEventCategory">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <SEPARATOR/>
 
 
      <ACTION_CONTROL LABEL="ActionControl.Label.AddLifeEvent">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_addLifeEventTypesToCategory">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>      
                
      <ACTION_CONTROL LABEL="ActionControl.Label.ReorderLifeEvent">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_reorderLifeEventType">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.ChangeImage">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_upsertCategoryImage">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>          
        </LINK>
      </ACTION_CONTROL>       
      
      <SEPARATOR/>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_deleteLifeEventTypeCategory">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
    </ACTION_SET>
    
    <DETAILS_ROW>
	    <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_viewLifeEventCategory">
	    	<CONNECT>
	    	  <SOURCE NAME="DISPLAY" PROPERTY="id"/>
	    	  <TARGET NAME="PAGE" PROPERTY="categoryID"/>
	    	</CONNECT>
	    </INLINE_PAGE>
	  </DETAILS_ROW>                

    <FIELD LABEL="Field.Label.Name" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description" WIDTH="65">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
    </FIELD>

  </LIST>

</PAGE>

