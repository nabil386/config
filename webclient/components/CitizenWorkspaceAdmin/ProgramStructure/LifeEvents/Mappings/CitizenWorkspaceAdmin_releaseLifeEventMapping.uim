<PAGE PAGE_ID="CitizenWorkspaceAdmin_releaseLifeEventMapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" WINDOW_OPTIONS="width=550">

    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
        </CONNECT>
    </PAGE_TITLE>

    <INFORMATIONAL>
        <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$informationMsgTxt"/>
        </CONNECT>
    </INFORMATIONAL>

    <SERVER_INTERFACE CLASS="MappingConfiguration" NAME="DISPLAY"
        OPERATION="checkLifeEventMappingForRelease" PHASE="DISPLAY"/>
    <SERVER_INTERFACE CLASS="MappingConfiguration" NAME="ACTION"
        OPERATION="releaseLifeEventMapping" PHASE="ACTION"/>

    <PAGE_PARAMETER NAME="configurationID"/>
    <PAGE_PARAMETER NAME="versionNo"/>

    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="configurationID"/>
        <TARGET NAME="DISPLAY" PROPERTY="configurationID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="configurationID"/>
        <TARGET NAME="ACTION" PROPERTY="details$dtls$configurationID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
        <TARGET NAME="ACTION" PROPERTY="details$dtls$versionNo"/>
    </CONNECT>

    <ACTION_SET>
        <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
        <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
    </ACTION_SET>

    <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
        <FIELD>
            <CONNECT>
                <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.RemoveMappingConfiguration"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>

</PAGE>
