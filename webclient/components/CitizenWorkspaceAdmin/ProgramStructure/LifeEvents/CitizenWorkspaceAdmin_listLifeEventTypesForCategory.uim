<PAGE PAGE_ID="CitizenWorkspaceAdmin_listLifeEventTypesForCategory" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE CLASS="LifeEventAdmin" NAME="GET_ASSOCIATED_LIFE_EVENT_TYPES" OPERATION="getLifeEventTypesForID" />
  
  <PAGE_PARAMETER NAME="categoryID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="categoryID"/>
    <TARGET NAME="GET_ASSOCIATED_LIFE_EVENT_TYPES" PROPERTY="key$id"/>
  </CONNECT>
  
  <LIST TITLE="List.Title.LifeEvents">
    
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_removeLifeEventTypeFromCategory">
          <CONNECT>
            <SOURCE NAME="GET_ASSOCIATED_LIFE_EVENT_TYPES" PROPERTY="result$dtls$id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="categoryID"/>
            <TARGET NAME="PAGE" PROPERTY="categoryID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    
    <FIELD LABEL="Field.Label.Name" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="GET_ASSOCIATED_LIFE_EVENT_TYPES" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description" WIDTH="65">
      <CONNECT>
        <SOURCE NAME="GET_ASSOCIATED_LIFE_EVENT_TYPES" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  
  </LIST>

</PAGE>

