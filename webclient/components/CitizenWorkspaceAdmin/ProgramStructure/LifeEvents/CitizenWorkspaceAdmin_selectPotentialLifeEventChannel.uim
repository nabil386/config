<PAGE PAGE_ID="CitizenWorkspaceAdmin_selectPotentialLifeEventChannel" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="lifeEventTypeID"/>

  <SERVER_INTERFACE PHASE="DISPLAY" CLASS="LifeEventAdmin" NAME="GET_POTENTIAL_CHANNELS" OPERATION="getPotentialChannelsForID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="lifeEventTypeID"/>
    <TARGET NAME="GET_POTENTIAL_CHANNELS" PROPERTY="id"/>
  </CONNECT>

  <!-- 'Close' button at the top -->
  <ACTION_SET ALIGNMENT="CENTER" BOTTOM="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Close">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_viewLifeEventType" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>

  <LIST TITLE="List.Title.Channels">
    <FIELD LABEL="Field.Label.ChannelType">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_addLifeEventChannel" OPEN_MODAL="true"  WINDOW_OPTIONS="width=745">
        <CONNECT>
          <SOURCE NAME="GET_POTENTIAL_CHANNELS" PROPERTY="channelType"/>
          <TARGET NAME="PAGE" PROPERTY="channelType"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
        </CONNECT>
      </LINK>
      <CONNECT>
        <SOURCE NAME="GET_POTENTIAL_CHANNELS" PROPERTY="channelType"/>
      </CONNECT>
    </FIELD>
  </LIST>

</PAGE>
