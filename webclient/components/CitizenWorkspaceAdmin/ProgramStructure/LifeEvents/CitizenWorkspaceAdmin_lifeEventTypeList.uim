<PAGE PAGE_ID="CitizenWorkspaceAdmin_lifeEventTypeList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="LifeEventAdmin" NAME="DISPLAY" OPERATION="getAllLifeEventTypes"/>

  <!-- New Life Event button -->
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.CreateNew">
      <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_createLifeEventType"/> 
    </ACTION_CONTROL>
  </ACTION_SET>

  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_editLifeEventType">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>  
      
      <SEPARATOR/>
              
     <ACTION_CONTROL LABEL="ActionControl.Label.Add">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_addLifeEventTypeToCategory">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.AddTargetSystem">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_createRemoteSystemForLifeEvent">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
            <TARGET NAME="PAGE" PROPERTY="channel"/>
          </CONNECT> 
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.AddCases">
        <LINK OPEN_MODAL="true"  PAGE_ID="CitizenWorkspaceAdmin_addLifeEventCases">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
            <TARGET NAME="PAGE" PROPERTY="channel"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.ChangeImage">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_upsertContextImage">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
            <TARGET NAME="PAGE" PROPERTY="channelType"/>
          </CONNECT>          
        </LINK>
      </ACTION_CONTROL>

      <ACTION_CONTROL LABEL="ActionControl.Label.NewMapping">
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="displayNewMappingOption"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_newLifeEventMapping">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
            <TARGET NAME="PAGE" PROPERTY="channelType"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>

      <SEPARATOR/>
      
     <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="CitizenWorkspaceAdmin_deleteLifeEventType">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="id"/>
            <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_viewLifeEventType">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="id"/>
          <TARGET NAME="PAGE" PROPERTY="lifeEventTypeID"/>
        </CONNECT> 
        <CONNECT>
	 <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
	 <TARGET NAME="PAGE" PROPERTY="channel"/>
        </CONNECT>  
      </INLINE_PAGE>
    </DETAILS_ROW>
    
    
    <FIELD LABEL="Field.Label.Name" WIDTH="25">
     <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Description" WIDTH="50">
         <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="description"/>
          </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Channel" WIDTH="25">
         <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="channel"/>
          </CONNECT>
    </FIELD>


    </LIST>

</PAGE>