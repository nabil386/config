<PAGE PAGE_ID="CitizenWorkspaceAdmin_listAuthenticationFactors" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
   HIDE_CONDITIONAL_LINKS="false">
  
  <SERVER_INTERFACE CLASS="AuthenticationFactor" NAME="DISPLAY" OPERATION="listAll" />
  <SERVER_INTERFACE CLASS="ConfigurationProfile" NAME="DISPLAY_HELP" OPERATION="readConfigurationProfile" />

  
  <INFORMATIONAL>
  	<CONNECT>
		<SOURCE NAME="DISPLAY_HELP" PROPERTY="result$dtls$message" />
	</CONNECT> 	
  </INFORMATIONAL>

  <ACTION_SET >
    <ACTION_CONTROL  LABEL="ActionControl.Label.New">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_newAuthenticationFactor" OPEN_MODAL="true" />
    </ACTION_CONTROL>
    <SEPARATOR/>
    <ACTION_CONTROL LABEL="ActionControl.Label.ActivateSecondLevel" >
      <CONDITION>
        <IS_FALSE NAME="DISPLAY" PROPERTY="propertyValueInd"/> 		
      </CONDITION> 	
      <LINK PAGE_ID="CitizenWorkspaceAdmin_activateSecondLevelAuthentication" OPEN_MODAL="true"/>
    </ACTION_CONTROL>			
    <ACTION_CONTROL LABEL="ActionControl.Label.DeactivateSecondLevel" >
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="propertyValueInd"/> 		
      </CONDITION> 	
      <LINK PAGE_ID="CitizenWorkspaceAdmin_deactivateSecondLevelAuthentication" OPEN_MODAL="true"/>
    </ACTION_CONTROL>			
    <SEPARATOR/>
    <ACTION_CONTROL LABEL="ActionControl.Label.EditHelpText">
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="propertyValueInd"/> 		
      </CONDITION> 	
      <LINK PAGE_ID="CitizenWorkspaceAdmin_editHelpTextForAuthenticationFactor" OPEN_MODAL="true">
        <CONNECT>
          <SOURCE NAME="DISPLAY_HELP" PROPERTY="configProfileID" />
          <TARGET NAME="PAGE" PROPERTY="configProfileID"/>
        </CONNECT>              
      </LINK>
    </ACTION_CONTROL>		
    
  </ACTION_SET> 	
  
  <CLUSTER LABEL_WIDTH="25" TITLE="Cluster.Title.Settings">
    <FIELD LABEL="Field.Label.SecondLevelAuthenticationActive">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="propertyValueInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


 	<CLUSTER TITLE="Cluster.Title.HelpText"  SHOW_LABELS="false">
    <FIELD>
   		<CONNECT>
     		<SOURCE NAME="DISPLAY_HELP" PROPERTY="result$helpText$helpText" />
   		</CONNECT>
   	</FIELD>  		
 	</CLUSTER>


 	<LIST TITLE="List.Title.Active">
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_modifyAuthenticationFactor" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
     <ACTION_CONTROL LABEL="ActionControl.Label.Deactivate">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_deactivateAuthenticationFactor" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$name" />
            <TARGET NAME="PAGE" PROPERTY="name" />
          </CONNECT>          
        </LINK>
      </ACTION_CONTROL> 
      <SEPARATOR/>   
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_deleteAuthenticationFactor" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$name" />
            <TARGET NAME="PAGE" PROPERTY="name" />
          </CONNECT>                    
        </LINK>
      </ACTION_CONTROL>  
                  
    </ACTION_SET>
    
	<DETAILS_ROW>
	  <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_viewAuthenticationFactor">
		<CONNECT>
		  <SOURCE
			NAME="DISPLAY"
			PROPERTY="result$activeList$details$authFactorID"
		  />
		  <TARGET
			NAME="PAGE"
			PROPERTY="authFactorID"
		  />
		</CONNECT>
	  </INLINE_PAGE>
	</DETAILS_ROW>                
	
	
    <FIELD LABEL="Field.Title.Name" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="activeList$details$name" />
      </CONNECT>
    </FIELD>
 	  <FIELD LABEL="Field.Title.Title" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="activeList$localText$titleText" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Description" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="activeList$localText$descriptionText" />
      </CONNECT>
    </FIELD>    
   </LIST>
   
 <LIST TITLE="List.Title.Inactive">
<!--
 	<CONDITION>
		<IS_TRUE
			NAME="DISPLAY"
			PROPERTY="propertyValueInd"
		/>
	</CONDITION>
-->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_modifyAuthenticationFactor" OPEN_MODAL="true" >
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      
      <ACTION_CONTROL LABEL="ActionControl.Label.Activate">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_activateAuthenticationFactor" OPEN_MODAL="true" >
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$name" />
            <TARGET NAME="PAGE" PROPERTY="name" />
          </CONNECT>          
        </LINK>
      </ACTION_CONTROL> 
      <SEPARATOR/>   
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_deleteAuthenticationFactor" OPEN_MODAL="true" >
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$authFactorID" />
            <TARGET NAME="PAGE" PROPERTY="authFactorID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$name" />
            <TARGET NAME="PAGE" PROPERTY="name" />
          </CONNECT>                    
        </LINK>
      </ACTION_CONTROL>          
    </ACTION_SET>
    
	<DETAILS_ROW>
	  <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_viewAuthenticationFactor">
		<CONNECT>
		  <SOURCE
			NAME="DISPLAY"
			PROPERTY="result$inactiveList$details$authFactorID"
		  />
		  <TARGET
			NAME="PAGE"
			PROPERTY="authFactorID"
		  />
		</CONNECT>
	  </INLINE_PAGE>
	</DETAILS_ROW>                
	
    <FIELD LABEL="Field.Title.Name" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$details$name" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Title" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$localText$titleText" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Description" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="inactiveList$localText$descriptionText" />
      </CONNECT>
    </FIELD>    
   </LIST>   
</PAGE>
