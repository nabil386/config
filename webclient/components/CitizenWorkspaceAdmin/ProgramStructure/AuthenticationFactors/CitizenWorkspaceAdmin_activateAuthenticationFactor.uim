<PAGE PAGE_ID="CitizenWorkspaceAdmin_activateAuthenticationFactor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
     WINDOW_OPTIONS="width=550">
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
    <SERVER_INTERFACE CLASS="AuthenticationFactor" NAME="ACTION" OPERATION="activate" PHASE="ACTION" />
    <PAGE_PARAMETER NAME="authFactorID" />
    <PAGE_PARAMETER NAME="name" />
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="authFactorID" />
        <TARGET NAME="ACTION" PROPERTY="authFactorID" />
    </CONNECT>
    <ACTION_SET >
        <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
        <ACTION_CONTROL LABEL="ActionControl.Label.No" />
    </ACTION_SET>
    
    <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
        <FIELD>
            <CONNECT>
                <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.ActivateAuthenticationFactor" />
            </CONNECT>
        </FIELD>
    </CLUSTER>
</PAGE>
