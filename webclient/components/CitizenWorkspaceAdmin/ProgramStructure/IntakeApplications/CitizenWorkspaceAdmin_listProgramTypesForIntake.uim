<PAGE PAGE_ID="CitizenWorkspaceAdmin_listProgramTypesForIntake"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
    </CONNECT>
  </PAGE_TITLE>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL LABEL="ActionControl.Label.IntakeDetails" STYLE="in-page-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_viewIntakeApplicationType" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Programs" STYLE="in-page-current-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_listProgramTypesForIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.WithdrawalRequests" STYLE="in-page-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_listWithdrawalRequestsForIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <SERVER_INTERFACE CLASS="IntakeApplicationType" NAME="DISPLAY"
    OPERATION="listProgramsForIntakeApplication"/>
  <PAGE_PARAMETER NAME="intakeApplicationTypeID"/>
  <PAGE_PARAMETER NAME="intakeApplicationTypeName"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$intakeApplicationTypeID"/>
  </CONNECT>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Change">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_modifyIntakeProgramDisplayOrder" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="programTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="programTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.CreateMapping">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_newIntakeApplicationProgramMapping" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="programTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="programTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_removeProgramIntakeApplicationLink" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="programTypeID"/>
            <TARGET NAME="PAGE" PROPERTY="programTypeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CitizenWorkspaceAdmin_intakeApplicationProgramMappingsDropdown">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="programTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="programTypeID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="name"/>
          <TARGET NAME="PAGE" PROPERTY="programName"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <FIELD LABEL="Field.Title.Name" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
      <LINK PAGE_ID="CitizenWorkspaceAdmin_programHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="programTypeID"/>
          <TARGET NAME="PAGE" PROPERTY="programTypeID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Title.Description" WIDTH="50">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.DisplayOrder" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="displayOrder"/>
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
