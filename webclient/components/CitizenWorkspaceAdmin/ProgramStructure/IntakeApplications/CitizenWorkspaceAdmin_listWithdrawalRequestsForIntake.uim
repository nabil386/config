<PAGE PAGE_ID="CitizenWorkspaceAdmin_listWithdrawalRequestsForIntake" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
    </CONNECT>
  </PAGE_TITLE>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL LABEL="ActionControl.Label.IntakeDetails" STYLE="in-page-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_viewIntakeApplicationType" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Programs" STYLE="in-page-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_listProgramTypesForIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.WithdrawalRequests" STYLE="in-page-current-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_listWithdrawalRequestsForIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>    
  <PAGE_PARAMETER NAME="intakeApplicationTypeID" />
  <PAGE_PARAMETER NAME="intakeApplicationTypeName" />
  <SERVER_INTERFACE CLASS="IntakeApplicationType" NAME="DISPLAY" OPERATION="listWithdrawalReasonsForIntake" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
    <TARGET NAME="DISPLAY" PROPERTY="key$intakeApplicationTypeID" />
  </CONNECT>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_removeIntakeWithdrawalReason" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="withdrawalReqReasonID" />
            <TARGET NAME="PAGE" PROPERTY="withdrawalReqReasonID" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Title.Reason" WIDTH="100">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="reasonCode" />
      </CONNECT>
    </FIELD>
  </LIST>  
</PAGE>
