<PAGE PAGE_ID="CitizenWorkspaceAdmin_listIntakeApplicationProgramMappings" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
	<SERVER_INTERFACE CLASS="MappingConfiguration" NAME="DISPLAY" OPERATION="listMappingsForAppTypeProgramLink" />
  	
  	<PAGE_PARAMETER NAME="intakeApplicationTypeID" />
  	<PAGE_PARAMETER NAME="programTypeID" />
  	<PAGE_PARAMETER NAME="intakeApplicationTypeName" />
  	<PAGE_PARAMETER NAME="programName" />
  	
  	<CONNECT>
    	<SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
    	<TARGET NAME="DISPLAY" PROPERTY="key$intakeApplicationTypeID" />
  	</CONNECT>
  	<CONNECT>
    	<SOURCE NAME="PAGE" PROPERTY="programTypeID" />
    	<TARGET NAME="DISPLAY" PROPERTY="key$programTypeID" />
  	</CONNECT>
  	
    <MENU MODE="IN_PAGE_NAVIGATION">
      <ACTION_CONTROL LABEL="ActionControl.Label.Details" STYLE="in-page-link">
        <LINK PAGE_ID="CitizenWorkspaceAdmin_viewProgramFromIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programTypeID" />
          <TARGET NAME="PAGE" PROPERTY="programTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT> 
		<CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
        </CONNECT> 
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.LocalOffices" STYLE="in-page-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_viewLocalOfficeFromIntake" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programTypeID" />
          <TARGET NAME="PAGE" PROPERTY="programTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT> 
		<CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
        </CONNECT> 
		<CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programName" />
          <TARGET NAME="PAGE" PROPERTY="programName" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Mappings" STYLE="in-page-current-link">
      <LINK PAGE_ID="CitizenWorkspaceAdmin_listIntakeApplicationProgramMappings" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programName" />
          <TARGET NAME="PAGE" PROPERTY="programName" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programTypeID" />
          <TARGET NAME="PAGE" PROPERTY="programTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
          <TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
        </CONNECT>                
      </LINK>
    </ACTION_CONTROL>
    </MENU>  	
    
 	<LIST>
		<ACTION_SET TYPE="LIST_ROW_MENU">
			<ACTION_CONTROL LABEL="ActionControl.Label.Edit" APPEND_ELLIPSIS="true">
			  <CONDITION>
			     <IS_TRUE NAME="DISPLAY" PROPERTY="displayEditOption"/>
			  </CONDITION>
				<LINK PAGE_ID="CitizenWorkspaceAdmin_editMappingConfiguration">
					<CONNECT>
  					<SOURCE NAME="DISPLAY" PROPERTY="configurationID" />
  					<TARGET NAME="PAGE" PROPERTY="configurationID" />
					</CONNECT>
					<CONNECT>
  					<SOURCE NAME="PAGE" PROPERTY="programName" />
  					<TARGET NAME="PAGE" PROPERTY="programName" />
					</CONNECT>
					<CONNECT>
  					<SOURCE NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
  					<TARGET NAME="PAGE" PROPERTY="intakeApplicationTypeName" />
					</CONNECT>
					<CONNECT>
  					<SOURCE NAME="DISPLAY" PROPERTY="schemaName" />
  					<TARGET NAME="PAGE" PROPERTY="schemaName" />
					</CONNECT>
        			<CONNECT>
          			<SOURCE NAME="DISPLAY" PROPERTY="relatedCaseTypeCode" />
          			<TARGET NAME="PAGE" PROPERTY="relatedCaseTypeCode" />
        			</CONNECT>
        			<CONNECT>
          			<SOURCE NAME="DISPLAY" PROPERTY="caseConfigurationID" />
          			<TARGET NAME="PAGE" PROPERTY="caseConfigurationID" />
        			</CONNECT>         			
				</LINK>
			</ACTION_CONTROL>    	
			
			<ACTION_CONTROL LABEL="ActionControl.Label.Release">
			  <CONDITION>
           <IS_TRUE NAME="DISPLAY" PROPERTY="displayReleaseOption"/>
        </CONDITION>
				<LINK PAGE_ID="CitizenWorkspaceAdmin_releaseMappingConfiguration" OPEN_MODAL="true" WINDOW_OPTIONS="width=900,height=768">
					<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="configurationID" />
					<TARGET NAME="PAGE" PROPERTY="configurationID" />
					</CONNECT>
					<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="versionNo" />
					<TARGET NAME="PAGE" PROPERTY="versionNo" />
					</CONNECT>          			
				</LINK>
			</ACTION_CONTROL>
			
			<ACTION_CONTROL LABEL="ActionControl.Label.Remove">
			  <CONDITION>
           <IS_TRUE NAME="DISPLAY" PROPERTY="displayCancelOption"/>
        </CONDITION>
			  <LINK PAGE_ID="CitizenWorkspaceAdmin_removeMappingConfiguration" OPEN_MODAL="true" WINDOW_OPTIONS="width=900,height=768">
					<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="configurationID" />
					<TARGET NAME="PAGE" PROPERTY="configurationID" />
					</CONNECT>
					<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="versionNo" />
					<TARGET NAME="PAGE" PROPERTY="versionNo" />
					</CONNECT>          			
				</LINK>
			</ACTION_CONTROL>            
		</ACTION_SET>
		
		<FIELD LABEL="Field.Title.EffectiveDate" WIDTH="20">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="effectiveDate" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Title.Status" WIDTH="30">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="status" />
			</CONNECT>
		</FIELD>		
		<FIELD LABEL="Field.Title.ReleaseDate" WIDTH="20">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="releaseDate" />
			</CONNECT>
		</FIELD>   		
		<FIELD LABEL="Field.Title.CreatedBy" WIDTH="30">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="createdByUserFullName"/>
			</CONNECT>
			<LINK OPEN_MODAL="true" PAGE_ID="Organization_viewUserDetails">
				<CONNECT>
		  			<SOURCE NAME="DISPLAY" PROPERTY="createdBy"/>
		  			<TARGET NAME="PAGE" PROPERTY="userName"/>
				</CONNECT>
			</LINK>
		</FIELD>		
	</LIST>
</PAGE>
