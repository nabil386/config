<PAGE PAGE_ID="CitizenWorkspaceAdmin_newIntakeApplicationType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=1075,height=700">
  
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="ACTION" OPERATION="createIntakeApplicationType" PHASE="ACTION" />
  <SERVER_INTERFACE CLASS="IntakeApplicationType" NAME="DISPLAY" OPERATION="getIntitialDataForNewIntake"/>  
  
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT" />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" />
  </ACTION_SET>
  
  <CLUSTER LABEL_WIDTH="60" NUM_COLS="2">
    <FIELD LABEL="Field.Title.Name" >
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.SchemaName"  USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="intakeSchemaName" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.URL">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="url" />
      </CONNECT> 
    </FIELD>   
    <FIELD LABEL="Field.Title.ProgramSelection">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="programSelectionInd" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.ClientRegistration" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$clientRegistration" />
      </CONNECT>
    </FIELD>     
    <FIELD LABEL="Field.Title.ApplicationScriptName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="intakeScriptID" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.SubmissionScriptName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="submissionScriptID" />
      </CONNECT>
    </FIELD>    
    <FIELD LABEL="Field.Title.PDFForm"  USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="pdfFormID" NAME="DISPLAY" PROPERTY="result$pdfDetails$dtls$name" />
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="pdfFormID" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.SubmitOnCompletion">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="submitOnCompletionInd" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Icon">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$details$icon" />
      </CONNECT>
    </FIELD>
    
  </CLUSTER>
  <CLUSTER TITLE="Cluster.Title.Summary" SHOW_LABELS="false">
    <FIELD HEIGHT="150">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="summary" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER TITLE="Cluster.Title.Description" SHOW_LABELS="false">
    <FIELD HEIGHT="150">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <CLUSTER LABEL_WIDTH="12.5" TITLE="Cluster.Title.SubmissionConfirmationDetails">
    <FIELD LABEL="Field.Title.ClusterTitle">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="submissionConfirmPageTitle" />
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Title.Text" HEIGHT="150">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="submissionConfirmPageText" />
      </CONNECT>
    </FIELD>
    
  </CLUSTER>
</PAGE>
