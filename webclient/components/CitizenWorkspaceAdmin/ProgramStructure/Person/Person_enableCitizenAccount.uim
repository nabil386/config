<PAGE PAGE_ID="Person_enableCitizenAccount"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" WINDOW_OPTIONS="width=550">

    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
        </CONNECT>
    </PAGE_TITLE>

    <SERVER_INTERFACE CLASS="CitizenWorkspaceAdmin" NAME="ACTION"
        OPERATION="enableCitizenAccount" PHASE="ACTION"/>

    <PAGE_PARAMETER NAME="concernRoleID"/>

    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
        <TARGET NAME="ACTION" PROPERTY="key$concernRoleID"/>
    </CONNECT>

    <ACTION_SET>
        <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
        <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
    </ACTION_SET>

    <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
        <FIELD>
            <CONNECT>
                <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.EnableCitizenAccount"/>
            </CONNECT>
        </FIELD>
    </CLUSTER>

</PAGE>
