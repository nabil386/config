<APP_CONFIG>
  <POPUP_PAGE PAGE_ID="CitizenWorkspaceAdmin_listProgramGroupsPopup">
    <DOMAIN>ONLINE_PROGRAM_GROUP_ID</DOMAIN>
    <CT_CODE />
    <WIDTH>600</WIDTH>
    <HEIGHT>400</HEIGHT>
    <SCROLLBARS>true</SCROLLBARS>
    <IMAGE>Images/search.gif</IMAGE>
    <LABEL>label.search</LABEL>
  </POPUP_PAGE>
  <FILE_DOWNLOAD_CONFIG>
    <FILE_DOWNLOAD CLASS="curam.interfaces.PDFFormPkg.PDFForm_viewPDFFormContent_TH"
      PAGE_ID="CitizenWorkspaceAdmin_viewPDFForm">
      <INPUT PAGE_PARAM="pdfFormResourceName" PROPERTY="key$name" />
      <INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />
      <FILE_DATA PROPERTY="result$pdfFormContent" />
      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.PDFFormPkg.PDFForm_viewPDFFormContent_TH"
      PAGE_ID="CitizenWorkspaceAdmin_viewProgram">
      <INPUT PAGE_PARAM="pdfFormResourceName" PROPERTY="key$name" />
      <INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />
      <FILE_DATA PROPERTY="result$pdfFormContent" />
      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.PDFFormPkg.PDFForm_viewPDFFormContent_TH"
      PAGE_ID="CitizenWorkspaceAdmin_viewProgramNoNavigation">
      <INPUT PAGE_PARAM="pdfFormResourceName" PROPERTY="key$name" />
      <INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />
      <FILE_DATA PROPERTY="result$pdfFormContent" />
      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.PDFFormPkg.PDFForm_viewPDFFormContent_TH"
      PAGE_ID="CitizenWorkspaceAdmin_viewProgramFromIntake">
      <INPUT PAGE_PARAM="pdfFormResourceName" PROPERTY="key$name" />
      <INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />
      <FILE_DATA PROPERTY="result$pdfFormContent" />
      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.PDFFormPkg.PDFForm_viewPDFFormContent_TH"
	      PAGE_ID="CitizenWorkspaceAdmin_viewProgramFromScreening">
	      <INPUT PAGE_PARAM="pdfFormResourceName" PROPERTY="key$name" />
	      <INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />
	      <FILE_DATA PROPERTY="result$pdfFormContent" />
	      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.CitizenWorkspaceAdminPkg.CitizenWorkspaceAdmin_readAppResourceContent_TH"
      PAGE_ID="CitizenWorkspaceAdmin_viewProgramMapping">
      <INPUT PAGE_PARAM="name" PROPERTY="key$name" />
      <!--<INPUT PAGE_PARAM="locale" PROPERTY="key$locale" />-->
      <FILE_DATA PROPERTY="result$content" />
      <FILE_NAME PROPERTY="result$name" />
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD CLASS="curam.interfaces.CitizenWorkspaceLocalePkg.CitizenWorkspaceLocale_readLocale_TH"
      PAGE_ID="CitizenWorkspaceLocale_modifyLocaleFromList">
      <INPUT PAGE_PARAM="citizenWorkspaceLocaleID" PROPERTY="key$citizenWorkspaceLocaleID" />
      <FILE_DATA PROPERTY="result$imageContent" />
      <FILE_NAME PROPERTY="result$fileName" />
    </FILE_DOWNLOAD>
  </FILE_DOWNLOAD_CONFIG>
</APP_CONFIG>
