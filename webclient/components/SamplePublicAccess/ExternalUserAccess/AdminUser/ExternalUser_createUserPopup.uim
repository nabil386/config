<!-- Create a new external user                                             -->
<PAGE
  PAGE_ID="ExternalUser_createUserPopup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ExternalUserAccess"
    NAME="ACTION"
    OPERATION="createUser"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ExternalUsers_userSearchPopup"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="SaveAndNewButton"
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ExternalUser_createUserPopup"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK PAGE_ID="ExternalUsers_userSearchPopup"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >
    <FIELD LABEL="Field.Label.Title">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="title"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.FirstName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="firstname"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Sensitivity">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="sensitivity"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LastName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="surname"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER>
    <CONTAINER LABEL="Field.Label.RelatedParticipantSearch">
      <FIELD WIDTH="25">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="externalUserParticipantType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="74">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="participantRoleID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.Security"
  >
    <FIELD LABEL="Field.Label.UserName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$details$userName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Password">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="password"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.PasswordExpiryDays"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="pwdChangeEveryXDay"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.AccountExpiry"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="passwordExpiryDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.RoleName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="roleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PasswordConfirm">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="confirmPassword"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PasswordExpiryLogs">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="pwdChangeAfterXLog"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AccountEnabled">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="accountEnabled"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.AccessPeriods"
  >
    <FIELD LABEL="Field.Label.SetAccess">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginRestrictions"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDayMon">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDayMon"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDayWed">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDayWed"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDayFri">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDayFri"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LoginTimeFrom"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginTimeFrom"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDaySun">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDaySun"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDayTues">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDayTues"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDayThurs">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDayThurs"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LoginDaySat">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginDaySat"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LoginTimeTo"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="loginTimeTo"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
