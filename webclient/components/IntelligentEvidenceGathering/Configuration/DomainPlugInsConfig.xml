<pc:plug-ins
    xmlns:pc="http://www.curamsoftware.com/curam/util/common/plug-in-config">
  <!--
  The main IEG Player renderer.
  -->
  <pc:plug-in purpose="ieg-player" name="edit-renderer"
              class="curam.ieg.player.IEGPlayerRenderer">
    <!--
    The name the resource in the resource store to use as the template for
    the dynamic CSS content and the name of the properties resource that
    provides the values to be inserted into that template. All are mandatory.
    -->
    <pc:property name="css-template-resource">ieg-css-template</pc:property>
    <pc:property name="hc-css-template-resource">ieg-hc-css-template</pc:property>
	<pc:property name="external-css-template-resource">ieg-external-css-template</pc:property>
    <pc:property name="css-template-resource-rtl">ieg-css-template-rtl</pc:property>
    <pc:property name="external-css-template-resource-rtl">ieg-external-css-template-rtl</pc:property>
    <pc:property name="external-hc-css-template-resource">ieg-external-hc-css-template</pc:property>
    <pc:property name="css-properties-resource">ieg-config-properties</pc:property>
    <pc:property name="custom-css-resource">custom.css.resource</pc:property>
    <pc:property name="custom-css-properties-resource">custom.css.properties.resource</pc:property>    
    <!--
    The relative URI of the main, static CSS files for the different text
    directions (left-to-right or right-to-left). The direction is chosen
    dynamically depending on the active user's locale.
    -->
    <pc:property name="css-main-ltr">../themes/cw/css/main.css</pc:property>
    <pc:property name="css-main-rtl">../themes/cw/css/main.css</pc:property>
    <!--
    The CSS class to apply to the "body" element of the HTML content. The
    class "rtl" will be added automatically if the text direction is
    right-to-left. This property defines an arbitrary additional class.
    -->
    <pc:property name="css-body-class">tundra</pc:property>
	<pc:property name="css-external-body-class">oneui</pc:property>

    <!--
    The name of the properties resource in the resource store to use for
    dynamic configuration and the names of the properties that will be used
    from that resource. The Player renderer will concatenate the system and
    application titles to produce the window title. All are optional.
    -->
    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="sys-title-prop">banner.systitle</pc:property>
    <pc:property name="app-title-prop">banner.apptitle</pc:property>
    <pc:property name="notes-title-prop">notes.panel.title.text</pc:property>
    <pc:property name="highlight-validation-prop">validation.highlight</pc:property>
    <pc:property name="nav-error-message-prop">navigation.error.message</pc:property>
    <pc:property name="nav-link-message-prop">navigation.link.message</pc:property>
    <pc:property name="session-timeout-error-message-prop">session.timeout.error.message</pc:property>
    <pc:property name="session-timeout-link-message-prop">session.timeout.link.message</pc:property>
    <pc:property name="session-timeout-link-url-prop">session.timeout.link.url</pc:property>
    <pc:property name="nav-update-message-display-prop">navigation.update.message.display</pc:property>
    <pc:property name="nav-update-message-prop">navigation.update.message</pc:property>
    <pc:property name="label-align-prop">label.align</pc:property>
    <pc:property name="back-label-prop">navpanel.button.back.text</pc:property>
    <pc:property name="exit-label-prop">navpanel.button.exit.text</pc:property>
    <pc:property name="quit-label-prop">navpanel.button.quit.text</pc:property>
    <pc:property name="next-label-prop">navpanel.button.next.text</pc:property>
    <pc:property name="link-skip">link.skip</pc:property>
    <pc:property name="form-content">form.content</pc:property>

    <!--
    Properties defining literal values to use if the above dynamic properties
    are not defined or cannot be resolved. All are mandatory.
    -->
    <pc:property name="sys-title">CURAM</pc:property>
    <pc:property name="app-title">CITIZEN WORKSPACE</pc:property>
    <pc:property name="notes-title">Notes</pc:property>
    <pc:property name="nav-error-message">Please do not use the browser back button.</pc:property>
    <pc:property name="nav-link-message">Click here to continue.</pc:property>
    <pc:property name="session-timeout-error-message">The session has expired. Please re-enter your login details.</pc:property>
    <pc:property name="session-timeout-link-message">Click here to continue.</pc:property>
    <pc:property name="session-timeout-link-url">IEG2_listAllIEG2Scripts</pc:property>
    <pc:property name="nav-update-message-display">false</pc:property>
    <pc:property name="nav-update-message">Due to answers that have just changed subsequent answers need to be verified. Some navigation options have now been disabled.</pc:property>
    
    <!--
    Property that defines how the sections panel will be displayed.
    -->
    <pc:property name="section-style-prop">sectionspanel.style</pc:property>
    
    <!--
    Properties that specify display in modal dialogs.
    -->
    <pc:property name="modal-anchor-nav-panel-prop">modal.anchor.nav.panel</pc:property>
    <pc:property name="modal-hide-title-panel-prop">modal.hide.title.panel</pc:property>
    <pc:property name="modal-width-prop">modal.width</pc:property>
    <pc:property name="modal-height-prop">modal.height</pc:property>
    
    <!--
    Properties that specify Region Roles to aid screen readers
    -->
    <pc:property name="region-role-complementary">region.role.complementary.value</pc:property>
    <pc:property name="region-role-main">region.role.main.value</pc:property>
    
    <!--
    Properties defining literal values to use if the above dynamic properties
    are not defined or cannot be resolved. All are mandatory.
    -->
    <pc:property name="back-label">Back</pc:property>
    <pc:property name="exit-label">Exit</pc:property>
    <pc:property name="quit-label">Quit</pc:property>
    <pc:property name="next-label">Next</pc:property>
    <pc:property name="app-scan-enabled">false</pc:property>
    <pc:property name="set-page-title-as-window-title">set.window.title.to.page.title</pc:property>
	<pc:property name="sections-width-prop">sectionspanel.width</pc:property>
    <pc:property name="section-desc-prev-prop">sectionspanel.desc.prev</pc:property>
    <pc:property name="section-desc-current-prop">sectionspanel.desc.current</pc:property>
    <pc:property name="section-max-display-prop">sectionspanel.horizontal.max.display</pc:property>
    <pc:property name="section-truncate-limit-prop">sectionspanel.horizontal.truncate.limit</pc:property>
    <pc:property name="section-arrow-move-amount-prop">sectionspanel.horizontal.arrow.move.amount</pc:property>
    <pc:property name="section-back-arrow-title-prop">sectionspanel.horizontal.back.arrow.title</pc:property>
    <pc:property name="section-next-arrow-title-prop">sectionspanel.horizontal.next.arrow.title</pc:property>
	
	<pc:property name="perform-transitions-prop">transitions.perform</pc:property>
	<pc:property name="progress-desc-prop">progress.bar.text</pc:property>    
	<pc:property name="desc-prop">progress.bar.text</pc:property>
  </pc:plug-in>

  <!--
  Edit renderer for code-table values. There are three configuration
  properties: "config-resource" specifies the name of a properties resource
  from the resource store to use as a source of dynamic configuration
  properties; "blank-entry-desc-prop" is the name of the property within that
  configuration resource to use as the source of the text to display for the
  entry in the code-table list that indicates no selection; "blank-entry-desc"
  is the literal text to use if the first two properties are not defined or if
  the indicated property does not exist. Only the latter property is mandatory.
  -->
  <pc:plug-in purpose="ieg-edit-ct-list" name="edit-renderer"
      class="curam.ieg.player.internal.IEGCTListEditRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="blank-entry-desc-prop">dropdown.list.blank.entry.description</pc:property>
    <pc:property name="blank-entry-desc">--Please Select--</pc:property>
    <pc:property name="dropdown-expand">dropdown.expand</pc:property>
    <pc:property name="layout-optimum-columns">multiselect.layout.optimum.columns</pc:property>
    <pc:property name="blank-entry-hidden-label">dropdown.list.blank.hidden.label</pc:property>
  </pc:plug-in>

  <!-- The renderer used for code-table hierarchy lists. -->
  <pc:plug-in purpose="ieg-edit-ct-hierarchy-local" name="edit-renderer"
      class="curam.ieg.player.internal.IEGCTHierarchyListEditRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="blank-entry-desc-prop">dropdown.list.blank.entry.description</pc:property>
	<pc:property name="dropdown-expand">dropdown.expand</pc:property>
  </pc:plug-in>

  <!--
  The above renderer cannot translate IEG2-format labels, so this renderer
  wraps it to perform that translation before delegating to it.
  -->
  <pc:plug-in purpose="ieg-edit-ct-hierarchy" name="edit-renderer"
      class="curam.ieg.player.internal.IEGTextTranslationEditRenderer">
    <pc:local-plug-in local-purpose="delegate" name="edit-renderer"
                      purpose="ieg-edit-ct-hierarchy-local"/>
  </pc:plug-in>

  <!--
  The entry point for rendering code-tables. This will decide if the code-table
  is in a hierarchy or not and delegate to other renderers as appropriate.
  -->
  <pc:plug-in purpose="ieg-edit-ct" name="edit-renderer"
      class="curam.util.client.domain.render.edit.CodeTableEditRenderer">
    <pc:local-plug-in local-purpose="code-table-list" name="edit-renderer"
                      purpose="ieg-edit-ct-list"/>
    <pc:local-plug-in local-purpose="code-table-hierarchy" name="edit-renderer"
                      purpose="ieg-edit-ct-hierarchy"/>
  </pc:plug-in>
  
  <!--
  The entry point for rendering three-field dates. This will set a dojoType
  property for external mode and delegate to the Birthdate renderer.
  -->
  <pc:plug-in purpose="three-field-date" name="edit-renderer"
      class="curam.util.client.domain.render.edit.BirthdateEditRenderer">
  </pc:plug-in>
  
  <pc:plug-in purpose="three-field-date-external" name="edit-renderer"
      class="curam.util.client.domain.render.edit.BirthdateEditRenderer">
	<pc:property name="dojoType">dijit.form.Select</pc:property>
  </pc:plug-in>
  
  <pc:plug-in purpose="ieg-3-field-date" name="edit-renderer"
      class="curam.ieg.player.internal.IEGThreeFieldDateEditRenderer">
    <pc:local-plug-in local-purpose="delegate" name="edit-renderer"
                      purpose="three-field-date"/>
    <pc:local-plug-in local-purpose="delegate-external" name="edit-renderer"
                      purpose="three-field-date-external"/>
  </pc:plug-in>
  
  <!--
  Edit renderer for text values.
  -->
  <pc:plug-in purpose="ieg-edit-text" name="edit-renderer"
              class="curam.ieg.player.internal.IEGTextEditRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>
  </pc:plug-in>
  
  <!--
  The basic renderer used for most domains: the formatted string value is
  just sent to the page.
  -->
  <pc:plug-in purpose="ieg-view-text" name="view-renderer"
              class="curam.ieg.player.internal.IEGTextViewRenderer"/>

  <pc:plug-in purpose="ieg-list-question" name="list-question-renderer"
              class="curam.ieg.player.IEGListQuestionRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="blank-entry-desc-prop">dropdown.list.blank.entry.description</pc:property>
    <pc:property name="blank-entry-desc">--Please Select--</pc:property>
    <pc:property name="list-dropdown-desc-prop">dropdown.list.description</pc:property>  
    <pc:property name="dropdown-expand">dropdown.expand</pc:property>
    <pc:property name="item-label-maxLength-entry">item.itemLabel.maxLength</pc:property>     
    <pc:property name="policy-label-prop">help.link.policy</pc:property>
    <pc:property name="policy-desc-prop">help.link.desc.policy</pc:property>
    <pc:property name="legislation-label-prop">help.link.legislation</pc:property>
    <pc:property name="legislation-desc-prop">help.link.desc.legislation</pc:property>
    <pc:property name="help-panel-close-prop">help.panel.close</pc:property>
    <pc:property name="close-help-link-title-prop">help.close.title</pc:property>    
    <pc:property name="compile-cluster-help-prop">compile.cluster.help</pc:property>   
    <pc:property name="more-info-desc-prop">help.link.desc.moreinfo</pc:property>
    <pc:property name="more-info-desc-cxt-prop">help.link.desc.cxt.moreinfo</pc:property>	
    <!--
      The values to use for alt texts on person images.
    -->	
    <pc:property name="persontabs-adult-male-alt">persontabs.adult.male.alt</pc:property>
    <pc:property name="persontabs-adult-female-alt">persontabs.adult.female.alt</pc:property>
    <pc:property name="persontabs-child-male-alt">persontabs.child.male.alt</pc:property>
    <pc:property name="persontabs-child-female-alt">persontabs.child.female.alt</pc:property>
  </pc:plug-in>
  
  <!--
  View renderer for Boolean values. This translates "true" and "false" values
  to more typical natural language values such as "Yes" and "No".
  -->
  <pc:plug-in purpose="ieg-view-boolean" name="view-renderer"
              class="curam.ieg.player.internal.IEGBooleanViewRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>

  </pc:plug-in>
  
  <!--
  Edit renderer for date values.
  -->
  <pc:plug-in purpose="ieg-edit-date" name="edit-renderer"
              class="curam.ieg.player.IEGDateRenderer">
    <pc:property name="config-resource">ieg-config-properties</pc:property>
  </pc:plug-in>
  
  <!--
  The renderer used for codetable list questions AKA question matrix.
  -->
  <pc:plug-in purpose="ieg-ct-list-question" name="view-renderer"
              class="curam.ieg.player.IEGCTListQuestionRenderer">

    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="policy-label-prop">help.link.policy</pc:property>
    <pc:property name="policy-desc-prop">help.link.desc.policy</pc:property>
    <pc:property name="legislation-label-prop">help.link.legislation</pc:property>
    <pc:property name="legislation-desc-prop">help.link.desc.legislation</pc:property>
    <pc:property name="help-panel-close-prop">help.panel.close</pc:property>
    <pc:property name="close-help-link-title-prop">help.close.title</pc:property>    
    <pc:property name="compile-cluster-help-prop">compile.cluster.help</pc:property>   
    <pc:property name="more-info-desc-prop">help.link.desc.moreinfo</pc:property>
    <pc:property name="more-info-desc-cxt-prop">help.link.desc.cxt.moreinfo</pc:property>
    <pc:property name="label-weight-prop">matrix.label.weight</pc:property>
  </pc:plug-in>

  <!--
  The renderer used for codetable questions with indented radio button layout.
  -->
  <pc:plug-in purpose="ieg-ct-radio" name="edit-renderer"
              class="curam.ieg.player.internal.IEGCTRadioRenderer">

    <pc:property name="config-resource">ieg-config-properties</pc:property>
    <pc:property name="policy-label-prop">help.link.policy</pc:property>
    <pc:property name="policy-desc-prop">help.link.desc.policy</pc:property>
    <pc:property name="legislation-label-prop">help.link.legislation</pc:property>
    <pc:property name="legislation-desc-prop">help.link.desc.legislation</pc:property>
    <pc:property name="help-panel-close-prop">help.panel.close</pc:property>
    <pc:property name="close-help-link-title-prop">help.close.title</pc:property>    
    <pc:property name="compile-cluster-help-prop">compile.cluster.help</pc:property> 
    <pc:property name="more-info-desc-prop">help.link.desc.moreinfo</pc:property>
    <pc:property name="more-info-desc-cxt-prop">help.link.desc.cxt.moreinfo</pc:property>
    <pc:property name="label-weight-prop">radio.label.weight</pc:property>
  </pc:plug-in>
</pc:plug-ins>
