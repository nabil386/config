<dc:domains
    xmlns:dc="http://www.curamsoftware.com/curam/util/common/domain-config">
  <!--
  Custom renderers used for the new IEG Player.
  -->
  <dc:domain name="IEG_ANSWERS">
    <dc:plug-in name="edit-renderer" purpose="ieg-player"/>
    <dc:plug-in name="marshal" class="curam.ieg.player.IEGPlayerMarshal"/>
  </dc:domain>

  <dc:domain name="IEG_RELATIONSHIPS">
    <dc:plug-in name="marshal"
                class="curam.ieg.player.IEGRelationshipsMarshal"/>
  </dc:domain>

  <!--
  The following domains are hardcoded into the player renderer at the moment.
  To be replaced when we get "real" types from CDS.
  -->
  <dc:domain name="IEG_STRING">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_PASSWORD">
    <dc:plug-in name="edit-renderer"
      class="curam.util.client.domain.render.edit.PasswordEditRenderer"/>
  </dc:domain>

  <dc:domain name="IEG_CHAR">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_BOOLEAN">   
    <dc:plug-in name="edit-renderer"
                class="curam.ieg.player.internal.IEGCheckboxEditRenderer"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-boolean"/>
  </dc:domain>

  <dc:domain name="IEG_INT8">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_INT16">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_INT32">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_INT64">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_FLOAT">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_DOUBLE">
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>

  <dc:domain name="IEG_MONEY"> 
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/> 
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/> 
  </dc:domain> 

  <dc:domain name="SVR_MONEY"> 
    <dc:plug-in name="converter" 
      class="curam.ieg.player.IEGMoneyConverter"/> 
  </dc:domain> 

  <dc:domain name="IEG_DATE">
    <dc:plug-in name="edit-renderer"  purpose="ieg-edit-date"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
    <dc:plug-in name="converter"
                class="curam.ieg.player.IEGDateConverter"/>
  </dc:domain>

  <dc:domain name="IEG_CODETABLE_CODE" plug-in-target="descendant">
    <dc:plug-in name="converter" purpose="convert-ct"/>
    <dc:plug-in name="comparator" purpose="compare-ct"/>
    <dc:plug-in name="default" purpose="default-value-ct"/>
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-ct"/>
    <dc:plug-in name="view-renderer" class="curam.ieg.player.internal.IEGCTViewRenderer"/>
  </dc:domain>

  <dc:domain name="IEG_LISTQUESTION">
    <dc:plug-in name="view-renderer" purpose="ieg-list-question"/>
  </dc:domain>
  
  <dc:domain name="IEG_CTLISTQUESTION">
    <dc:plug-in name="view-renderer" purpose="ieg-ct-list-question"/>
  </dc:domain>

  <dc:domain name="IEG_NON_FORMATTED_NUMBER">
    <dc:plug-in name="converter"
      class="curam.util.client.domain.convert.InternalIDConverter"/>
    <dc:plug-in name="edit-renderer" purpose="ieg-edit-text"/>
    <dc:plug-in name="view-renderer" purpose="ieg-view-text"/>
  </dc:domain>
  
  <dc:domain name="IEG_CT_RADIO">
    <dc:plug-in name="edit-renderer" purpose="ieg-ct-radio"/>
  </dc:domain>

  <dc:domain name="IEG_THREE_FIELD_DATE">
    <dc:plug-in name="edit-renderer" purpose="ieg-3-field-date"/>
    <dc:plug-in name="marshal" class="curam.util.client.domain.marshal.BirthdateMarshal"/>
	<dc:plug-in name="converter" class="curam.util.client.domain.convert.BirthdateConverter"/>
  </dc:domain>

</dc:domains>
