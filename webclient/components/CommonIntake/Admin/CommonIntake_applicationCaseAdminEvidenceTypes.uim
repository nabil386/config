<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page lists the evidence types associated to an application case   -->
<!-- administration.                                                        -->
<PAGE PAGE_ID="CommonIntake_applicationCaseAdminEvidenceTypes"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
   
  <SERVER_INTERFACE
    CLASS="EvidenceTypesLink"
    NAME="DISPLAY"
    OPERATION="listEvidenceLinks"
    PHASE="DISPLAY"
  />

  <PAGE_PARAMETER NAME="applicationCaseAdminID"/>
  
 <CONNECT>
   <SOURCE NAME="PAGE" PROPERTY="applicationCaseAdminID" />
   <TARGET NAME="DISPLAY" PROPERTY="key$caseAdminID" />
 </CONNECT>
  
 <CONNECT>
   <SOURCE NAME="CONSTANT" PROPERTY="Constant.ApplicationCaseType" />
   <TARGET NAME="DISPLAY" PROPERTY="key$caseType" />
 </CONNECT>  
  
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.AddEvidenceType">
      <LINK PAGE_ID="CommonIntake_applicationCaseAdminAddEvidenceType" OPEN_MODAL="true">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="applicationCaseAdminID"/>
          <TARGET NAME="PAGE" PROPERTY="applicationCaseAdminID"/>
        </CONNECT>      
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET> 

  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">    
      <ACTION_CONTROL LABEL="ActionControl.Label.Modify" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="CaseConfiguration_modifyEvidenceType">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="applicationCaseAdminID" />
            <TARGET NAME="PAGE" PROPERTY="caseAdminID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="CONSTANT" PROPERTY="Constant.ApplicationCaseType" />
            <TARGET NAME="PAGE" PROPERTY="caseType" />
          </CONNECT>          
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeLinkID" />
            <TARGET NAME="PAGE" PROPERTY="evidenceTypeLinkID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeCode" />
            <TARGET NAME="PAGE" PROPERTY="evidenceTypeCode" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo" />
            <TARGET NAME="PAGE" PROPERTY="versionNo" />
          </CONNECT>
        </LINK> 
      </ACTION_CONTROL>          
           
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete" TYPE="ACTION" >
        <LINK OPEN_MODAL="true"  PAGE_ID="CaseConfiguration_deleteEvidenceType" >
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeLinkID" />
            <TARGET NAME="PAGE" PROPERTY="evidenceTypeLinkID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="applicationCaseAdminID" />
            <TARGET NAME="PAGE" PROPERTY="associatedID" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="CONSTANT" PROPERTY="Constant.ApplicationCaseType" />
            <TARGET NAME="PAGE" PROPERTY="associatedCaseTypeOpt" />
          </CONNECT>           
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeCode" />
            <TARGET NAME="PAGE" PROPERTY="evidenceTypeCode" />
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo" />
            <TARGET NAME="PAGE" PROPERTY="versionNo" />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    
    <FIELD LABEL="Field.Label.Type" WIDTH="30" >
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeCode" />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Category.Field.Label.Type" WIDTH="30" >
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="category"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="SortOrder.Field.Label.Type" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="sortOrder"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="QuickLink.Field.Label.Type" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="quickLinkInd" />
      </CONNECT>
    </FIELD>        
  </LIST>
  
</PAGE>
