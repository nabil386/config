<!-- This page displays the list of Application Case Administration Records.    -->
<PAGE PAGE_ID="CommonIntake_listApplicationCaseAdmin"
  HIDE_CONDITIONAL_LINKS="false"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <SERVER_INTERFACE CLASS="ApplicationCaseAdmin" NAME="DISPLAY" OPERATION="listActiveApplicationCaseAdmin"
    PHASE="DISPLAY"/>

   <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <LINK PAGE_ID="CommonIntake_createApplicationCaseAdmin" OPEN_MODAL="true"/>
    </ACTION_CONTROL>
  </ACTION_SET>

   <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK PAGE_ID="CommonIntake_editApplicationCaseAdmin" OPEN_MODAL="true">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="applicationCaseAdminID"/>
            <TARGET NAME="PAGE" PROPERTY="applicationCaseAdminID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete" TYPE="ACTION">
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="deleteEnabledInd" />
        </CONDITION>
        <LINK OPEN_MODAL="true" WINDOW_OPTIONS="width=550"
          PAGE_ID="CommonIntake_cancelApplicationCaseAdmin">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="applicationCaseAdminID"/>
            <TARGET NAME="PAGE" PROPERTY="applicationCaseAdminID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$list$dtls$versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>

    <FIELD LABEL="Field.Label.Name" WIDTH="30">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
      <LINK PAGE_ID="CommonIntake_applicationCaseAdminHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="applicationCaseAdminID"/>
          <TARGET NAME="PAGE" PROPERTY="applicationCaseAdminID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Description" WIDTH="70">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
