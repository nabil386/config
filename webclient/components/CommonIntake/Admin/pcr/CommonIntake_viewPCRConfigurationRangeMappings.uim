<!-- Displays the details of a PCR Configuration and a tab to view all      -->
<!-- range mappings for the configuration.                                  -->
<PAGE
  PAGE_ID="CommonIntake_viewPCRConfigurationRangeMappings"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_PARAMETER NAME="pcrConfigurationID"/>

  <SERVER_INTERFACE
    CLASS="PCRAdmin"
    NAME="DISPLAY"
    OPERATION="listPCRRangeMapping"
  />

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="pcrConfigurationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$pcrConfigurationID"
    />
  </CONNECT>

  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CommonIntake_viewPCRConfiguration"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>

    <ACTION_CONTROL
      LABEL="ActionControl.Label.RangeMapping"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="PCRAdmin_viewPCRConfigurationRangeMappings"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pcrConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>

  <CLUSTER
    NUM_COLS="1"
    STYLE="outer-cluster-borderless"
  >
    <LIST
      PAGINATED="false"
      
    >

      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="PCRAdmin_modifyPCRRangeMapping"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="pcrRangeMappingID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pcrRangeMappingID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="versionNo"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>

        <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="PCRAdmin_deletePCRRangeMapping"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="pcrRangeMappingID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pcrRangeMappingID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="pcrConfigurationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pcrConfigurationID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>

      <FIELD LABEL="Field.Title.Type" WIDTH="20">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="type"
          />
        </CONNECT>
      </FIELD>

      <FIELD LABEL="Field.Title.Classification" WIDTH="32">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="classification"
          />
        </CONNECT>
      </FIELD>

      <FIELD LABEL="Field.Title.RangeStart" WIDTH="26">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="rangeStart"
          />
        </CONNECT>
      </FIELD>

      <FIELD LABEL="Field.Title.RangeEnd" WIDTH="22">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="rangeEnd"
          />
        </CONNECT>
      </FIELD>
    </LIST>

  </CLUSTER>
</PAGE>
