<!-- Popup configuration file for the web client application.                        -->
<APP_CONFIG>
    <FILE_DOWNLOAD_CONFIG>
        <FILE_DOWNLOAD
            CLASS="curam.interfaces.MeetingMinutesManagementPkg.MeetingMinutesManagement_generatePDF_TH"
            PAGE_ID="ApplicationCase_listMeetingMinutes">
            <INPUT PAGE_PARAM="meetingID" PROPERTY="key$meetingID"/>
            <FILE_NAME PROPERTY="result$fileName"/>
            <FILE_DATA PROPERTY="result$data"/>
        </FILE_DOWNLOAD>
        <FILE_DOWNLOAD
            CLASS="curam.interfaces.MeetingMinutesManagementPkg.MeetingMinutesManagement_generatePDF_TH"
            PAGE_ID="ApplicationCase_listEvent">
            <INPUT PAGE_PARAM="meetingID" PROPERTY="key$meetingID"/>
            <FILE_NAME PROPERTY="result$fileName"/>
            <FILE_DATA PROPERTY="result$data"/>
        </FILE_DOWNLOAD>
        <FILE_DOWNLOAD
            CLASS="curam.interfaces.CommunicationPkg.Communication_printCommunication_TH"
            PAGE_ID="CommonIntake_listApplicationCommunications">
            <INPUT PAGE_PARAM="communicationID" PROPERTY="communicationIDKey$communicationID"/>
            <FILE_DATA PROPERTY="result$fileContent"/>
            <FILE_NAME PROPERTY="result$fileName"/>
        </FILE_DOWNLOAD>			
    </FILE_DOWNLOAD_CONFIG>
    
    <POPUP_PAGES>
        <POPUP_PAGE PAGE_ID="CommonIntake_applicationCaseAdminVerificationSearchPopup">
            <DOMAIN>VCT27000</DOMAIN>
            <CT_CODE>VCT27000</CT_CODE>
            <WIDTH>800</WIDTH>
            <HEIGHT>600</HEIGHT>
            <SCROLLBARS>true</SCROLLBARS>
            <IMAGE>themes/v6/images/search--20-enabled.svg</IMAGE>
            <LABEL_PROPERTY>label.search</LABEL_PROPERTY>
        </POPUP_PAGE>
    </POPUP_PAGES>
</APP_CONFIG>
