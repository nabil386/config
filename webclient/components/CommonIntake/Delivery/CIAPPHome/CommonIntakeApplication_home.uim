<!-- Home page for the common intake application view                     -->
<PAGE
  PAGE_ID="CommonIntakeApplication_home"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

<PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <!-- Announcements -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ANNOUNCEMENTDISPLAY"
    OPERATION="getAnnouncementsAsXML"
  />
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="announcements-cluster"
    WIDTH="100"
    >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="ANNOUNCEMENTDISPLAY"
          PROPERTY="result$contentXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <!--End Announcements -->
      
  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="CIAPPPODPAGE"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageID$pageID"
    />
  </CONNECT>


  <INCLUDE FILE_NAME="PodContainer.vim"/>  

</PAGE>
