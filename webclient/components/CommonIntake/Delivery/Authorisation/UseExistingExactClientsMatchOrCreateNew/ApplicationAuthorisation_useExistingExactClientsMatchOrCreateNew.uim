<!-- This page is used for Application Authorization for the strategy.  -->

<PAGE
  PAGE_ID="ApplicationAuthorisation_useExistingExactClientsMatchOrCreateNew"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="caseID"/>

   <SERVER_INTERFACE 
    CLASS="ApplicationCase" 
    NAME="DISPLAY3" 
    OPERATION="checkAnyPendingCases"  
    PHASE="DISPLAY"
  />
  

  <SERVER_INTERFACE
    CLASS="ApplicationAuthorisation"
    NAME="DISPLAY"
    OPERATION="listClientsExistingCases"
    PHASE="DISPLAY"
  />

  <SERVER_INTERFACE
    CLASS="ApplicationAuthorisation"
    NAME="ACTION"
    OPERATION="authorise"
    PHASE="ACTION"
  />

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY3"  PROPERTY="key$caseID" />
  </CONNECT>

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseKey$applicationCaseID"
    />
  </CONNECT>

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationCaseID"
    />
  </CONNECT>

	<INFORMATIONAL>
     <CONNECT>
        <SOURCE NAME="DISPLAY3" PROPERTY="result$informationalDtls$dtls$informationMsgTxt"/>
     </CONNECT>
   </INFORMATIONAL>

 <INCLUDE FILE_NAME="Authorisation_useExistingExactClientsMatchOrCreateNewView.vim"/>
 
</PAGE>


