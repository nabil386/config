<PAGE
  PAGE_ID="CommonIntake_selectMatchConfirmation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=580">
 
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
   
   <SERVER_INTERFACE
    CLASS="ApplicationCase"
    NAME="ACTION"
    OPERATION="searchOrMatchClient"
    PHASE="ACTION"
  />
  
  <PAGE_PARAMETER NAME="origionalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="origionalConcernRoleID" />
    <TARGET NAME="ACTION" PROPERTY="key$concernRoleID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="duplicateConcernRoleID" />
    <TARGET NAME="ACTION" PROPERTY="key$duplicateConcernRoleID" />
  </CONNECT>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID" />
    <TARGET NAME="ACTION" PROPERTY="key$caseID" />
  </CONNECT>
  
    <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    >
    </ACTION_CONTROL>
  </ACTION_SET>

  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD LABEL="Field.Label.MatchClient">
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.MatchClient"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
</PAGE>
