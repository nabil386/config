<PAGE PAGE_ID="CommonIntake_createReferralApplicationCaseForConcernRole"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" WINDOW_OPTIONS="width=975">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE CLASS="ApplicationCase" NAME="DISPLAY" OPERATION="readReferralCreateDetails" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="ApplicationCase" NAME="ACTION" OPERATION="createReferralApplicationCaseForConcernRole" PHASE="ACTION"/>

  <PAGE_PARAMETER NAME="concernRoleID"/>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
    <TARGET NAME="ACTION" PROPERTY="concernRoleID"/>
  </CONNECT>

  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="CommonIntake_resolveApplicationCaseHomepage">
        <CONNECT>
            <SOURCE NAME="ACTION" PROPERTY="result$applicationCaseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
  
  <CLUSTER NUM_COLS="2">
    <!-- Col 1 -->
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <INITIAL NAME="DISPLAY" PROPERTY="name" HIDDEN_PROPERTY="applicationCaseAdminID"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="applicationCaseAdminID"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MethodOfApplication" USE_DEFAULT="false">
      <CONNECT>
        <INITIAL NAME="DISPLAY" PROPERTY="codeTableItemDetails$description"  HIDDEN_PROPERTY="codeTableItemDetails$code"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="methodOfApplication"/>
      </CONNECT>
    </FIELD>	
    <FIELD LABEL="Field.Label.ReferralDate" USE_DEFAULT="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="applicationDate"/>
      </CONNECT>
    </FIELD>	
    <FIELD LABEL="Field.Label.PreferredContact" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="preferredContact"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
 <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.ReferralSource">  
    <FIELD LABEL="Field.Label.ReferralSource">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="referralSource"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ReferredBy">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="referredBy"/>
      </CONNECT>
    </FIELD>		

    <FIELD LABEL="Field.Label.ReferralSourceDescription">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="referralSourceDescription"/>
      </CONNECT>
    </FIELD>	
	<FIELD LABEL="Field.Label.ReferralReason" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="referralReason"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <FIELD LABEL="Cluster.Title.Comments" HEIGHT="4">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
</PAGE>
