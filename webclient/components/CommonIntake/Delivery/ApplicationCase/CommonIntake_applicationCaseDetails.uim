<!-- This page allows users to search for an Application Case.                           -->
<PAGE PAGE_ID="CommonIntake_applicationCaseDetails" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <SERVER_INTERFACE 
    CLASS="ApplicationCase" 
    NAME="DISPLAY" 
    OPERATION="readCaseDetails"  
    PHASE="DISPLAY"
  />
  
  <PAGE_PARAMETER NAME="applicationCaseID"/>
  
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="applicationCaseID"/>
    <TARGET NAME="DISPLAY"  PROPERTY="applicationCaseID" />
  </CONNECT>
  
  <CLUSTER NUM_COLS="2" LABEL_WIDTH="50">
    <CONDITION>
       <IS_TRUE NAME="DISPLAY" PROPERTY="timerInd"/>
    </CONDITION>
    
    <FIELD LABEL="Field.Label.ReceivedDateTime">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="referralDateTime"/>
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseTypeList"/>
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.CompletionDate">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="completionDate"/>
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.Label.MethodOfReceipt">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="methodOfApplication"/>
      </CONNECT>
    </FIELD>

    <FIELD LABEL="Field.Label.ExpiryDate">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="expiryDate"/>
      </CONNECT>
    </FIELD>

  </CLUSTER>


  <CLUSTER NUM_COLS="2" LABEL_WIDTH="50">
     <CONDITION>
       <IS_FALSE NAME="DISPLAY" PROPERTY="timerInd"/>
    </CONDITION>

    <FIELD LABEL="Field.Label.ReceivedDateTime">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="referralDateTime"/>
      </CONNECT>
    </FIELD>
 
     <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseTypeList"/>
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.MethodOfReceipt">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="methodOfApplication"/>
      </CONNECT>
    </FIELD>

  </CLUSTER>


</PAGE>