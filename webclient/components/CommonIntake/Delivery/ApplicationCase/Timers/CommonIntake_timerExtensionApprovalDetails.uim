<!-- Timer Extension approval Details                                       -->
<PAGE
  PAGE_ID="CommonIntake_timerExtensionApprovalDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="milestoneWaiverRequestID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneWaiverRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$milestoneWaiverRequestID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="ApplicationCaseTimer"
    NAME="DISPLAY"
    OPERATION="showApprovalDetails"
  />


  <CLUSTER>


    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$isEnableCluster"
      />
    </CONDITION>


    <CLUSTER
      NUM_COLS="2" >
      <FIELD
        LABEL="Field.Label.ApprovalDate"
        WIDTH="50"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$approvedDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.ApprovedBy"
        WIDTH="50"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$approvedBy"
          />
        </CONNECT>
          <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
        WINDOW_OPTIONS="width=900"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$approvedBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
      </FIELD>
    </CLUSTER>


    <CLUSTER
       NUM_COLS="1" TITLE="Field.Label.ApprovalComments" SHOW_LABELS="false"
    >
      <FIELD WIDTH="95">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$approvalComments"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


</PAGE>
