<!-- Extend the program timer          -->
<PAGE PAGE_ID="CommonIntake_extendTimers" WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="milestoneDeliveryID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="milestoneDeliveryID" />
    <TARGET NAME="ACTION" PROPERTY="key$milestoneDeliveryID" />
  </CONNECT>
  <SERVER_INTERFACE CLASS="ApplicationCaseTimer" NAME="ACTION"
    OPERATION="extendTimer" PHASE="ACTION" />


  <CLUSTER LABEL_WIDTH="55">
    <FIELD LABEL="Field.Label.DateExtended" USE_DEFAULT="false" WIDTH="45">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="key$requestedExpiryDate" />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER NUM_COLS="1" SHOW_LABELS="false" TITLE="Field.Label.Reason">
    <FIELD HEIGHT="5" LABEL="Field.Label.Reason">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="key$reason" />
      </CONNECT>
    </FIELD>
  </CLUSTER>

  <ACTION_SET ALIGNMENT="RIGHT" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" />
  </ACTION_SET>

</PAGE>
