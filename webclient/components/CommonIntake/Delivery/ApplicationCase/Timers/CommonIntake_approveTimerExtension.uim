<!-- Approves the timer extension    -->
<PAGE
  PAGE_ID="CommonIntake_approveTimerExtension"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="milestoneWaiverRequestID"/>


  <SERVER_INTERFACE
    CLASS="ApplicationCaseTimer"
    NAME="ACTION"
    OPERATION="approveTimerExtension"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneWaiverRequestID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$milestoneWaiverRequestID"
    />
  </CONNECT>


  <CLUSTER
    LAYOUT_ORDER="FIELD"
    SHOW_LABELS="false"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Label.Question"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Field.Label.ApprovalComment"
  >
    <FIELD HEIGHT="5" LABEL="Field.Label.ApprovalComment">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$approvalComments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="RIGHT">
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    
      
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


</PAGE>
