    <FIELD LABEL="Field.Label.Date" WIDTH="26" ALIGNMENT="LEFT">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="authorisationDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.IntegratedCase" WIDTH="26">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="integratedCaseReference"/>
