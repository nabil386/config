  <LIST>
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="planOnlyInd"/>
    </CONDITION>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CommonIntake_listProgramAuthorisationDetails">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="programAuthorisationDataID" />
          <TARGET NAME="PAGE" PROPERTY="programAuthorisationDataID" />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <FIELD LABEL="Field.Label.Date" WIDTH="39" ALIGNMENT="LEFT">
