        </CONNECT>
      </LINK>
    </FIELD>
  </LIST> 
  
  <LIST>
    <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="productDeliveryIntegratedCaseInd"/>
    </CONDITION>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CommonIntake_listProgramAuthorisationDetails">
