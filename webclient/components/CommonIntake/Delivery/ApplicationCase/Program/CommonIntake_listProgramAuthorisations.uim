      <IS_TRUE NAME="DISPLAY" PROPERTY="integratedCaseOnlyInd"/>
    </CONDITION>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="CommonIntake_listProgramAuthorisationDetails">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="programAuthorisationDataID" />
          <TARGET NAME="PAGE" PROPERTY="programAuthorisationDataID" />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <FIELD LABEL="Field.Label.Date" WIDTH="39" ALIGNMENT="LEFT">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="authorisationDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.IntegratedCase" WIDTH="39">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="integratedCaseReference"/>
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="integratedCaseID"/>
