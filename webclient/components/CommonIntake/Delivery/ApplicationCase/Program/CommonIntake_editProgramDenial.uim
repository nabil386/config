 <PAGE PAGE_ID="CommonIntake_editProgramDenial"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
     WINDOW_OPTIONS="width=680">
    
     <SERVER_INTERFACE CLASS="ProgramAuthorisation" NAME="DISPLAY" OPERATION="readProgramDenialDetails"
        PHASE="DISPLAY"/>
     <SERVER_INTERFACE CLASS="ProgramAuthorisation" NAME="ACTION" OPERATION="modifyProgramDenial"
        PHASE="ACTION"/>
    
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
        </CONNECT>
    </PAGE_TITLE>
    
    <PAGE_PARAMETER NAME="programDenialID"/>

    
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="programDenialID"/>
        <TARGET NAME="DISPLAY" PROPERTY="key$programDenialID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
        <TARGET NAME="ACTION" PROPERTY="versionNo"/>
    </CONNECT>
    
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="programDenialID"/>
        <TARGET NAME="ACTION" PROPERTY="dtls$programDenialID"/>
    </CONNECT>
    
    <ACTION_SET >
        <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>

        <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
    </ACTION_SET>
    
   
     <CLUSTER NUM_COLS="1" LABEL_WIDTH="50">
        
        <FIELD LABEL="Field.Label.DenialReason">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="reason"/>
            </CONNECT>
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="reason"/>
            </CONNECT>
        </FIELD>
    </CLUSTER> 
        <CLUSTER TITLE="Cluster.Title.Comments" SHOW_LABELS="false">
            <FIELD LABEL="Cluster.Title.Comments">
                <CONNECT>
                    <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
                </CONNECT>
                <CONNECT>
                    <TARGET NAME="ACTION" PROPERTY="comments"
                    />
                </CONNECT>
            </FIELD>
        </CLUSTER>
       

   
</PAGE>

