<!-- Lists the evidecne used as part of an authorisations event.          -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="CommonIntake_listProgramAuthorisationDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

 <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>

  <PAGE_PARAMETER NAME="programAuthorisationDataID"/>

  <SERVER_INTERFACE CLASS="ProgramAuthorisation" NAME="DISPLAY" OPERATION="listAuthorisationDetails"  PHASE="DISPLAY"/>

  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="programAuthorisationDataID"/>
    <TARGET NAME="DISPLAY"  PROPERTY="programAuthorisationDataID" />
  </CONNECT>
  
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL LABEL="Menu.Details" STYLE="in-page-current-link">
      <LINK PAGE_ID="CommonIntake_listProgramAuthorisationDetails" >
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programAuthorisationDataID"/>
          <TARGET NAME="PAGE" PROPERTY="programAuthorisationDataID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="Menu.Evidence" STYLE="in-page-link">
      <LINK PAGE_ID="CommonIntake_listProgramAuthorisationEvidence" >
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="programAuthorisationDataID"/>
          <TARGET NAME="PAGE" PROPERTY="programAuthorisationDataID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  
  <CLUSTER NUM_COLS="2">
     <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="bothOPPDClients"/>
    </CONDITION>
     <FIELD LABEL="Field.Label.ProductDeliveryClients">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="productDeliveryClients"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PlanClients">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="outcomePlanClients"/>
      </CONNECT>
    </FIELD>    
  </CLUSTER>

<CLUSTER  NUM_COLS="2">
     <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="onlyPDClients"/>
    </CONDITION>
     <FIELD LABEL="Field.Label.ProductDeliveryClients">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="productDeliveryClients"/>
      </CONNECT>
    </FIELD>    
  </CLUSTER>


<CLUSTER  NUM_COLS="2">
     <CONDITION>
      <IS_TRUE NAME="DISPLAY" PROPERTY="onlyOPClients"/>
    </CONDITION>
    <FIELD LABEL="Field.Label.PlanClients">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="outcomePlanClients"/>
      </CONNECT>
    </FIELD>    
 </CLUSTER>


  <CLUSTER TITLE="Field.Label.Comments" SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>

</PAGE>


