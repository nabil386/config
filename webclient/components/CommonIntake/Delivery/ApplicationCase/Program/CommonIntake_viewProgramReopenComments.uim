<!-- View the Comments for a Reopen record                                  -->
<PAGE
  PAGE_ID="CommonIntake_viewProgramReopenComments"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="programReopenInfoID"/>


  <SERVER_INTERFACE
    CLASS="ApplicationCaseProgram"
    NAME="DISPLAY"
    OPERATION="readReopenDetailsForModify"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="programReopenInfoID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programReopenInfoID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Cluster.Title.ReopeningComments"
  >
    <FIELD HEIGHT="5">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
