<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to search for the employer that the person -->
<!-- has previously been registered as.                                   -->
<PAGE
  PAGE_ID="Appeal_createThirdParty"
  WINDOW_OPTIONS="width=900"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ThirdParty"
    NAME="ACTION"
    OPERATION="addThirdParty"
    PHASE="ACTION"
  />


  <!-- BEGIN, CR00266910, MC -->
  <SERVER_INTERFACE
    CLASS="ThirdParty"
    NAME="DISPLAY"
    OPERATION="populateParticipantList"
    PHASE="DISPLAY"
  />
  <!-- END, CR00266910 -->


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>
  <CLUSTER NUM_COLS="1">
    <!-- BEGIN, CR00266910, MC -->
    <CLUSTER
      DESCRIPTION="Cluster.Title.Description1"
      LABEL_WIDTH="30"
      NUM_COLS="1"
    >
      <CONTAINER LABEL="Field.Label.ThirdParty">
        <FIELD
          LABEL="Field.Label.ThirdParty"
          WIDTH="45"
        >
          <CONNECT>
            <INITIAL
              HIDDEN_PROPERTY="participantType"
              NAME="DISPLAY"
              PROPERTY="participantDescription"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="thirdPartyType"
            />
          </CONNECT>
        </FIELD>
        <FIELD
          LABEL="Field.Label.ThirdParty"
          WIDTH="50"
        >
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
      <!-- END, CR00266910 -->
    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.Title.Description2"
      LABEL_WIDTH="60"
      NUM_COLS="2"
    >
      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="thirdPartyName"
          />
        </CONNECT>
      </FIELD>


      <FIELD CONTROL="SKIP"/>


    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="60"
      NUM_COLS="2"
    >
      <!-- BEGIN, CR00198210, PS -->
      <FIELD LABEL="Field.Label.Address">
        <!-- END, CR00198210 -->
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="addressData"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="60"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD
        LABEL="Field.Label.PhoneAreaCode"
        WIDTH="50"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneAreaCode"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Phone">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneNumber"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <CLUSTER
    DESCRIPTION="Cluster.Title.Description3"
    LABEL_WIDTH="60"
    NUM_COLS="2"
    STYLE="grouped-cluster"
  >
    <FIELD LABEL="Field.Label.AttendanceRequired">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="attendanceRequired"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>


  <!--END CR00080895-->
  <!--END CR00095084-->
  <!-- END, CR00248337 -->


  <CLUSTER
    LABEL_WIDTH="30"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00198210, CR00407812, PS, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00198210, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
