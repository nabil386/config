<!-- This page is used to display a calendar for a Judicial Review on an     -->
<!-- integrated case.                                                       -->
<PAGE
  PAGE_ID="Appeal_judicialReviewCalendarForIC"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

    <!-- BEGIN, CR00267858, MC -->
    <MENU MODE="IN_PAGE_NAVIGATION">
  
  
      <ACTION_CONTROL
        LABEL="ActionControl.Label.ViewCalendar"
        STYLE="in-page-current-link"
      >
        <LINK
          PAGE_ID="Appeal_resolveJudicialReviewCalendarForIC"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.ViewEvents"
        STYLE="in-page-link"
      >
        <LINK PAGE_ID="Appeal_listJudicialReviewEventForIC">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
  
  
    </MENU>
  <!-- END, CR00267858 -->

  <!-- BEGIN, CR00125204, RKi -->
  <PAGE_PARAMETER NAME="caseID"/>
  <!-- END, CR00125204 -->

  <!-- BEGIN, CR00236544, AK -->
  <INCLUDE FILE_NAME="ICProduct_eventCalendarView1.vim"/>
  <!-- END, CR00236544 -->

</PAGE>
