<!-- This page allows the user to close a judicial review case              -->
<?curam-deprecated Since Curam 6.0 SP2, replaced with Appeal_closeJudicialReview1. As
  part of the change to Appeal_closeJudicialReview all pages relating to Appeal_closeJudicialReview have been replaced. See release note: CR00301189?>
<PAGE
  PAGE_ID="Appeal_closeJudicialReview"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="JudicialReview"
    NAME="ACTION"
    OPERATION="close"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="hearingDecisionID"/>
  <PAGE_PARAMETER NAME="decisionVersionNo"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingDecisionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="hearingDecisionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionVersionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="decisionVersionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.CloseCase"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
