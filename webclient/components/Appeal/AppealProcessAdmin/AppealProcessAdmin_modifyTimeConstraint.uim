<!-- Modifys an Appeal Stage Time Constraint                                -->
<PAGE
  PAGE_ID="AppealProcessAdmin_modifyTimeConstraint"
  WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="appealStageTimeConstraintID"/>
  <PAGE_PARAMETER NAME="caseType"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageTimeConstraintID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$appealStageTimeConstraintID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageTimeConstraintID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="appealStageTimeConstraintID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="recordStatus"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="recordStatus"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="relatedAppealStage"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="relatedAppealStage"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="DISPLAY"
    OPERATION="readTimeConstraint"
  />


  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="ACTION"
    OPERATION="modifyTimeConstraint"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="RemoveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="35">
    <FIELD
      LABEL="Field.Label.NumberDays"
      WIDTH="3"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="numberOfDays"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="numberOfDays"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ConstraintType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="constraintTypeCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="constraintTypeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.FromDate"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fromDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
