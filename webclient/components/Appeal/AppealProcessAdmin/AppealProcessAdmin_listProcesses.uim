<!-- List all of the Appeal Processes.                                      -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="AppealProcessAdmin_listProcesses"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="DISPLAY"
    OPERATION="listAppealProcesses"
  />


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="AppealProcessAdminWizard_selectStages"
      >
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="wizardStateID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="wizardStateID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.NewStage">


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AppealProcessAdmin_createAppealStage"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appealProcessID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealProcessID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseTypeCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseType"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.EditTimeConstraints">


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AppealProcessAdmin_modifyAllTimeConstraints"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appealProcessID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealProcessID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseTypeCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseType"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <SEPARATOR/>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="deleteProcessEnabledInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AppealProcessAdmin_cancelAppealProcess"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appealProcessID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealProcessID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseTypeCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseType"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="AppealProcessAdmin_listAppealProcessTimeConstraints">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appealProcessID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealProcessID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseTypeCode"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseType"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <FIELD
      LABEL="Field.Label.CaseCatType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseConfigurationName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CaseType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseTypeCode"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00360130, PS -->
    <FIELD
      LABEL="Field.Label.AppealStages"
      WIDTH="23"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appealStageList"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="22"
    >
      <!-- END, CR00360130 -->


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.RecordStatus"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>
</PAGE>
