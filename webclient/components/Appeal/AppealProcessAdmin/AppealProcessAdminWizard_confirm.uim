<!-- Appeal Process Administration Wizard.                                  -->
<!-- Display the summary details for the appeal process.                    -->
<PAGE
  PAGE_ID="AppealProcessAdminWizard_confirm"
  WINDOW_OPTIONS="width=900,height=480"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="DISPLAYWIZARD"
    OPERATION="getAppealProcessWizard"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="DISPLAY"
    OPERATION="getAppealProcessConfirmDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="ACTION"
    OPERATION="createAppealProcess"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="wizardStateID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="wizardStateID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="wizardStateID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="wizardStateID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="wizardStateID"
    />
  </CONNECT>
  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>
  <ACTION_SET
    BOTTOM="true"
    TOP="true"
  >
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Back">
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="AppealProcessAdminWizard_assignCaseTypes"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="wizardStateID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="wizardStateID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
  </ACTION_SET>
  <LIST TITLE="Cluster.Title.AppealStages">
    <FIELD
      LABEL="Field.Label.Stage"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="stage"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.TimeConstraints"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="timeConstraints"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <LIST TITLE="Cluster.Title.CaseTypes">
    <FIELD
      LABEL="Field.Label.CaseName"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseConfigurationName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CaseCategory"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseType"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
