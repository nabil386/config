<!-- Creates an Appeal Stage                                                -->
<PAGE
  PAGE_ID="AppealProcessAdmin_modifyAppealStage"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="DISPLAY"
    OPERATION="readStage"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="AppealProcessAdmin"
    NAME="ACTION"
    OPERATION="modifyStage"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="appealStageID"/>
  <PAGE_PARAMETER NAME="stageSequence"/>
  <PAGE_PARAMETER NAME="caseType"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="appealStageConfID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseType"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$appealStageConfID"
    />
  </CONNECT>


  <CLUSTER>
    <FIELD LABEL="Field.Label.AppealStage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appealTypeCode"
        />
      </CONNECT>


      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="appealTypeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Order">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="stageSequence"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
