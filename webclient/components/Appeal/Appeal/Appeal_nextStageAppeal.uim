<!-- This page allows the user to select the appellant and respondent for   -->
<!-- the appeal                                                             -->
<!-- BEGIN, CR00369210, PS -->
<PAGE
  PAGE_ID="Appeal_nextStageAppeal"
  WINDOW_OPTIONS="width=1025"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00369210 -->


  <INCLUDE FILE_NAME="Appeal_newAppealView.vim"/>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="appealTypeCode"/>
  <PAGE_PARAMETER NAME="priorAppealID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealTypeCode"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseIDpriorAppealID$appealType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="priorAppealID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseIDpriorAppealID$priorAppealID"
    />
  </CONNECT>


</PAGE>
