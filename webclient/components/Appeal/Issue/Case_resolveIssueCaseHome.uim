<!-- This jsp redirects the user to a Case Home Page.-->
<!-- Each case type value is matched to a Home Page name.-->
<PAGE
  PAGE_ID="Case_resolveIssueCaseHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
          
      
      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String caseID = request.getParameter("caseID");
      String homePageName="";
      String url = "";
  
      if (caseID == null) {
        // BEGIN, CR00126196, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "caseID");
       throw e;
       // END, CR00126196
      }
      
      curam.interfaces.AppealPkg.Appeal_resolveCaseHomePageName_TH
        th = new curam.interfaces.AppealPkg.Appeal_resolveCaseHomePageName_TH();
        
      th.setFieldValue(th.key$appealCaseKey$caseID_idx,caseID);

      th.callServer();
     
      homePageName=th.getFieldValue(th.result$homePageName$homePageName_idx);
      
      url = context + curam.omega3.request.RequestUtils.escapeURL(homePageName) + "Page.do?caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID);

      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));      
      
      
    
  </JSP_SCRIPTLET>
</PAGE>
