<!-- This page allows the user to display a list of hearing review issues   -->
<!-- for appealed cases for an integrated case.                             -->
<PAGE
  PAGE_ID="Appeal_listHearingReviewIssueAppealedCasesForIC"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Appeal"
    NAME="DISPLAY"
    OPERATION="listAppealedCasesForIC"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Appeal"
    NAME="DISPLAY1"
    OPERATION="multipleAppellantsIndicator"
    PHASE="DISPLAY"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="true"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="AcknowledgeReceiptsButton"
      LABEL="ActionControl.Label.AcknowledgeReceipts"
      TYPE="ACTION"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_acknowledgeReceipts"
      >
        <CONNECT>
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="appealCaseKey$caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$appealCaseKey$caseID"
    />
  </CONNECT>


  <!-- BEGIN, CR00021313, RKi -->


  <JSP_SCRIPTLET>
    curam.omega3.texthelper.TextHelper  th
                                    = (curam.omega3.texthelper.TextHelper)pageContext.findAttribute("DISPLAY1");
                  String multipleAppellantInd = 
                    th.getFieldValue("result$multipleAppellants");
                        if (multipleAppellantInd.equals("false"))
                        {
         
  </JSP_SCRIPTLET>
  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Appeal_viewHearingReviewAppealedCaseForIC">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appealRelationshipID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Appeal_modifyAppealedCaseForIC"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appealRelationshipID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealRelationshipID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <INCLUDE FILE_NAME="Appeal_listAppealedCaseView.vim"/>
  </LIST>


  <JSP_SCRIPTLET>
    }else if(multipleAppellantInd.equals("true"))
                        {
    </JSP_SCRIPTLET>


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Appeal_viewHearingReviewAppealedCaseForIC">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appealRelationshipID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Appeal_modifyAppealedCaseForIC"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appealRelationshipID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealRelationshipID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <INCLUDE FILE_NAME="Appeal_listAppealedCasesView.vim"/>
  </LIST>
  <JSP_SCRIPTLET>
  }
  </JSP_SCRIPTLET>
