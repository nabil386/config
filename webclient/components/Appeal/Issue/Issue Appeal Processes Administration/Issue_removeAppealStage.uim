<PAGE
  PAGE_ID="Issue_removeAppealStage"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="IssueAppealProcess"
    NAME="ACTION"
    OPERATION="cancelStage"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$appealStageConfID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Issue_viewAppealProcess">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealProcessID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealProcessID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="issueConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="issueConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="Issue_viewAppealStage">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealStageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealStageID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealProcessID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealProcessID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="stageSequence"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="stageSequence"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="issueConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="issueConfigurationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <PAGE_PARAMETER NAME="appealStageID"/>
  <PAGE_PARAMETER NAME="appealProcessID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="stageSequence"/>
  <PAGE_PARAMETER NAME="issueConfigurationID"/>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.RemoveAppealStage"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
