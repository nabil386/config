<PAGE
  PAGE_ID="Issue_modifyAppealStage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IssueAppealProcess"
    NAME="DISPLAY"
    OPERATION="readIssueStage"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="IssueAppealProcess"
    NAME="ACTION"
    OPERATION="modifyIssueStageDetails"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="appealStageID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="stageSequence"/>
  <PAGE_PARAMETER NAME="issueConfigurationID"/>

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="appeaStageConfigurationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="StageConfigID$appealStageConfID"
    />
  </CONNECT>


  <!--CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="productID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
    </CONNECT>-->


  <CLUSTER>
    <FIELD LABEL="Field.Label.AppealStage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appealTypeCode"
        />
      </CONNECT>


      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="appealTypeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Order">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="stageSequence"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>