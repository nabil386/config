<!-- This page allows user to search issues for judicial review.            -->
<PAGE
  PAGE_ID="Appeal_searchIssueForJudicialReview"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="IssueCase"
    NAME="searchIssueCasesBean"
    OPERATION="searchIssueCases"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="appealCaseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD LABEL="Field.Label.CaseReference">
      <CONNECT>
        <TARGET
          NAME="searchIssueCasesBean"
          PROPERTY="key$key$key$caseReference"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.IssueReference">
      <CONNECT>
        <TARGET
          NAME="searchIssueCasesBean"
          PROPERTY="key$key$key$issueReference"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00094193, RKi -->
  <CLUSTER LABEL_WIDTH="20">
    <CONTAINER LABEL="Field.Label.Client">
      <FIELD WIDTH="37">
        <CONNECT>
          <TARGET
            NAME="searchIssueCasesBean"
            PROPERTY="concernRoleType"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="61">
        <CONNECT>
          <TARGET
            NAME="searchIssueCasesBean"
            PROPERTY="key$key$key$concernRoleID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <!-- END, CR00094193 -->
  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
  >
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <!-- BEGIN, CR00099039, RKi -->
        <LINK
          PAGE_ID="Appeal_searchIssueForJudicialReview"
          SAVE_LINK="FALSE"
        />
        <!-- END, CR00099039 -->
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="CancelButton"
        LABEL="ActionControl.Label.Cancel"
      />
    </ACTION_SET>
  </CLUSTER>
  <LIST TITLE="List.Title.SearchResult">
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="20"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Select"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Appeal_addIssueToJudicialReview"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="searchIssueCasesBean"
              PROPERTY="result$dtlsList$dtlsList$dtls$issueCaseReference"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="referenceNumber"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="appealCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="appealCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="searchIssueCasesBean"
              PROPERTY="issueDeliveryCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="issueDeliveryCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- BEGIN, CR00094201, RKi -->
    <FIELD
      LABEL="Field.Title.CaseReference"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="searchIssueCasesBean"
          PROPERTY="result$dtlsList$dtlsList$dtls$relatedCaseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONNECT>
          <SOURCE
            NAME="searchIssueCasesBean"
            PROPERTY="relatedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Title.IssueReference"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="searchIssueCasesBean"
          PROPERTY="result$dtlsList$dtlsList$dtls$issueCaseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONNECT>
          <SOURCE
            NAME="searchIssueCasesBean"
            PROPERTY="result$dtlsList$dtlsList$dtls$issueDeliveryCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Client"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="searchIssueCasesBean"
          PROPERTY="result$dtlsList$dtlsList$dtls$concernRoleName"
        />
      </CONNECT>
      <!-- BEGIN, CR00198790, GP -->
      <LINK PAGE_ID="Person_homePagePDC">
        <!-- END, CR00198790 -->
        <CONNECT>
          <SOURCE
            NAME="searchIssueCasesBean"
            PROPERTY="result$dtlsList$dtlsList$dtls$concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <!-- END, CR00094201 -->
    <FIELD
      LABEL="Field.Title.IssueType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="searchIssueCasesBean"
          PROPERTY="result$dtlsList$dtlsList$dtls$issueType"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
