<!-- Reads an Hearing Review Appeal hearings details.                       -->
<PAGE
  PAGE_ID="Appeal_hearingReviewHearingHomeForICTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- BEGIN, CR00248590, GBA -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Hearing"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="primaryClientName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Hearing"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="primaryClientName"
      />
    </CONNECT>
  </TAB_NAME>
  <!-- END, CR00248590 -->


  <SERVER_INTERFACE
    CLASS="HearingTab"
    NAME="DISPLAY"
    OPERATION="readHearingTabDetail"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="hearingID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingID"
    />
  </CONNECT>


  <!-- BEGIN, CR00248590, GBA -->
  <FIELD>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$dtls$hearingTabDetails"
      />
    </CONNECT>
  </FIELD>
  <!-- END, CR00248590 -->


</PAGE>
