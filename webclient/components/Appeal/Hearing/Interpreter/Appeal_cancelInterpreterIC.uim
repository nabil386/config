<!-- Description -->
<!-- =========== -->
<!-- Page to cancel a hearing interpreter for integrated cases.             -->
<PAGE
  PAGE_ID="Appeal_cancelInterpreterIC"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingInterpreter"
    NAME="ACTION"
    OPERATION="cancel"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingServiceSupplierID"/>
  <PAGE_PARAMETER NAME="linkVersionNo"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingServiceSupplierID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="hearingServiceSupplierID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="linkVersionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="linkVersionNo"
    />
  </CONNECT>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.CancelInterpreter"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
