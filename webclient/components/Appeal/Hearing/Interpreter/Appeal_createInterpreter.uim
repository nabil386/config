<!-- Description -->
<!-- =========== -->
<!-- Page to create a hearing interpreter.   -->
<PAGE
  PAGE_ID="Appeal_createInterpreter"
  WINDOW_OPTIONS="width=850"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingInterpreter"
    NAME="ACTION"
    OPERATION="insert"
    PHASE="ACTION"
  />


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="hearingID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <!--BEGIN CR00095084, PN-->
  <!-- BEGIN, CR00039300, DK -->
  <CLUSTER STYLE="outest-cluster-edit">
    <CLUSTER
      DESCRIPTION="Cluster.Description.RegisteredInterpreter"
      LABEL_WIDTH="27.5"
      STYLE="grouped-cluster"
    >
      <CONTAINER LABEL="Field.Label.Interpreter">
        <FIELD
          LABEL="Field.Label.Interpreter"
          WIDTH="45"
        >
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="interpreterType"
            />
          </CONNECT>
        </FIELD>
        <FIELD
          LABEL="Field.Label.Interpreter"
          WIDTH="50"
        >
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="interpreterID"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.Description.NotRegisteredInterpreter"
      LABEL_WIDTH="50"
      NUM_COLS="2"
      STYLE="grouped-cluster"
      TAB_ORDER="ROW"
    >
      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="fullName"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="50"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.Address">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="addressData"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="50"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <!-- BEGIN, CR00249680, SS -->
      <FIELD
        LABEL="Field.Label.PhoneAreaCode"
        WIDTH="50"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneAreaCode"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.Phone">
        <!-- END, CR00249680 -->
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneNumber"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
  <!-- END, CR00039300-->
  <!--END CR00095084-->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00407812, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
