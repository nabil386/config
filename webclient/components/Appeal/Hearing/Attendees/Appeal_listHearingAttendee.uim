    CLASS="Hearing"
    NAME="DISPLAY"
    OPERATION="listUsers"
    PHASE="DISPLAY"
  />


  <INCLUDE FILE_NAME="Appeal_listHearingAttendeeView.vim"/>


</PAGE>
