<!-- Description -->
<!-- =========== -->
<!-- This page allows user to view transcription requests.                  -->
<PAGE
  PAGE_ID="Appeal_viewTranscription"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingTranscription"
    NAME="DISPLAY"
    OPERATION="readRequest"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingTranscriptionRequestID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingTranscriptionRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingTranscriptionRequestID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingTranscriptionRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingTranscriptionRequestID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
      TYPE="ACTION"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_modifyTranscriptionFromView"
        SAVE_LINK="false"
      >


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingTranscriptionRequestID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingTranscriptionRequestID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>


      </LINK>


    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="DeleteButton"
      LABEL="ActionControl.Label.Delete"
      TYPE="ACTION"
    >


      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_cancelTranscription"
        SAVE_LINK="false"
      >


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingTranscriptionRequestID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingTranscriptionRequestID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>


      </LINK>


    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
      TYPE="ACTION"
    >
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >


    <FIELD LABEL="Field.Label.Transcriber">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transcriberName"
        />
      </CONNECT>


      <!-- BEGIN, CR00198815, GP -->
      <LINK PAGE_ID="Appeal_resolveParticipantHome">
        <!-- END, CR00198815 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="transcriberID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>


    </FIELD>


    <FIELD LABEL="Field.Label.DateRequested">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateRequested"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.DueDate">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateDue"
        />
      </CONNECT>


    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <FIELD LABEL="Field.Label.DocumentReference">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fileReferenceNumber"
        />
      </CONNECT>


    </FIELD>


    <CONTAINER LABEL="Field.Label.AttachedFile">


      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="attachmentName"
          />
        </CONNECT>
      </FIELD>


      <ACTION_CONTROL
        LABEL="ActionControl.Label.View"
        TYPE="FILE_DOWNLOAD"
      >
        <LINK>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="attachmentID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="attachmentID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>


    </CONTAINER>


    <FIELD LABEL="Field.Label.RequestedBy">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="requestedByName"
        />
      </CONNECT>


      <!-- BEGIN, CR00198815, GP -->
      <LINK PAGE_ID="Appeal_resolveParticipantHome">
        <!-- END, CR00198815 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="requestedByID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>


    </FIELD>


    <FIELD LABEL="Field.Label.DateSentToTranscriber">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateSentToTranscriber"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.DateSentToRequester">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateSentToRequester"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.DateReceivedFromTranscriber">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateReceivedFromTranscriber"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.DocumentLocation">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fileLocation"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.Status">


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatusCode"
        />
      </CONNECT>


    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>


    </FIELD>


  </CLUSTER>


</PAGE>
