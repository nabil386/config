<!-- Page to create a hearing transcription.   -->
<PAGE
  PAGE_ID="Appeal_createTranscription"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Hearing"
    NAME="DISPLAY"
    OPERATION="listCaseParticipantsDetails"
  />


  <SERVER_INTERFACE
    CLASS="HearingTranscription"
    NAME="ACTION"
    OPERATION="createRequest"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="hearingID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <!--BEGIN CR00095084, PN-->
  <CLUSTER>
    <CLUSTER
      DESCRIPTION="Cluster.Description.RegisteredTranscriber"
      LABEL_WIDTH="22.5"
    >
      <FIELD LABEL="Field.Label.Transcriber">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="createTranscriptionDetails$concernRoleID"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.Description.NotRegisteredTranscriber"
      LABEL_WIDTH="45"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.Transcriber">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="representativeName"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.Address">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="addressData"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.PhoneAreaCode">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneAreaCode"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.Phone">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="phoneNumber"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD
        LABEL="Field.Label.DateRequested"
        WIDTH="60"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dateRequested"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.RequestedBy"
        USE_BLANK="TRUE"
      >
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="result$memberDetailsList$dtls$caseParticipantRoleID"
            NAME="DISPLAY"
            PROPERTY="result$memberDetailsList$dtls$name"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="details$createTranscriptionDetails$requestedByID"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.DateSentToTranscriber"
        WIDTH="60"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dateSentToTranscriber"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.DueDate"
        WIDTH="60"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dateDue"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <!--END CR00095084-->
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00407812, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="createTranscriptionDetails$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
