<!-- Description -->
<!-- =========== -->
<!-- This page allows user to view witnesses.                               -->
<PAGE
  PAGE_ID="Appeal_viewWitness"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingWitness"
    NAME="DISPLAY"
    OPERATION="viewHearingWitness"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingWitnessID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingWitnessID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingWitnessIDKeytls$hearingWitnessID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingWitnessID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="hearingWitnessIDKeytls$hearingWitnessID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
      TYPE="ACTION"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_modifyWitnessFromView"
        SAVE_LINK="FALSE"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingWitnessIDKeytls$hearingWitnessID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingWitnessID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="DeleteButton"
      LABEL="ActionControl.Label.Delete"
      TYPE="ACTION"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_cancelWitness"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingWitnessIDKeytls$hearingWitnessID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingWitnessID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$readHearingWitnessDetails$readHearingWitnessDetails$versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >
      <LINK
        PAGE_ID="Appeal_listWitness"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >
    <FIELD LABEL="Field.Label.WitnessName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="readHearingWitnessDetails$name"
        />
      </CONNECT>
      <!-- BEGIN, CR00198815, GP -->
      <LINK PAGE_ID="Appeal_resolveParticipantHome">
        <!-- END, CR00198815 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="readHearingWitnessDetails$caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Attendance">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="participatedCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Voluntary">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="voluntaryIndicator"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.BehalfOf">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="behalfOfCode"
        />
      </CONNECT>
    </FIELD>
    <!--    Begin CR00117296 LP-->
    <FIELD LABEL="Field.Label.Participant">
      <!--    Begin CR00117296 LP-->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="participantNames"
        />
      </CONNECT>
    </FIELD>
    <!--    End CR00117296 LP-->
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatusCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- Begin LP -->
  <CLUSTER>
    <ACTION_SET BOTTOM="false">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Add"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Appeal_addCaseParticipantRoleLink"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="readHearingWitnessDetails$caseParticipantRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="relatedID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <LIST>
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Remove"
          TYPE="ACTION"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="Appeal_removeCaseParticipantRoleLink"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="caseParticipantRoleLinkID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseParticipantRoleLinkID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>


      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$name"
          />
        </CONNECT>
        <LINK PAGE_ID="Appeal_resolveParticipantHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dtls$caseParticipantRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>
  </CLUSTER>
  <!-- End LP -->
</PAGE>
