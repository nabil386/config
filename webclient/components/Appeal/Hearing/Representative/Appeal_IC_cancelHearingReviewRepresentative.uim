<!-- This page allows a user to cancel a hearing representative.  -->
<PAGE
  PAGE_ID="Appeal_IC_cancelHearingReviewRepresentative"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="HearingRepresentative"
    NAME="ACTION"
    OPERATION="cancelHearingRepresentative"
    PHASE="ACTION"
  />
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00083100, RKi -->
      <LINK
        PAGE_ID="Appeal_IC_viewHearingReviewRepresentative"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="representativeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="representativeID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00083100 -->
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
      TYPE="ACTION"
    >
      <LINK
        PAGE_ID="Appeal_IC_viewHearingReviewRepresentative"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="representativeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="representativeID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <PAGE_PARAMETER NAME="representativeID"/>
  <PAGE_PARAMETER NAME="caseParticipantRoleVersionNo"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="representativeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dtls$modifyStatus$hearingRepresentativeIDKey$hearingRepresentativeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseParticipantRoleVersionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dtls$modifyStatus$cancelCaseParticipantRoleDetails$versionNo"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Label.Confirmation"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
