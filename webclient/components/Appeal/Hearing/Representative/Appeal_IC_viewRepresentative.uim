<!-- This pages display hearing representative details                      -->
<PAGE
  PAGE_ID="Appeal_IC_viewRepresentative"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingRepresentative"
    NAME="DISPLAY"
    OPERATION="viewHearingRepresentative"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="representativeID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="representativeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$hearingRepresentativeIDKey$hearingRepresentativeIDKey$hearingRepresentativeID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.FeeApproval">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$readDetails$readDetails$feeApprovedCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.BehalfOf">
      <!-- BEGIN, CR00273651, MV -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$readDetails$readDetails$behalfOfCode"
        />
      </CONNECT>
      <!-- END, CR00273651 -->
    </FIELD>
  </CLUSTER>


  <!-- Start CR00124935 LP -->
  <CLUSTER>
    <LIST TITLE="Cluster.Title.Participant">
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Remove"
          TYPE="ACTION"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="Appeal_removeCaseParticipantRoleLink"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="caseParticipantRoleLinkID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseParticipantRoleLinkID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>


      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtls$name"
          />
        </CONNECT>
        <LINK PAGE_ID="Appeal_resolveParticipantHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dtls$caseParticipantRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </LINK>
      </FIELD>


    </LIST>
  </CLUSTER>
  <!-- End CR00124935 LP -->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00417165, GK -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00417165 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$readDetails$readDetails$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
