<!-- Description -->
<!-- =========== -->
<!-- This page allows a user to create a new hearing representative.  -->
<PAGE
  PAGE_ID="Appeal_createRepresentative"
  WINDOW_OPTIONS="width=1000"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingRepresentative"
    NAME="ACTION"
    OPERATION="createHearingRepresentative"
    PHASE="ACTION"
  />
  <!--    Begin CR00117296 LP-->
  <SERVER_INTERFACE
    CLASS="Hearing"
    NAME="DISPLAYPARTICIPANT"
    OPERATION="listActiveOnBehalfOfParticipants"
    PHASE="DISPLAY"
  />
  <!--    End CR00117296 LP-->


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="hearingID"/>


  <!--    Begin CR00117296 LP-->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="DISPLAYPARTICIPANT"
      PROPERTY="hearingID"
    />
  </CONNECT>
  <!--    End CR00117296 LP-->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="hearingID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dtls$createDetails$hearingRepresentativeDetails$hearingID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <!--BEGIN CR00095084, PN-->
  <!--BEGIN CR00081334, PN-->
  <!--BEGIN, CR00098402, RKi -->
  <CLUSTER STYLE="outest-cluster-edit">
    <CLUSTER
      DESCRIPTION="Cluster.Description.Details"
      LABEL_WIDTH="32.5"
      STYLE="grouped-cluster"
    >
      <CONTAINER LABEL="Field.Label.Representative">
        <FIELD
          LABEL="Field.Label.Representative"
          WIDTH="45"
        >
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="representativeConcernType"
            />
          </CONNECT>
        </FIELD>


        <FIELD
          LABEL="Field.Label.Representative"
          WIDTH="50"
        >
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="45"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$hearingRepresentativeDetails$typeCode"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <!-- END, CR00098402 -->


    <CLUSTER
      DESCRIPTION="Cluster.Description.Details2"
      LABEL_WIDTH="60"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.Name">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$representativeRegistrationDetails$representativeDtls$representativeName"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="60"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD LABEL="Field.Label.Address">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$representativeRegistrationDetails$representativeRegistrationDetails$addressData"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="60"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD
        LABEL="Field.Label.PhoneAreaCode"
        WIDTH="50"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$representativeRegistrationDetails$representativeRegistrationDetails$phoneAreaCode"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.PhoneNumber">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$representativeRegistrationDetails$representativeRegistrationDetails$phoneNumber"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="60"
      NUM_COLS="2"
      STYLE="grouped-cluster"
    >
      <FIELD
        LABEL="Field.Label.Fee"
        WIDTH="50"
      >
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="dtls$createDetails$hearingRepresentativeDetails$feeAmount"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.BehalfOf">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="behalfOfCode"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <LIST>
    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.Title.Participant"
      WIDTH="10"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAYPARTICIPANT"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="caseParticipantRoleIDs"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.Participant"
      WIDTH="90"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYPARTICIPANT"
          PROPERTY="concernRoleName"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!--END CR00095084-->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00407812, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dtls$createDetails$hearingRepresentativeDetails$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
