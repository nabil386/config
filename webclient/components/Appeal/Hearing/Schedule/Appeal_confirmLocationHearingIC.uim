<!-- This page allows the user to confirm location hearing.                 -->
<PAGE
  PAGE_ID="Appeal_confirmLocationHearingIC"
  WINDOW_OPTIONS="width=685"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <!-- BEGIN, CR00237446, GP -->
    <ACTION_CONTROL
      APPEND_ELLIPSIS="false"
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <!-- BEGIN, CR00246201, GP -->
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="false"
        PAGE_ID="Appeal_selectScheduleType_LocationHearingIC"
        SAVE_LINK="false"
      >
        <!-- END, CR00246201 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00237446 -->


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00237446, GP -->
      <LINK PAGE_ID="Appeal_hearingCaseHearingHomeForIC">
        <!-- END, CR00237446 -->
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="hearingID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingID"
          />
        </CONNECT>
        <!-- BEGIN, CR00246201, GP -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <!-- END, CR00246201 -->
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="Appeal_confirmLocationHearingView.vim"/>


</PAGE>
