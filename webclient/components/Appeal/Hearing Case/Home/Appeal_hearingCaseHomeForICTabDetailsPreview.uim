<!--This page allows user to view an appeal hearing case details for a                 -->
<!--  hearing case.                                                                                                  -->
<PAGE
  PAGE_ID="Appeal_hearingCaseHomeForICTabDetailsPreview"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="AppealTab"
    NAME="DISPLAY"
    OPERATION="readAppealCaseTabDetail"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="appealCaseTabDetailKey$caseID"
    />
  </CONNECT>


  <FIELD>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$dtls$hearingCaseTabDetails"
      />
    </CONNECT>
  </FIELD>


</PAGE>
