<!-- This page allows a user to modify hearing case cancellation details.  -->
<PAGE
  PAGE_ID="Appeal_modifyHearingCaseCancelDetail"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AppealCancellation"
    NAME="DISPLAY"
    OPERATION="read"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="AppealCancellation"
    NAME="ACTION"
    OPERATION="modify"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$readCancelledCaseKey_bo$readCancelledCaseDetails$caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$readCancelledCaseDetails$readCancelledCaseKey$appealCancellationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$modifyCancelCaseKey_bo$modifyCancelCaseKey$appealCancellationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$readCancelledCaseDetails$readCancelledCaseKey$versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$modifyCancelCaseDetails$modifyCancelCaseDetails$versionNo"
    />
  </CONNECT>


  <!-- BEGIN, CR00198210, PS -->
  <CLUSTER LABEL_WIDTH="20">
    <!-- END, CR00198210 -->
    <FIELD LABEL="Field.Label.Reason">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$readCancelledCaseDetails$readCancelledCaseKey$cancelReasonCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$modifyCancelCaseDetails$modifyCancelCaseDetails$cancelReasonCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00198210,CR00407812, PS, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00198210,CR00407812 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$readCancelledCaseDetails$readCancelledCaseKey$cancelReasonText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$modifyCancelCaseDetails$modifyCancelCaseDetails$cancelReasonText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
