<!-- This page allows the user to view /modify a hearing case cancellation  -->
<!-- details.                                                               -->
<PAGE
  PAGE_ID="Appeal_resolveViewHearingCaseCancelDetail"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
 
      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/"; 
      String caseID = request.getParameter("caseID");
      String pageDescription = request.getParameter("pageDescription");
      String caseStatus = request.getParameter("caseStatus");
      String url = "";
   
      if (caseID == null) {
        // BEGIN, CR00126196, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "caseID");
       System.out.println(e);
       throw e;
       // END, CR00126196
      }
 
      if (pageDescription == null) {
        // BEGIN, CR00126196, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "pageDescription");
       System.out.println(e);
       throw e;
       // END, CR00126196
      }

      if (caseStatus == null) {
        // BEGIN, CR00126196, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "caseStatus");
       System.out.println(e);
       throw e;
       // END, CR00126196
      }

      // Check for canceled status
      if (!caseStatus.equals("CS11")) {
        
        url = context + 
          "Appeal_viewHearingCaseNotCanceledPage.do?caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID)
          + "&amp;contextDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);

      } else {
        
        url = context + 
          "Appeal_viewHearingCaseCancelDetailPage.do?caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID) 
          + "&amp;contextDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);

      }

      url += "&amp;" + rh.getSystemParameters(); 
      
      response.sendRedirect(response.encodeRedirectURL(url));

    
  </JSP_SCRIPTLET>
</PAGE>
