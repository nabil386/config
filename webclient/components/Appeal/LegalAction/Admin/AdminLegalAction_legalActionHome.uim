<PAGE
  PAGE_ID="AdminLegalAction_legalActionHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file:\\Curam\CuramUIMSchema"
>


  <!-- The page title for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- Actionset contains link to create NewLegalAction and NewLegalCategory -->
  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
  >
    <ACTION_CONTROL LABEL="ActionControl.Label.NewLegalAction">
      <LINK PAGE_ID="AdminLegalAction_createLegalAction"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewLegalCategory">
      <LINK PAGE_ID="AdminLegalAction_createLegalCategories"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Publish"> </ACTION_CONTROL>
  </ACTION_SET>


  <!-- Includes a file to list legal actions -->
  <INCLUDE FILE_NAME="AdminLegalAction_listLegalActionView.vim"/>
  <!-- Includes a file to list legal categories -->
  <INCLUDE FILE_NAME="AdminLegalAction_listLegalCategoriesView.vim"/>


</PAGE>
