<PAGE
  PAGE_ID="AdminLegalAction_publishLegalAction"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- The page title for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- Declare the 'action' server bean -->
  <SERVER_INTERFACE
    CLASS="AdminLegalAction"
    NAME="ACTION"
    OPERATION="publish"
    PHASE="ACTION"
  />
  <!-- BEGIN, CR00169111, SS -->
  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="true"
  >
    <ACTION_CONTROL
      DEFAULT="true"
      LABEL="ActionControl.Label.Publish"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="AdminLegalAction_listLegalCategories"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel">
      <LINK PAGE_ID="AdminLegalAction_listLegalCategories"/>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Text.Publish"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- END, CR00169111 -->
</PAGE>
