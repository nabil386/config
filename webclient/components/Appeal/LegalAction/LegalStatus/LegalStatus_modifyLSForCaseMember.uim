<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This Page allows users to modify association of participants with      -->
<!-- legal action.                                                          -->
<PAGE
  PAGE_ID="LegalStatus_modifyLSForCaseMember"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="CuramUIMSchema"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Statement"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="legalStatusID"/>


  <PAGE_PARAMETER NAME="caseParticipantRoleID"/>


  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="ACTION"
    OPERATION="modifyLegalStatus"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="DISPLAY"
    OPERATION="readLegalStatus"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="DISPLAYCONTEXT"
    OPERATION="getContextDescription"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="legalStatusID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$legalStatusID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="legalStatusID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$details$legalStatusID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseParticipantRoleID"
    />
    <TARGET
      NAME="DISPLAYCONTEXT"
      PROPERTY="caseParticipantRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseParticipantRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$details$caseParticipantRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$details$startDate"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$details$startDate"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$details$legalStatusTypeTabList"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$details$legalStatusTypeTabList"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    >
            </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="Control.Label.Cancel"
      TYPE="ACTION"
    >
            </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.ModifyLegalStatus"
  >
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LegalStatus"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$legalStatusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$endDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$details$endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LegalAction">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="legalActionName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="legalActionID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$details$legalActionID"
        />
      </CONNECT>
      <LINK>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>


</PAGE>
