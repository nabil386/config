<!-- This Page allows users to modify association of participants with      -->
<!-- legal action.                                                          -->
<PAGE PAGE_ID="AdminLegalStatus_addLegalStatus">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Statement"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseType"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="intCaseType"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="AdminLegalStatus"
    NAME="ACTION"
    OPERATION="configureLegalStatus"
    PHASE="ACTION"
  />


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="Control.Label.Save"
      TYPE="SUBMIT"
    >
 </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="Control.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="AdminLegalStatus_addLegalStatus">
     </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="Control.Label.Cancel"
      TYPE="ACTION"
    >
  </ACTION_CONTROL>
  </ACTION_SET>
  <!-- BEGIN, CR00248028, SS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00248028 -->
    <FIELD
      LABEL="Field.Label.LegalStatus"
      USE_BLANK="true"
      USE_DEFAULT="true"
      WIDTH="75"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="legalStatusTypeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="75"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$legalStatusDetails$startDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
