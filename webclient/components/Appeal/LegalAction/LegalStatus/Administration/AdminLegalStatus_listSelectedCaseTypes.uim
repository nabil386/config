<PAGE
  PAGE_ID="AdminLegalStatus_listSelectedCaseTypes"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file:\\Curam\CuramUIMSchema"
>


  <!-- The page title for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- Declare the 'action' server bean -->
  <SERVER_INTERFACE
    CLASS="AdminLegalStatus"
    NAME="DISPLAY"
    OPERATION="listAllAssociations"
    PHASE="DISPLAY"
  />


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.LegalStatus"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="AdminLegalStatus_listLegalStatus"
        SAVE_LINK="false"
      >
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ConfigureCaseType"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="AdminLegalStatus_listSelectedCaseTypes"
        SAVE_LINK="false"
      >
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <LIST TITLE="List.Label.IntegratedCase">
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="AdminLegalStatus_viewConfiguredCaseType">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="integratedCaseList$caseTypeCode"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseTypeCode"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="integratedCaseList$integratedCaseList$dtls$relatedID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="relatedID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.AddLegalStatus"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdminLegalStatus_associateCaseType"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="integratedCaseList$caseTypeCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseTypeCode"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="integratedCaseList$integratedCaseList$dtls$relatedID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="relatedID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="Field.Label.CaseType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="integratedCaseList$integratedCaseList$dtls$caseSubTypeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Multiple">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="integratedCaseList$integratedCaseList$dtls$multipleLegalStatusInd"
        />
      </CONNECT>
    </FIELD>
  </LIST>


