<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="LegalStatus_listLSForCaseMember"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- The page title for this page -->
  <PAGE_TITLE>


    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseParticipantRoleID"/>


  <!--Server Interfaces-->
  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="DISPLAYLIST"
    OPERATION="listLegalStatusHistory"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="DISPLAYCONTEXT"
    OPERATION="getContextDescription"
    PHASE="DISPLAY"
  />


  <!--  <SERVER_INTERFACE CLASS="YouthIntegratedCase" NAME="DISPLAYCASEDETAILS" OPERATION="getContextDescription" /> -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseParticipantRoleID"
    />
    <TARGET
      NAME="DISPLAYLIST"
      PROPERTY="key$caseParticipantRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseParticipantRoleID"
    />
    <TARGET
      NAME="DISPLAYCONTEXT"
      PROPERTY="caseParticipantRoleID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="FALSE"
  >
    <ACTION_CONTROL
      LABEL="Control.Label.New"
      TYPE="ACTION"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="LegalStatus_addLSForCaseMember"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST TITLE="List.LegalStatus">
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="LegalStatus_viewLSForCaseMember">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYLIST"
            PROPERTY="legalStatusID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="legalStatusID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAYLIST"
            PROPERTY="multipleLSEnabledInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="LegalStatus_modifyLSForCaseMember"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAYLIST"
              PROPERTY="legalStatusID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="legalStatusID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAYLIST"
            PROPERTY="multipleLSEnabledInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00409027, SS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="LegalStatus_deleteLegalStatus1"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAYLIST"
              PROPERTY="legalStatusID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="legalStatusID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAYLIST"
              PROPERTY="caseIDOpt"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <!-- END, CR00409027 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseParticipantRoleID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.LegalStatus"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLIST"
          PROPERTY="result$detailsList$legalStatusDetailsList$legalStatusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LegalAction"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLIST"
          PROPERTY="result$detailsList$legalStatusDetailsList$legalActionName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLIST"
          PROPERTY="result$detailsList$legalStatusDetailsList$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLIST"
          PROPERTY="result$detailsList$legalStatusDetailsList$endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLIST"
          PROPERTY="result$detailsList$legalStatusDetailsList$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
