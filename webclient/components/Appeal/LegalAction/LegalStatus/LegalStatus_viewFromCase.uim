<!-- ===========                                                            -->
<!-- This Page allows view the legal status details of a case member from a -->
<!-- case                                                                   -->
<!-- legal action.                                                          -->
<PAGE
  PAGE_ID="LegalStatus_viewFromCase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="CuramUIMSchema"
>


  <PAGE_PARAMETER NAME="legalStatusID"/>
  <PAGE_PARAMETER NAME="caseParticipantRoleID"/>
  <PAGE_PARAMETER NAME="participantName"/>


  <SERVER_INTERFACE
    CLASS="LegalStatus"
    NAME="DISPLAY"
    OPERATION="readLegalStatus"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="legalStatusID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$legalStatusID"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Field.Label.CaseMember">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="participantName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.LegalStatus"
      WIDTH="75"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$legalStatusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="75"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.RecordStatus"
      WIDTH="75"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$recordStatus"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.LegalAction"
      WIDTH="75"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$legalActionName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="75"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$details$endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
