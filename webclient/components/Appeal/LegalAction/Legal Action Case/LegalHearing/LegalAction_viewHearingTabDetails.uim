<!-- Reads the legal action tab pane details.                               -->
<PAGE
  PAGE_ID="LegalAction_viewHearingTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE
    CLASS="LegalActionTab"
    NAME="DISPLAY"
    OPERATION="readHearingTabDetail"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="2">
    
      <FIELD LABEL="Field.Label.NameCategory">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hearingCategoryCode"
          />
        </CONNECT>
      </FIELD>
     
    
    
  

    <FIELD LABEL="Field.Label.DocketNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="docketNumber"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CourtLocation">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="courtConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="hearingOutcomeCode"
        />
      </CONNECT>
    </FIELD>
  
    <FIELD LABEL="Field.Label.LegalAction">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="hearingNameCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Judge">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="judgeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StatusCode">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
 

 
</PAGE>
