<!-- This Page allows users to view legal action record                     -->
<PAGE
  PAGE_ID="LegalAction_viewOrderTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE
    CLASS="LegalActionTab"
    NAME="DISPLAY"
    OPERATION="readOrderTabDetail"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <!--Legal Order Details-->
  <CLUSTER
    NUM_COLS="2"
   
  >
    
    <FIELD LABEL="Field.Label.LegalCategory">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="hearingCategoryCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OrderEffectiveDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.JurisidictionID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="courtConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LegalAction">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="orderNameCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.IssueDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="issuedDate"
        />
      </CONNECT>
    </FIELD>
  
   
    <FIELD LABEL="Field.Label.JudgeNameID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="judgeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
   
  </CLUSTER>
 

</PAGE>
