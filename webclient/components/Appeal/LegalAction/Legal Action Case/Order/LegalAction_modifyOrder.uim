<!-- This Page allows users to create a new legal action -->
<PAGE
  PAGE_ID="LegalAction_modifyOrder"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="CuramUIMSchema"
>
  <!--Page Title-->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Statement"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseID"/>
  <!--Server Interface-->
  <SERVER_INTERFACE
    CLASS="LegalOrder"
    NAME="ACTION"
    OPERATION="modifyLegalOrder"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="LegalOrder"
    NAME="DISPLAY"
    OPERATION="readLegalOrder"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$legalActionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="legalOrderDtls$dtls$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <!--Action Set-->
  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    > </ACTION_CONTROL>
  </ACTION_SET>


  <!--Legal Order details-->
  <!-- BEGIN, CR00186980, SS -->
  <CLUSTER
    NUM_COLS="2"
  >
    <!-- END, CR00186980 -->
    <FIELD
      LABEL="Field.Label.LegalCategory"
      USE_BLANK="TRUE"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="legalCategoryNameCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.JurisidictionID">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="locationName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="locationID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="locationID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.IssueDate"
      USE_DEFAULT="false"
      
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="legalActionDtls$dtls$startDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.OrderEndDate"
      USE_DEFAULT="false"
     
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.LegalAction"
      USE_BLANK="TRUE"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="legalActionNameCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.JudgeNameID"
      USE_BLANK="TRUE"
      USE_DEFAULT="TRUE"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="memberID"
          NAME="DISPLAY"
          PROPERTY="memberName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="adjudicateID"
        />
      </CONNECT>


      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="adjudicateID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.OrderEffectiveDate"
      USE_DEFAULT="false"
  
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <!--Legal order requirement and conditions-->
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.OrderRequirements"
  >
    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="requirements"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="requirements"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
