<!-- Popup External Party Office search page -->
<PAGE
  PAGE_ID="LegalAction_searchExternalOfficePopup"
  POPUP_PAGE="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="LegalAction"
    NAME="ACTION"
    OPERATION="searchExternalOfficeLocation"
    PHASE="ACTION"
  />
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD LABEL="Field.Label.ReferenceNumber">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="externalPartyReferenceNumber"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AddressLineOne">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="officeAddressLine1"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="externalPartyName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.City">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="officeCity"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Type"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="officeType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OfficeName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="officeSearchKey$officeName"
        />
      </CONNECT>
    </FIELD>


    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK PAGE_ID="LegalAction_searchExternalOfficePopup"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="CancelButton"
        LABEL="ActionControl.Label.Cancel"
        TYPE="DISMISS"
      />
    </ACTION_SET>
  </CLUSTER>
  <LIST TITLE="List.Title.SearchResults">
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="10"
    >
      <ACTION_CONTROL LABEL="ActionControl.Label.Select">
      <!--  BEGIN, 173623, RA-->       
	   <SCRIPT
          ACTION="PopulateOfficeMembers(this)"
          EVENT="ONCLICK"
          SCRIPT_FILE="PopulateMembers.js"
        />
		 <!-- END, 173623-->
        <LINK
          DISMISS_MODAL="false"
          OPEN_MODAL="false"
          OPEN_NEW="false"
          PAGE_ID="LegalAction_searchExternalOfficePopup"
        >


          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="officeMemberIDAndName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="officeMemberIDAndName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="officeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="value"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="resultList$officeName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- BEGIN, CR00248466, SS -->
    <FIELD
      LABEL="Field.Title.ReferenceNumber"
      WIDTH="15"
    >
      <!-- END, CR00248466 -->
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="resultList$referenceNumber"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="resultList$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.OfficeName"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="resultList$officeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.AddressLineOne"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="resultList$addressLine1"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="resultList$type"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
