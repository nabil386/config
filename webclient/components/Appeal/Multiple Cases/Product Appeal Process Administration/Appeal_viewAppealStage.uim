<PAGE
  PAGE_ID="Appeal_viewAppealStage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ProductAppealProcess"
    NAME="DISPLAY"
    OPERATION="readAppealStage"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="appealStageID"/>
  <PAGE_PARAMETER NAME="productAppealProcessID"/>
  <PAGE_PARAMETER NAME="productID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appealStageID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$appealStageID$appealStageID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="productID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="productID"
    />
  </CONNECT>


  <CLUSTER>
    <FIELD LABEL="Field.Label.AppealStage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="appealTypeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Order">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="stageSequence"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
