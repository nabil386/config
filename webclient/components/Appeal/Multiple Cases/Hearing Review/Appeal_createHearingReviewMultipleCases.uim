<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to create a hearing review case where        -->
<!-- multiple cases can be appealed on a single appeal case.                -->
<!-- Caters for where the decision being appealed is associated with an     -->
<!-- integrated case.                             -->
<?curam-deprecated Since Curam 6.0 SP2, replaced with Appeal_createHearingReviewObjectMultipleCases. As
  * part of the change to appeals all pages relating to Appeal_createHearingReviewMultipleCases have been replaced. See release note: CR00296108?>
<PAGE
  PAGE_ID="Appeal_createHearingReviewMultipleCases"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <INCLUDE FILE_NAME="Appeal_createHearingReviewMultipleCasesView.vim"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <!-- BEGIN, CR00247113, GP -->
    <ACTION_CONTROL
      APPEND_ELLIPSIS="false"
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Appeal_enterAppellantAndRespondentForHearingReview"
        SAVE_LINK="FALSE"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00247113 -->


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="Appeal_resolveHearingReviewHome"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$key$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


</PAGE>
