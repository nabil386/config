<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to add a case to a hearing review.           -->
<!--                                                                        -->
<!-- This page is for cases being appealed which are part of an             -->
<!-- integrated case.                                                       -->
<!--                                                                        -->
<PAGE
  PAGE_ID="Appeal_addHearingReviewCaseIC"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <INCLUDE FILE_NAME="Appeal_addHearingReviewView.vim"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >


      <LINK PAGE_ID="Appeal_hearingReviewHomeForIC">


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="hearingReviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <!-- BEGIN, CR00095199, RKi -->
      <LINK
        PAGE_ID="Appeal_searchHearingReviewCaseForIC"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="hearingReviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="hearingReviewID"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00095199 -->
    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
