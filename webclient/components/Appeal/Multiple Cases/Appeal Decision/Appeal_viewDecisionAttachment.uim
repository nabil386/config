<PAGE
  PAGE_ID="Appeal_viewDecisionAttachment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DecisionAttachment"
    NAME="DISPLAY"
    OPERATION="readExternal"
    PHASE="DISPLAY"
  />

  <PAGE_PARAMETER NAME="attachmentLinkID"/>
  <PAGE_PARAMETER NAME="appealCaseID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="attachmentLinkID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="attachmentLinkID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="2"
    LABEL_WIDTH="37"
  >

    <WIDGET TYPE="FILE_DOWNLOAD" LABEL="Field.Label.File">
      <WIDGET_PARAMETER NAME="LINK_TEXT">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="attachmentName"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="PARAMS">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="attachmentLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="attachmentLinkID"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
    

    <FIELD LABEL="Field.Label.DocumentLocation">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="documentLocation"
        />
      </CONNECT>
    </FIELD>

    <FIELD CONTROL="SKIP"/>

    <FIELD LABEL="Field.Label.DocumentReference">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="documentReference"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>



</PAGE>
