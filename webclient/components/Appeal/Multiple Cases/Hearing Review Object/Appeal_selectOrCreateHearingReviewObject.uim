<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to select an existing hearing review case    -->
<!-- to add a new appeal request to. Alternatively the user can select to   -->
<!-- create a new hearing review case.              -->
<!--                      -->
<!-- This page is for cases being appealed which are part of an             -->
<!-- standalone case.                                                       -->
<PAGE
  PAGE_ID="Appeal_selectOrCreateHearingReviewObject"
  WINDOW_OPTIONS="width=850"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="contextDescription"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingReview"
    NAME="DISPLAY"
    OPERATION="listActiveHearingReviews"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="implCaseID"/>
  <PAGE_PARAMETER NAME="participantRoleID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="priorAppealCaseID"/>
  <PAGE_PARAMETER NAME="appellantTypeCode"/>
  <PAGE_PARAMETER NAME="parentCaseID"/>
  <PAGE_PARAMETER NAME="appealObjectsDelimitedList"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="implCaseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="appellantTypeCode"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="appellantTypeCode"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participantRoleID"
    />
  </CONNECT>


  <CLUSTER DESCRIPTION="Cluster.Description">


    <LIST TITLE="List.Title.HearingReviewCases">


      <CONTAINER
        LABEL="Container.Label.Action"
        WIDTH="20"
      >
        <ACTION_CONTROL LABEL="ActionControl.Label.Select">
          <LINK
            DISMISS_MODAL="false"
            PAGE_ID="Appeal_addHearingReviewObjectCase"
            SAVE_LINK="false"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="appealCaseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="hearingReviewID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="caseReference"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseReference"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="implCaseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="implCaseID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="contextDescription"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="parentCaseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="patentCaseID"
              />
            </CONNECT>


            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="priorAppealCaseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="priorAppealCaseID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="appealObjectsDelimitedList"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="appealObjectsDelimitedList"
              />
            </CONNECT>


          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.CaseReference"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseReference"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Owner"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseOwner"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.ReceivedDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="receivedDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.DeadlineDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="deadlineDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Status"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="status"
          />
        </CONNECT>
      </FIELD>


    </LIST>


  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >


      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Appeal_enterAppellantAndRespondentForHearingReviewObject"
        SAVE_LINK="false"
      >


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealObjectsDelimitedList"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealObjectsDelimitedList"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="Create"
      LABEL="ActionControl.Label.Create"
    >


      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Appeal_createHearingReviewObjectMultipleCases"
        SAVE_LINK="FALSE"
      >


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="participantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appellantTypeCode"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appellantTypeCode"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="parentCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priorAppealCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealObjectsDelimitedList"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealObjectsDelimitedList"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


</PAGE>
