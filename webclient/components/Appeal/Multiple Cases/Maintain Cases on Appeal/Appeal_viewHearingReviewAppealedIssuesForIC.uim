<PAGE
  PAGE_ID="Appeal_viewHearingReviewAppealedIssuesForIC"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
      TYPE="ACTION"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_modifyAppealedIssuesForIC"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
        <!-- BEGIN, CR0009669, RKi -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <!-- END, CR00096692 -->
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="RemoveButton"
      LABEL="ActionControl.Label.Remove"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Appeal_removeHearingReviewAppealedIssueCaseForIC"
      >
        <!-- BEGIN, CR0009669, RKi -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
        <!-- END, CR00096692 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealRelationshipID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appealVersionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealVersionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseHeaderVersionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseHeaderVersionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
      TYPE="ACTION"
    >
      <!-- BEGIN, CR0009669, RKi -->
      <LINK PAGE_ID="Appeal_listHearingReviewAppealedIssuesForIC">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00096692 -->
    </ACTION_CONTROL>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="appealRelationshipID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="implCaseID"/>
  <!-- BEGIN, CR0009669, RKi -->
  <PAGE_PARAMETER NAME="pageDescription"/>
  <!-- END, CR00096692 -->


  <INCLUDE FILE_NAME="Appeal_viewAppealedIssueCaseView.vim"/>
</PAGE>
