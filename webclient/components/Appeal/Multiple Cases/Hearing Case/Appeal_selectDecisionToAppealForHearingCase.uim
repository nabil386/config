<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Allows the user to select a decision from a prior appeal case to appeal-->
<!-- to a hearing case.                                                     -->
<?curam-deprecated Since Curam 6.0 SP2, replaced with Appeal_selectDecisionToAppealForHearingCaseObject. As
  * part of the change to appeals all pages relating to Appeal_selectDecisionToAppealForHearingCase have been replaced. See release note: CR00295312?>
<PAGE
  PAGE_ID="Appeal_selectDecisionToAppealForHearingCase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="HearingCase"
    NAME="DISPLAY"
    OPERATION="listAppealDecisionsForCase"
    PHASE="DISPLAY"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="implCaseID"/>
  <PAGE_PARAMETER NAME="parentCaseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="implCaseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <LIST>


    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="15"
    >
      <ACTION_CONTROL LABEL="ActionControl.Label.Select">
        <!-- BEGIN, CR00288015, DK -->
        <LINK
          DISMISS_MODAL="false"
          PAGE_ID="Appeal_enterAppellantAndRespondentForHearingCase"
          SAVE_LINK="false"
        >
          <!-- END, CR00288015 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dtls$caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="priorAppealCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="implCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="implCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="parentCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="parentCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.Resolution"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="resolutionCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Date"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="decisionDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Reference"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseReference"
        />
      </CONNECT>


    </FIELD>


  </LIST>


</PAGE>
