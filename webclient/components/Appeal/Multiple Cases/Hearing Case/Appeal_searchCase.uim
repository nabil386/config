<!-- This page allows users to search for a Case to add to an appeal        -->
<PAGE
  PAGE_ID="Appeal_searchCase"
  WINDOW_OPTIONS="width=775"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <!-- BEGIN, CR00285568, ZV -->
  <SERVER_INTERFACE
    CLASS="Case"
    NAME="ACTION"
    OPERATION="caseSearch2"
    PHASE="ACTION"
  />


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00285568 -->

  <PAGE_PARAMETER NAME="hearingCaseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  
  <CLUSTER
    NUM_COLS="2"
    LABEL_WIDTH="45"
  >
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK
          PAGE_ID="THIS"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK
          PAGE_ID="Appeal_searchCase"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="hearingCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="hearingCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <!-- BEGIN, CR00093601, RKi -->
    <FIELD
      LABEL="Field.Label.CaseReference"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <!-- BEGIN, CR00225696, ASN -->
        <TARGET
          NAME="ACTION"
          PROPERTY="key$caseReference"
        />
        <!-- END, CR00225696 -->
      </CONNECT>
    </FIELD>
    <!-- END, CR00093601 -->


  </CLUSTER>
  <LIST TITLE="List.Title.SearchResult">
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="10"
    >
      <ACTION_CONTROL  LABEL="ActionControl.Label.Select">
        <LINK
          OPEN_MODAL="false"
          DISMISS_MODAL="false"
          SAVE_LINK="false"
          PAGE_ID="Appeal_addHearingCase"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="hearingCaseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="hearingCaseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="searchDtls$caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="implCaseID"
            />
          </CONNECT>
          <!-- BEGIN, CR00095178, RKi -->
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="result$listDtls$searchDtls$caseReference"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseReference"
            />
          </CONNECT>
          <!-- END, CR00095178 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    
    <!-- BEGIN, CR00093601, RKi -->
    <FIELD
      WIDTH="15"
      LABEL="Field.Label.Reference"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchDtls$caseReference"
        />
      </CONNECT>
      <!-- END, CR00093601 -->
    </FIELD>
    
    <FIELD
      WIDTH="25"
      LABEL="Field.Label.PrimaryClient"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="searchDtls$concernRoleName"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD
      WIDTH="25"
      LABEL="Field.Label.CaseType"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="searchDtls$caseTypeCode"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD
      WIDTH="15"
      LABEL="Field.Label.StartDate"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="searchDtls$startDate"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD
      WIDTH="10"
      LABEL="Field.Label.Status"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="searchDtls$statusCode"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>
</PAGE>
