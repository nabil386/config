<!-- Description -->
<!-- =========== -->
<!-- This page is used to display a list of decisions for a determination   -->
<!-- on a case.                                                             -->
<PAGE
  PAGE_ID="AppealWizard_listCreoleDecisionsForDetermination"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="determinationID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$determinationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="CaseDetermination"
    NAME="DISPLAY"
    OPERATION="listDecisionPeriodsForDetermination"
  />


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
    TITLE="Cluster.Title.DeterminationResults"
  >
    <LIST>
      <FIELD
        LABEL="Field.Label.CoverPeriod"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="coverPeriod"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.Decision"
        WIDTH="40"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="resultCode"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.Summary"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="summary"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>


</PAGE>
