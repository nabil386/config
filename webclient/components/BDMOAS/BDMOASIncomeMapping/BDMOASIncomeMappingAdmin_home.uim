<?xml version="1.0" encoding="UTF-8"?>
<PAGE PAGE_ID="BDMOASIncomeMappingAdmin_home"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
	</PAGE_TITLE>

	<SERVER_INTERFACE PHASE="DISPLAY" NAME="DISPLAY"
		CLASS="BDMOASIncomeMapping" OPERATION="listMappings" />

	<ACTION_SET>
		<ACTION_CONTROL LABEL="ActionControl.Label.New">
			<LINK PAGE_ID="BDMOASIncomeMappingAdmin_createMapping" OPEN_MODAL="true" />
		</ACTION_CONTROL>
	</ACTION_SET>

	<LIST>
		<FIELD LABEL="Field.Label.TaxYear">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="taxYear" />
			</CONNECT>
			<LINK DISMISS_MODAL="false" PAGE_ID="BDMOASIncomeMapping_home"
				SAVE_LINK="false">
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="mappingID" />
					<TARGET NAME="PAGE" PROPERTY="mappingID" />
				</CONNECT>
			</LINK>
		</FIELD>
		<FIELD LABEL="Field.Label.Status">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="status" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.CreatedBy">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="createdByFullName" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.CreatedOn">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="createdOn" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.UpdatedBy">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="updatedByFullName" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.UpdatedOn">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="lastUpdatedOn" />
			</CONNECT>
		</FIELD>
	</LIST>

</PAGE>