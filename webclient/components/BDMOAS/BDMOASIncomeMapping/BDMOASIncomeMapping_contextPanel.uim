	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

    <PAGE_PARAMETER NAME="mappingID" />

    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="mappingID" />
        <TARGET NAME="DISPLAY" PROPERTY="key$mappingID" />
    </CONNECT>
    
    <SERVER_INTERFACE CLASS="BDMOASIncomeMapping" NAME="DISPLAY" OPERATION="getMappingDetails"
        PHASE="DISPLAY"/>

	<TAB_NAME>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
		<CONNECT>
			<SOURCE NAME="DISPLAY" PROPERTY="taxYear" />
		</CONNECT>
	</TAB_NAME>

	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
		<CONNECT>
			<SOURCE NAME="DISPLAY" PROPERTY="taxYear" />
		</CONNECT>
	</PAGE_TITLE>

</PAGE>