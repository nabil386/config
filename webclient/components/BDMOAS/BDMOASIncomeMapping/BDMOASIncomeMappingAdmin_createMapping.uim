<?xml version="1.0" encoding="UTF-8"?>
<PAGE PAGE_ID="BDMOASIncomeMappingAdmin_createMapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
	</PAGE_TITLE>

	<SERVER_INTERFACE PHASE="ACTION" NAME="ACTION"
		CLASS="BDMOASIncomeMapping" OPERATION="createMapping" />

	<CLUSTER>
		<FIELD LABEL="TaxYear.String.Label" USE_DEFAULT="false" USE_BLANK="true">
			<CONNECT>
				<TARGET NAME="ACTION" PROPERTY="key$taxYear" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

	<ACTION_SET BOTTOM="true">
		<ACTION_CONTROL ALIGNMENT="LEFT"
			LABEL="ActionControl.Label.Cancel" />
		<ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
			<LINK DISMISS_MODAL="false" PAGE_ID="BDMOASIncomeMapping_home"
				SAVE_LINK="false">
				<CONNECT>
					<SOURCE NAME="ACTION" PROPERTY="result$mappingID" />
					<TARGET NAME="PAGE" PROPERTY="mappingID" />
				</CONNECT>
			</LINK>
		</ACTION_CONTROL>
	</ACTION_SET>

</PAGE>