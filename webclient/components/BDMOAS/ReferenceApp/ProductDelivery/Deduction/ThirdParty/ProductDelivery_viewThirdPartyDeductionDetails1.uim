<!-- This page allows the user to view details of an third party deduction. -->
<PAGE
  PAGE_ID="ProductDelivery_viewThirdPartyDeductionDetails1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <!-- BEGIN - CK- Task 61207 - DOJ deduction details -->
  <SERVER_INTERFACE
    CLASS="BDMProductDelivery"
    NAME="DISPLAY"
    OPERATION="readDeductionDetails"
  />
  <!-- END - CK- Task 61207 - DOJ deduction details -->
  
  <!-- BEGIN Feature 106244: Manage NRT -->
  
  <SERVER_INTERFACE
    CLASS="BDMOASProductDeliveryDeduction"
    NAME="DISPLAYOAS_PRODUCTDELIVERY_DEDUCTION"
    OPERATION="readNRTDeductionCountryDetails"
  />
  
  <!-- END Feature 106244: Manage NRT -->


  <SERVER_INTERFACE
    CLASS="ProductDelivery"
    NAME="DISPLAYHISTORY"
    OPERATION="listSortedThirdPartyDeductionHistory"
  />


  <PAGE_PARAMETER NAME="caseDeductionItemID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseDeductionItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseDeductionItemID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseDeductionItemID"
    />
    <TARGET
      NAME="DISPLAYHISTORY"
      PROPERTY="key$caseDeductionItemID"
    />
  </CONNECT>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseDeductionItemID"
    />
    <TARGET
      NAME="DISPLAYOAS_PRODUCTDELIVERY_DEDUCTION"
      PROPERTY="key$caseDeductionItemID"
    />
  </CONNECT>
  
  <!-- BEGIN - CK- Task 61207 - DOJ deduction details -->
  <CLUSTER NUM_COLS="2">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="isDOJArrearsDeduction"
      />
    </CONDITION>
    
    <FIELD LABEL="Field.Label.DOJ.Percentage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="holdbackDeductionRate"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.DOJ.AmountPerPayment">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="perPayDeductionAmount"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.DOJ.DebtorFixedAmount">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="debtorFixedAmount"
        />
      </CONNECT>
    </FIELD>
    
    <FIELD LABEL="Field.Label.DOJ.FixedAmountFlag">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fixedAmountFlag"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- END - CK- Task 61207 - DOJ deduction details -->
  
  <!-- BEGIN Feature 106244: Manage NRT -->
  
  <CLUSTER NUM_COLS="2">
  	<CONDITION>
      <IS_TRUE
        NAME="DISPLAYOAS_PRODUCTDELIVERY_DEDUCTION"
        PROPERTY="bdmOASNRTCountryInd"
      />
    </CONDITION>
    <FIELD LABEL="Field.Label.Nominee">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseNomineeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="priorityPosition"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Country">
      <CONNECT>
        <SOURCE
          NAME="DISPLAYOAS_PRODUCTDELIVERY_DEDUCTION"
          PROPERTY="bdmOASNRTCountry"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Component">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="readDeductionDetails$rulesObjectiveType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActionType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="actionType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <!-- END Feature 106244: Manage NRT -->


  <CLUSTER NUM_COLS="2">
  	<CONDITION>
      <IS_FALSE
        NAME="DISPLAYOAS_PRODUCTDELIVERY_DEDUCTION"
        PROPERTY="bdmOASNRTCountryInd"
      />
    </CONDITION>
    <FIELD LABEL="Field.Label.Nominee">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseNomineeConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="priorityPosition"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Component">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="readDeductionDetails$rulesObjectiveType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActionType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="actionType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.ThirdPartyDetails"
  >


    <FIELD LABEL="Field.Label.ThirdParty">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="thirdPartyConcernRoleName"
        />
      </CONNECT>
      <LINK PAGE_ID="Participant_resolveRoleHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="thirdPartyConcernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="thirdPartyConcernRoleType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantType"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.AccountName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="customerAccName"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
    <FIELD LABEL="Field.Label.AccountNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="customerAccNumber"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.ChangeHistory">
    <DETAILS_ROW>
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAYHISTORY"
          PROPERTY="instructionInd"
        />
      </CONDITION>
      <INLINE_PAGE PAGE_ID="Financial_viewPaymentInstructionInline">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYHISTORY"
            PROPERTY="instructionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="finInstructionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYHISTORY"
            PROPERTY="key$caseDeductionItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseDeductionItemID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.Description"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYHISTORY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Date"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYHISTORY"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.HistoryAmount"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYHISTORY"
          PROPERTY="caseDeductionDtls$amount"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
