 <PAGE PAGE_ID="BDMOAS_closeAllBenefits"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" 
    WINDOW_OPTIONS="width=680">
    
     <SERVER_INTERFACE CLASS="BDMOASProgramAuthorisation" NAME="ACTION" OPERATION="closeAllBenefits"
        PHASE="ACTION"/>
    
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
        </CONNECT>
    </PAGE_TITLE>
    
    <PAGE_PARAMETER NAME="caseID"/>
   
    
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="caseID"/>
        <TARGET NAME="ACTION" PROPERTY="caseID"/>
    </CONNECT>
       
    <ACTION_SET >
        <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>

        <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
    </ACTION_SET>
    
   
     <CLUSTER NUM_COLS="1" LABEL_WIDTH="50">
        
         <FIELD LABEL="Field.Label.DenialReason" USE_BLANK="true">
           
            <CONNECT>
                <TARGET NAME="ACTION" PROPERTY="denialReason"/>
            </CONNECT>
        </FIELD> 
    
     </CLUSTER>
     <CLUSTER TITLE="Cluster.Title.Comments" SHOW_LABELS="false">
         <FIELD LABEL="Cluster.Title.Comments">
             <CONNECT>
                 <TARGET
                     NAME="ACTION"
                     PROPERTY="comments"
                 />
             </CONNECT>
             </FIELD>
        </CLUSTER>
</PAGE>

