<?xml version="1.0" encoding="UTF-8"?>
<!-- TASK 53598 - Manage Penalty -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="BDMPerson_listExternalLiabilitiesDetails" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

    <PAGE_PARAMETER NAME="externalLiabilityID" />

    <SERVER_INTERFACE CLASS="BDMOASExternalLiability" NAME="DISPLAY" OPERATION="listLiabilityHistory" />
    <SERVER_INTERFACE CLASS="BDMOASExternalLiability" NAME="DISPLAYOVERPAYMENTS" OPERATION="listOverPaymentHistoryByExternalLiabilityID" />

    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="externalLiabilityID" />
        <TARGET NAME="DISPLAY" PROPERTY="externalLiabilityID" />
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="externalLiabilityID" />
        <TARGET NAME="DISPLAYOVERPAYMENTS" PROPERTY="externalLiabilityID" />
    </CONNECT>

    <CLUSTER TITLE="Cluster.OverPaymentDetails.Title">
        <CONDITION>
         	<IS_TRUE NAME="DISPLAYOVERPAYMENTS" PROPERTY="displayOverPaymentDetailsInd"/>
        </CONDITION>
        <FIELD LABEL="Field.Label.Case">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="overPaymentCaseRefName" />
            </CONNECT>
        </FIELD>
    </CLUSTER>

    <LIST TITLE="List.Title.OverPaymentHistoricalData">
        <CONDITION>
         	<IS_TRUE NAME="DISPLAYOVERPAYMENTS" PROPERTY="displayOverPaymentDetailsInd"/>
        </CONDITION>
        <FIELD LABEL="Field.Label.Date">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="overPaymentDate" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.PreviousCase">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="previousCase" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.CurrentCase">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="currentCase" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.User">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="user" />
            </CONNECT>
        </FIELD>
    </LIST>

    <CLUSTER TITLE="Cluster.InterestDetails.Title">
         <CONDITION>
         	<IS_TRUE NAME="DISPLAYOVERPAYMENTS" PROPERTY="displayOverPaymentDetailsInd"/>
        </CONDITION>
        <FIELD LABEL="Field.Label.SequenceNumber">
            <CONNECT>
                <SOURCE NAME="DISPLAYOVERPAYMENTS" PROPERTY="interestSequenceNumber" />
            </CONNECT>
        </FIELD>
    </CLUSTER>

    <LIST TITLE="List.Title.HistoricalData">
        <FIELD LABEL="Field.Label.Date" WIDTH="9">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="modifyDate" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.OldBalance" WIDTH="18">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="originalLiabilityAmount" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.NewBalance" WIDTH="18">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="liabilityAmount" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.User" WIDTH="15">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="userFullName" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.Status" WIDTH="15">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="liabilityStatusCode" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.ChangeReason" WIDTH="20">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="changeReason" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.Comments" WIDTH="35">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="comments" />
            </CONNECT>
        </FIELD>
    </LIST>

</PAGE>