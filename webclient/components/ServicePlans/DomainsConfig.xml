<dc:domains xmlns:dc="http://www.curamsoftware.com/curam/util/common/domain-config">


  <!-- Render a Gantt Chart -->
  <dc:domain name="GANTT_R2_XML">
    <dc:plug-in
      name="view-renderer"
      purpose="view-ispspggantt"
    />
    <dc:plug-in
      name="marshal"
      purpose="marshal-ispspggantt"
    />
  </dc:domain>
  <!--BEGIN CR00117532, GBA-->
  <dc:domain name="OPTIONAL_INTEGER">
    <dc:plug-in
      class="core.OptionalIntegerConverter"
      name="converter"
    />
    <dc:plug-in
      class="core.OptionalIntegerDefault"
      name="default"
    />
  </dc:domain>
  <dc:domain name="OPTIONAL_POSITIVE_MONEY">
    <dc:plug-in
      class="core.OptionalMoneyConverter"
      name="converter"
    />
    <dc:plug-in
      class="core.OptionalMoneyDefault"
      name="default"
    />
  </dc:domain>
  <!--END CR00117532-->


  <!-- BEGIN, CR00241441, PDN -->
  <dc:domain name="APPROVAL_CHECK_ESTIMATED_COST">
    <dc:plug-in
      name="converter"
      purpose="set-symbol"
    />
  </dc:domain>
  <dc:domain name="PLAN_ITEM_COST">
    <dc:plug-in
      name="converter"
      purpose="set-symbol"
    />
  </dc:domain>
  <!-- END, CR00241441 -->


</dc:domains>
