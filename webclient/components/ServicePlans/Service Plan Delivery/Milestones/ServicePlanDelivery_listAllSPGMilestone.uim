<!-- ===========                                                            -->
<!-- This page allows the user to view all the milestone associated with    -->
<!-- the service plan delivery.                                             -->
<PAGE
  PAGE_ID="ServicePlanDelivery_listAllSPGMilestone"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanGroupDelivery"
    NAME="DISPLAY"
    OPERATION="viewMilestone"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_DESC"
    OPERATION="getServicePlanGroupContextDescription"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_MENU"
    OPERATION="getICServicePlanGroupMenuData"
  />

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupDeliveryId"
    />
    <TARGET
      NAME="DISPLAY_MENU"
      PROPERTY="servicePlanGroupDeliveryID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="servicePlanGroupDeliveryId"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupDeliveryId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="Constant.True"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="allMileStones"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupDeliveryId"
    />
    <TARGET
      NAME="DISPLAY_DESC"
      PROPERTY="servicePlanGroupDeliveryID"
    />
  </CONNECT>
  
  <!-- BEGIN, CR00246725, MR -->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.IncompletedMilestones"
      STYLE="in-page-link"
      TYPE="ACTION"
      >
      <LINK
        PAGE_ID="ServicePlanDelivery_listSPGMilestone"
        SAVE_LINK="false"
        >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    
    <ACTION_CONTROL
      LABEL="ActionControl.Label.AllMilestones"
      STYLE="in-page-current-link"
      TYPE="ACTION"
      >
      <LINK
        PAGE_ID="ServicePlanDelivery_listAllSPGMilestone"
        SAVE_LINK="false"
        >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- END, CR00246725 -->


  <INCLUDE FILE_NAME="ServicePlanDelivery_listMileStone.vim"/>
</PAGE>
