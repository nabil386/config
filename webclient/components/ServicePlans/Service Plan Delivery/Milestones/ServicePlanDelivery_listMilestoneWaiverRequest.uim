<!-- This page allows a user to list the details of a milestone waiver      -->
<PAGE
  PAGE_ID="ServicePlanDelivery_listMilestoneWaiverRequest"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewMilestone"
      STYLE="in-page-link"
      TYPE="ACTION"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_viewMilestone"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="milestoneDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="milestoneDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <!-- BEGIN, CR00147167, SAI -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <!-- END, CR00147167 -->
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.WaiverRequest"
      STYLE="in-page-current-link"
      TYPE="ACTION"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_listMilestoneWaiverRequest"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="milestoneDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="milestoneDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <!-- BEGIN, CR00147167, SAI -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <!-- END, CR00147167 -->
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainWaiverApprovalRequest"
    NAME="DISPLAY"
    OPERATION="listMilestoneWaiverRequests"
  />


  <PAGE_PARAMETER NAME="milestoneDeliveryID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneDeliveryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="milestoneDeliveryID"
    />
  </CONNECT>


  <CLUSTER>
    <LIST>


      <ACTION_SET TYPE="LIST_ROW_MENU">


        <ACTION_CONTROL
          IMAGE="ApproveButton"
          LABEL="ActionControl.Approve.label"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="ServicePlanDelivery_approveWaiver"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="waiverApprovalRequestID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="waiverApprovalRequestID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
            </CONNECT>
            <!-- BEGIN, CR00147167, SAI -->
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="caseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseID"
              />
            </CONNECT>
            <!-- END, CR00147167 -->
          </LINK>
        </ACTION_CONTROL>


        <ACTION_CONTROL
          IMAGE="ModifyAndApproveButton"
          LABEL="ActionControl.ModifyAndApprove.label"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="ServicePlanDelivery_modifyAndApproveWaiver"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="waiverApprovalRequestID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="waiverApprovalRequestID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
            </CONNECT>
            <!-- BEGIN, CR00147167, SAI -->
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="caseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseID"
              />
            </CONNECT>
            <!-- END, CR00147167 -->
          </LINK>
        </ACTION_CONTROL>


        <ACTION_CONTROL
          IMAGE="RejectButton"
          LABEL="ActionControl.Reject.label"
        >
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="ServicePlanDelivery_rejectWaiver"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="waiverApprovalRequestID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="waiverApprovalRequestID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="milestoneDeliveryID"
              />
            </CONNECT>
            <!-- BEGIN, CR00147167, SAI -->
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="caseID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="caseID"
              />
            </CONNECT>
            <!-- END, CR00147167 -->
          </LINK>
        </ACTION_CONTROL>


      </ACTION_SET>


      <FIELD
        LABEL="Field.Label.RequestedDate"
        WIDTH="18"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="requestedDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.RequestedExpectedStartDate"
        WIDTH="32"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="requestedExpStartDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.RequestedExpectedEndDate"
        WIDTH="32"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="requestedExpEndDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Status"
        WIDTH="18"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="approvalReqStatus"
          />
        </CONNECT>
      </FIELD>


      <DETAILS_ROW>
        <INLINE_PAGE PAGE_ID="ServicePlanDelivery_viewMilestoneWaiverRequest">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="milestoneDeliveryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="milestoneDeliveryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="waiverApprovalRequestID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="waiverApprovalRequestID"
            />
          </CONNECT>
        </INLINE_PAGE>
      </DETAILS_ROW>


    </LIST>


  </CLUSTER>


</PAGE>
