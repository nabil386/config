<!-- This page allows a user to approve a milestone waiver request          -->
<PAGE PAGE_ID="ServicePlanDelivery_approveWaiver" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="MaintainWaiverApprovalRequest" NAME="ACTION" OPERATION="approveWaiver" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="waiverApprovalRequestID"/>
  <PAGE_PARAMETER NAME="milestoneDeliveryID"/>
  <!-- BEGIN, CR00147167, SAI -->
  <PAGE_PARAMETER NAME="caseID"/>
  <!-- END, CR00147167 -->


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="waiverApprovalRequestID"/>
    <TARGET NAME="ACTION" PROPERTY="waiverApprovalRequestID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.Approve"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>