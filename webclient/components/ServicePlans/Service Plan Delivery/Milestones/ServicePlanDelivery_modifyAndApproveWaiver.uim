<!-- This page allows a user to modify the waiver request and approve it.   -->
<PAGE
  PAGE_ID="ServicePlanDelivery_modifyAndApproveWaiver"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainWaiverApprovalRequest"
    NAME="ACTION"
    OPERATION="modifyAndApprove"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="MaintainWaiverApprovalRequest"
    NAME="DISPLAY"
    OPERATION="readExpStartAndEndDate"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="waiverApprovalRequestID"/>
  <PAGE_PARAMETER NAME="milestoneDeliveryID"/>
  <!-- BEGIN, CR00147167, SAI -->
  <PAGE_PARAMETER NAME="caseID"/>
  <!-- END, CR00147167 -->


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Approve"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
    </ACTION_CONTROL>


  </ACTION_SET>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waiverApprovalRequestID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="waiverApprovalRequestID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waiverApprovalRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="waiverApprovalRequestID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="2">


    <FIELD
      LABEL="Field.Label.ExpectedStartDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="expectedStartDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedStartDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.ExpectedEndDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.ApprovalComment"
  >
    <!-- BEGIN, CR00377016, DJ -->
    <FIELD HEIGHT="5" LABEL="Field.Label.ApprovalComment">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="approvalComments"
        />
      </CONNECT>
      <!-- END, CR00377016 -->
    </FIELD>
  </CLUSTER>


</PAGE>
