<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view integrated service plan delivery     -->
<!-- milestones.                                                            -->
<PAGE
  PAGE_ID="ServicePlanDelivery_listISPMilestones"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="IntegratedServicePlan"
    NAME="DISPLAY_MENU"
    OPERATION="readHomePageDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="IntegratedServicePlan"
    NAME="DISPLAY"
    OPERATION="listInProgressAndFutureMilestones"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_DESC"
    OPERATION="getIntegratedServicePlanContextDescription"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="Constant.False"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="allMileStones"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY_DESC"
      PROPERTY="integratedCaseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY_MENU"
      PROPERTY="integratedServicePlanHomePageKey$integratedCaseID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.IncompletedMilestones"
      STYLE="in-page-current-link"
      TYPE="ACTION"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_listISPMilestones"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.AllMilestones"
      STYLE="in-page-link"
      TYPE="ACTION"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_listAllISPMilestones"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <INCLUDE FILE_NAME="ServicePlanDelivery_listISPMilestoneView.vim"/>
</PAGE>
