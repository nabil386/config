<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view Client Participation details for     -->
<!-- particular plan item of service plan.                                     -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewClientParticipation_from_ServicePlanClientParticipationListForDay"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <INCLUDE FILE_NAME="ServicePlanDelivery_viewClientParticipation.vim"/>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanDelivery_modifyClientParticipation"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="dailyAttendanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="dailyAttendanceID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="DeleteButton"
      LABEL="ActionControl.Label.Delete"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanDelivery_cancelClientParticipation_fromCaseClientParticipationDailyList"
        WINDOW_OPTIONS="width=500"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="dailyAttendanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="dailyAttendanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dailyAttendanceDtls$dtls$dtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dailyAttendanceDtls$dtls$dtls$serviceDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="serviceDate"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    >


      <LINK PAGE_ID="ServicePlanDelivery_caseClientParticipationDailyList">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dailyAttendanceDtls$dtls$dtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dailyAttendanceDtls$dtls$dtls$serviceDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="serviceDate"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
