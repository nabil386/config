<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to add client participation                  -->
<!-- to the particular plan item of the service plan                        -->
<PAGE
  PAGE_ID="ServicePlanDelivery_addClientParticipationDefaultPlanItem"
  WINDOW_OPTIONS="width=725"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="ACTION"
    OPERATION="addClientParticipationDefaultPlanItem"
    PHASE="ACTION"
  />
  

  <PAGE_PARAMETER NAME="plannedItemID"/>
  <PAGE_PARAMETER NAME="name"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dailyAttendanceDtls$plannedItemID"
    />
  </CONNECT>

  <!--BEGIN, CR00234759, MR-->
  <SERVER_INTERFACE
    CLASS="ServicePlanTab"
    NAME="DISPLAY"
    OPERATION="readPlanItemTabDetails"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="plannedItemID"
    />
  </CONNECT>
  <!--END, CR00234759-->
  

  <CLUSTER
    LABEL_WIDTH="41"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.PlanItemName">
      <CONNECT>
        <!--BEGIN, CR00234759, MR-->
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
        <!--END, CR00234759-->
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ParticipationType">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$attendance"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.AbsenceReason"
      USE_BLANK="true"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$absenceReason"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Date"
      USE_BLANK="true"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$serviceDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="41"
    NUM_COLS="2"
    TITLE="Cluster.Title.TimeParticipated"
  >


    <FIELD
      LABEL="Field.Label.TotalHours"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="totalHours"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.TotalMinutes"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="totalMinutes"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <!--BEGIN, CR00168425, GBA-->
  <CLUSTER
    LABEL_WIDTH="41"
    NUM_COLS="2"
    TITLE="Cluster.Title.TimeAbsent"
  >


    <FIELD
      LABEL="Field.Label.HoursAbsent"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="hoursAbsent"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.MinutesAbsent"
      WIDTH="30"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="minutesAbsent"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
  <!--END, CR00168425-->


  <CLUSTER
    LABEL_WIDTH="20.5"
    NUM_COLS="1"
    TITLE="Cluster.Title.Progress"
  >
    <FIELD
      LABEL="Field.Label.Progress"
      USE_BLANK="true"
      WIDTH="25"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$progress"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.ProgressReason"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$progressReason"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dailyAttendanceDtls$dtls$dtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


</PAGE>
