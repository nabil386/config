<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page lists client participation for an plan item of the service      -->
<!-- for a particular week                                                  -->
<PAGE
  PAGE_ID="ServicePlanDelivery_plannedItemClientParticipationForWeekList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="plannedItemParticipationForWeekList"
  />


  <PAGE_PARAMETER NAME="plannedItemID"/>
  <PAGE_PARAMETER NAME="name"/>
  <PAGE_PARAMETER NAME="fromDate"/>
  <PAGE_PARAMETER NAME="toDate"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participationsPlanItemAndDateRangeKey$plannedItemID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fromDate"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participationsPlanItemAndDateRangeKey$fromDate"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="toDate"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participationsPlanItemAndDateRangeKey$toDate"
    />
  </CONNECT>


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="ServicePlanDelivery_viewClientParticipation_from_PlanItemClientParticipationListForWeek">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="piDtlsList$dtls$dailyAttendanceID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="dailyAttendanceID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fromDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fromDate"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="toDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="toDate"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanDelivery_modifyClientParticipation"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="piDtlsList$dtls$dailyAttendanceID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="dailyAttendanceID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <!-- BEGIN, CR00234759, MR -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanDelivery_cancelClientParticipation_fromPlannedItemClientParticipationListForWeek"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="piDtlsList$dtls$dailyAttendanceID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="dailyAttendanceID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="plannedItemID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="plannedItemID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="name"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="name"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="fromDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fromDate"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="toDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="toDate"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- END, CR00234759 -->
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.Date"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="piDtlsList$dtls$serviceDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ParticipationType"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="piDtlsList$dtls$attendance"
        />
      </CONNECT>
    </FIELD>


    <CONTAINER
      LABEL="Container.Label.totalTime"
      STYLE="date-time"
    >


      <FIELD ALIGNMENT="RIGHT">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="totalHours"
          />
        </CONNECT>
      </FIELD>


      <FIELD ALIGNMENT="CENTER">
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Container.TotalTime.Separator"
          />
        </CONNECT>
      </FIELD>


      <FIELD ALIGNMENT="LEFT">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="totalMinutes"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <CONTAINER
      LABEL="Container.Label.TimeAbsent"
      STYLE="date-time"
    >


      <FIELD ALIGNMENT="RIGHT">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="hoursAbsent"
          />
        </CONNECT>
      </FIELD>


      <FIELD ALIGNMENT="CENTER">
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Container.TotalTime.Separator"
          />
        </CONNECT>
      </FIELD>


      <FIELD ALIGNMENT="LEFT">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="minutesAbsent"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="piDtlsList$dtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
