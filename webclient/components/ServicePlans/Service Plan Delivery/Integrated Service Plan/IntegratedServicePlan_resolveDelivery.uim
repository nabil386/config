<!-- This jsp redirects the user to the correct service plan delivery page  -->
<PAGE PAGE_ID="IntegratedServicePlan_resolveDelivery">


  <JSP_SCRIPTLET>
  
      String summaryID = request.getParameter("summaryID");
      String summaryType = request.getParameter("summaryType");
      String description = request.getParameter("description");
      
      // BEGIN, CR00246725, MR
      String caseID = request.getParameter("caseID");
      String servicePlanDeliveryResolvePage ="ServicePlanDelivery_home1";
      String servicePlanGroupDeliveryResolvePage ="ServicePlanGroupDelivery_homeActiveOnly";
      // END, CR00246725
      
 
      curam.omega3.request.RequestHandler rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
      
      StringBuilder sbContext = new StringBuilder("");
      sbContext.append(request.getContextPath());
      sbContext.append("/");
      
      sbContext.append(curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale());
      sbContext.append("/");
  
      StringBuilder sbURL = new StringBuilder("");
      if(summaryType.equals("SPDT1"))
      {
        sbURL.append(sbContext.toString());
        sbURL.append(servicePlanDeliveryResolvePage);
        sbURL.append("Page.do?");
        
        sbURL.append("caseID=");
        sbURL.append(curam.omega3.request.RequestUtils.escapeURL(summaryID));
        
        sbURL.append("&amp;");
        sbURL.append(rh.getSystemParameters());
      }
      else if(summaryType.equals("SPDT2"))
      {
        sbURL.append(sbContext.toString());
        sbURL.append(servicePlanGroupDeliveryResolvePage);
        sbURL.append("Page.do?");
        
        sbURL.append("servicePlanGroupDeliveryId=");
        sbURL.append(curam.omega3.request.RequestUtils.escapeURL(summaryID));
        
        // BEGIN, CR00246725, MR
        sbURL.append("&amp;caseID=");
        sbURL.append(curam.omega3.request.RequestUtils.escapeURL(caseID));
        // END, CR00246725
        sbURL.append("&amp;description=");
        sbURL.append(curam.omega3.request.RequestUtils.escapeURL(description));
        
        sbURL.append("&amp;");
        sbURL.append(rh.getSystemParameters());
      }
      String url = sbURL.toString();
      response.sendRedirect(response.encodeRedirectURL(url));      
    
    </JSP_SCRIPTLET>


</PAGE>
