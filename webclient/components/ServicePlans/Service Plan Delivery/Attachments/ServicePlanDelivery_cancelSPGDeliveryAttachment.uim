<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to cancel a service plan case attachment.    -->

<!-- BEGIN, CR00246725, MR -->
<PAGE PAGE_ID="ServicePlanDelivery_cancelSPGDeliveryAttachment" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
<!-- END, CR00246725 -->

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="SPGDeliveryAttachment" NAME="ACTION" OPERATION="removeAttachment" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="attachmentID"/>
  <PAGE_PARAMETER NAME="deliveryAttachmentLinkId"/>
  <PAGE_PARAMETER NAME="attachmentVersionNo"/>
  <PAGE_PARAMETER NAME="attachmentLinkVersionNo"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <PAGE_PARAMETER NAME="servicePlanGroupDeliveryId"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="attachmentID"/>
    <TARGET NAME="ACTION" PROPERTY="attachmentID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="attachmentVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="attachmentDtls$versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="attachmentLinkVersionNo"/>
    <TARGET NAME="ACTION" PROPERTY="spgAttachmentDtls$versionNo"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="deliveryAttachmentLinkId"/>
    <TARGET NAME="ACTION" PROPERTY="deliveryAttachmentLinkId"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="ServicePlanDelivery_listSPGDeliveryAttachment" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupDeliveryId"/>
          <TARGET NAME="PAGE" PROPERTY="servicePlanGroupDeliveryId"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="ServicePlanDelivery_viewSPGDeliveryAttachment" SAVE_LINK="false">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="attachmentID"/>
          <TARGET NAME="PAGE" PROPERTY="attachmentID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="deliveryAttachmentLinkId"/>
          <TARGET NAME="PAGE" PROPERTY="deliveryAttachmentLinkId"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="pageDescription"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupDeliveryId"/>
          <TARGET NAME="PAGE" PROPERTY="servicePlanGroupDeliveryId"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">


    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelAttachment"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>