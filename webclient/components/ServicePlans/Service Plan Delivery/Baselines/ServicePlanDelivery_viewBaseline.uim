<!-- Page to view the details of a baseline                        -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewBaseline"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewBaseline"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_viewBaseline"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.PlanContent"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_baselinePlanContent"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Milestones"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_listBaselineMilestones"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Gantt"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanDelivery_viewBaselineGantt"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- BEGIN, CR00069948, GBA -->
  <!-- END, CR00069948 -->
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewBaselineHome"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="baselineID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baselineID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="baselineKey$key$baselineKey$baseLineID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Title.Goal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="goalName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster_Title_Comments"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
