<!-- This page allows the user to select baseline for comparison.           -->
<PAGE
  PAGE_ID="ServicePlanDelivery_selectBaselineForCompare"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="readBaselinesForComparison"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="ACTION"
    OPERATION="compareBaselines"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="baseLineID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baseLineID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$baselineIDAndCaseIDKey$baselineIDAndCaseIDKey$baseLineID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baseLineID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$baselineCompareKey$originalBaselineID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="Compare"
      LABEL="ActionControl.Label.Compare"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ServicePlanDelivery_baselineComparisonGantt">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="key$baselineCompareKey$originalBaselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="originalBaselineID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="selectedBaselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="selectedBaselineID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="currentPlanInd"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="currentPlanInd"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK PAGE_ID="ServicePlanDelivery_viewBaseline">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baseLineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER
    DESCRIPTION="Cluster.Details.Description"
    LABEL_WIDTH="50"
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >


    <FIELD LABEL="Field.Label.Baseline">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$existingBaselinesDetails$baselineNameDetails$name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.CompareWith"
      USE_BLANK="true"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$existingBaselinesDetails$baselineNameAndIDDetailsList$dtls$baseLineID"
          NAME="DISPLAY"
          PROPERTY="result$existingBaselinesDetails$baselineNameAndIDDetailsList$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="selectedBaselineID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CurrentPlan">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="currentPlanInd"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
