<!-- Page to view the details of a baseline sub-goal                        -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewBaselineSubGoal"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewServicePlanBaselineSubGoalDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="baselineID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="baselineSubGoalID"/>
  <PAGE_PARAMETER NAME="plannedSubGoalID"/>
  <PAGE_PARAMETER NAME="baselinePlanItemID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baselineSubGoalID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$baselineSubGoalID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Title.Sensitivity">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$baselineSubGoalDetails$sensitivityCode"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <LIST TITLE="Cluster.Title.PlanItems">
    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="list$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ExpectedStartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="list$expectedStartDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ExpectedEndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="list$expectedEndDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ActualStartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="list$actualStartDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ActualEndDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="list$actualEndDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00204807, MC -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="ServicePlanDelivery_viewBaselinePlanItem">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="list$baselinePlanItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselinePlanItemID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00204807 -->
  </LIST>
  <LIST TITLE="Cluster.Title.Milestones">
    <!-- BEGIN, CR00306738, VT -->
    <FIELD
      LABEL="Field.Title.MilestoneName"
      WIDTH="20"
    >
      <!-- END, CR00306738 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$name"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00306738, VT -->
    <FIELD
      LABEL="Field.Title.MilestoneExpectedStartDate"
      WIDTH="20"
    >
      <!-- END, CR00306738 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$expectedStartDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00306738, VT -->
    <FIELD
      LABEL="Field.Title.MilestoneExpectedEndDate"
      WIDTH="20"
    >
      <!-- END, CR00306738 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$expectedEndDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00306738, VT -->
    <FIELD
      LABEL="Field.Title.MilestoneActualStartDate"
      WIDTH="20"
    >
      <!-- END, CR00306738 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$actualStartDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00306738, VT -->
    <FIELD
      LABEL="Field.Title.MilestoneActualEndDate"
      WIDTH="20"
    >
      <!-- END, CR00306738 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$actualEndDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00204807, MC -->
    <!-- BEGIN, CR00236322, MC -->
    <DETAILS_ROW>
      <!-- BEGIN, CR00282078, AKr -->
      <INLINE_PAGE PAGE_ID="ServicePlanDelivery_resolveBaselineMilestoneExists">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$baselineSubGoalDetails$milestoneList$listDetails$dtls$milestoneDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="milestoneDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="baselineID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="baselineID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </INLINE_PAGE>
      <!-- END, CR00282078 -->
    </DETAILS_ROW>
    <!-- END, CR00236322 -->
    <!-- END, CR00204807 -->
  </LIST>
</PAGE>
