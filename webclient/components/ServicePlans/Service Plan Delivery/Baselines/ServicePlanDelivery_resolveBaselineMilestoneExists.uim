<!-- This jsp redirects the user to the appropriate page                    -->
<!-- to view details of a plan content item.                                -->
<PAGE
  PAGE_ID="ServicePlanDelivery_resolveBaselineMilestoneExists"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
      String milestoneDeliveryID = request.getParameter("milestoneDeliveryID");
      String description = request.getParameter("description");
      String url = "";
      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    
      if (milestoneDeliveryID == null)
      {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "milestoneDeliveryID");
       throw e;
       // END, CR00110216
      }
      if (description == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "description");
       throw e;
       // END, CR00110216
      }
      // BEGIN, CR00282078, AKr
      String baselineID = request.getParameter("baselineID");  
      if (baselineID == null) {
      core.ScriptletMissingParamException e = new
      core.ScriptletMissingParamException(-20002, "baselineID");
      throw e;
      }
      
      if (!baselineID.equals("0")) {
      curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_readBaselineMilestoneID_TH
      th = new curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_readBaselineMilestoneID_TH();      
      
      th.setFieldValue(th.baselineAndMilestoneDeliveryKey$milestoneDeliveryID_idx, milestoneDeliveryID);
      th.setFieldValue(th.baselineAndMilestoneDeliveryKey$baselineID_idx, baselineID);
      th.callServer();
      String baselineMilestoneID =th.getFieldValue(th.result$baselineMilestoneID_idx); 
      
      if (milestoneDeliveryID.equals("0"))
      {
      url = context + "ServicePlanDelivery_viewBaselineMilestoneDoesNotExistPage.do?description=" + curam.omega3.request.RequestUtils.escapeURL(description);
      }
      else 
      {
      url = context + "ServicePlanDelivery_viewMilestoneForBaselinePage.do?baselineMilestoneID=" + curam.omega3.request.RequestUtils.escapeURL(baselineMilestoneID) +"&amp;description=" + curam.omega3.request.RequestUtils.escapeURL(description);      
      }
      }  
      // END, CR00282078
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
