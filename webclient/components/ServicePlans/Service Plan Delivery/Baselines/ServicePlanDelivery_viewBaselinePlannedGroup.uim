<!-- Page to view the details of a baseline planned group                   -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewBaselinePlannedGroup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewBaselinePlanGroupDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY2"
    OPERATION="viewBaselineHome"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="baselinePlannedGroupID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="baselineID"/>
  <PAGE_PARAMETER NAME="plannedGroupID"/>


  <PAGE_PARAMETER NAME="baselinePlanContentType"/>


  <PAGE_PARAMETER NAME="baselinePlanContentID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baselineID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="baselineID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baselinePlannedGroupID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="baselinePlanGroupID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedGroupID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="baselinePlanGroupReadDetailsKey$plannedGroupID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="baselineID"
    />
    <TARGET
      NAME="DISPLAY2"
      PROPERTY="baselineKey$baseLineID"
    />
  </CONNECT>


  <LIST>
    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="31"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD WIDTH="5">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="planGroupOrSubGoalInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ExpectedStart"
      WIDTH="17"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$expectedStartDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ExpectedEnd"
      WIDTH="17"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$expectedEndDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ActualStart"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$actualStartDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.ActualEnd"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$actualEndDate"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
