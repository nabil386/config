<!-- This page allows the user to cancel a planned item attachment.         -->
<PAGE PAGE_ID="ServicePlanDelivery_cancelPlannedItemAttachment" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <!-- Page Title information for this page -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- Page Parameters used in a page -->
  <PAGE_PARAMETER NAME="plannedItemID"/>
  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="plannedItemAttachmentLinkID"/>


  <!-- Server Access Beans used in this page -->
  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="DISPLAY" OPERATION="viewAttachment" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="ACTION" OPERATION="deleteAttachment" PHASE="ACTION"/>


  <!-- Page Level Connect Statements -->
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="plannedItemAttachmentLinkID"/>
    <TARGET NAME="DISPLAY" PROPERTY="plannedItemAttachmentLinkKey$plannedItemAttachmentLinkID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="plannedItemAttachmentLinkID"/>
    <TARGET NAME="ACTION" PROPERTY="plannedItemAttachmentLinkKey$plannedItemAttachmentLinkID"/>
  </CONNECT>


  <!-- Cluster to display a Confirmation for deletion of Attachment for a planned item -->
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelAttachment"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>