<!-- This page allows the user to add a basic plan item to a sub goal.         -->
<PAGE
  PAGE_ID="ServicePlanDelivery_addBasicPlanItemToSubGoal"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00226831, LP -->
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="readPossibleOutcomesAndSubGoalNameDetailsForPlanItem"
    PHASE="DISPLAY"
  />
  <!-- END, CR00226831 -->


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="ACTION"
    OPERATION="createBasicPlanItemToSubGoal"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="planItemID"/>
  <PAGE_PARAMETER NAME="plannedSubGoalID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedSubGoalID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="plannedSubGoalID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedSubGoalID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="plannedSubGoalID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="planItemName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="name"
    />
  </CONNECT>
  <!-- BEGIN, CR00161962, LJ -->
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="guidanceURL"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="createPlannedItemDetailsStruct$createPlannedItemDetailsStruct$plannedItemDtls$guidanceURL"
    />
  </CONNECT>
  <!-- END, CR00161962 -->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.SubGoal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="subGoalName"
        />
      </CONNECT>
    </FIELD>


    <!--BEGIN CR00109001, GBA-->
    <!--BEGIN, CR00129695, GBA-->
    <FIELD LABEL="Field.Label.Concerning">
      <!--BEGIN, CR00146503, GBA-->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="participantList$list$dtls$concernRoleID"
          NAME="DISPLAY"
          PROPERTY="participantList$list$dtls$concernRoleName"
        />
      </CONNECT>
      <!--END, CR00146503-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="concerningID"
        />
      </CONNECT>
    </FIELD>
    <!--END, CR00129695-->
    <!--END CR00109001-->


    <FIELD LABEL="Field.Label.ExpectedStartDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedStartDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedOutcome">
      <!-- BEGIN, CR00052590, NRV -->
      <!-- BEGIN HARP, 71280, PN-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="plannedItemDtls$expectedOutcomeID"
        />
      </CONNECT>
      <LINK>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
      <!-- END HARP, 71280 -->
      <!-- END CR00052590, -->
    </FIELD>


    <!-- BEGIN, CR00161962, LJ -->
    <FIELD LABEL="Field.Label.GuidanceURL">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="guidanceURL"
        />
      </CONNECT>
      <LINK
        OPEN_NEW="true"
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="guidanceURL"
      />
    </FIELD>
    <!-- END, CR00161962 -->


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="planItemName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Sensitivity"
      WIDTH="20"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="sensitivityCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedEndDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EstimatedCost"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="estimatedCost"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <!--BEGIN CR00144311, GBA-->
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Description"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="4"
      LABEL="Cluster.Label.Description"
    >
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!--END CR00144311-->


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TITLE="Cluster.Label.Responsibility"
  >


    <FIELD LABEL="Field.Label.ResponsibilitySetToMe">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToMeInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResponsibilitySetToSubGoalOwner">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToSubGoalOwnerInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResponsibilitySetToUser">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToUser"
        />
      </CONNECT>
    </FIELD>
    <!--BEGIN CR00108989, GBA-->
    <FIELD
      LABEL="Field.Label.Participants"
      USE_BLANK="true"
    >
      <!--BEGIN, CR00146503, GBA-->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="respParticipantList$list$dtls$concernRoleID"
          NAME="DISPLAY"
          PROPERTY="respParticipantList$list$dtls$concernRoleName"
        />
      </CONNECT>
      <!--END, CR00146503-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToParticipant"
        />
      </CONNECT>
    </FIELD>
    <!--END CR00108989-->
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TITLE="Cluster.Label.Owner"
  >


    <FIELD LABEL="Field.Label.OwnerSetToSubGoalOwner">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ownerSetToSubGoalOwnerInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OwnerSetToUser">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ownerSetToUser"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Comments"
  >
    <!-- BEGIN, CR00417389, VT -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00417389 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
