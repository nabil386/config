<!-- This page allows the user to view the custom basic plan item details   -->
<!-- from Gantt Chart                                                       -->
<PAGE PAGE_ID="ServicePlanDelivery_viewCustomPlanItem_fromGantt" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <INCLUDE FILE_NAME="ServicePlanDelivery_viewServicePlanBasicPlanItemView.vim"/>

  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="EditButton" LABEL="ActionControl.Label.Edit">
      <LINK PAGE_ID="ServicePlanDelivery_resolveModifyPlanItemPage">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="plannedItemID"/>
          <TARGET NAME="PAGE" PROPERTY="plannedItemID"/>
        </CONNECT>
        <CONNECT>
          <!--BEGIN CR00144311, GBA-->
          <SOURCE NAME="DISPLAY" PROPERTY="description$description"/>
          <!--END CR00144311-->
          <TARGET NAME="PAGE" PROPERTY="description"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="DeleteButton" LABEL="ActionControl.Label.Delete">
      <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanDelivery_removeServicePlanPlanItem" WINDOW_OPTIONS="width=400,height=150">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="plannedItemID"/>
          <TARGET NAME="PAGE" PROPERTY="plannedItemID"/>
        </CONNECT>
        <CONNECT>
          <!--BEGIN CR00144311, GBA-->
          <SOURCE NAME="DISPLAY" PROPERTY="description$description"/>
          <!--END CR00144311-->
          <TARGET NAME="PAGE" PROPERTY="description"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    
  </ACTION_SET>
  
</PAGE>