<!-- This jsp redirects the user to the View PlanItem Page.-->
<PAGE
  PAGE_ID="ServicePlanDelivery_resolveViewPlanItemPage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
                 
      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String description = request.getParameter("description");     
      String plannedItemID = request.getParameter("plannedItemID");
      // BEGIN, CR00161962, LJ
      String caseID = request.getParameter("caseID");  
      // END, CR00161962    
      String viewPlanItemPageName="";
      String viewPlanItemPageParam="";
      String url = "";
        
      if (plannedItemID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "plannedItemID");
       throw e;
       // END, CR00110216
      }
      // BEGIN, CR00161962, LJ
      if (caseID == null) {
        curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_readCaseID_TH
         thCaseID = new curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_readCaseID_TH();
      
        thCaseID.setFieldValue(thCaseID.key$plannedItemIDKey$plannedItemIDKey$plannedItemID_idx,plannedItemID);
        thCaseID.callServer();
        
        caseID = thCaseID.getFieldValue(thCaseID.result$servicePlanDeliveryKey$key$caseID_idx);
      }
      // END, CR00161962

      curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_resolveViewPlanItemPage_TH
        th = new curam.interfaces.ServicePlanDeliveryPkg.ServicePlanDelivery_resolveViewPlanItemPage_TH();
      
      try {
       
      th.setFieldValue(th.plannedItemIDKey$plannedItemIDKey$plannedItemIDKey$plannedItemID_idx,plannedItemID);

      th.callServer();
     
      viewPlanItemPageName=th.getFieldValue(th.result$viewPageDetailsStruct$viewPageDetailsStruct$viewPageName_idx);
      viewPlanItemPageParam=th.getFieldValue(th.result$viewPageDetailsStruct$viewPageDetailsStruct$viewPagePlanItemParamName_idx);

    // BEGIN, CR00060281, MC
    // BEGIN, CR00110216, SK
    // BEGIN, CR00161962, LJ
    if(viewPlanItemPageName.equals("")){
       url = context + "ServicePlanAdmin_planItemErrorhomePage.do";
    } 
    
    // BEGIN, CR00236314, MR
     else if(description !=null) {
       url = context + curam.omega3.request.RequestUtils.escapeURL(viewPlanItemPageName) + "Page.do?" + curam.omega3.request.RequestUtils.escapeURL(viewPlanItemPageParam) + "=" + curam.omega3.request.RequestUtils.escapeURL(plannedItemID) + "&amp;caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID) + "&amp;description=" + curam.omega3.request.RequestUtils.escapeURL(description);
       url += "&amp;" + rh.getSystemParameters();     
    }
    // END, CR00236314
    else {
        url = context + curam.omega3.request.RequestUtils.escapeURL(viewPlanItemPageName) + "Page.do?" + curam.omega3.request.RequestUtils.escapeURL(viewPlanItemPageParam) + "=" + curam.omega3.request.RequestUtils.escapeURL(plannedItemID) + "&amp;caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID);
        url += "&amp;" + rh.getSystemParameters();
   }
    // END, CR00161962
      response.sendRedirect(response.encodeRedirectURL(url));
      
      }
      catch(java.lang.Exception e) {
        url = context + "ServicePlanDelivery_viewServicePlanBasicPlanItemPage.do?plannedItemID=" + curam.omega3.request.RequestUtils.escapeURL(plannedItemID) + "&amp;amp;description=" + curam.omega3.request.RequestUtils.escapeURL(description); 
        url += "&amp;" + rh.getSystemParameters();
        
        response.sendRedirect(response.encodeRedirectURL(url));
      }
      // END, CR00110216
    // END, CR00060281
  </JSP_SCRIPTLET>
</PAGE>
