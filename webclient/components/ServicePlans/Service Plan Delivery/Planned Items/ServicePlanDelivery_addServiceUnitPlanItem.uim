<!-- This page allows the user to add a service unit delivery plan item to a sub goal.   -->
<PAGE
  PAGE_ID="ServicePlanDelivery_addServiceUnitPlanItem"
  WINDOW_OPTIONS="width=750"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00226831, LP -->
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="readDtlsForAddServiceUnitPlannedItem"
    PHASE="DISPLAY"
  />
  <!-- END, CR00226831 -->


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="ACTION"
    OPERATION="createServiceUnitDeliveryPlanItemToSubGoal"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="planItemID"/>
  <PAGE_PARAMETER NAME="plannedSubGoalID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedSubGoalID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="plannedSubGoalID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedSubGoalID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="plannedSubGoalID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="planItemName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="name"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="authorizedUnits"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="authorizedUnits"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="maximumUnits"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="maximumUnits"
    />
  </CONNECT>
  <!-- BEGIN, CR00161962, LJ -->
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$guidanceURL"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$createPlannedItemDetailsStruct$plannedItemDtls$guidanceURL"
    />
  </CONNECT>
  <!-- END, CR00161962 -->
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.SubGoal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="subGoalName"
        />
      </CONNECT>
    </FIELD>


    <!--BEGIN CR00124520, GBA-->
    <FIELD LABEL="Field.Label.Concerning">
      <!--BEGIN, CR00146503, GBA-->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="participants$list$dtls$concernRoleID"
          NAME="DISPLAY"
          PROPERTY="participants$list$dtls$concernRoleName"
        />
      </CONNECT>
      <!--END, CR00146503-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="concerningID"
        />
      </CONNECT>
    </FIELD>
    <!--END CR00124520-->


    <FIELD LABEL="Field.Label.UnitType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="unitType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedStartDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedStartDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedOutcome">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="outcomesForPlanItem$dtls$dtls$outcomeID"
          NAME="DISPLAY"
          PROPERTY="outcomesForPlanItem$dtls$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="plannedItemDtls$expectedOutcomeID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00161962, LJ -->
    <FIELD LABEL="Field.Label.GuidanceURL">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$guidanceURL"
        />
      </CONNECT>
      <LINK
        OPEN_NEW="true"
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="result$guidanceURL"
      />
    </FIELD>
    <!-- END, CR00161962 -->


    <FIELD
      LABEL="Field.Label.Sensitivity"
      WIDTH="20"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="sensitivityCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="planItemName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AuthorizedUnits">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="authorizedUnits"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedEndDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expectedEndDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EstimatedCost"
      WIDTH="35"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="estimatedCost"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TITLE="Cluster.Label.Responsibility"
  >


    <FIELD LABEL="Field.Label.ResponsibilitySetToMe">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToMeInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ResponsibilitySetToSubGoalOwner">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToSubGoalOwnerInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ResponsibilitySetToUser">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToUser"
        />
      </CONNECT>
    </FIELD>


    <!--BEGIN CR00124520, GBA-->
    <FIELD
      LABEL="Field.Label.Participants"
      USE_BLANK="true"
    >
      <!--BEGIN, CR00146503, GBA-->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="respParticipantList$list$dtls$concernRoleID"
          NAME="DISPLAY"
          PROPERTY="respParticipantList$list$dtls$concernRoleName"
        />
      </CONNECT>
      <!--END, CR00146503-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="responsibilitySetToParticipant"
        />
      </CONNECT>
    </FIELD>
    <!--END CR00124520-->


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TITLE="Cluster.Label.Owner"
  >


    <FIELD LABEL="Field.Label.OwnerSetToSubGoalOwner">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ownerSetToSubGoalOwnerInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.OwnerSetToUser">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ownerSetToUser"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Comments"
  >
    <!-- BEGIN, CR00417389, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00417389 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
