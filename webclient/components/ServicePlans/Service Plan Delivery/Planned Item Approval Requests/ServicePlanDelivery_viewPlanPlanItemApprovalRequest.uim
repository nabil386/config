<!-- This page allows the user to view an approval request for a specific   -->
<!-- Planned Item                                                         -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewPlanPlanItemApprovalRequest"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="description"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewPlanItemApprovalRequest"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="plannedItemID"/>
  <PAGE_PARAMETER NAME="plannedItemApprovalRequestID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedItemApprovalRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="paApprovalRequestKey$plannedItemApprovalRequestID"
    />
  </CONNECT>

  <CLUSTER
    LABEL_WIDTH="21"
    NUM_COLS="1"
    TITLE="Cluster.Title.Rejection"
  >


    <FIELD
      LABEL="Field.Label.RejectionReason"
      WIDTH="36"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$plannedItemApprovalRequestDetails$approvalRequestDtls$rejectionReason"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.RejectionComments"
      WIDTH="36"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$plannedItemApprovalRequestDetails$approvalRequestDtls$rejectionComments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
