<!-- This page allows the user to view the Service Plan Tracking Gantt.     -->
<PAGE
  PAGE_ID="ServicePlanDelivery_cost"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00222522, ELG -->
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="displayServicePlanStatement"
    PHASE="DISPLAY"
  />
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00222522 -->


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
  >
    <!-- BEGIN, CR00222522, ELG -->
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAY"
        PROPERTY="statementDetailsXMLEmptyInd"
      />
    </CONDITION>
    <!-- END, CR00222522 -->


    <FIELD CONFIG="Statement.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="statementDetails"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
