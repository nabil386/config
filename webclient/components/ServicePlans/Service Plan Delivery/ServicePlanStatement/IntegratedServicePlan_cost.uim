<!-- This page allows the user to view the Integrated Service Plan Costs.   -->
<PAGE
  PAGE_ID="IntegratedServicePlan_cost"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="showIntegratedServicePlanStatement"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="integratedCaseID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
  >
    <FIELD CONFIG="R2.Statement.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="newStatementDetails"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
