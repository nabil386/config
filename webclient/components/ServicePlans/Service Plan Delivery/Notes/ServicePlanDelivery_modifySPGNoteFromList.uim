<!-- This page allows a user to modify a service plan group delivery note.  -->
<PAGE
  PAGE_ID="ServicePlanDelivery_modifySPGNoteFromList"
  WINDOW_OPTIONS="width=705,height=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  
   <SERVER_INTERFACE
    CLASS="SPGDeliveryNote"
    NAME="DISPLAY_STATUS"
    OPERATION="readNoteLockedStatus"
  />

  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="spgDeliveryNoteLinkId"
    />
    <TARGET
      NAME="DISPLAY_STATUS"
      PROPERTY="spgDeliveryNoteLinkId"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupDeliveryId"
    />
    <TARGET
      NAME="DISPLAY_STATUS"
      PROPERTY="servicePlanGroupDeliveryId"
    />
  </CONNECT>

  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <CONDITION>
        <IS_FALSE NAME="DISPLAY_STATUS" PROPERTY="noteLockedInd" />
      </CONDITION>

      <LINK PAGE_ID="ServicePlanDelivery_listSPGNote1">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupDeliveryId"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="spgDeliveryNoteLinkId"/>
  <PAGE_PARAMETER NAME="servicePlanGroupDeliveryId"/>
  <PAGE_PARAMETER NAME="pageDescription"/>


  <INCLUDE FILE_NAME="ServicePlanDelivery_modifySPGNote1.vim"/>


</PAGE>
