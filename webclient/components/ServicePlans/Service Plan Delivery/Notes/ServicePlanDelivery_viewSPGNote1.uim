<!-- This page allows a user to view a service plan group delivery note.    -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewSPGNote1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="servicePlanGroupDeliveryId"/>
  <PAGE_PARAMETER NAME="spgDeliveryNoteLinkId"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupDeliveryId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$servicePlanGroupDeliveryId"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="spgDeliveryNoteLinkId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$spgDeliveryNoteLinkId"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="SPGDeliveryNote"
    NAME="DISPLAY"
    OPERATION="viewNote"
  />


  <CLUSTER>


    <CLUSTER
      LABEL_WIDTH="28"
      NUM_COLS="2"
      STYLE="cluster-cpr-no-border"
    >


      <FIELD LABEL="Field.Label.Sensitivity">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="sensitivityCode"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Cluster.Label.Comments"
  >
  
    <!-- conditional cluster that shows an icon and message if the note is editable -->
     <CLUSTER>
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="noteEditableInd"/>
      </CONDITION>
      <CONTAINER>
              <IMAGE IMAGE="UnlockedIcon" LABEL="Icon.Editable"/>
              <FIELD>
                    <CONNECT>
                      <SOURCE NAME="DISPLAY" PROPERTY="editableNoteMessageOpt" />
                    </CONNECT>
              </FIELD>
            </CONTAINER>
    </CLUSTER>
    <!-- conditional cluster that shows a different icon and message if the note is locked for editing --> 
    <CLUSTER>
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="noteLockedInd" />
      </CONDITION>
      <CONTAINER>
              <IMAGE IMAGE="LockedIcon" LABEL="Icon.Locked"/>
              <FIELD>
                    <CONNECT>
                      <SOURCE NAME="DISPLAY" PROPERTY="editableNoteMessageOpt" />
                    </CONNECT>
              </FIELD>
            </CONTAINER>
    </CLUSTER>
    
    <!-- BEGIN, CR00416277, VT -->
    <FIELD
      HEIGHT="3"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00416277 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="notesText"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
