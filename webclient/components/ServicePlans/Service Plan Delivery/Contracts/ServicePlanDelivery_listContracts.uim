<!-- Page to list contracts for an service plan delivery.                   -->
<PAGE
  PAGE_ID="ServicePlanDelivery_listContracts"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!--BEGIN, CR00145825, SK-->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="listContractswithCaseStatus"
    PHASE="DISPLAY"
  />
  <!--END, CR00145825-->
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="description"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <!--BEGIN, CR00145825, SK-->
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$servicePlanDeliveryKey$key$caseID"
    />
    <!--END, CR00145825-->
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
    TOP="true"
  >
    <!--BEGIN, CR00145825, SK-->
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="caseStatusInd"
      />
    </CONDITION>
    <!--END, CR00145825-->


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <!-- BEGIN, CR00236562, SS-->
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="caseStatusInd"
        />
      </CONDITION>
      <!-- END, CR00236562 -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanDelivery_selectContractClient"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanDelivery_modifyContractFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="servicePlanContractID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="servicePlanContractID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
          <!-- BEGIN, CR00123092, MC -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <!-- END, CR00123092 -->


          <!-- BEGIN, CR00124499, MC -->


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="issuedToParticipantRole"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="issuedToParticipantRole"
            />
          </CONNECT>


          <!-- END, CR00124499-->


        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="PrintButton"
        LABEL="ActionControl.Label.Print"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanDelivery_printContract"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="servicePlanContractID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="servicePlanContractID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ViewContract"
        LABEL="ActionControl.Label.ViewContract"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanDelivery_previewContract"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="servicePlanContractID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="servicePlanContractID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>
    <FIELD
      LABEL="Field.Title.IssuedTo"
      WIDTH="33"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="clientName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.DateIssued"
      WIDTH="33"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateIssued"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Status"
      WIDTH="34"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="ServicePlanDelivery_viewContractHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$servicePlanDeliveryKey$key$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="servicePlanContractID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanContractID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>
</PAGE>
