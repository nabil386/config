<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to create a service plan contract for a      -->
<!-- service plan delivery.                                                 -->
<PAGE
  PAGE_ID="ServicePlanDelivery_createContract"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="readContractDetailsForCreateContract"
    PHASE="DISPLAY"
  />


  <!--BEGIN, CR00123092, MC -->


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_PLANNED_PARTICIPANTS"
    OPERATION="listPlanParticipantsForContract"
    PHASE="DISPLAY"
  />
  <!--END, CR00123092 -->


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="ACTION"
    OPERATION="createContract"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="issuedToParticipantRole"/>
  <PAGE_PARAMETER NAME="caseParticipantRoleID"/>
  <PAGE_PARAMETER NAME="name"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="issuedToParticipantRole"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="issuedToParticipantRole"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="issuedToParticipantRole"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="participantRoleID"
    />
  </CONNECT>


  <!--BEGIN, CR00123092, MC -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="issuedToParticipantRole"
    />
    <TARGET
      NAME="DISPLAY_PLANNED_PARTICIPANTS"
      PROPERTY="issueToID"
    />
  </CONNECT>
  <!--END, CR00123092 -->


  <!-- BEGIN, CR00124499, MC -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY_PLANNED_PARTICIPANTS"
      PROPERTY="caseID"
    />
  </CONNECT>


  <!-- END, CR00124499-->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ServicePlanDelivery_listContracts">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!-- BEGIN, CR00233700, SS -->
    <ACTION_CONTROL
      APPEND_ELLIPSIS="false"
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <!-- END, CR00233700 -->
      <!-- BEGIN, CR00234028, SS -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="ServicePlanDelivery_selectContractClient"
      >
        <!-- END, CR00234028 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="SaveAndPrint"
      LABEL="ActionControl.Label.SaveAndPrint"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00104430, ELG -->
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanDelivery_resolvePrintContract"
        WINDOW_OPTIONS="width=500,height=150"
      >
        <!-- END, CR00104430 -->
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="createSPContractKey$servicePlanContractID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanContractID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK PAGE_ID="ServicePlanDelivery_listContracts">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="name"
        />
      </CONNECT>


    </FIELD>


    <FIELD LABEL="Field.Label.Address">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="addressID"
        />
      </CONNECT>
      <LINK>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="issuedToParticipantRole"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Label.IssueDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dateIssued"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Reason">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="issueReason"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <!-- BEGIN, CR00123092, MC -->
  <!-- List of Planned participants for the selected service plan -->
  <LIST TITLE="List.Title.SignedBy">
    <!-- BEGIN, CR00125260, ANK -->
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY_PLANNED_PARTICIPANTS"
        PROPERTY="havingMultiplePlanParticipants"
      />
    </CONDITION>
    <!-- END, CR00125260 -->
    <CONTAINER
      ALIGNMENT="CENTER"
      WIDTH="10"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY_PLANNED_PARTICIPANTS"
              PROPERTY="participantRoleID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="participantRoleID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="signedBy"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>
    <FIELD LABEL="Field.Label.PlannedParticipantName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY_PLANNED_PARTICIPANTS"
          PROPERTY="fullName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY_PLANNED_PARTICIPANTS"
          PROPERTY="typeCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.FromDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY_PLANNED_PARTICIPANTS"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ToDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY_PLANNED_PARTICIPANTS"
          PROPERTY="toDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY_PLANNED_PARTICIPANTS"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!-- END, CR00123092 -->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >

    <!-- BEGIN, CR00408407, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00408407 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
