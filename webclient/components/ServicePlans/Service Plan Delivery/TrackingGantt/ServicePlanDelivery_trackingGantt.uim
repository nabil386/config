<!-- This page allows the user to view the Service Plan Tracking Gantt.     -->
<PAGE
  PAGE_ID="ServicePlanDelivery_trackingGantt"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewTrackingGantt"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <!--BEGIN CR00139524, GBA-->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="displayPlanInd"
      />
    </CONDITION>
    <FIELD CONFIG="Gantt.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="trackGanttXmlString"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!--END CR00139524-->


</PAGE>
