<!-- This page allows the user to record a service unit delivery.              -->
<PAGE PAGE_ID="ServicePlanDelivery_recordServiceUnitDelivery" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="DISPLAY" OPERATION="readDetailsForAddServiceUnitDelivery" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="ACTION" OPERATION="createServiceUnitDelivery" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="plannedItemID"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="plannedItemID"/>
    <TARGET NAME="ACTION" PROPERTY="plannedItemID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="plannedItemID"/>
    <TARGET NAME="DISPLAY" PROPERTY="plannedItemIDKey$plannedItemID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="true">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
      <LINK PAGE_ID="ServicePlanDelivery_viewServiceUnitPlanItem">


        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="plannedItemID"/>
          <TARGET NAME="PAGE" PROPERTY="plannedItemID"/>
        </CONNECT>


        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="description"/>
          <TARGET NAME="PAGE" PROPERTY="description"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="55" NUM_COLS="2">


    <FIELD LABEL="Field.Label.UnitsReceived">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="unitsReceivedToDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.NumberOfUnits" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="numberofUnits"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="numberofUnits"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.UnitType">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="unitType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DeliveryDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="deliveryDate"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Label.Comments">
    <!-- BEGIN, CR00417389, VT -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00417389 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>