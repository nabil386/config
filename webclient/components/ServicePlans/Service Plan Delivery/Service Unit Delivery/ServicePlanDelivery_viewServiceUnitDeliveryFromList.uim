<!-- This page allows the user to view the details of a service unit delivery -->
<!-- when accessed from the list page.                                        -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewServiceUnitDeliveryFromList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="EditButton"
      LABEL="ActionControl.Label.Edit"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanDelivery_modifyServiceUnitDelivery"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="serviceUnitDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="serviceUnitDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="plannedItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="plannedItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="ServicePlanDelivery_viewServiceUnitDeliveryView.vim"/>


</PAGE>
