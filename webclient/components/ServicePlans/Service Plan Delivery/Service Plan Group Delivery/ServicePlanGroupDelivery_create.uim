<!-- Create page for the service plan group delivery.                       -->
<PAGE PAGE_ID="ServicePlanGroupDelivery_create" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="SPDISPLAY" OPERATION="getIntegratedServicePlanContextDescription"/>
  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="DISPLAY_MENU" OPERATION="getICIntegratedServicePlanMenuData"/>
  <SERVER_INTERFACE CLASS="ServicePlanGroup" NAME="DISPLAY" OPERATION="readAllActive"/>
  <SERVER_INTERFACE CLASS="ServicePlanGroupDelivery" NAME="ACTION" OPERATION="create" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="caseID"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="message"/>
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="ACTION" PROPERTY="integratedCaseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY_MENU" PROPERTY="integratedCaseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="SPDISPLAY" PROPERTY="integratedCaseID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="40">
    <FIELD LABEL="Field.Label.SelectPlanGroup">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="servicePlanGroupId" NAME="DISPLAY" PROPERTY="servicePlanGroupName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="servicePlanGroupId"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <!-- BEGIN, CR00246725, MR -->
      <LINK PAGE_ID="ServicePlanGroupDelivery_homeActiveOnly">
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="result$servicePlanGroupDeliveryId"/>
          <TARGET NAME="PAGE" PROPERTY="servicePlanGroupDeliveryId"/>
        </CONNECT>
        
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="integratedCaseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
 
        <CONNECT>
          <SOURCE NAME="CONSTANT" PROPERTY="activeInd.true"/>
          <TARGET NAME="PAGE" PROPERTY="activeInd"/>
        </CONNECT>
        <!-- END, CR00246725 -->
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel">
      <LINK PAGE_ID="IntegratedServicePlan_home">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>