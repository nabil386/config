<!-- Page to view the details of a planned group                            -->
<PAGE
  PAGE_ID="ServicePlanDelivery_viewPlannedGroup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.GroupItems"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="ServicePlanDelivery_listGroupItems">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Milestones"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="ServicePlanDelivery_listGroupMilestones">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Comments"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="ServicePlanDelivery_viewPlannedGroup">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="plannedGroupID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY"
    OPERATION="viewPlanGroupDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="plannedGroupID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="plannedGroupID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$plannedGroupID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_MENU"
    OPERATION="getICServicePlanMenuData"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY_MENU"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
   
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>

</PAGE>
