<!-- Page to select sub-goal type for the creation of a planned sub-goal    -->
<PAGE PAGE_ID="ServicePlanDelivery_selectPlannedSubGoalType" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlanDelivery" NAME="DISPLAY" OPERATION="validateGoalExistsAndReturnSubGoalTypes" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="parentGroupID"/>
  <PAGE_PARAMETER NAME="typeCode"/>
  <PAGE_PARAMETER NAME="description"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="plannedSubGoalCaseIDKey$plannedSubGoalCaseIDKey$key$caseID"/>
  </CONNECT>


  <!-- BEGIN, CR00237630, MR -->
  <LIST>
  <!-- END, CR00237630 -->
    <CONTAINER LABEL="Container.Label.Select" WIDTH="20">
      <ACTION_CONTROL LABEL="ActionControl.Label.Select">
        <LINK OPEN_MODAL="false" DISMISS_MODAL="false" SAVE_LINK="false" PAGE_ID="ServicePlanDelivery_resolvePlannedSubGoal">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="parentGroupID"/>
            <TARGET NAME="PAGE" PROPERTY="parentGroupID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$listSubGoalType$plannedSubGoalTypeCodeDetails$typeCode"/>
            <TARGET NAME="PAGE" PROPERTY="typeCode"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="description"/>
            <TARGET NAME="PAGE" PROPERTY="description"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Title.SubGoalTypeCode" WIDTH="80">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$listSubGoalType$plannedSubGoalTypeCodeDetails$typeCode"/>
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


</PAGE>