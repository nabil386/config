<PAGE
  PAGE_ID="ServicePlanDelivery_viewPlannedSubGoal"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ServicePlanDelivery"
    NAME="DISPLAY_MENU"
    OPERATION="getICServicePlanMenuData"
    PHASE="DISPLAY"
  />


  <!-- BEGIN, CR00237333, MR-->
