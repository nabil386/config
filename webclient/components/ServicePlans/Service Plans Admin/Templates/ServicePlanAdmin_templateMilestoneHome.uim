<!-- Page to view the details of a template milestone                       -->
<PAGE PAGE_ID="ServicePlanAdmin_templateMilestoneHome" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="milestoneName"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00229083, GP -->
  <SERVER_INTERFACE CLASS="ServicePlan" NAME="DISPLAY" OPERATION="readPlanTemplateMilestone" PHASE="DISPLAY"/>
  <!-- END, CR00229083 -->


  <PAGE_PARAMETER NAME="planTemplateMilestoneID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
    <TARGET NAME="DISPLAY" PROPERTY="planTemplateMilestoneID"/>
  </CONNECT>


  <CLUSTER NUM_COLS="3" SHOW_LABELS="false" TITLE="Cluster.Title.Options">
    <CONTAINER>
      <ACTION_CONTROL IMAGE="Edit" LABEL="Field.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_modifyTemplateMilestone">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_modifyTemplateMilestone">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL IMAGE="Delete" LABEL="Field.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_removeTemplateMilestone" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="milestoneName"/>
            <TARGET NAME="PAGE" PROPERTY="contextDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_removeTemplateMilestone" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="milestoneName"/>
            <TARGET NAME="PAGE" PROPERTY="contextDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="milestoneName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartAfterPlanStart">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="startAfter"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="milestoneType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Duration">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="duration"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00229083, GP -->
    <CONTAINER>
      <FIELD HEIGHT="4">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="comments"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="LocalizableTextTranslation" LABEL="ActionControl.Label.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_listLocalizablePlanTemplateMilestoneCommentsText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="commentsTextID"/>
            <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateMilestoneID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateMilestoneID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00229083  -->
  </CLUSTER>


</PAGE>