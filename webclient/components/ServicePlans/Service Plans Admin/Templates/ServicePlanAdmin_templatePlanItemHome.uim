<PAGE PAGE_ID="ServicePlanAdmin_templatePlanItemHome" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00226792 MN -->
  <SERVER_INTERFACE CLASS="ServicePlan" NAME="DISPLAY" OPERATION="readPlanTemplatePlanItem" PHASE="DISPLAY"/>
  <!-- END, CR00226792 -->


  <PAGE_PARAMETER NAME="planTemplatePlanItemID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$key$key$planTemplatePlanItemID"/>
  </CONNECT>


  <CLUSTER NUM_COLS="3" SHOW_LABELS="false" TITLE="Cluster.Title.Options">
    <CONTAINER>
      <ACTION_CONTROL IMAGE="Edit" LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_modifyTemplatePlanItem">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateSubGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateSubGoalID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="contextDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_modifyTemplatePlanItem">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateSubGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateSubGoalID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="contextDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL IMAGE="Delete" LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_removeTemplatePlanItem" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateSubGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateSubGoalID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_removeTemplatePlanItem" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="planTemplateSubGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplateSubGoalID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <!-- BEGIN, CR00161962, LJ -->
    <CONTAINER>
      <ACTION_CONTROL IMAGE="ApprovalCriteria" LABEL="ActionControl.Label.AddApprovalCriteria">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_addTemplatePlanItemApprovalCriteria">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.AddApprovalCriteria">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_addTemplatePlanItemApprovalCriteria">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="contextDescription"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00161962 -->
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.Details">


    <FIELD LABEL="Field.Label.PlanItem">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="expectedOutcomeName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.isMandatory">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$isMandatoryInd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.ApprovalDetails">


    <FIELD LABEL="Field.Label.ApprovalRequired">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$details$dtls$approvalReqInd"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.DeliveryDetails">
    <FIELD LABEL="Field.Label.StartAfterPlanStart">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="startDay"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AuthorizedUnits">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="authorizedUnits"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Duration">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="duration"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MaximumUnits">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="maximumUnits"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">


    <!-- BEGIN, CR00229083, GP -->
    <CONTAINER>
      <FIELD HEIGHT="4">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="description"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL IMAGE="LocalizableTextTranslation" LABEL="ActionControl.Label.TextTranslation">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_listLocalizablePlanTemplatePlanItemDescriptionText">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="descriptionTextID"/>
            <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="key$key$key$planTemplatePlanItemID"/>
            <TARGET NAME="PAGE" PROPERTY="planTemplatePlanItemID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00229083  -->
  </CLUSTER>


</PAGE>