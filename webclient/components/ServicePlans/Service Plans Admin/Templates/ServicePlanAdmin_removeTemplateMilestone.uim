<!-- This page allows a user to remove a milestone from a template.         -->
<PAGE
  PAGE_ID="ServicePlanAdmin_removeTemplateMilestone"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="ACTION"
    OPERATION="removeTemplateMilestone"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="planTemplateID"/>
  <PAGE_PARAMETER NAME="planTemplateMilestoneID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planTemplateMilestoneID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="planTemplateMilestoneID"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.RemoveTemplateMilestone"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="YesButton"
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ServicePlanAdmin_templateHome">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planTemplateID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planTemplateID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="NoButton"
      LABEL="ActionControl.Label.No"
    />
  </ACTION_SET>
</PAGE>
