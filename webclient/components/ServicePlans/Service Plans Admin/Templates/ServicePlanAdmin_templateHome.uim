<!-- This page allows the user to see the service plan template home.       -->
<PAGE
  PAGE_ID="ServicePlanAdmin_templateHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="DISPLAY"
    OPERATION="readTemplate"
    PHASE="DISPLAY"
  />


  <CLUSTER
    NUM_COLS="4"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Options"
  >
    <CONTAINER>
      <ACTION_CONTROL
        IMAGE="SubGoal"
        LABEL="Field.Label.AddSubGoal"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_addTemplateSubGoal"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.AddSubGoal">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_addTemplateSubGoal"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL
        IMAGE="PlanGroup"
        LABEL="Field.Label.AddPlanGroup"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_createTemplatePlanGroup"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.AddPlanGroup">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_createTemplatePlanGroup"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL
        IMAGE="PlanGroup"
        LABEL="Field.Label.AddMilestone"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_addTemplateMilestone"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.AddMilestone">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_addTemplateMilestone"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <ACTION_CONTROL
        IMAGE="Edit"
        LABEL="Field.Label.Edit"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_modifyTemplate"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_modifyTemplate"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


  <PAGE_PARAMETER NAME="planTemplateID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planTemplateID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$planTemplateKey$planTemplateID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >


    <!-- BEGIN, CR00226647, GP -->
    <CONTAINER LABEL="Field.Label.Name">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$details$dtls$name"
          />
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_listLocalizablePlanTemplateNameText"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$nameTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00226647 -->


    <FIELD LABEL="Field.Label.DateCreated">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateCreated"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Goal">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="goalName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <!-- BEGIN, CR00226647, GP -->
    <CONTAINER>
      <FIELD HEIGHT="4">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_listLocalizablePlanTemplateDescriptionText"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$descriptionTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$planTemplateID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planTemplateID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00226647 -->


  </CLUSTER>


</PAGE>
