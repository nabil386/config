<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows a user to add an plan item to a service plan template -->
<!-- sub goal.                                                              -->
<PAGE
  PAGE_ID="ServicePlanAdmin_confirmAddTemplatePlanItem"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>

<!-- BEGIN, CR00226792 MN -->
  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="ACTION"
    OPERATION="addPlanTemplatePlanItem"
    PHASE="ACTION"
  />
<!-- END, CR00226792 -->

  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="DISPLAY"
    OPERATION="readPlanItemAndOutcomeName"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="planTemplateSubGoalID"/>
  <PAGE_PARAMETER NAME="planItemID"/>
  <PAGE_PARAMETER NAME="expectedOutcomeID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planTemplateSubGoalID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="planTemplateSubGoalID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="expectedOutcomeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="outcomeID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="expectedOutcomeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="expectedOutcomeID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Add"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ServicePlanAdmin_templateSubGoalHome">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planTemplateSubGoalID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planTemplateSubGoalID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK PAGE_ID="ServicePlanAdmin_templateSubGoalHome">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planTemplateSubGoalID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planTemplateSubGoalID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Title.PlanItemDetails"
  >


    <FIELD LABEL="Field.Label.PlanItemName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$planItemAndOutcomeNameDetails$planItemNameDetails$name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartAfterPlanStart">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startDay"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedOutcomeName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$planItemAndOutcomeNameDetails$outcomeNameDetails$name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Duration">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="duration"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
