<!-- This page allows the user to remove a goal from a service plan.        -->
<PAGE PAGE_ID="ServicePlanAdmin_removeTemplateFromServicePlan" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ServicePlan" NAME="ACTION" OPERATION="removeTemplateFromServicePlan" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="servicePlanID"/>
  <PAGE_PARAMETER NAME="planTemplateID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="planTemplateID"/>
    <TARGET NAME="ACTION" PROPERTY="planTemplateID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanID"/>
    <TARGET NAME="ACTION" PROPERTY="servicePlanID"/>
  </CONNECT>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.RemoveTemplate"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
     
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
</PAGE>