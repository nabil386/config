<!-- Lists service plan sub goals.                                          -->
<PAGE PAGE_ID="ServicePlanAdmin_listSubGoals" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ServicePlan" NAME="DISPLAY" OPERATION="listSubGoals" PHASE="DISPLAY"/>
  <ACTION_SET ALIGNMENT="LEFT" BOTTOM="false" TOP="true">
    <ACTION_CONTROL IMAGE="NewButton" LABEL="ActionControl.Label.New">
      <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_createSubGoal"/>
    </ACTION_CONTROL>
  </ACTION_SET>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL IMAGE="NewButton" LABEL="ActionControl.Label.NewContract">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_createContractTextForSubGoal">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL IMAGE="NewButton" LABEL="ActionControl.Label.NewPlanItem">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_createPlanItemForSubGoal">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL IMAGE="AddExisting" LABEL="ActionControl.Label.AddExisting">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_addPlanItemsToSubGoal">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <SEPARATOR/>


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_modifySubGoal">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="DeleteButton" LABEL="ActionControl.Label.Delete">
        <LINK OPEN_MODAL="true" PAGE_ID="ServicePlanAdmin_cancelSubGoal" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
            <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="typeCode"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.DateCreated">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dateCreated"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Status">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="recordStatus"/>
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="ServicePlanAdmin_subGoalHome">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="subGoalID"/>
          <TARGET NAME="PAGE" PROPERTY="subGoalID"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>
</PAGE>