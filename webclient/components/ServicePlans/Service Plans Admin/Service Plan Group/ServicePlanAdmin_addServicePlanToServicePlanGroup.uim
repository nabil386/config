<!-- This page allows the user to add a Service Plan to a Service plan Group.                    -->

<!-- BEGIN, CR00236022, MR -->
<PAGE
  PAGE_ID="ServicePlanAdmin_addServicePlanToServicePlanGroup"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
<!-- END, CR00236022 -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="DISPLAY"
    OPERATION="searchForServicePlans"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="DISPLAY_DESC"
    OPERATION="getDescription"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="ACTION"
    OPERATION="createLinks"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="servicePlanGroupId"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="message"
      />
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="servicePlanGroupId"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupId"
    />
    <TARGET
      NAME="DISPLAY_DESC"
      PROPERTY="servicePlanGroupId"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupId"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="servicePlanGroupId"
    />
  </CONNECT>


  <!-- BEGIN, CR00236022, MR -->
  <LIST >
  <!-- END, CR00236022 -->


    <CONTAINER
      LABEL="Container.Label.Action"
      SEPARATOR="Container.Separator"
      WIDTH="15"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="servicePlanID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="servicePlanIds"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="Field.Label.Name">
      <LINK
        PAGE_ID="ServicePlanAdmin_servicePlanHome"
        SAVE_LINK="FALSE"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="servicePlanID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanID"
          />
        </CONNECT>
      </LINK>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="servicePlanType"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


</PAGE>
