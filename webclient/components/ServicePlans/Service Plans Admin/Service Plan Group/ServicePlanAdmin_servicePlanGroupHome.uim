<!-- This page allows the user to see the Service Plan Group details.       -->
<PAGE
  PAGE_ID="ServicePlanAdmin_servicePlanGroupHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SPGHome"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_servicePlanGroupHome"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupId"
          />
          <!-- BEGIN, CR00237467, GP -->
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupId"
          />
          <!-- END, CR00237467 -->
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ServicePlans"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_listServicePlansForServicePlanGroup"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupId"
          />
          <!-- BEGIN, CR00237467, GP -->
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupId"
          />
          <!-- END, CR00237467 -->
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="DISPLAY"
    OPERATION="read"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="servicePlanGroupId"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupId"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="servicePlanGroupKey$servicePlanGroupId"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <!-- BEGIN, CR00235328, GP -->
    <CONTAINER>
      <!-- END, CR00235328 -->
      <FIELD HEIGHT="4">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="servicePlanGroupDesc"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00235328, GP -->
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <!-- BEGIN, CR00369242, SS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_listLocalizableServicePlanGroupDescriptionText"          
        >
          <!-- END, CR00369242 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$spgDtls$descriptionTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="servicePlanGroupId"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="servicePlanGroupID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$spgDtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00235328  -->


  </CLUSTER>
</PAGE>
