<!--This page allows you to view the list of the text translations               -->
<!-- for ServicePlanGroup attribute Description.                                                   -->
<PAGE PAGE_ID="ServicePlanAdmin_listLocalizableServicePlanGroupDescriptionText" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ServicePlanGroup" NAME="DISPLAY" OPERATION="listLocalizableDescriptionText"/>
  <PAGE_PARAMETER NAME="localizableTextID"/>
  <PAGE_PARAMETER NAME="servicePlanGroupID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="localizableTextID"/>
    <TARGET NAME="DISPLAY" PROPERTY="details$dtls$localizableTextID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupID"/>
    <TARGET NAME="DISPLAY" PROPERTY="details$dtls$localizableTextParentID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="DISPLAY" PROPERTY="details$dtls$versionNo"/>
  </CONNECT>


  <LIST TITLE="List.Title.Translations">


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <!-- BEGIN, CR00369242, SS -->
        <LINK DISMISS_MODAL="false" PAGE_ID="ServicePlanAdmin_modifyLocalizableServicePlanGroupDescriptionTextTranslation" WINDOW_OPTIONS="width=500">
          <!-- END, CR00369242 -->
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$translations$dtls$dtls$textTranslationID"/>
            <TARGET NAME="PAGE" PROPERTY="textTranslationID"/>
          </CONNECT>


          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="localizableTextID"/>
            <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
          </CONNECT>


          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupID"/>
            <TARGET NAME="PAGE" PROPERTY="servicePlanGroupID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="Field.Title.Language" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$translations$dtls$dtls$localeCode"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Text">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$translations$dtls$dtls$text"/>
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL LABEL="ActionControl.Label.AddTranslation">
      <!-- BEGIN, CR00369242, SS -->
      <LINK DISMISS_MODAL="false" PAGE_ID="ServicePlanAdmin_addServicePlanGroupDescriptionTextTranslation" WINDOW_OPTIONS="width=500">
        <!-- END, CR00369242 -->        
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="localizableTextID"/>
          <TARGET NAME="PAGE" PROPERTY="localizableTextID"/>
        </CONNECT>

        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupID"/>
          <TARGET NAME="PAGE" PROPERTY="servicePlanGroupID"/>
        </CONNECT>

        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
          <TARGET NAME="PAGE" PROPERTY="versionNo"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>

    <ACTION_CONTROL LABEL="ActionControl.Label.Close"/>
         
  </ACTION_SET>


</PAGE>