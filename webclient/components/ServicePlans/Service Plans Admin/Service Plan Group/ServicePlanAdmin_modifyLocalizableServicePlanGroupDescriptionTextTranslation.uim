<!-- This page allows you to update a localizable text translation                          -->
<!--  to the sServicePlanGroup attribute Description.                                                -->
<PAGE
  PAGE_ID="ServicePlanAdmin_modifyLocalizableServicePlanGroupDescriptionTextTranslation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="DISPLAY"
    OPERATION="readDescriptionTextTranslation"
  />
  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="ACTION"
    OPERATION="modifyDescriptionTextTranslation"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="textTranslationID"/>
  <PAGE_PARAMETER NAME="localizableTextID"/>
  <PAGE_PARAMETER NAME="servicePlanGroupID"/>
  <PAGE_PARAMETER NAME="versionNo"/>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00369242, SS -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="ServicePlanAdmin_listLocalizableServicePlanGroupDescriptionText"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="localizableTextID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="localizableTextID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00369242 -->
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="textTranslationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="details$dtls$textTranslationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="textTranslationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="textTranslationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="localizableTextParentID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="localizableTextParentID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="localizableTextID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="localizableTextID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="localizableTextID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="details$dtls$localizableTextID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="result$dtls$localeCode"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="localeCode"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="20">
    <FIELD
      LABEL="Field.Title.Language"
      WIDTH="20"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$localeCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Field.Title.Text"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="5"
      LABEL="Field.Title.Text"
    >
      <!-- END, CR00463142 -->


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$text"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
