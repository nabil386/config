<!-- This page allows the user to  add a localizable text translation to the       -->
<!-- ServicePlanGroup attribute Description.                                                       -->
<PAGE
  PAGE_ID="ServicePlanAdmin_addServicePlanGroupDescriptionTextTranslation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ServicePlanGroup"
    NAME="ACTION"
    OPERATION="addDescriptionTextTranslation"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="servicePlanGroupID"/>
  <PAGE_PARAMETER NAME="localizableTextID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="localizableTextID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$localizableTextID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="servicePlanGroupID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$localizableTextParentID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$dtls$versionNo"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00369242, SS -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="ServicePlanAdmin_listLocalizableServicePlanGroupDescriptionText"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="localizableTextID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="localizableTextID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="servicePlanGroupID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="servicePlanGroupID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00369242 -->
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN, CR00249121, GP -->
  <CLUSTER LABEL_WIDTH="30">
    <!-- END, CR00249121 -->
    <FIELD
      LABEL="Field.Title.Language"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="12"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$localeCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Field.Title.Text"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Title.Text"
    >
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
