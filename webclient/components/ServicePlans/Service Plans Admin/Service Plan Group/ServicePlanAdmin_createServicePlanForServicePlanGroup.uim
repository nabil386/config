<!-- This page allows the user to create a Service Plan for a Service       -->
<!-- Plan Group.                                                            -->
<PAGE PAGE_ID="ServicePlanAdmin_createServicePlanForServicePlanGroup" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlanGroup" NAME="DISPLAY" OPERATION="readForUpdate" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ServicePlanGroup" NAME="DISPLAY_DESC" OPERATION="getDescription" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ServicePlanGroup" NAME="ACTION" OPERATION="createServicePlanForServicePlanGroupAndLinkUp" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="servicePlanGroupId"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="message"/>
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupId"/>
    <TARGET NAME="ACTION" PROPERTY="servicePlanGroupId"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupId"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$servicePlanGroupId"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="servicePlanGroupId"/>
    <TARGET NAME="DISPLAY_DESC" PROPERTY="servicePlanGroupId"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2">


    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="servicePlanType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MultipleContractsInd">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="mulContsAllowedInd"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00246725, MR -->
    <FIELD LABEL="Field.Label.OwnershipStrategy">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="ownershipStrategyName"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00246725 -->


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.SecurityIdentifiers">


    <FIELD LABEL="Field.Label.CreateRightsSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createRights"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ApproveRightsSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="approveRights"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ViewRightsSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="viewRights"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MaintainRightsSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="maintainRights"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CloneRightsSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="cloneRights"/>
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


</PAGE>