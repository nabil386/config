        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ApprovalRequired">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="requiresApproval"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.ServiceUnitDetails"
  >
    <FIELD LABEL="Field.Label.UnitType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="unitType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MaximumUnits">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="maximumUnits"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AuthorizedUnits">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="authorizedUnits"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ModifyAuthorizedSID">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="modifyAuthorizedSID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="40"
    TITLE="Cluster.Title.AssociatedPageDetails"
  >
    <FIELD LABEL="Field.Label.CreatePageName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createPageName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CreatePagePlanItemIDParamName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createPagePlanItemIDParamName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CreatePageSubGoalIDParamName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createPageSubGoalIDParamName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ModifyPageName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="modifyPageName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ModifyPagePlannedItemParamName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="modifyPagePlanItemParamName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ViewPageName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="viewPageName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ViewPagePlannedItemParamName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="viewPagePlanItemParamName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <!-- BEGIN, CR00234442, GP -->
    <CONTAINER>
      <!-- END, CR00234442 -->
      <FIELD HEIGHT="4">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00234442, GP -->
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_listLocalizablePlanItemDescriptionText"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="descriptionTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="planItemID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planItemID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$details$dtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- END, CR00234442  -->


  </CLUSTER>
</PAGE>
