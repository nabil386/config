<!-- This page allows the user to modify contract text for an plan item.       -->
<PAGE PAGE_ID="ServicePlanAdmin_modifyContractTextForPlanItem" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlan" NAME="DISPLAY" OPERATION="readContractTextForPlanItem" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="ServicePlan" NAME="ACTION" OPERATION="modifyContractTextForPlanItem" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="contractTextID"/>
  <PAGE_PARAMETER NAME="planItemID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="contractTextID"/>
    <TARGET NAME="DISPLAY" PROPERTY="contractTextKey$contractTextID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="planItemID"/>
    <TARGET NAME="DISPLAY" PROPERTY="planItemID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="contractTextID"/>
    <TARGET NAME="ACTION" PROPERTY="contractTextID"/>
  </CONNECT>
  <!-- BEGIN CR00156191, SS -->
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="planItemID"/>
    <TARGET NAME="ACTION" PROPERTY="planItemID"/>
  </CONNECT>
  <!-- END CR00156191 -->
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2">
    <FIELD LABEL="Field.Label.Language">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="languageCode"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="12" SHOW_LABELS="false" TITLE="Cluster.Title.ContractText">
    <FIELD HEIGHT="4" LABEL="Field.Label.Text">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="contractText"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="contractText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>