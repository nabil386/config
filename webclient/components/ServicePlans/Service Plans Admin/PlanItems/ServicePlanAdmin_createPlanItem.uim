<!-- Page for creating a service plan plan item.                            -->
<PAGE PAGE_ID="ServicePlanAdmin_createPlanItem" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="ServicePlan" NAME="ACTION" OPERATION="createPlanItemDetails" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2">


    <FIELD LABEL="Field.Label.PlanItemReference">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="planItemReference"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Cost">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="cost"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AssociatedType" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="associatedType"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00161962, LJ -->
    <FIELD CONTROL="SKIP"/>
    <!-- END, CR00161962 -->


    <FIELD LABEL="Field.Label.TypeCode">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="typeCode"/>
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00236077, NS -->
    <FIELD LABEL="Field.Label.NonEditableName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$details$nameNotEditableInd"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00236077 -->


    <!-- BEGIN, CR00161962, LJ -->
    <FIELD LABEL="Field.Label.GuidanceURL" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="guidanceURL"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TaskConfiguration">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="taskConfigID"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00161962 -->
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2">


    <FIELD LABEL="Field.Label.ApprovalSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="approvalSID"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ApprovalRequired">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="requiresApproval"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER LABEL_WIDTH="50" NUM_COLS="2" TITLE="Cluster.Title.ServiceUnitDetails">


    <FIELD LABEL="Field.Label.UnitType" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="unitType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MaximumUnits" WIDTH="35">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="maximumUnits"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AuthorizedUnits" WIDTH="35">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="authorizedUnits"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ModifyAuthorizedSID">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="modifyAuthorizedSID"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="47.5" TITLE="Cluster.Title.AssociatedPageDetails">


    <FIELD LABEL="Field.Label.CreatePageName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createPageName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreatePagePlanItemIDParamName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createPagePlanItemIDParamName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreatePageSubGoalIDParamName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="createPageSubGoalIDParamName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ModifyPageName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="modifyPageName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ModifyPagePlannedItemParamName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="modifyPagePlanItemParamName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ViewPageName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="viewPageName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ViewPagePlannedItemParamName">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="viewPagePlanItemParamName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">


    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->


      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>