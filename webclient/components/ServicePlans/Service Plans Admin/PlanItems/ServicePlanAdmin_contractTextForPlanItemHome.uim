<!-- This page allows the user to see the plan item contract text details.     -->
<PAGE
  PAGE_ID="ServicePlanAdmin_contractTextForPlanItemHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="DISPLAY"
    OPERATION="readContractTextForPlanItem"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="planItemID"/>
  <PAGE_PARAMETER NAME="contractTextID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="contractTextID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$contractTextKey$contractTextID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >
    <FIELD LABEL="Field.Label.Language">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="languageCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.ContractText"
  >
    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="contractText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
