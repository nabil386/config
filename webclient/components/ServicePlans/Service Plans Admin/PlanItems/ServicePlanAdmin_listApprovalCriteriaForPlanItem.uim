<!-- Page to list approval criteria causes for an plan item.                -->
<PAGE
  PAGE_ID="ServicePlanAdmin_listApprovalCriteriaForPlanItem"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.PlanItemHome"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_planItemHome"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.ContractText"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_listContractTextsForPlanItem"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Outcomes"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_listOutcomesForPlanItem"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.GoodCauses"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_listGoodCausesForPlanItem"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.ApprovalCriteria"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="ServicePlanAdmin_listApprovalCriteriaForPlanItem"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="planItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Title"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ServicePlan"
    NAME="DISPLAY"
    OPERATION="listApprovalCriteriaForPlanItem"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="planItemID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="planItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="planItemID"
    />
  </CONNECT>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_modifyApprovalCriteriaForPlanItem"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="planItemApprovalCriteriaLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planItemApprovalCriteriaLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ServicePlanAdmin_removeApprovalCriteriaFromPlanItem"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="planItemApprovalCriteriaLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="planItemApprovalCriteriaLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.CriteriaName"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="criteriaName"
        />
      </CONNECT>
      
      <!-- BEGIN, CR00246725, MR -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ServicePlanAdmin_viewApprovalCriteriaModal"
        >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="approvalCriteriaID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="approvalCriteriaID"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00246725 -->
    </FIELD>


    <FIELD
      LABEL="Field.Title.OccursWhen"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="occursWhen"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Priority"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="priority"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      
      <!-- BEGIN, CR00246725, MR -->
      <INLINE_PAGE PAGE_ID="ServicePlanAdmin_viewApprovalCriteriaForPlanItem">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="planItemApprovalCriteriaLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="planItemApprovalCriteriaLinkID"
          />
        </CONNECT>
      </INLINE_PAGE>
      <!-- END, CR00246725 -->
    </DETAILS_ROW>


  </LIST>
</PAGE>
