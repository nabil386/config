<PAGE
  PAGE_ID="SPAE_searchLinkedParticipant"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ExternalUserParticipantAccess"
    NAME="DISPLAY"
    OPERATION="searchParticipantLinksByCurrentUser"
    PHASE="DISPLAY"
  />


  <LIST TITLE="List.Title.SearchResults">


    <FIELD
      LABEL="Field.Label.Relationship"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="extUserPartRelType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Participant"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="participantName"
        />
      </CONNECT>
      <LINK PAGE_ID="SPAE_Person_home">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="linkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="linkID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


  </LIST>


</PAGE>
