<!-- Evidence Details for Non MAGI Medicaid Eligibility Evidence            -->
<PAGE
  PAGE_ID="EvidenceDetails_show"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="ssn"/>
  <PAGE_PARAMETER NAME="firstName"/>
  <PAGE_PARAMETER NAME="lastName"/>
  <PAGE_PARAMETER NAME="gender"/>
  <PAGE_PARAMETER NAME="dateOfBirth"/>
  <PAGE_PARAMETER NAME="isEligible"/>
  <PAGE_PARAMETER NAME="dateReceived"/>


  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Label.Name"
  >


    <FIELD LABEL="Field.Label.FirstName">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="firstName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Gender">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="gender"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Ssn">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="ssn"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DateReceived">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="dateReceived"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.LastName">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="lastName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DateOfBirth">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="dateOfBirth"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.IsEligible">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="isEligible"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
