<!-- configuration file for the web client application.                         -->
<APP_CONFIG>


  <FILE_DOWNLOAD_CONFIG>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.MaintainChangeSetPkg.MaintainChangeSet_getChangeSetXMLDocument_TH"
      PAGE_ID="CTM_downloadWithEncryptionConfirm"
    >
      <INPUT
        PAGE_PARAM="changeSetID"
        PROPERTY="changeSetXMLEncryptionDetails$changeSetID"
      />
      <INPUT
        PAGE_PARAM="enCrypted"
        PROPERTY="changeSetXMLEncryptionDetails$XMLEncryptionIndicator"
      />
      <INPUT
        PAGE_PARAM="changeSetName"
        PROPERTY="changeSetXMLEncryptionDetails$changeSetName"
      />


      <FILE_NAME PROPERTY="result$changeSetName"/>
      <FILE_DATA PROPERTY="result$changeSetXMLDocument"/>
    </FILE_DOWNLOAD>
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.MaintainChangeSetPkg.MaintainChangeSet_getChangeSetXMLDocument_TH"
      PAGE_ID="CTM_downloadWithOutEncryptionConfirm"
    >
      <INPUT
        PAGE_PARAM="changeSetID"
        PROPERTY="changeSetXMLEncryptionDetails$changeSetID"
      />
      <INPUT
        PAGE_PARAM="enCrypted"
        PROPERTY="changeSetXMLEncryptionDetails$XMLEncryptionIndicator"
      />
      <INPUT
        PAGE_PARAM="changeSetName"
        PROPERTY="changeSetXMLEncryptionDetails$changeSetName"
      />


      <FILE_NAME PROPERTY="result$changeSetName"/>
      <FILE_DATA PROPERTY="result$changeSetXMLDocument"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.MaintainChangeSetPkg.MaintainChangeSet_viewBusinessObjectXMLDocument_TH"
      PAGE_ID="CTM_viewReleasedChangeSet"
    >
      <INPUT
        PAGE_PARAM="changeSetID"
        PROPERTY="businessObjectIdentifier$changeSetID"
      />
      <INPUT
        PAGE_PARAM="businessObjectIdentifier"
        PROPERTY="businessObjectIdentifier$businessObjectIdentifier"
      />
      <INPUT
        PAGE_PARAM="businessObjectName"
        PROPERTY="businessObjectIdentifier$businessObjectName"
      />
      <FILE_NAME PROPERTY="result$businessObjectName"/>
      <FILE_DATA PROPERTY="result$businessObjectXMLDocument"/>
    </FILE_DOWNLOAD>


    <!-- BEGIN, CR00280244, SS -->
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.MaintainChangeSetPkg.MaintainChangeSet_export_TH"
      PAGE_ID="CTM_listChangeSet"
    >
      <!-- END, CR00280244 -->
      <INPUT
        PAGE_PARAM="changeSetID"
        PROPERTY="details$changeSetID"
      />
      <INPUT
        PAGE_PARAM="encryptInd"
        PROPERTY="details$encryptInd"
      />
      <INPUT
        PAGE_PARAM="versionNo"
        PROPERTY="details$versionNo"
      />
      <FILE_NAME PROPERTY="result$changeSetName"/>
      <FILE_DATA PROPERTY="result$changeSetXMLDocument"/>
    </FILE_DOWNLOAD>


  </FILE_DOWNLOAD_CONFIG>


  <MULTIPLE_SELECT>
    <DOMAIN
      MULTIPLE="true"
      NAME="BUSINESS_OBJECT_ID_TAB_LIST"
    />
  </MULTIPLE_SELECT>
  <ENABLE_ACTION_SET_IMAGES>true</ENABLE_ACTION_SET_IMAGES>


</APP_CONFIG>
