<!-- This page allows the user to remove business objects from a  change set. -->
<PAGE
  PAGE_ID="CTM_removeBusinessObjects"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=850"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="viewChangeSet"
  />


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="ACTION"
    OPERATION="removeBusinessObjects"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="changeSetID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$changeSetID"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Remove"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"
    /> 
  </ACTION_SET>

  <!-- BEGIN, CR00280244, SS -->
  <LIST >
    <!-- END, CR00280244 -->
      <CONTAINER WIDTH="7">
        <WIDGET
          ALIGNMENT="CENTER"
          LABEL="List.Label.Select"
          TYPE="MULTISELECT"
        >
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="result$businessObjects$dtlsList$dtls$businessObjectIdentifier"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION"
                PROPERTY="details$businessObjectIdentifierTabList"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="businessObjectName"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="businessObjectType"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.AddedDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="addedDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.AddedBy"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="addedByFullName"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="addedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>

</PAGE>
