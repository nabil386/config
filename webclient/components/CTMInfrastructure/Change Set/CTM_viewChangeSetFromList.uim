<!-- Description -->
<!-- =========== -->
<!-- This page displays the details of the change set from the change set list.    -->
<PAGE
  PAGE_ID="CTM_viewChangeSetFromList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAYCHANGESET"
    OPERATION="viewChangeSet"
  />


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAYRELEASEDCHANGESET"
    OPERATION="viewReleasedChangeSet"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAYCHANGESET"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAYRELEASEDCHANGESET"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="changeSetID"/>


  <!-- Display Released Change Set Details-->
  <CLUSTER WIDTH="40">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAYRELEASEDCHANGESET"
        PROPERTY="isReleased"
      />
    </CONDITION>


    <CONTAINER LABEL="Container.Label.Released">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="releasedDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.StaticTextBy"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="releasedBy"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAYRELEASEDCHANGESET"
              PROPERTY="releasedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>


    <CONTAINER LABEL="Container.Label.Created">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="creationDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.StaticTextBy"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="createdBy"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAYRELEASEDCHANGESET"
              PROPERTY="createdBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>
    <FIELD LABEL="Field.Label.AssociatedChangeSet">
      <CONNECT>
        <SOURCE
          NAME="DISPLAYRELEASEDCHANGESET"
          PROPERTY="associatedWith"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ChangeSetStatus"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYRELEASEDCHANGESET"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAYRELEASEDCHANGESET"
        PROPERTY="isReleased"
      />
    </CONDITION>
    <!-- BEGIN, CR00439725, GK -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00439725 -->


      <CONNECT>
        <SOURCE
          NAME="DISPLAYRELEASEDCHANGESET"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAYRELEASEDCHANGESET"
        PROPERTY="isReleased"
      />
    </CONDITION>
    <LIST TITLE="List.Title.BusinessObjects">
      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="businessObjectName"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.AddedBy"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYRELEASEDCHANGESET"
            PROPERTY="addedBy"
          />
        </CONNECT>
      </FIELD>
      <CONTAINER LABEL="Container.Label.Action">
        <ACTION_CONTROL
          LABEL="ActionControl.Label.ViewBusinessObjectXMLFile"
          TYPE="FILE_DOWNLOAD"
        >
          <LINK>
            <CONNECT>
              <SOURCE
                NAME="DISPLAYRELEASEDCHANGESET"
                PROPERTY="result$changeSetID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="changeSetID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAYRELEASEDCHANGESET"
                PROPERTY="result$businessObjectList$dtlsList$dtls$businessObjectIdentifier"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="businessObjectIdentifier"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="DISPLAYRELEASEDCHANGESET"
                PROPERTY="result$businessObjectList$dtlsList$businessObjectName"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="businessObjectName"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>
    </LIST>
  </CLUSTER>


  <!-- Display Open Change Set Details -->
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAYRELEASEDCHANGESET"
        PROPERTY="isReleased"
      />
    </CONDITION>
    <!-- BEGIN, CR00439725, GK -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00439725 -->


      <CONNECT>
        <SOURCE
          NAME="DISPLAYCHANGESET"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAYCHANGESET"
        PROPERTY="showBusinessObjectsInd"
      />
    </CONDITION>
    <LIST TITLE="List.Title.BusinessObjects">


      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCHANGESET"
            PROPERTY="businessObjectName"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCHANGESET"
            PROPERTY="businessObjectType"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.AddedDate"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCHANGESET"
            PROPERTY="addedDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.AddedBy"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCHANGESET"
            PROPERTY="addedBy"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAYCHANGESET"
              PROPERTY="addedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>
  </CLUSTER>


</PAGE>
