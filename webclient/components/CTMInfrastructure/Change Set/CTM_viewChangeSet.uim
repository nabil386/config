<!-- This page allows the user to view a change set content.                -->
<PAGE
  PAGE_ID="CTM_viewChangeSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="viewChangeSet"
  />


  <PAGE_PARAMETER NAME="changeSetID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="50">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$isDeleted"
      />
    </CONDITION>
    <CONTAINER LABEL="Container.Label.Deleted">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$deletionDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Field.StaticTextBy"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="deletedByFullName"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$deletedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00439725, GK -->
    <FIELD LABEL="Field.Label.Comments">
      <!-- END, CR00439725 -->


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="showBusinessObjectsInd"
      />
    </CONDITION>


    <LIST
      
      TITLE="List.Title.BusinessObjects"
    >


      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL LABEL="ActionControl.Label.AddRelatedBusinessObject">
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="CTM_addRelatedBusinessObjects"
          >
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="businessObjectIdentifier"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="businessObjectIdentifier"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="changeSetID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="changeSetID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.Label.ViewObject">
          <CONDITION>
            <IS_TRUE
              NAME="DISPLAY"
              PROPERTY="result$businessObjects$dtlsList$hasBusinessObjectHomePage"
            />
          </CONDITION>
          <LINK PAGE_ID="CTM_resolveBusinessObjectHome">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="businessObjectIdentifier"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="businessObjectIdentifier"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>


      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="businessObjectName"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="25"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="businessObjectType"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.AddedDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="addedDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.AddedBy"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="addedByFullName"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="addedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>


    </LIST>
  </CLUSTER>
</PAGE>
