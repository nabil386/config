<!-- Description -->
<!-- =========== -->
<!-- This page displays the list of all the apply history of a change set    -->
<!-- to the current system.                 -->
<PAGE
  PAGE_ID="CTM_listChangeSetUndoHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="listChangeSetUndoHistory"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="changeSetID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewChangeSet"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CTM_viewReleasedChangeSet"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.TransportHistory"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CTM_listChangeSetTransportHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ApplyHistory"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CTM_listChangeSetApplyHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL
      LABEL="ActionControl.Label.UndoHistory"
      STYLE="in-page-current-link"
      >
      <LINK
        PAGE_ID="CTM_listChangeSetUndoHistory"
        SAVE_LINK="false"
        >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    
  </MENU>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="hasRemoteSystemInd"
      />
    </CONDITION>
    <LIST>
      <FIELD LABEL="Field.Label.RemoteSystemName">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="remoteSystemName"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.UndoDate">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appliedDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.UndoActionBy">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appliedByFullName"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
          >
          <CONDITION>
            <IS_TRUE NAME="DISPLAY" PROPERTY="result$dtlsList$isAppliedByUserExists"/>
          </CONDITION>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appliedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAY"
        PROPERTY="hasRemoteSystemInd"
      />
    </CONDITION>
    <LIST>
      <FIELD LABEL="Field.Label.UndoDate">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appliedDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.UndoActionBy">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="appliedByFullName"
          />
        </CONNECT>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_viewUserDetails"
          WINDOW_OPTIONS="width=900,height=300"
        >
          <CONDITION>
            <IS_TRUE NAME="DISPLAY" PROPERTY="result$dtlsList$isAppliedByUserExists"/>
          </CONDITION>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="appliedBy"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="userName"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>
  </CLUSTER>


</PAGE>
