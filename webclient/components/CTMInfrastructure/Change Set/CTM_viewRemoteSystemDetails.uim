<!-- This page allows the user to view a remote system details.            -->
<PAGE PAGE_ID="CTM_viewRemoteSystemDetails" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
    
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
        </CONNECT>
    </PAGE_TITLE>
    
    <SERVER_INTERFACE CLASS="MaintainChangeSet" NAME="DISPLAY" OPERATION="viewReleasedChangeSet"/>
    
    
    <PAGE_PARAMETER NAME="changeSetID"/>
    
    
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="changeSetID"/>
        <TARGET NAME="DISPLAY" PROPERTY="key$changeSetID"/>
    </CONNECT>
    
    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
        
        <ACTION_CONTROL LABEL="ActionControl.Label.Close">
        </ACTION_CONTROL>
        
    </ACTION_SET>
    
    <CLUSTER LABEL_WIDTH="25">
        
        <FIELD LABEL="Field.Label.System">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="remoteSystemName"/>
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Label.URL">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="remoteSystemURL"/>
            </CONNECT>
        </FIELD>
        
    </CLUSTER>
    
</PAGE>