<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to remove business objects from a  change set. -->
<!-- BEGIN, CR00291752, SS -->
<PAGE
  PAGE_ID="CTM_addRelatedBusinessObjects"  
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00291752 -->
  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="getDependentBusinessObjects"
  />
  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="ACTION"
    OPERATION="addDependentBusinessObjectsConfirm"
    PHASE="ACTION"
  />

  <!-- BEGIN, CR00280244, SS -->
  <PAGE_TITLE>
    <!-- END, CR00280244 -->
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="changeSetID"/>
  <PAGE_PARAMETER NAME="businessObjectIdentifier"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="changeSetBOIdentifier$changeSetID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="businessObjectIdentifier"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="changeSetBOIdentifier$businessObjectIdentifier"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$changeSetID"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.AddToChangeSet"
      TYPE="SUBMIT"
    />

    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" />
      
  </ACTION_SET>

  <!-- BEGIN, CR00280244, SS -->
    <LIST >
      <!-- END, CR00280244 -->
      <CONTAINER ALIGNMENT="CENTER" WIDTH="10">
        <WIDGET
          TYPE="MULTISELECT"
        >
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="dtls$businessObjectIdentifier"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION"
                PROPERTY="details$businessObjectIdentifierTabList"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="31"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dtlsList$businessObjectName"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Type"
        WIDTH="32"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="businessObjectType"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.RelatedTo"
        WIDTH="32"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="relatedBusinessObject"
          />
        </CONNECT>
      </FIELD>
    </LIST>
 
</PAGE>
