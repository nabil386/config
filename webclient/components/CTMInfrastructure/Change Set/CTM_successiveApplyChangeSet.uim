<!-- This page allows a user to apply the change set to the destination system successive times.-->
<!-- BEGIN, CR00291752, SS -->
<PAGE
    PAGE_ID="CTM_successiveApplyChangeSet"
    WINDOW_OPTIONS="width=400"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
    >
    <!-- END, CR00291752 -->
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE
                NAME="TEXT"
                PROPERTY="PageTitle.StaticText1"
            />
        </CONNECT>
    </PAGE_TITLE>
    
    
    <SERVER_INTERFACE
        CLASS="MaintainChangeSet"
        NAME="ACTION"
        OPERATION="applyChangeSet"
        PHASE="ACTION"
    />
    
    
    <ACTION_SET
        ALIGNMENT="CENTER"
        TOP="false"
        >
        <ACTION_CONTROL
            LABEL="ActionControl.Label.Yes"
            TYPE="SUBMIT"
            >
            
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
    </ACTION_SET>
    
    
    <PAGE_PARAMETER NAME="changeSetID"/>
    <PAGE_PARAMETER NAME="versionNo"/>
    
    
    <CONNECT>
        <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
        />
        <TARGET
            NAME="ACTION"
            PROPERTY="changeSetID"
        />
    </CONNECT>
    
    
    <CONNECT>
        <SOURCE
            NAME="PAGE"
            PROPERTY="versionNo"
        />
        <TARGET
            NAME="ACTION"
            PROPERTY="versionNo"
        />
    </CONNECT>
    
    
    <CLUSTER
        SHOW_LABELS="false"
        STYLE="outer-cluster-borderless"
        >
        <FIELD>
            <CONNECT>
                <SOURCE
                    NAME="TEXT"
                    PROPERTY="Question.Description"
                />
            </CONNECT>
        </FIELD>
    </CLUSTER>
    
    
</PAGE>
