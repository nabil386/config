<!-- Description -->
<!-- =========== -->
<!-- This page displays the list of all the transport history of a change set    -->
<!-- from the current system.                 -->
<PAGE
  PAGE_ID="CTM_listChangeSetTransportHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  
  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY1"
    OPERATION="viewReleasedChangeSet"
  />
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$changeSetID"
    />
  </CONNECT>

  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="listChangeSetTransportHistory"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="changeSetkey$changeSetID"
    />
  </CONNECT>

  <PAGE_PARAMETER NAME="changeSetID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewChangeSet"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CTM_viewReleasedChangeSet"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.TransportHistory"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="CTM_listChangeSetTransportHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ApplyHistory"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CTM_listChangeSetApplyHistory"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <!-- BEGIN, CR00291489, SS -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.UndoHistory"
      STYLE="in-page-link"
      >
      <LINK
        PAGE_ID="CTM_listChangeSetUndoHistory"
        SAVE_LINK="false"
        >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00291489 -->
    
  </MENU>


  <LIST>
    <FIELD LABEL="Field.Label.TransportType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transportType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SourceSystemName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sourceSystemName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TargetSystemName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="targetSystemName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TransportedDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transportedDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.TransportedBy">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transportedByFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
        WINDOW_OPTIONS="width=900,height=300"
      >
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="result$dtlsList$isTransportedByUserExists"/>
        </CONDITION>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="transportedBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>


</PAGE>
