<!-- This page allows the user to select a change set fow download.       -->
<PAGE
  PAGE_ID="CTM_selectChangeSetForDownload"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="changeSetID"/>
  
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="changeSetID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="changeSetKey$changeSetID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="changeSetKey$changeSetID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="changeSetXMLEncryptionDetails$changeSetID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="businessObjects$changeSetName"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="changeSetXMLEncryptionDetails$changeSetName"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="DISPLAY"
    OPERATION="selectChangeSetForManualTransport"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainChangeSet"
    NAME="ACTION"
    OPERATION="downloadChangeSetXMLDocument"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >


      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="CTM_resolveChangeSetForDownload"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="changeSetXMLEncryptionDetails$changeSetID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="changeSetXMLEncryptionDetails$XMLEncryptionIndicator"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="enCrypted"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="changeSetXMLEncryptionDetails$changeSetName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="changeSetName"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    NUM_COLS="1"
    WIDTH="60"
  >
    <FIELD LABEL="Field.Label.ChangeSetName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$changeSetName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Encrypt">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="changeSetXMLEncryptionDetails$XMLEncryptionIndicator"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
