<!-- This page displays the list of change sets in which the business object -->
<!-- instance is already present.                                            -->
<PAGE PAGE_ID="CTM_multipleBOInstancesInChangeSets" WINDOW_OPTIONS="width=700,height=450" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE DESCRIPTION="Field.StaticText">
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="businessObjectName"/>
  <PAGE_PARAMETER NAME="businessObjectIdentifier"/>


  <SERVER_INTERFACE CLASS="MaintainChangeSet" NAME="DISPLAY" OPERATION="getChangeSetsForBusinessObject" PHASE="DISPLAY"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="businessObjectIdentifier"/>
    <TARGET NAME="DISPLAY" PROPERTY="businessObjectIdentifier$businessObjectIdentifier"/>
  </CONNECT>


  <LIST TITLE="List.Title.ChangeSets">


    <FIELD LABEL="Field.Label.Name" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtlsList$businessObjectName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AddedDate" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtlsList$dtls$addedDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AddedBy" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtlsList$dtls$addedBy"/>
      </CONNECT>
      <LINK OPEN_MODAL="true" PAGE_ID="Organization_viewUserDetails" WINDOW_OPTIONS="width=900,height=300">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="addedBy"/>
          <TARGET NAME="PAGE" PROPERTY="userName"/>
        </CONNECT>
      </LINK>
    </FIELD>


  </LIST>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Close"/>
  </ACTION_SET>
</PAGE>