<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows you to view the assessment details where        	    -->
<!-- change of circumstances are supported and contains no total score.     -->
<PAGE
  PAGE_ID="DecisionAssistApplication_viewAssessmentWithCOCNoTotalScore"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="caseID"/>
  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$assessmentCaseID"
    />
  </CONNECT>
  <!-- BEGIN, CR00248133, AK -->
  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="VIEW_ASSESSMENT_WITH_COC"
    OPERATION="viewAssessmentDetailsWithCOC"
    PHASE="DISPLAY"
  />
  <!-- END, CR00248133 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="VIEW_ASSESSMENT_WITH_COC"
      PROPERTY="key$dtls$dtls$assessmentCaseID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00248133, AK -->
  <LIST TITLE="Cluster.Title.DeterminationDecisions">
    <!-- BEGIN, CR00214223, PS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00214223 -->
      <ACTION_CONTROL
        LABEL="ActionControl.Label.CompareDecisions"
        TYPE="ACTION"
      >
        <!-- BEGIN, CR00214223, PS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistApplication_selectDecisionFromViewAssessment"
        >
          <!-- END, CR00214223 -->
          <CONNECT>
            <SOURCE
              NAME="VIEW_ASSESSMENT_WITH_COC"
              PROPERTY="result$determinationDecisionDtlsList$determinationDecisionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDecisionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="VIEW_ASSESSMENT_WITH_COC"
              PROPERTY="result$determinationDecisionDtlsList$determinationDeliveryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDeliveryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00214223, PS -->
    </ACTION_SET>


    <FIELD LABEL="Field.Label.EffectiveDate">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$determinationDecisionDtlsList$effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DeterminationName">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$determinationDecisionDtlsList$determinationName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DecisionType">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$determinationDecisionDtlsList$decisionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultType">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$determinationDecisionDtlsList$resultType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Outcomes">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$determinationDecisionDtlsList$outcome"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistApplication_viewDecisionDetails">
        <CONNECT>
          <SOURCE
            NAME="VIEW_ASSESSMENT_WITH_COC"
            PROPERTY="result$determinationDecisionDtlsList$determinationDecisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="VIEW_ASSESSMENT_WITH_COC"
            PROPERTY="result$determinationDecisionDtlsList$determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00214223 -->
  </LIST>
  <!-- END, CR00248133 -->


</PAGE>
