<PAGE
  PAGE_ID="DecisionAssistApplication_viewAssessmentStatusHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="assessmentCaseID"/>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentCaseID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$assessmentCaseID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="VIEW_ASSESSMENT_STATUS_HISTORY"
    OPERATION="readAllAssessmentWithUserFullNameStatusHistory"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentCaseID"
    />
    <TARGET
      NAME="VIEW_ASSESSMENT_STATUS_HISTORY"
      PROPERTY="key$dtls$dtls$assessmentCaseID"
    />
  </CONNECT>


  <LIST>
    <FIELD
      LABEL="Field.Label.Date"
      WIDTH="30"
    >
      <CONNECT>`
            <SOURCE
          NAME="VIEW_ASSESSMENT_STATUS_HISTORY"
          PROPERTY="result$list$list$actionDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.User"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_STATUS_HISTORY"
          PROPERTY="result$list$list$actionedBy"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_STATUS_HISTORY"
          PROPERTY="result$list$list$status"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
