<!-- This page allows you to view a determination-based assessment where     -->
<!--  change of circumstances are supported.                                 -->
<PAGE
  PAGE_ID="DecisionAssistApplication_viewAssessmentWithCOCTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="VIEW_ASSESSMENT_WITH_COC"
        PROPERTY="result$assessmentDtls$assessmentName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_ASSESSMENT_WITH_COC"
        PROPERTY="result$assessmentDtls$concernRoleName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="VIEW_ASSESSMENT_WITH_COC"
        PROPERTY="result$assessmentDtls$assessmentName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.StaticText"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_ASSESSMENT_WITH_COC"
        PROPERTY="result$assessmentDtls$concernRoleName"
      />
    </CONNECT>
  </TAB_NAME>


  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="VIEW_ASSESSMENT_WITH_COC"
    OPERATION="viewAssessmentWithCOC"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="VIEW_ASSESSMENT_WITH_COC"
      PROPERTY="key$dtls$dtls$assessmentCaseID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="36"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.ClientName">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$concernRoleName"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_resolveViewMemberHome">
        <CONNECT>
          <SOURCE
            NAME="VIEW_ASSESSMENT_WITH_COC"
            PROPERTY="result$assessmentDtls$caseParticipantRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseParticipantRoleID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.ReferenceNumber">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$caseReference"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AssessmentDate">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$assessmentDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$assessmentName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$assessmentType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="VIEW_ASSESSMENT_WITH_COC"
          PROPERTY="result$assessmentDtls$caseStatusCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
