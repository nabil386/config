<!-- This jsp redirects the user to a Latest Questionnaire Page.-->
<PAGE
  PAGE_ID="DA_resolveAssessment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
    curam.omega3.request.RequestHandler rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
    String context = request.getContextPath() + "/";
    // BEGIN, CR00214223, PS
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    // END, CR00214223
    String url = "";
    
    String CaseID = request.getParameter("caseID");  
      
      curam.interfaces.DAAssessmentPkg.DAAssessment_readAssessmentDeliveryDetails_TH th1 =
      new curam.interfaces.DAAssessmentPkg.DAAssessment_readAssessmentDeliveryDetails_TH();
      th1.setFieldValue(th1.key$dtls$caseID_idx,CaseID);
      th1.callServer();
      
      String assessmentConfigurationID = th1.getFieldValue(th1.result$dtls$dtls$assessmentConfigurationID_idx);  
      
    // BEGIN, CR00282454, AC
    curam.interfaces.DeterminationConfigPkg.DeterminationConfig_getDeterminationConfigIndicatorDetails_TH 
    th= new curam.interfaces.DeterminationConfigPkg.DeterminationConfig_getDeterminationConfigIndicatorDetails_TH ();
   
    //BEGIN, CR00291662, AC
   th.setFieldValue(th.key$dtls$dtls$assessmentConfigurationID_idx,assessmentConfigurationID);
    //END, CR00291662
    th.callServer();
    
    String standAlone = th.getFieldValue(th.result$standAlone_idx);
    
    String supportCocProcessing = th.getFieldValue(th.result$supportCocProcessing_idx);
    
    String multipleSourceInd = th.getFieldValue(th.result$multipleSourceInd_idx);
    
    String automaticConsolidation = th.getFieldValue(th.result$automaticConsolidation_idx);
    // END, CR00282454

      if(supportCocProcessing.equalsIgnoreCase("true"))
     {         
     // BEGIN, CR00214223, PS
     	url= context+"DecisionAssistApplication_resolveViewAssessmentWithCOCPage.do?caseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID);
     	// END, CR00214223
     }
     else
     {
     	if(multipleSourceInd.equalsIgnoreCase("true"))
     	{
     	
     	  if (automaticConsolidation.equalsIgnoreCase("true")){
     	
     	url= context+"DecisionAssistApplication_viewAssessmentNoCOCMultiSourceACPage.do?caseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID) +
     	  "&amp;assessmentCaseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID);
     	
     	  } else {
     	
     	  url= context+"DecisionAssistApplication_viewAssessmentNoCOCMultiSourceMCPage.do?caseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID) + 
     	  "&amp;assessmentCaseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID);
     	  
     	  } 
     	 
     	 } else {
     	 
     	 url= context+"DecisionAssistApplication_viewAssessmentNoCOCSingleSourcePage.do?caseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID) + 
     	 "&amp;assessmentCaseID="+curam.omega3.request.RequestUtils.escapeURL(CaseID);
     	 
     	 }
     
     }
      url += "&amp;" + rh.getSystemParameters();

      response.sendRedirect(response.encodeRedirectURL(url));
     
    </JSP_SCRIPTLET>
</PAGE>
