<!-- This page allows the user to answer a questionnaire.                   -->
<PAGE
  PAGE_ID="DecisionAssistApplication_answerQuestionnaire"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTCONTEXT"
        PROPERTY="result$dtls$assessmentName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Hyphen"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTCONTEXT"
        PROPERTY="result$dtls$caseReference"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Hyphen"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTCONTEXT"
        PROPERTY="result$dtls$concernRoleName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Hyphen"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTCONTEXT"
        PROPERTY="result$dtls$primaryAlternateID"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="executionID"/>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>
</PAGE>
