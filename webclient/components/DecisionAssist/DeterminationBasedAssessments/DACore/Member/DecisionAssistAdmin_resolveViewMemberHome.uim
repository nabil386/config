<!-- This page is a resolve page which is used to navigate to the person    -->
<!-- home page.                                                             -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_resolveViewMemberHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        
      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String caseParticipantRoleID = request.getParameter("caseParticipantRoleID");
      curam.interfaces.DAAssessmentPkg.DAAssessment_viewCaseMemberDetails_TH  
        viewCaseMemberDetailsTH = new curam.interfaces.DAAssessmentPkg.DAAssessment_viewCaseMemberDetails_TH();
      
      viewCaseMemberDetailsTH.setFieldValue(viewCaseMemberDetailsTH.key$dtls$caseParticipantRoleID_idx,caseParticipantRoleID);
      
      viewCaseMemberDetailsTH.callServer();
      
      String concernRoleID = viewCaseMemberDetailsTH.getFieldValue(viewCaseMemberDetailsTH.result$details$concernRoleID_idx);
      String participantType = viewCaseMemberDetailsTH.getFieldValue(viewCaseMemberDetailsTH.result$details$concernRoleType_idx);

      String url = context + "Participant_resolveRoleHomePage.do?concernRoleID="+ 
      curam.omega3.request.RequestUtils.escapeURL(concernRoleID)
      +"&amp;participantType="+ 
      curam.omega3.request.RequestUtils.escapeURL(participantType);
          
      url += "&amp;" + rh.getSystemParameters(); 
      response.sendRedirect(response.encodeRedirectURL(url));
  </JSP_SCRIPTLET>
</PAGE>
