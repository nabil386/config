<!-- ===========                                                            -->
<!-- This page allows the user to record a change in circumstance for       -->
<!-- a determination-based assessment.                                      -->
<PAGE
  PAGE_ID="DecisionAssistApplication_recordChangeOfCircumstance"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="RECORD_COC"
    OPERATION="recordChangeOfCircumstance"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="COMMON_VIEW"
    OPERATION="viewDeterminationDelivery"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="COMMON_VIEW"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="RECORD_COC"
      PROPERTY="key$key$determinationDeliveryID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- BEGIN, CR00198489, SS -->
  <CLUSTER
    LABEL_WIDTH="60"
    NUM_COLS="1"
  >
    <!-- END, CR00198489 -->
    <FIELD
      LABEL="Field.Label.EffectiveDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="RECORD_COC"
          PROPERTY="key$key$effectiveDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"> </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
