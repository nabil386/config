<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to view determination details for an         -->
<!-- assessment which supports multi source and manual consolidation.       -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="CDAApplication_viewDeterminationMultiSourceMCDecisionList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuQuestionnaires"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CDAApplication_viewDeterminationMultiSourceMC"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuThirdPartyRequests"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="CDAApplication_viewDeterminationMultiSourceMCThirdPartyRequestList"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuDecisions"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="CDAApplication_viewDeterminationMultiSourceMCDecisionList"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="COMMON_VIEW"
    OPERATION="viewDeterminationDelivery"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="COMMON_VIEW"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>


  <LIST>
    <CONDITION>
      <IS_FALSE
        NAME="COMMON_VIEW"
        PROPERTY="result$assessmentDtls$totalScoreInd"
      />
    </CONDITION>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.CompareDecisions"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistApplication_selectDecisionFromViewDetermination"
        >
          <CONNECT>
            <SOURCE
              NAME="COMMON_VIEW"
              PROPERTY="result$decisionDtls$dtls$dtls$determinationDecisionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDecisionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="COMMON_VIEW"
              PROPERTY="result$ddID$dtls$dtls$determinationDeliveryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDeliveryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00214223, PS -->
    </ACTION_SET>
    <FIELD LABEL="Field.Label.Date">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$decisionDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$decisionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultType">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$resultType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$outcome"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistApplication_viewDecisionDetails">
        <CONNECT>
          <SOURCE
            NAME="COMMON_VIEW"
            PROPERTY="result$decisionDtls$dtls$dtls$determinationDecisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="COMMON_VIEW"
            PROPERTY="result$ddID$dtls$dtls$determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>
  <LIST>
    <CONDITION>
      <IS_TRUE
        NAME="COMMON_VIEW"
        PROPERTY="result$assessmentDtls$totalScoreInd"
      />
    </CONDITION>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.CompareDecisions"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistApplication_selectDecisionFromViewDetermination"
        >
          <CONNECT>
            <SOURCE
              NAME="COMMON_VIEW"
              PROPERTY="result$decisionDtls$dtls$dtls$determinationDecisionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDecisionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="COMMON_VIEW"
              PROPERTY="result$ddID$dtls$dtls$determinationDeliveryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationDeliveryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="Field.Label.Date">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$decisionDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$decisionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ResultType">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$resultType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$outcome"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TotalScore">
      <CONNECT>
        <SOURCE
          NAME="COMMON_VIEW"
          PROPERTY="result$decisionDtls$dtls$dtls$totalScore"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistApplication_viewDecisionDetails">
        <CONNECT>
          <SOURCE
            NAME="COMMON_VIEW"
            PROPERTY="result$decisionDtls$dtls$dtls$determinationDecisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="COMMON_VIEW"
            PROPERTY="result$ddID$dtls$dtls$determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>


</PAGE>
