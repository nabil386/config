<!-- This page allows the user to make a decision.                          -->
<PAGE PAGE_ID="DecisionAssistApplication_makeDecisionForAssessment" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="DAAssessment" NAME="ARE_NEW_ANSWERS_PROVIDED" OPERATION="areNewAnswersProvided" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="DAAssessment" NAME="MAKE_DECISION" OPERATION="makeDecision" PHASE="ACTION"/>
  <SERVER_INTERFACE CLASS="DAAssessment" NAME="ASSESSMENTCONTEXT" OPERATION="viewAssessmentContextDescription" PHASE="DISPLAY"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="assessmentCaseID"/>
    <TARGET NAME="ASSESSMENTCONTEXT" PROPERTY="key$dtls$dtls$assessmentCaseID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="assessmentCaseID"/>
    <TARGET NAME="ARE_NEW_ANSWERS_PROVIDED" PROPERTY="key$dtls$dtls$assessmentCaseID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="assessmentCaseID"/>
    <TARGET NAME="MAKE_DECISION" PROPERTY="key$dtls$dtls$assessmentCaseID"/>
  </CONNECT>
  <PAGE_PARAMETER NAME="assessmentCaseID"/>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <CONDITION>
      <IS_TRUE NAME="ARE_NEW_ANSWERS_PROVIDED" PROPERTY="result$dtls$answersModifiedInd"/>
    </CONDITION>
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.MakeDecisionInfo"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.MakeDecision"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
</PAGE>