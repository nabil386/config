<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows you to view decision details for a                    -->
<!-- determination-based assessment.                                        -->
<PAGE
  PAGE_ID="DecisionAssistApplication_viewDecisionAnswerDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuDecision"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="DecisionAssistApplication_viewDecisionDetails"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuAnswers"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="DecisionAssistApplication_viewDecisionAnswerDetails"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDecisionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="DeterminationExecution"
    NAME="DISPLAY"
    OPERATION="viewDecisionAnswerDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="determinationDecisionID"/>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDecisionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$determinationDecisionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$determinationDeliveryID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$answerDtls$menuText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
