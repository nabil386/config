<!-- This page allows the user to create a third party request.             -->
<PAGE
  PAGE_ID="DecisionAssistApplication_createThirdParty"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DAAssessment"
    NAME="DISPLAY"
    OPERATION="listCaseParticipants"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="ThirdPartyRequest"
    NAME="CREATE_THIRD_PARTY_REQUEST"
    OPERATION="createThirdPartyRequest"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="CREATE_THIRD_PARTY_REQUEST"
      PROPERTY="dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <!-- BEGIN, CR00198489, SS -->
  <!-- BEGIN, CR00214223, PS -->
  <CLUSTER
    LABEL_WIDTH="22.5"
    NUM_COLS="1"
  >
    <!-- END, CR00214223 -->
    <FIELD
      LABEL="Field.Label.ThirdPartyType"
      USE_BLANK="TRUE"
      WIDTH="30"
    >
      <!-- END, CR00198489 -->
      <CONNECT>
        <TARGET
          NAME="CREATE_THIRD_PARTY_REQUEST"
          PROPERTY="dtls$dtls$type"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="32.5"
    NUM_COLS="1"
    SHOW_LABELS="true"
    TITLE="Cluster.Title.ThirdParty"
  >


    <CLUSTER
      DESCRIPTION="Cluster.CaseParticipantInfo.Description"
      LABEL_WIDTH="22.5"
      NUM_COLS="1"
    >


      <FIELD
        LABEL="Field.Label.ThirdPartyParticipant"
        USE_BLANK="true"
        WIDTH="50"
      >
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="caseParticipantRoleID"
            NAME="DISPLAY"
            PROPERTY="name"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$tpParticipant"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.RegisteredPersonInfo.Description"
      LABEL_WIDTH="22.5"
      NUM_COLS="1"
    >


      <CONTAINER LABEL="Field.Label.ThirdParty">
        <!--BEGIN, CR00454503, KK-->
        <FIELD
          HEIGHT="25"
          LABEL="Field.Label.Dummy"
        >
          <!--END, CR00454503 -->
          <CONNECT>
            <TARGET
              NAME="CREATE_THIRD_PARTY_REQUEST"
              PROPERTY="dtls$dtls$concernRoleType"
            />
          </CONNECT>
        </FIELD>
        <!--BEGIN, CR00454503, KK-->
        <FIELD
          HEIGHT="50"
          LABEL="Field.Label.Dummy"
        >
          <!--END, CR00454503 -->
          <CONNECT>
            <TARGET
              NAME="CREATE_THIRD_PARTY_REQUEST"
              PROPERTY="dtls$dtls$tpRegisteredUser"
            />
          </CONNECT>
        </FIELD>


      </CONTAINER>


    </CLUSTER>


    <CLUSTER
      DESCRIPTION="Cluster.ThirdPartyInfo.Description"
      LABEL_WIDTH="45"
      NUM_COLS="2"
    >
      <!-- BEGIN, CR00214223, PS -->
      <FIELD LABEL="Field.Label.ThirdPartyName">
        <!-- END, CR00214223 -->
        <!-- END, CR00198489 -->
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$name"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <!-- BEGIN, CR00198489, SS -->
    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
      TAB_ORDER="ROW"
    >
      <!--BEGIN, CR00454503, KK-->
      <FIELD LABEL="Field.Label.Dummy">
        <!--END, CR00454503 -->
        <!-- END, CR00198489 -->
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$address"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <!-- BEGIN, CR00198489, SS -->
    <CLUSTER
      LABEL_WIDTH="45"
      NUM_COLS="2"
    >
      <FIELD LABEL="Field.Label.PhoneAreaCode">
        <!-- END, CR00198489 -->
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$phoneAreaCode"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00198489, SS -->
      <FIELD LABEL="Field.Label.Type">
        <!-- END, CR00198489 -->
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$representativeType"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00198489, SS -->
      <FIELD LABEL="Field.Label.PhoneNumber">
        <!-- END, CR00198489 -->
        <CONNECT>
          <TARGET
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="dtls$dtls$phoneNo"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00214223, PS -->
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="true"
        PAGE_ID="DecisionAssistApplication_createThirdPartyRequests"
        SAVE_LINK="false"
      >
        <!-- END, CR00214223 -->
        <CONNECT>
          <SOURCE
            NAME="CREATE_THIRD_PARTY_REQUEST"
            PROPERTY="result$key$key$thirdPartyRequestID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="thirdPartyRequestID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationDeliveryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
