<!-- This page allows the user to edit third party request details.         -->
<PAGE
  PAGE_ID="DecisionAssistApplication_editThirdPartyRequest"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="thirdPartyRequestID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationDelivery"
    NAME="ASSESSMENTCONTEXT"
    OPERATION="viewAssessmentContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="ASSESSMENTCONTEXT"
      PROPERTY="key$dtls$dtls$determinationDeliveryID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ThirdPartyRequest"
    NAME="LOAD_THIRD_PARTY_REQUEST"
    OPERATION="readThirdPartyRequest"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="thirdPartyRequestID"
    />
    <TARGET
      NAME="LOAD_THIRD_PARTY_REQUEST"
      PROPERTY="key$key$key$thirdPartyRequestID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationDeliveryID"
    />
    <TARGET
      NAME="LOAD_THIRD_PARTY_REQUEST"
      PROPERTY="key$key$key$determinationDeliveryID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="ThirdPartyRequest"
    NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
    OPERATION="editThirdPartyRequest"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="thirdPartyRequestID"
    />
    <TARGET
      NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
      PROPERTY="dtls$dtls$thirdPartyRequestID"
    />
  </CONNECT>
  <LIST TITLE="List.Title.QuestionsAnsweredNotConsolidated">
    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.Multiselect.Header"
      WIDTH="5"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$unConsolidatedDtls$dtls$dtls$questionID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$initialMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
              PROPERTY="dtls$dtls$unconsolidatedMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>
    <FIELD
      LABEL="ActionControl.Label.Questionnaire"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unConsolidatedDtls$dtls$dtls$questionnaireName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.Question">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unConsolidatedDtls$dtls$dtls$questionText"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.ClientAnswer">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unConsolidatedDtls$dtls$dtls$clientAnswer"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <LIST TITLE="List.Title.QuestionsAnsweredConsolidated">
    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.Multiselect.Header"
      WIDTH="5"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$consolidatedDtls$dtls$dtls$questionID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$initialMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
              PROPERTY="dtls$dtls$consolidatedMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>
    <FIELD
      LABEL="ActionControl.Label.Questionnaire"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$consolidatedDtls$dtls$dtls$questionnaireName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.Question">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$consolidatedDtls$dtls$dtls$questionText"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.ClientAnswer">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$consolidatedDtls$dtls$dtls$clientAnswer"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <LIST TITLE="List.Title.UnansweredQuestions">
    <CONTAINER
      ALIGNMENT="CENTER"
      LABEL="List.Multiselect.Header"
      WIDTH="5"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$unAnsweredDtls$dtls$dtls$questionID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
          <CONNECT>
            <SOURCE
              NAME="LOAD_THIRD_PARTY_REQUEST"
              PROPERTY="result$initialMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
              PROPERTY="dtls$dtls$unansweredMultiSelectQuestionParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>
    <FIELD
      LABEL="ActionControl.Label.Questionnaire"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unAnsweredDtls$dtls$dtls$questionnaireName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.Question">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unAnsweredDtls$dtls$dtls$questionText"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="ActionControl.Label.ClientAnswer">
      <CONNECT>
        <SOURCE
          NAME="LOAD_THIRD_PARTY_REQUEST"
          PROPERTY="result$unAnsweredDtls$dtls$dtls$clientAnswer"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!-- BEGIN, CR00214223, PS -->
  <CLUSTER
    SHOW_LABELS="FALSE"
    TITLE="Cluster.Title.AdditionalInformation"
  >
    <FIELD HEIGHT="4">
      <!-- END, CR00214223 -->
      <CONNECT>
        <TARGET
          NAME="EDIT_THIRD_PARTY_REQUEST_QUESTIONS"
          PROPERTY="dtls$dtls$additionalInfo"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="true"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
