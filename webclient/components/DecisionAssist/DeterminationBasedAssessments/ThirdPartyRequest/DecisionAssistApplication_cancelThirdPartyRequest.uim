<!-- This page allows the user to cancel a third party request.             -->
<PAGE PAGE_ID="DecisionAssistApplication_cancelThirdPartyRequest" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_PARAMETER NAME="thirdPartyRequestID"/>
  <PAGE_PARAMETER NAME="determinationDeliveryID"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <SERVER_INTERFACE CLASS="DeterminationDelivery" NAME="ASSESSMENTCONTEXT" OPERATION="viewAssessmentContextDescription" PHASE="DISPLAY"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="determinationDeliveryID"/>
    <TARGET NAME="ASSESSMENTCONTEXT" PROPERTY="key$dtls$dtls$determinationDeliveryID"/>
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ThirdPartyRequest" NAME="CANCEL_TPR" OPERATION="cancelThirdPartyRequest" PHASE="ACTION"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="thirdPartyRequestID"/>
    <TARGET NAME="CANCEL_TPR" PROPERTY="key$key$key$thirdPartyRequestID"/>
  </CONNECT>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.ConfirmMessage"/>
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>
  <!-- BEGIN, CR00187368, AK -->
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.No" TYPE="ACTION"> </ACTION_CONTROL>
  </ACTION_SET>
  <!-- END, CR00187368 -->
</PAGE>