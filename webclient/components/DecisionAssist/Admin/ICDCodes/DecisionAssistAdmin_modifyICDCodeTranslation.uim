<!-- This page allows the user to modify ICD code translation details.      -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyICDCodeTranslation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="SI_VIEWICD_TRANSLATION"
    OPERATION="readICDTextTranslation"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="SI_MODIFYICDCODETRANSLATION"
    OPERATION="modifyICDTextTranslation"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="translationID"/>
  <PAGE_PARAMETER NAME="ICDCode"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="translationID"
    />
    <TARGET
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="key$dtls$dtls$dtls$translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="result$dtls$dtls$dtls$translationID"
    />
    <TARGET
      NAME="SI_MODIFYICDCODETRANSLATION"
      PROPERTY="translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="subjectID"
    />
    <TARGET
      NAME="SI_MODIFYICDCODETRANSLATION"
      PROPERTY="subjectID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="subjectType"
    />
    <TARGET
      NAME="SI_MODIFYICDCODETRANSLATION"
      PROPERTY="subjectType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="recordStatus"
    />
    <TARGET
      NAME="SI_MODIFYICDCODETRANSLATION"
      PROPERTY="recordStatus"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEWICD_TRANSLATION"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="SI_MODIFYICDCODETRANSLATION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <!-- BEGIN, CR00198297, SS -->
  <!-- BEGIN, CR00207167, SS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.Language"
      WIDTH="75"
    >
      <!-- END, CR00198297 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWICD_TRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFYICDCODETRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <!-- BEGIN, CR00198297, SS -->
    <FIELD
      LABEL="Field.Label.Translation"
      WIDTH="73"
    >
      <!-- END, CR00198297 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWICD_TRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFYICDCODETRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
