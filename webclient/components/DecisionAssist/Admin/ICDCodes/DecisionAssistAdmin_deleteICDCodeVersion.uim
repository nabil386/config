<!-- This page allows the user to delete the ICD code version               -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deleteICDCodeVersion"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="REMOVE_ICD_CODE_VERSION"
    OPERATION="removeICDCodeVersion"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="ICDCodeVersionID"/>
  <PAGE_PARAMETER NAME="ICDCodeVersion"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ICDCodeVersionID"
    />
    <TARGET
      NAME="REMOVE_ICD_CODE_VERSION"
      PROPERTY="key$versionKey$ICDCodeVersionID"
    />
  </CONNECT>
