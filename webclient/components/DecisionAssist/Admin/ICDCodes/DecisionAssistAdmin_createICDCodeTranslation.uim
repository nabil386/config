<!-- This page allows the user to create an ICD code translation.           -->
<!-- BEGIN, CR00237410, AK -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createICDCodeTranslation"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00237410 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="SI_CREATEICDCODETRANSLATION"
    OPERATION="createICDTextTranslation"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="ICDCode"/>
  <PAGE_PARAMETER NAME="ICDCodeID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ICDCodeID"
    />
    <TARGET
      NAME="SI_CREATEICDCODETRANSLATION"
      PROPERTY="subjectID"
    />
  </CONNECT>
  <!-- BEGIN, CR00207167, SS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.Language"
      WIDTH="75"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="SI_CREATEICDCODETRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.Translation"
      WIDTH="73"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="SI_CREATEICDCODETRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
