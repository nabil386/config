<!-- This page allows the user to modify ICD code details.                  -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyICDCode"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="VIEW_ICD_CODE"
    OPERATION="viewICDCodeDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="MODIFY_ICD_CODE"
    OPERATION="modifyICDCode"
    PHASE="ACTION"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="ICDCodeID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ICDCodeID"
    />
    <TARGET
      NAME="VIEW_ICD_CODE"
      PROPERTY="key$keyDtls$ICDCodeID"
    />
  </CONNECT>
  <CONNECT>
    <TARGET
      NAME="MODIFY_ICD_CODE"
      PROPERTY="details$dtls$modifyDtls$ICDCodeID"
    />
    <SOURCE
      NAME="VIEW_ICD_CODE"
      PROPERTY="result$viewCodeDtls$viewDtls$ICDCodeID"
    />
  </CONNECT>
  <!-- BEGIN, CR00198884, SS -->
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER LABEL_WIDTH="30">
    <FIELD
      LABEL="Field.Label.ICDText"
      WIDTH="98"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <SOURCE
          NAME="VIEW_ICD_CODE"
          PROPERTY="result$viewCodeDtls$viewDtls$ICDText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFY_ICD_CODE"
          PROPERTY="details$dtls$modifyDtls$ICDText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="1"
  >
    <!-- END, CR00198884 -->
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="VIEW_ICD_CODE"
          PROPERTY="result$viewCodeDtls$viewDtls$endDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFY_ICD_CODE"
          PROPERTY="details$dtls$modifyDtls$endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00198297 -->
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
   </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    >
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
