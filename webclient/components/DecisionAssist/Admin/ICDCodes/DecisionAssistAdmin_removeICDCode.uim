<!-- This page allows the user to delete an ICD code.                       -->
<PAGE PAGE_ID="DecisionAssistAdmin_removeICDCode" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="ICDCodes" NAME="REMOVE_ICD_CODE" OPERATION="removeICDCode" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="ICDCodeID"/>
  <PAGE_PARAMETER NAME="ICDCode"/>
  <PAGE_PARAMETER NAME="startDate"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="ICDCodeID"/>
    <TARGET NAME="REMOVE_ICD_CODE" PROPERTY="key$keyDtls$ICDCodeID"/>
  </CONNECT>
  <!-- BEGIN, CR00198884, SS -->
  <!-- BEGIN, CR00226313, PS -->
  <CLUSTER LABEL_WIDTH="25">
    <!-- END, CR00226313 -->
    <!-- END, CR00198884 -->
    <FIELD LABEL="Field.Label.StartDate" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate" WIDTH="35">
      <CONNECT>
        <TARGET NAME="REMOVE_ICD_CODE" PROPERTY="details$dtls$removeDtls$endDate"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00247672, AK -->
  <ACTION_SET ALIGNMENT="RIGHT" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel" TYPE="ACTION"/>
  </ACTION_SET>
  <!-- END, CR00247672 -->
</PAGE>