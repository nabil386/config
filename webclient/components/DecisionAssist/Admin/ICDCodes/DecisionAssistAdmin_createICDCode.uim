<!-- This page allows the user to create an ICD code.                       -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createICDCode"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="ICD_VERSION_NUMBER"
    OPERATION="readActiveVersionNumbers"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="CREATE_ICD_CODE"
    OPERATION="createICDCode"
    PHASE="ACTION"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER
    LABEL_WIDTH="55"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.ICDCode"
      WIDTH="60"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODE"
          PROPERTY="details$createDtls$createDtls$ICDCode"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00258324, PS -->
    <FIELD LABEL="Field.Label.StartDate">
      <!-- END, CR00258324 -->
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODE"
          PROPERTY="details$createDtls$createDtls$startDate"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00198297, SS -->
    <FIELD
      LABEL="Field.Label.Version"
      WIDTH="60"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$versionDtls$versionNum$dtls$icdCodeVersionID"
          NAME="ICD_VERSION_NUMBER"
          PROPERTY="result$versionDtls$versionNum$dtls$versionNumber"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODE"
          PROPERTY="details$createDtls$createDtls$ICDCodeVersionID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODE"
          PROPERTY="details$createDtls$createDtls$endDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER LABEL_WIDTH="28">
    <FIELD
      LABEL="Field.Label.ICDText"
      WIDTH="98"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODE"
          PROPERTY="details$createDtls$createDtls$ICDText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <!-- BEGIN, CR00215172, PS -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SavenNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_createICDCode"/>
    </ACTION_CONTROL>
    <!-- END, CR00215172 -->


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    >
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
