<!-- This page allows the user to upload an ICD code.                       -->
<!-- BEGIN, CR00340898, PB -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_uploadICDCodes"
  WINDOW_OPTIONS="width=640"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00340898 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="UPLOAD_ICD_CODES"
    OPERATION="uploadICDCodes"
    PHASE="ACTION"
  />
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="UPLOAD_ICD_CODES"
        PROPERTY="result$dtls$message"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER
    LABEL_WIDTH="48"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.ICDCodeVersion"
      USE_DEFAULT="false"
      WIDTH="40"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <TARGET
          NAME="UPLOAD_ICD_CODES"
          PROPERTY="details$uploadVersionDtls$createVersionDtls$versionNumber"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198297, SS -->
    <WIDGET
      LABEL="Field.Label.File"
      TYPE="FILE_UPLOAD"
      WIDTH="60"
    >
      <!-- END, CR00198297 -->
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="UPLOAD_ICD_CODES"
            PROPERTY="details$fileContent"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
      <WIDGET_PARAMETER NAME="FILE_NAME">
        <CONNECT>
          <TARGET
            NAME="UPLOAD_ICD_CODES"
            PROPERTY="details$fileURL"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>
    <!-- BEGIN, CR00198297, SS -->
    <FIELD
      LABEL="Field.Label.Volume"
      WIDTH="40"
    >


      <CONNECT>
        <TARGET
          NAME="UPLOAD_ICD_CODES"
          PROPERTY="details$uploadVersionDtls$createVersionDtls$volume"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Classification"
      WIDTH="40"
    >
      <CONNECT>
        <TARGET
          NAME="UPLOAD_ICD_CODES"
          PROPERTY="details$uploadVersionDtls$createVersionDtls$classification"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00237410, AK -->
    <FIELD
      LABEL="Field.Label.FileFormat"
      USE_BLANK="false"
      USE_DEFAULT="true"
      WIDTH="42"
    >
      <!-- END, CR00237410 -->


      <CONNECT>
        <TARGET
          NAME="UPLOAD_ICD_CODES"
          PROPERTY="details$fileFormat"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00198297 -->
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Upload"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    > </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
