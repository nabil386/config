<!-- This page allows you to create an outcome.                             -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createOutcome"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Outcome"
    NAME="CREATEOUTCOME"
    OPERATION="createOutcome"
    PHASE="ACTION"
  />
  <!-- BEGIN, CR00207167, SS -->
  <!-- BEGIN, CR00198144, SS -->
  <CLUSTER
    LABEL_WIDTH="37"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="75"
    >
      <!-- END, CR00198144 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="CREATEOUTCOME"
          PROPERTY="outcomeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="48"
    >
      <CONNECT>
        <TARGET
          NAME="CREATEOUTCOME"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="70"
    >
      <!-- END, CR00198144 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="CREATEOUTCOME"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
      WIDTH="45"
    >
      <CONNECT>
        <TARGET
          NAME="CREATEOUTCOME"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00198144, CR00407812, SS, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00198144, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="CREATEOUTCOME"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SaveNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_createOutcome"> </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    > </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
