<!-- This page allows the user to confirm the deletion of a baseline record.-->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deleteOutcome"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="Outcome"
    NAME="DEL_OUTCOME"
    OPERATION="deleteOutcome"
    PHASE="ACTION"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="outcomeID"/>
  <PAGE_PARAMETER NAME="outcomeName"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="recordStatus"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="outcomeID"
    />
    <TARGET
      NAME="DEL_OUTCOME"
      PROPERTY="outcomeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="DEL_OUTCOME"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="recordStatus"
    />
    <TARGET
      NAME="DEL_OUTCOME"
      PROPERTY="recordStatus"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listOutcome"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeleteOutcome"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
