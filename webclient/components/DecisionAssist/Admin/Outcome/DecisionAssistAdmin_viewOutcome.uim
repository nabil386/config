<!-- This page allows the user to view outcome details.                     -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewOutcome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SHORTCUT_TITLE ICON="outcome.icon">
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWOUTCOME"
        PROPERTY="outcomeName"
      />
    </CONNECT>
  </SHORTCUT_TITLE>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWOUTCOME"
        PROPERTY="outcomeName"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="outcomeID"/>
  <PAGE_PARAMETER NAME="outcomeName"/>
  <SERVER_INTERFACE
    CLASS="Outcome"
    NAME="SI_VIEWOUTCOME"
    OPERATION="viewOutcome"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="outcomeID"
    />
    <TARGET
      NAME="SI_VIEWOUTCOME"
      PROPERTY="key$dtls$key$outcomeID"
    />
  </CONNECT>
  <!-- BEGIN, CR00215172, PS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00215172 -->
    <FIELD LABEL="Field.Label.DateCreated">
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWOUTCOME"
          PROPERTY="dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreatedBy"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWOUTCOME"
          PROPERTY="result$dtls$createdUserFullName"
        />
      </CONNECT>
      <!-- BEGIN, CR00184681, AK -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <!-- END, CR00184681 -->
        <CONNECT>
          <SOURCE
            NAME="SI_VIEWOUTCOME"
            PROPERTY="result$dtls$dtls$createdBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    NUM_COLS="2"
    STYLE="outer-cluster-borderless"
  >
    <LIST TITLE="Cluster.Title.NameTranslation">
      <!-- BEGIN, CR00207167, SS -->
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <!-- END, CR00207167 -->
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Edit"
          TYPE="ACTION"
        >
          <!-- BEGIN, CR00198297, SS -->
          <!-- BEGIN, CR00237410, AK -->
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_modifyOutcomeNameTranslation"
            WINDOW_OPTIONS="width=500"
          >
            <!-- END, CR00237410 -->
            <!-- END, CR00198297 -->
            <CONNECT>
              <SOURCE
                NAME="SI_VIEWOUTCOME"
                PROPERTY="translationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="translationID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Delete"
          TYPE="ACTION"
        >
          <!-- BEGIN, CR00198297, SS -->
          <!-- BEGIN, CR00187459, SS -->
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_deleteOutcomeNameTranslation"
          >
            <!-- END, CR00187459 -->
            <!-- END, CR00198297 -->
            <CONNECT>
              <SOURCE
                NAME="SI_VIEWOUTCOME"
                PROPERTY="translationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="translationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="SI_VIEWOUTCOME"
                PROPERTY="translationText"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="translation"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="SI_VIEWOUTCOME"
                PROPERTY="result$transDtls$dtls$dtls$dtls$versionNo"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <!-- BEGIN, CR00207167, SS -->
      </ACTION_SET>
      <FIELD
        LABEL="Field.Label.Language"
        WIDTH="40"
      >
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="SI_VIEWOUTCOME"
            PROPERTY="languageID"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00207167, SS -->
      <FIELD
        LABEL="Field.Label.Translation"
        WIDTH="60"
      >
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="SI_VIEWOUTCOME"
            PROPERTY="translationText"
          />
        </CONNECT>
      </FIELD>
    </LIST>
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.Comments"
    >
      <!-- BEGIN, CR00417165, GK -->
      <FIELD
        HEIGHT="3"
        LABEL="Field.Label.Comments"
      >
        <!-- END, CR00417165 -->
        <CONNECT>
          <SOURCE
            NAME="SI_VIEWOUTCOME"
            PROPERTY="comments"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
</PAGE>
