<!-- This page allows the user to view a list of outcomes.                  -->
<PAGE PAGE_ID="DecisionAssistAdmin_listOutcome" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- BEGIN, CR00208034, PS -->
  <SERVER_INTERFACE CLASS="Outcome" NAME="SI_LIST_OUTCOME" OPERATION="listOutcomeDetails"/>
  <LIST>
    <!-- BEGIN, CR00207167, SS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207167 -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit" TYPE="ACTION">
        <CONDITION>
          <IS_TRUE NAME="SI_LIST_OUTCOME" PROPERTY="activeStatusInd"/>
        </CONDITION>
        <!-- BEGIN, CR00198297, SS -->
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_modifyOutcome" WINDOW_OPTIONS="width=700">
          <!-- END, CR00198297 -->
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeID"/>
            <TARGET NAME="PAGE" PROPERTY="outcomeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.AddOutcomeNameTranslation" TYPE="ACTION">
        <CONDITION>
          <IS_TRUE NAME="SI_LIST_OUTCOME" PROPERTY="activeStatusInd"/>
        </CONDITION>
        <!-- BEGIN, CR00237410, AK -->
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_createOutcomeNameTranslation" WINDOW_OPTIONS="width=500">
          <!-- END, CR00237410 -->
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeID"/>
            <TARGET NAME="PAGE" PROPERTY="outcomeID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete" TYPE="ACTION">
        <CONDITION>
          <IS_TRUE NAME="SI_LIST_OUTCOME" PROPERTY="activeStatusInd"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_deleteOutcome">
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeID"/>
            <TARGET NAME="PAGE" PROPERTY="outcomeID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeName"/>
            <TARGET NAME="PAGE" PROPERTY="outcomeName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="recordStatus"/>
            <TARGET NAME="PAGE" PROPERTY="recordStatus"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <!-- BEGIN, CR00207167, SS -->
    </ACTION_SET>
    <!-- END, CR00207167 -->
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="type"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="recordStatus"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00208034 -->
    <!-- BEGIN, CR00207167, SS -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistAdmin_viewOutcome">
        <CONNECT>
          <SOURCE NAME="SI_LIST_OUTCOME" PROPERTY="outcomeID"/>
          <TARGET NAME="PAGE" PROPERTY="outcomeID"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00207167 -->
  </LIST>
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.New" TYPE="ACTION">
      <!-- BEGIN, CR00198297, SS -->
      <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_createOutcome"/>
      <!-- END, CR00198297 -->
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>