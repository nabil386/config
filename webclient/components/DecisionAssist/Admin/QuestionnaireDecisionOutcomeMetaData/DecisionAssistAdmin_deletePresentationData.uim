<!-- This page allows the user to delete Presentation Data                  -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deletePresentationData"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="QuestionnaireDecisionOutcomeMetaData"
    NAME="SI_DELETEPRESENTATIONDATA"
    OPERATION="deletePresentationData"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="QuestionnaireDecisionOutcomeMetaData"
    NAME="SI_VIEWPRESENTATIONDATA"
    OPERATION="readQuestionnairePresentationData"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="QuestionnaireDecisionOutcomeMetaData"
    NAME="QUESTIONHEADER"
    OPERATION="readPresentationDataHeaderDetails"
    PHASE="DISPLAY"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWPRESENTATIONDATA"
        PROPERTY="decisionMatrixName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Hypne"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWPRESENTATIONDATA"
        PROPERTY="outcomeName"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="presentationDataID"/>
  <PAGE_PARAMETER NAME="dpQuestionnaireLinkID"/>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="presentationDataID"
    />
    <TARGET
      NAME="SI_VIEWPRESENTATIONDATA"
      PROPERTY="key$key$key$presentationDataID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="presentationDataID"
    />
    <TARGET
      NAME="SI_DELETEPRESENTATIONDATA"
      PROPERTY="details$dtls$dtls$presentationDataID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="dpQuestionnaireLinkID"
    />
    <TARGET
      NAME="SI_DELETEPRESENTATIONDATA"
      PROPERTY="details$dtls$dtls$dpQuestionnaireLinkID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="dpQuestionnaireLinkID"
    />
    <TARGET
      NAME="QUESTIONHEADER"
      PROPERTY="key$key$key$dpQuestionnaireLinkID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeletePresentationData"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
