<!-- This page allows the user to view presentation data details.           -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewPresentationData"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="MENUCONTEXT"
    OPERATION="viewDeterminationPackageMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
    <!-- BEGIN, CR00237410, AK -->
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWPRESENTATIONDATA"
        PROPERTY="result$presentationDtls$headerDtls$dtls$dPackageName"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Hypne"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="SI_VIEWPRESENTATIONDATA"
        PROPERTY="result$presentationDtls$headerDtls$dtls$questionnaireName"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="presentationDataID"/>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>


  <SERVER_INTERFACE
    CLASS="QuestionnaireDecisionOutcomeMetaData"
    NAME="SI_VIEWPRESENTATIONDATA"
    OPERATION="readQuestionnairePresentationData"
    PHASE="DISPLAY"
  />
  <!-- END, CR00237410 -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="presentationDataID"
    />
    <TARGET
      NAME="SI_VIEWPRESENTATIONDATA"
      PROPERTY="key$key$key$presentationDataID"
    />
  </CONNECT>
  <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Field.Label.DecisionMatrix">
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="decisionMatrixName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Presentation">
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="questionnairePresentationCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CreationDate">
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.LabelOutcome">
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="outcomeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreatedBy"
      USE_DEFAULT="false"
    >
      <!-- BEGIN, CR00237410, AK -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="createdUserFullName"
        />
      </CONNECT>
      <!-- END, CR00237410 -->
      <!-- BEGIN, CR00184681, AK -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <!-- END, CR00184681 -->
        <CONNECT>
          <SOURCE
            NAME="SI_VIEWPRESENTATIONDATA"
            PROPERTY="createdBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00417165, GK -->
    <FIELD
      HEIGHT="3"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00417165 -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEWPRESENTATIONDATA"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
