<!-- This page allows the user to create a Questionnaire.                   -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createQuestionnaireFromPackage"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>
  <PAGE_PARAMETER NAME="determinationName"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="pageID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="CREATE_QUESTIONNAIRE"
    OPERATION="createQuestionnaireForDeterminationPackage"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="CREATE_QUESTIONNAIRE"
      PROPERTY="request$dtls$determinationPackageVersionID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="READ_PACKAGE"
    OPERATION="readDeterminationPackage"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="READ_PACKAGE"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <INCLUDE FILE_NAME="DecisionAssistAdmin_createQuestionnaireView.vim"/>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="DecisionAssistAdmin_viewQuestionnaire"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="CREATE_QUESTIONNAIRE"
            PROPERTY="result$key$QuestionVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    > </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
