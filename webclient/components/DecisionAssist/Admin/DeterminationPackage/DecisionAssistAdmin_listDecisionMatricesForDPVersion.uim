<!-- Description -->
<!-- This page allows the user to view a list of decision matrices.         -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_listDecisionMatricesForDPVersion"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="VIEW_DETERMINATION_PACKAGE"
    OPERATION="viewDeterminationPackageDetails"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="LIST_DECISION_MATRIX"
    OPERATION="listDecisionMatrixVersionDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION_PACKAGE"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="LIST_DECISION_MATRIX"
      PROPERTY="key$key$determinationPackageVersionID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationPackageVersion">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="dCdtls$dtls$dtls$name"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="dtls$dtls$determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DecisionMatrix"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listDecisionMatricesForDPVersion">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER SHOW_LABELS="false">
    <LIST>
      <FIELD
        LABEL="Field.Label.DMName"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$dMDtls$list$list$dtls$name"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.DMVersion"
        WIDTH="10"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$dMDtls$list$list$dtls$versionNumber"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.DMType"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$dMDtls$list$list$dtls$type"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.DMFromDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$dMDtls$list$list$dtls$fromDate"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Label.DMToDate"
        WIDTH="15"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$dMDtls$list$list$dtls$toDate"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>
</PAGE>
