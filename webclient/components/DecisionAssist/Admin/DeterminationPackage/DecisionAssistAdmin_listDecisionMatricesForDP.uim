          <CONDITION>
            <IS_TRUE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$cumulativeInEditInd"
            />
          </CONDITION>
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_createAssociateOutcomes"
            SET_HIERARCHY_RETURN_PAGE="true"
            WINDOW_OPTIONS="width=625"
          >
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
