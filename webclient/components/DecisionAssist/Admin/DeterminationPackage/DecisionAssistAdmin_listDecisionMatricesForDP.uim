<!-- Description -->
<!-- This page allows the user to view a list of decision matrices.         -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="DecisionAssistAdmin_listDecisionMatricesForDP"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="VIEW_DETERMINATION_PACKAGE"
    OPERATION="viewDeterminationPackageDetails"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="LIST_DECISION_MATRIX"
    OPERATION="listDecisionMatrixVersionDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION_PACKAGE"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="LIST_DECISION_MATRIX"
      PROPERTY="key$key$determinationPackageVersionID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationPackage">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DecisionMatrices"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listDecisionMatricesForDP">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.VersionHistory"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listPackageVersionsHistory">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER SHOW_LABELS="false">
    <LIST>
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
          <CONDITION>
            <IS_TRUE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
          </CONDITION>
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_modifyDecisionMatrix"
            SET_HIERARCHY_RETURN_PAGE="true"
            WINDOW_OPTIONS="width=625"
          >
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL
          LABEL="ActionControl.Label.EditMatrix"
          TYPE="ACTION"
        >
          <CONDITION>
            <IS_TRUE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
          </CONDITION>
          <LINK PAGE_ID="DecisionMatrix_editDecisionMatrix">
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$name"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$decisionMatrixVersionNumber"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="CONSTANT"
                PROPERTY="ReadOnlyFlagFalse"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="isMatrixReadOnly"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL
          APPEND_ELLIPSIS="false"
          LABEL="ActionControl.Label.ViewMatrix"
          TYPE="ACTION"
        >
          <CONDITION>
            <IS_FALSE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
          </CONDITION>
          <LINK PAGE_ID="DecisionMatrix_editDecisionMatrix">
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$name"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$decisionMatrixVersionNumber"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionNumber"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="CONSTANT"
                PROPERTY="ReadOnlyFlagTrue"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="isMatrixReadOnly"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.Label.Release">
