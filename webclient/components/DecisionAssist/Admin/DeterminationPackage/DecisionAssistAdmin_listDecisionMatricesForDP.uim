              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="decisionMatrixID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.Label.Clone">
          <CONDITION>
            <IS_TRUE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
