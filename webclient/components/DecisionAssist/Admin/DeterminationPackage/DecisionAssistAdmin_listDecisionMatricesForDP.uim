        <ACTION_CONTROL
          LABEL="ActionControl.Label.Remove"
          TYPE="ACTION"
        >
          <CONDITION>
            <IS_FALSE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
          </CONDITION>
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_removeDecisionMatrixFromPackage"
            WINDOW_OPTIONS="width=500"
          >
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$dMDtls$list$list$dtls$decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$thisVersion$determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>
      <FIELD
