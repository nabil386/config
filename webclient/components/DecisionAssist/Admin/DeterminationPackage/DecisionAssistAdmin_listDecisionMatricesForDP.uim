              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <SEPARATOR/>
        <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
          <CONDITION>
            <IS_TRUE
              NAME="LIST_DECISION_MATRIX"
              PROPERTY="result$dtlsList$inEditStatusInd"
            />
          </CONDITION>
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_deleteDecisionMatrixVersion"
            WINDOW_OPTIONS="width=500"
          >
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$decisionMatrixVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$name"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="decisionMatrixName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$decisionMatrixVersionNumber"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNumber"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DECISION_MATRIX"
                PROPERTY="currentVersion$dtls$versionStatus"
              />
              <TARGET
                NAME="PAGE"
