<!-- This page allows the user to view a determination package version.     -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewDeterminationPackageVersion"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="MENUCONTEXT"
    OPERATION="viewDeterminationPackageMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>
  <PAGE_PARAMETER NAME="determinationName"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="VIEW_DETERMINATION_PACKAGE"
    OPERATION="viewDeterminationPackageDetails"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION_PACKAGE"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationPackageVersion">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DecisionMatrix"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listDecisionMatricesForDPVersion">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CLUSTER NUM_COLS="2">
    <FIELD LABEL="Field.Label.Gender">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="thisVersion$gender"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MinimumAge">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="thisVersion$minAge"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InitialReleasedOn">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="result$initialVersion$fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InitialCreationDate">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="result$initialVersion$dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>


    <FIELD LABEL="Field.Label.MaximumAge">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="thisVersion$maxAge"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Product">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="thisVersion$productID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.InitialCreatedBy">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION_PACKAGE"
          PROPERTY="result$initialVersion$createdUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$initialVersion$createdBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    NUM_COLS="2"
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.Questionnaires"
    >
      <LIST>
        <CONDITION>
          <IS_TRUE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$thisVersion$inEditStatusInd"
          />
        </CONDITION>
        <FIELD
          LABEL="Field.Label.QuestionnaireName"
          WIDTH="60"
        >
          <CONNECT>
            <SOURCE
              NAME="VIEW_DETERMINATION_PACKAGE"
              PROPERTY="result$questionnaires$dtls$dtls$name"
            />
          </CONNECT>
          <LINK PAGE_ID="DecisionAssistAdmin_resolveViewQuestionnaire">
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$questionnaires$dtls$dtls$questionnaireVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="questionnaireVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="CONSTANT"
                PROPERTY="DeterminationPageID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pageID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationName"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$dCdtls$dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </FIELD>
        <FIELD
          LABEL="Field.Label.QuestionnaireVersion"
          WIDTH="20"
        >
          <CONNECT>
            <SOURCE
              NAME="VIEW_DETERMINATION_PACKAGE"
              PROPERTY="version"
            />
          </CONNECT>
        </FIELD>
      </LIST>
      <LIST>
        <CONDITION>
          <IS_FALSE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="result$thisVersion$inEditStatusInd"
          />
        </CONDITION>
        <FIELD
          LABEL="Field.Label.QuestionnaireName"
          WIDTH="60"
        >
          <CONNECT>
            <SOURCE
              NAME="VIEW_DETERMINATION_PACKAGE"
              PROPERTY="result$questionnaires$dtls$dtls$name"
            />
          </CONNECT>
          <LINK PAGE_ID="DecisionAssistAdmin_resolveViewQuestionnaire">
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$questionnaires$dtls$dtls$questionnaireVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="questionnaireVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="CONSTANT"
                PROPERTY="DeterminationPageID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="pageID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationPackageVersionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="determinationName"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_DETERMINATION_PACKAGE"
                PROPERTY="result$dCdtls$dtls$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </FIELD>
        <FIELD
          LABEL="Field.Label.QuestionnaireVersion"
          WIDTH="20"
        >
          <CONNECT>
            <SOURCE
              NAME="VIEW_DETERMINATION_PACKAGE"
              PROPERTY="version"
            />
          </CONNECT>
        </FIELD>
      </LIST>
    </CLUSTER>
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.Comments"
      WIDTH="100"
    >
      <!-- BEGIN, CR00417165, GK -->
      <FIELD
        HEIGHT="3"
        LABEL="Field.Label.Comments"
      >
        <!-- END, CR00417165 -->
        <CONNECT>
          <SOURCE
            NAME="VIEW_DETERMINATION_PACKAGE"
            PROPERTY="thisVersion$comments"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
</PAGE>
