<!-- This page allows the user to Modify Determination Package record.      -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyDeterminationPackage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="read"
    OPERATION="readDeterminationPackage"
    PHASE="DISPLAY"
  > </SERVER_INTERFACE>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="read"
      PROPERTY="key$key$key$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="modify"
    OPERATION="modifyDeterminationPackage"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="modify"
      PROPERTY="dtls$dtls$determinationPackageVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="read"
      PROPERTY="result$dtls$versionNo"
    />
    <TARGET
      NAME="modify"
      PROPERTY="dtls$dtls$versionNo"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
  <!-- BEGIN, CR00198144, SS -->
  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="50"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.MinimumAge"
      WIDTH="50"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="minAge"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="minAge"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Gender"
      USE_BLANK="true"
      WIDTH="53"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="gender"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="gender"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Product"
      USE_BLANK="true"
      WIDTH="90"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="productID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="productID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.MaximumAge"
      WIDTH="50"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="maxAge"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="maxAge"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      HEIGHT="4"
      LABEL="Cluster.Title.Comments"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <SOURCE
          NAME="read"
          PROPERTY="comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="modify"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
