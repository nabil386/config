              PROPERTY="result$dtls$dtls$dtls$dpQuestionnaireLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="dpQuestionnaireLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="determinationPackageVersionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationPackageVersionID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="result$dtls$dtls$dtls$name"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_resolveViewQuestionnaire">
        <CONNECT>
          <SOURCE
            NAME="LIST_PACKAGES"
            PROPERTY="result$dtls$dtls$dtls$questionnaireVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="QuestionnaireDeterminationPageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="result$dtls$dtls$dtls$type"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Version"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="result$dtls$dtls$dtls$version"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewQuestionnaireForDP">
        <CONNECT>
          <SOURCE
            NAME="LIST_PACKAGES"
            PROPERTY="result$dtls$dtls$dtls$questionnaireVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="QuestionnaireDeterminationPageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="result$dtls$dtls$dtls$status"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
