<!-- This page allows you to view a determination configuration details.     -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewDeterminationConfigTabDetails"
  TYPE="DETAILS"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Hyphen"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_DETERMINATION"
        PROPERTY="result$dtls$dtls$name"
      />
    </CONNECT>
  </PAGE_TITLE>


  <TAB_NAME>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Hyphen"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_DETERMINATION"
        PROPERTY="result$dtls$dtls$name"
      />
    </CONNECT>
  </TAB_NAME>


  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="VIEW_DETERMINATION"
    OPERATION="readDeterminationConfig"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="determinationConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.DeterminationConfigName">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DeterminationConfigType">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DeterminationConfigReference">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$determinationalReference"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="result$dtls$dtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
