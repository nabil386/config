<!-- This page allows the user to modify the translation details for        -->
<!-- the determination configuration.                                       -->
<!-- BEGIN, CR00237410, AK -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyDeterminationConfigNameTranslation"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00237410 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="SI_MODIFYTRANSLATION"
    OPERATION="modifyDeterminationConfigNameTranslation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="VIEW_DETERMINATION"
    OPERATION="readDeterminationConfigNameTranslation"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="translationID"/>
  <PAGE_PARAMETER NAME="determinationName"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="translationID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION"
      PROPERTY="key$key$key$dtls$translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="VIEW_DETERMINATION"
      PROPERTY="key$key$key$dtls$translationID"
    />
    <TARGET
      NAME="SI_MODIFYTRANSLATION"
      PROPERTY="translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="VIEW_DETERMINATION"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="SI_MODIFYTRANSLATION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="VIEW_DETERMINATION"
      PROPERTY="subjectID"
    />
    <TARGET
      NAME="SI_MODIFYTRANSLATION"
      PROPERTY="subjectID"
    />
  </CONNECT>
  <!-- BEGIN, CR00207167, SS -->
  <!-- BEGIN, CR00198144, SS -->
  <!-- BEGIN, CR00215172, PS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="1"
  >
    <!-- END, CR00215172 -->
    <FIELD
      LABEL="Field.Label.Language"
      WIDTH="78"
    >
      <!-- END, CR00198144 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="languageID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFYTRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Translation"
      WIDTH="75"
    >
      <!-- END, CR00198144 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="VIEW_DETERMINATION"
          PROPERTY="translationText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFYTRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
