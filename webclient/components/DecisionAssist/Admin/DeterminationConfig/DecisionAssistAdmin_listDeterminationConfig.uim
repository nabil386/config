<PAGE PAGE_ID="DecisionAssistAdmin_listDeterminationConfig" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <SERVER_INTERFACE CLASS="DeterminationConfig" NAME="LIST_DETERMINATION" OPERATION="listDeterminationConfig" PHASE="DISPLAY"/>
  <LIST>
    <!-- BEGIN, CR00207167, SS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207167 -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit" TYPE="ACTION">
        <!-- BEGIN, CR00207167, SS -->
        <!-- BEGIN, CR00198297, SS -->
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_resolveModifyDeterminationConfig" WINDOW_OPTIONS="width=800">
          <!-- END, CR00198297 -->
          <!-- END, CR00207167 -->
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00207167, SS -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <!-- BEGIN, CR00237410, AK -->
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_deleteDeterminationConfig" WINDOW_OPTIONS="width=500">
          <!-- END, CR00237410 -->
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$determinationalReference"/>
            <TARGET NAME="PAGE" PROPERTY="determinationalReference"/>
