          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$startDate"/>
            <TARGET NAME="PAGE" PROPERTY="startDate"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$endDate"/>
            <TARGET NAME="PAGE" PROPERTY="endDate"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$recordStatus"/>
            <TARGET NAME="PAGE" PROPERTY="recordStatus"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.DeterminationConfigName" WIDTH="34">
      <CONNECT>
        <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$name"/>
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationConfig">
        <CONNECT>
          <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$determinationConfigID"/>
          <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.DeterminationConfigType" WIDTH="23">
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE NAME="LIST_DETERMINATION" PROPERTY="result$list$list$dtls$type"/>
