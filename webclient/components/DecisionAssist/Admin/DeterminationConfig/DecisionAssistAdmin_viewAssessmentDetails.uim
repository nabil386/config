<!-- Description                                                            -->
<!-- This page allows the user to view the CDA assessment details.          -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewAssessmentDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SHORTCUT_TITLE ICON="assessment.icon">
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTS_DETAILS"
        PROPERTY="result$dtls$assessmentDtls$name"
      />
    </CONNECT>
  </SHORTCUT_TITLE>


  <PAGE_PARAMETER NAME="assessmentConfigurationID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="ASSESSMENTS_DETAILS"
    OPERATION="readAssessment"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentConfigurationID"
    />
    <TARGET
      NAME="ASSESSMENTS_DETAILS"
      PROPERTY="key$dtls$dtls$assessmentConfigurationID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="ASSESSMENTS_DETAILS"
        PROPERTY="result$dtls$assessmentDtls$name"
      />
    </CONNECT>
  </PAGE_TITLE>


  <CLUSTER
    LABEL_WIDTH="36"
    NUM_COLS="2"
  >
    <!-- BEGIN, CR00207167, SS -->
    <FIELD LABEL="Field.Label.HomePageIdentifier">
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$assessmentDtls$homePageIdentifier"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00207167 -->
    <FIELD
      LABEL="Field.Label.CreationDate"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$assessmentDtls$creationDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$assessmentDtls$assessmentType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00207167, SS -->
  <CLUSTER
    NUM_COLS="2"
    SHOW_LABELS="false"
  >
    <!-- END, CR00207167 -->
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.Description"
    >
      <!-- BEGIN, CR00207167, SS -->
      <FIELD HEIGHT="4">
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="ASSESSMENTS_DETAILS"
            PROPERTY="result$dtls$assessmentDtls$description"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
    <!-- BEGIN, CR00207167, SS -->
    <CLUSTER
      SHOW_LABELS="false"
      TITLE="Cluster.Title.Comments"
    >
      <!-- BEGIN, CR00417165, GK -->
      <FIELD
        HEIGHT="4"
        LABEL="Field.Label.Comments"
      >
        <!-- END, CR00417165 -->
        <CONNECT>
          <SOURCE
            NAME="ASSESSMENTS_DETAILS"
            PROPERTY="result$dtls$assessmentDtls$comments"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>
  <!-- END, CR00207167 -->
  <LIST TITLE="Cluster.Title.DeterminationConfiguration">
    <CONDITION>
      <IS_TRUE
        NAME="ASSESSMENTS_DETAILS"
        PROPERTY="result$dtls$standalone"
      />
    </CONDITION>
    <!-- BEGIN, CR00207167, SS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207167 -->


      <ACTION_CONTROL
        LABEL="ActionControl.Label.Delete"
        TYPE="ACTION"
      >
        <!-- BEGIN, CR00198884, SS -->
        <!-- BEGIN, CR00198144, SS -->
        <!-- BEGIN, CR00187459, SS -->
        <!-- BEGIN, CR00237410, AK -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistAdmin_deleteDeterminationConfigurationAssignment"
          WINDOW_OPTIONS="width=500"
        >
          <!-- END, CR00237410 -->
          <!-- END, CR00187459 -->
          <!-- END, CR00198144 -->
          <!-- END, CR00198884 -->
          <CONNECT>
            <SOURCE
              NAME="ASSESSMENTS_DETAILS"
              PROPERTY="result$dtls$detConfigList$list$dtls$detAssessmentConfigLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="detAssessmentConfigLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="assessmentConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="assessmentConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00207167, SS -->
    </ACTION_SET>
    <FIELD
      LABEL="Field.Label.DCName"
      WIDTH="40"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$name"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationConfig">
        <CONNECT>
          <SOURCE
            NAME="ASSESSMENTS_DETAILS"
            PROPERTY="result$dtls$detConfigList$list$dtls$determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCStartDate"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCEndDate"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCStatus"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <LIST TITLE="Cluster.Title.DeterminationConfiguration">
    <CONDITION>
      <IS_FALSE
        NAME="ASSESSMENTS_DETAILS"
        PROPERTY="result$dtls$standalone"
      />
    </CONDITION>
    <!-- BEGIN, CR00207167, SS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207167 -->
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Delete"
        TYPE="ACTION"
      >
        <!-- BEGIN, CR00198884, SS -->
        <!-- BEGIN, CR00198144, SS -->
        <!-- BEGIN, CR00187459, SS -->
        <!-- BEGIN, CR00237410, AK -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistAdmin_deleteDeterminationConfigurationAssignment"
          WINDOW_OPTIONS="width=500"
        >
          <!-- END, CR00237410 -->
          <!-- END, CR00187459 -->
          <!-- END, CR00198144 -->
          <!-- END, CR00198884 -->
          <CONNECT>
            <SOURCE
              NAME="ASSESSMENTS_DETAILS"
              PROPERTY="result$dtls$detConfigList$list$dtls$detAssessmentConfigLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="detAssessmentConfigLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="assessmentConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="assessmentConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00207167, SS -->
    </ACTION_SET>
    <FIELD
      LABEL="Field.Label.DCName"
      WIDTH="40"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$name"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationConfig">
        <CONNECT>
          <SOURCE
            NAME="ASSESSMENTS_DETAILS"
            PROPERTY="result$dtls$detConfigList$list$dtls$determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCStartDate"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCEndDate"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.DCStatus"
      WIDTH="20"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ASSESSMENTS_DETAILS"
          PROPERTY="result$dtls$detConfigList$list$dtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
