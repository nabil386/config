<!-- This page allows the user to create an asessment.                      -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createAssessment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="ACTION"
    OPERATION="createDAAssessment"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <!-- BEGIN, CR00198144, SS -->
  <!-- BEGIN, CR00226313, PS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >
    <!-- END, CR00226313 -->
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="90"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.HomePage"
      WIDTH="87"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="homePageIdentifier"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="65"
    >
      <!-- END, CR00198144 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="assessmentType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndDate"
      USE_DEFAULT="false"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >


    <!-- BEGIN, CR00198144, SS -->
    <FIELD HEIGHT="4">
      <!-- END, CR00198144 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00198144, CR00407812, SS, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00198144, CR00407812 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
</PAGE>
