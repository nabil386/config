<!-- Description                                                            -->
<!-- This page allows the user to view a list of conditions for a           -->
<!-- determination configuration.                                           -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_listConditionForDetermination"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="MENUCONTEXT"
    OPERATION="viewDeterminationConfigMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="conditionID"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="LIST_CONDITION_DETERMINATION"
    OPERATION="listConditionsAssociatedToDC"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="DETERMINATION_DETAILS"
    OPERATION="readDeterminationConfig"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="LIST_CONDITION_DETERMINATION"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="DETERMINATION_DETAILS"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <LIST>
    <!-- BEGIN, CR00214609, PS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00214609 -->
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Remove"
        TYPE="ACTION"
      >
        <!-- BEGIN, CR00237620, AK -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="DecisionAssistAdmin_deleteConditionLink"
          WINDOW_OPTIONS="width=500"
        >
          <!-- END, CR00237620 -->
          <CONNECT>
            <SOURCE
              NAME="LIST_CONDITION_DETERMINATION"
              PROPERTY="result$dtls$dtls$dtls$conditionDCLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="conditionDCLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="determinationConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationConfigID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="LIST_CONDITION_DETERMINATION"
              PROPERTY="result$dtls$dtls$dtls$conditionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="conditionID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00214609, PS -->
    </ACTION_SET>
    <!-- END, CR00214609 -->


    <FIELD
      LABEL="Field.Label.ConditionName"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_CONDITION_DETERMINATION"
          PROPERTY="result$dtls$dtls$dtls$conditionName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="LIST_CONDITION_DETERMINATION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="LIST_CONDITION_DETERMINATION"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00214609, PS -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistAdmin_viewCondition">
        <CONNECT>
          <SOURCE
            NAME="LIST_CONDITION_DETERMINATION"
            PROPERTY="result$dtls$dtls$dtls$conditionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="conditionID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00214609 -->
  </LIST>
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <CONDITION>
        <IS_TRUE
          NAME="DETERMINATION_DETAILS"
          PROPERTY="result$activeStatusInd"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="DecisionAssistAdmin_assignConditionToDC"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
