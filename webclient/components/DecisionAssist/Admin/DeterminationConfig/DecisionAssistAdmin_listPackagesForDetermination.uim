<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to view the list of packages in determination.-->
<PAGE
  PAGE_ID="DecisionAssistAdmin_listPackagesForDetermination"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="MENUCONTEXT"
    OPERATION="viewDeterminationConfigMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="determinationName"/>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="READ_DETERMINATION_DETAILS"
        PROPERTY="result$dtls$dtls$name"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="READ_DETERMINATION_DETAILS"
    OPERATION="readDeterminationConfig"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="DeterminationPackage"
    NAME="LIST_PACKAGES"
    OPERATION="readAllDPsForDeterminationConfig"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="LIST_PACKAGES"
      PROPERTY="determinationConfigID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="READ_DETERMINATION_DETAILS"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <LIST>
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="35"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="name"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationPackage">
        <CONNECT>
          <SOURCE
            NAME="LIST_PACKAGES"
            PROPERTY="latestDeterminationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Version"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="result$dtls$determinationPkgVersionNumber"
        />
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationPackage">
        <CONNECT>
          <SOURCE
            NAME="LIST_PACKAGES"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="versionStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.From"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.To"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_PACKAGES"
          PROPERTY="toDate"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.New"
      TYPE="ACTION"
    >
      <CONDITION>
        <IS_TRUE
          NAME="READ_DETERMINATION_DETAILS"
          PROPERTY="result$activeStatusInd"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="DecisionAssistAdmin_createDeterminationPackage"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="READ_DETERMINATION_DETAILS"
            PROPERTY="result$dtls$dtls$name"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
