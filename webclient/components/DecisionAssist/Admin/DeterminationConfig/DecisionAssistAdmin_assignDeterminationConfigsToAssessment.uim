<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to assign one or more determination          -->
<!-- configurations to an assessment.                                       -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_assignDeterminationConfigsToAssessment"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="assessmentConfigurationID"/>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="ASSESSMENTS_DETAILS"
    OPERATION="readAssessment"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentConfigurationID"
    />
    <TARGET
      NAME="ASSESSMENTS_DETAILS"
      PROPERTY="key$dtls$dtls$assessmentConfigurationID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="ACTIVE_DETERMINATION_LIST"
    OPERATION="listActiveDeterminationConfig"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentConfigurationID"
    />
    <TARGET
      NAME="ACTIVE_DETERMINATION_LIST"
      PROPERTY="key$dtls$dtls$assessmentConfigurationID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="ASSIGN_DETERMINATION_CONFIGS"
    OPERATION="assignDCsToAssessment"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="assessmentConfigurationID"
    />
    <TARGET
      NAME="ASSIGN_DETERMINATION_CONFIGS"
      PROPERTY="dtls$key$dtls$dtls$assessmentConfigurationID"
    />
  </CONNECT>
  <!-- BEGIN, CR00207167, SS -->
  <CLUSTER SHOW_LABELS="false">
    <!-- END, CR00207167 -->
    <FIELD CONTROL="SKIP"/>
    <FIELD CONTROL="TRANSFER_LIST">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$list$list$dtls$determinationConfigID"
          NAME="ACTIVE_DETERMINATION_LIST"
          PROPERTY="result$list$list$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ASSIGN_DETERMINATION_CONFIGS"
          PROPERTY="dtls$DeterminationConfigIDs"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
</PAGE>
