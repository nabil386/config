<!-- This page allows the user to view the list of in edit packages         -->
<PAGE PAGE_ID="DecisionAssistAdmin_listInEditPackages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="DeterminationConfig" NAME="VIEW_DETERMINATION" OPERATION="readDeterminationConfig" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="determinationConfigID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
    <TARGET NAME="VIEW_DETERMINATION" PROPERTY="key$determinationConfigID"/>
  </CONNECT>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_modifyDeterminationPackage" WINDOW_OPTIONS="width=700">
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00246866, AK -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Release" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_releasePackageVersion" WINDOW_OPTIONS="width=520">
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPkgVersionNumber"/>
            <TARGET NAME="PAGE" PROPERTY="VersionNumber"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$versionNo"/>
            <TARGET NAME="PAGE" PROPERTY="versionNo"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <SEPARATOR/>
      <ACTION_CONTROL LABEL="ActionControl.Label.CreateQuestionnaire">
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_createQuestionnaireFromPackage" WINDOW_OPTIONS="width=500">
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="CONSTANT" PROPERTY="DeterminationPageID"/>
            <TARGET NAME="PAGE" PROPERTY="pageID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.AddExistingQuestionnaire">
        <!-- BEGIN, CR00237410, AK -->
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_addExistingQuestionnaire" WINDOW_OPTIONS="width=500">
          <!-- END, CR00237410 -->
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.createDecisionMatrix">
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_createDecisionMatrix" WINDOW_OPTIONS="width=625">
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPkgVersionNumber"/>
            <TARGET NAME="PAGE" PROPERTY="versionNumber"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <SEPARATOR/>
      <!-- END, CR00246866 -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_deleteDeterminationPackage" WINDOW_OPTIONS="width=530">
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPkgVersionNumber"/>
            <TARGET NAME="PAGE" PROPERTY="versionNumber"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="determinationName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
            <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.PackageName" WIDTH="60">
      <CONNECT>
        <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.PackageVersion" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPkgVersionNumber"/>
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="DecisionAssistAdmin_viewDeterminationPackage">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="determinationConfigID"/>
          <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$determinationPackageVersionID"/>
          <TARGET NAME="PAGE" PROPERTY="determinationPackageVersionID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$inEditPackages$dtlslist$dtls$name"/>
          <TARGET NAME="PAGE" PROPERTY="determinationName"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>
  <!-- BEGIN, CR00246866, AK -->
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.New">
      <CONDITION>
        <IS_TRUE NAME="VIEW_DETERMINATION" PROPERTY="activeStatusInd"/>
      </CONDITION>
      <LINK OPEN_MODAL="true" PAGE_ID="DecisionAssistAdmin_createDeterminationPackage" WINDOW_OPTIONS="width=700">
        <CONNECT>
          <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$dtls$dtls$name"/>
          <TARGET NAME="PAGE" PROPERTY="determinationName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="VIEW_DETERMINATION" PROPERTY="result$dtls$dtls$determinationConfigID"/>
          <TARGET NAME="PAGE" PROPERTY="determinationConfigID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <!-- END, CR00246866 -->
</PAGE>