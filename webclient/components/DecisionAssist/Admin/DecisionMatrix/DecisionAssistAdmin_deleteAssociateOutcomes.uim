<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to delete the outcomes associated to the     -->
<!-- decision matrix version.                                               -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deleteAssociateOutcomes"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="decisionMatrixVersionID"/>
  <PAGE_PARAMETER NAME="decisionMatrixName"/>
  <PAGE_PARAMETER NAME="versionNumber"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="outcomeScoreAssociationID"/>
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="REMOVE_OUTCOME_ASSOCIATION"
    OPERATION="deleteOutcomeAssociation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="VIEW_DECISION_MATRIX"
    OPERATION="readDecisionMatrixVersion"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="outcomeScoreAssociationID"
    />
    <TARGET
      NAME="REMOVE_OUTCOME_ASSOCIATION"
      PROPERTY="key$dtls$outcomeScoreAssociationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="VIEW_DECISION_MATRIX"
      PROPERTY="key$key$key$decisionMatrixVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_DECISION_MATRIX"
        PROPERTY="result$currentVersion$dtls$name"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Separator"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_DECISION_MATRIX"
        PROPERTY="result$currentVersion$dtls$decisionMatrixVersionNumber"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.ConfirmDeleteMessage"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00187368, AK -->
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.No"
      TYPE="ACTION"
    />
  </ACTION_SET>
  <!-- END, CR00187368 -->
</PAGE>
