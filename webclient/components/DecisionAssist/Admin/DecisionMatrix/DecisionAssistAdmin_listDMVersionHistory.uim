<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to view the list of decision                 -->
<!-- matrix version history details.                                        -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_listDMVersionHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="MENUCONTEXT"
    OPERATION="viewDecisionMatrixMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$dtls$dtls$decisionMatrixVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$dtls$dtls$determinationPackageVersionID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="decisionMatrixID"/>
  <PAGE_PARAMETER NAME="decisionMatrixVersionID"/>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>


  <!-- BEGIN, CR00246866, AK -->
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="LIST_DECISION_MATRICES"
    OPERATION="readAllDecisionMatrixVersionsHistory"
    PHASE="DISPLAY"
  />
  <!-- END, CR00246866 -->
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="GET_DECISION_MATRIX_NAME"
    OPERATION="readDecisionMatrixHeaderDetails"
    PHASE="DISPLAY"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="GET_DECISION_MATRIX_NAME"
        PROPERTY="result$dtls$dtls$name"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="GET_DECISION_MATRIX_NAME"
      PROPERTY="key$key$key$decisionMatrixVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="LIST_DECISION_MATRICES"
      PROPERTY="key$key$key$decisionMatrixVersionID"
    />
  </CONNECT>


  <!-- BEGIN, CR00226313, PS -->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewDecisionMatrix">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.VersionHistory"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listDMVersionHistory">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- END, CR00226313 -->


  <!-- BEGIN, CR00246866, AK -->
  <LIST>
    <FIELD
      LABEL="Field.Label.Version"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$dtls$decisionMatrixVersionNumber"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$dtls$versionStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.From"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$dtls$fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.To"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$dtls$toDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Created"
      WIDTH="18"
    >
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$dtls$dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreatedBy"
      WIDTH="26"
    >
      <!-- BEGIN, CR00246866, AK -->
      <CONNECT>
        <SOURCE
          NAME="LIST_DECISION_MATRICES"
          PROPERTY="result$dtlsList$versionHistoryDtls$createdUserFullName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="LIST_DECISION_MATRICES"
            PROPERTY="result$dtlsList$versionHistoryDtls$dtls$createdBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
      <!-- END, CR00246866 -->
    </FIELD>
  </LIST>
  <!-- END, CR00246866 -->
</PAGE>
