<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyDecisionMatrix"
  WINDOW_OPTIONS="width=700"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="decisionMatrixVersionID"/>
  <PAGE_PARAMETER NAME="determinationPackageVersionID"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="VIEWDECISIONMATRIXDETAILS"
    OPERATION="readDecisionMatrixVersion"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="VIEWDECISIONMATRIXDETAILS"
      PROPERTY="key$key$key$decisionMatrixVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="MODIFYDECISIONMATRIX"
    OPERATION="modifyDecisionMatrixVersion"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="MODIFYDECISIONMATRIX"
      PROPERTY="dtls$dtls$dtls$decisionMatrixVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="VIEWDECISIONMATRIXDETAILS"
      PROPERTY="result$currentVersion$dtls$versionNo"
    />
    <TARGET
      NAME="MODIFYDECISIONMATRIX"
      PROPERTY="dtls$dtls$dtls$versionNo"
    />
  </CONNECT>
  <!-- BEGIN, CR00198144, SS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >
    <!-- BEGIN, CR00226313, PS -->
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="75"
    >
      <!-- END, CR00226313 -->
      <!-- END, CR00198224 -->
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.CumulativeScore">
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$cumulativeScore"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$cumulativeScore"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ScoringByOutcomes">
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$scoringByOutcomes"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$scoringByOutcomes"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198224, SS -->
    <!-- BEGIN, CR00198144, SS -->
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="68"
    >
      <!-- END, CR00198144 -->
      <!-- END, CR00198224 -->
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$type"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MultipleOutcomes">
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$multipleOutcomes"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$multipleOutcomes"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00198144, CR00407812, SS, RB -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00198144, CR00407812 -->
      <CONNECT>
        <SOURCE
          NAME="VIEWDECISIONMATRIXDETAILS"
          PROPERTY="result$currentVersion$dtls$comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFYDECISIONMATRIX"
          PROPERTY="dtls$dtls$dtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
