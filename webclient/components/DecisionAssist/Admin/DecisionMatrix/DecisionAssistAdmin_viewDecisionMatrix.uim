<!-- This page allows the user to view decision matrix details.             -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="DecisionAssistAdmin_viewDecisionMatrix"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="MENUCONTEXT"
    OPERATION="viewDecisionMatrixMenuContextDescription"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="decisionMatrixVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$dtls$dtls$decisionMatrixVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationPackageVersionID"
    />
    <TARGET
      NAME="MENUCONTEXT"
      PROPERTY="key$dtls$dtls$determinationPackageVersionID"
    />
  </CONNECT>
  <!-- BEGIN, CR00226313, PS -->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewDecisionMatrix">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.VersionHistory"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listDMVersionHistory">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationPackageVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="decisionMatrixID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="determinationConfigID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- END, CR00226313 -->
  <INCLUDE FILE_NAME="DecisionAssistAdmin_viewDecisionMatrixView.vim"/>
</PAGE>
