<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows you to view the list of outcomes to be added to the   -->
<!-- decision matrix widget.                                                -->
<PAGE
  PAGE_ID="DecisionMatrixOutcomePopup"
  POPUP_PAGE="true"
  SCRIPT_FILE="DecisionAssistAdmin_decisionMatrixWidget.js"
  WINDOW_OPTIONS="height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SCRIPT
    ACTION="addMatrixOutcomesPopupListener()"
    EVENT="ONLOAD"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="DISPLAY"
    OPERATION="listOutcomesForMatrix"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="DecisionMatrix"
    NAME="ACTION"
    OPERATION="readMultiSelect"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="matrixID"/>
  <PAGE_PARAMETER NAME="existingOutcomeIds"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="matrixID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="existingOutcomeDetails$decisionMatrixVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="existingOutcomeIds"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="existingOutcomeDetails$existingOutcomeIds"
    />
  </CONNECT>
  <LIST TITLE="List.Title">
    <!-- BEGIN, CR00246242, AK -->
    <CONTAINER
      ALIGNMENT="LEFT"
      LABEL="List.Multiselect.Header"
      WIDTH="5"
    >
      <WIDGET
        ALIGNMENT="LEFT"
        TYPE="MULTISELECT"
        WIDTH="10"
      >
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtlsList$multiselectParam"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="key$multiselect"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>
    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Title.Outcome"
    >
      <!-- END, CR00246242 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtlsList$outcomeName"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!-- BEGIN, CR00246242, AK -->
  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.AddOutcomes"
      TYPE="SUBMIT"
    >
      <SCRIPT
        ACTION="addOutcomesFromPopup()"
        EVENT="ONCLICK"
        SCRIPT_FILE="DecisionAssistAdmin_decisionMatrixWidget.js"
      />
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="DISMISS"
    />
  </ACTION_SET>
  <!-- END, CR00246242 -->
</PAGE>
