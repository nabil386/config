<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" PAGE_ID="DecisionAssistAdmin_searchQuestionnaire" POPUP_PAGE="true">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="Questionnaire" NAME="LIST_QUESTIONNAIRE" OPERATION="listQuestionnaires"
    PHASE="DISPLAY"/>
  <LIST>
    <CONTAINER LABEL="Container.Label.Action" WIDTH="10">
      <ACTION_CONTROL LABEL="ActionControl.Label.Select" TYPE="DISMISS">
        <LINK>
          <CONNECT>
            <SOURCE NAME="LIST_QUESTIONNAIRE"
              PROPERTY="result$dtls$dtls$dtls$questionnaireVersionID"/>
            <TARGET NAME="PAGE" PROPERTY="value"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$name"/>
            <TARGET NAME="PAGE" PROPERTY="description"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Label.QuestionnaireName" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$name"/>
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewQuestionnairePop">
        <CONNECT>
          <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$questionnaireVersionID"/>
          <TARGET NAME="PAGE" PROPERTY="questionnaireVersionID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionnaireType" WIDTH="10">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$type"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionnaireVersion" WIDTH="10">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$version"/>
      </CONNECT>
      <LINK PAGE_ID="DecisionAssistAdmin_viewQuestionnairePop">
        <CONNECT>
          <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$questionnaireVersionID"/>
          <TARGET NAME="PAGE" PROPERTY="questionnaireVersionID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionnaireStatus" WIDTH="10">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$status"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionnaireFrom" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$fromDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.QuestionnaireTo" WIDTH="20">
      <CONNECT>
        <SOURCE NAME="LIST_QUESTIONNAIRE" PROPERTY="result$dtls$dtls$dtls$toDate"/>
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
