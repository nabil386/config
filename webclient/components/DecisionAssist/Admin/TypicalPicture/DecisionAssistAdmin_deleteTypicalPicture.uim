<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows the user to confirm the deletion of                   -->
<!-- a typical picture version.                                             -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deleteTypicalPicture"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="TypicalPicture"
    NAME="VIEW_TYPICAL_PICTURE_VERSION"
    OPERATION="readTypicalPicture"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="typicalPictureVersionID"/>
  <PAGE_PARAMETER NAME="questionnaireVersionID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="typicalPictureVersionID"
    />
    <TARGET
      NAME="VIEW_TYPICAL_PICTURE_VERSION"
      PROPERTY="key$key$typicalPictureVersionID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="TypicalPicture"
    NAME="DELETE_TYPICAL_PICTURE"
    OPERATION="deleteTypicalPictureVersion"
    PHASE="ACTION"
  />
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="typicalPictureVersionID"
    />
    <TARGET
      NAME="VIEW_TYPICAL_PICTURE_VERSION"
      PROPERTY="key$key$typicalPictureVersionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="typicalPictureVersionID"
    />
    <TARGET
      NAME="DELETE_TYPICAL_PICTURE"
      PROPERTY="key$key$typicalPictureVersionID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listTPForQuestionnaire">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="DecisionAssistAdmin_viewTypicalPicture">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="typicalPictureVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="typicalPictureVersionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="questionnaireVersionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.deleteTypicalPictureVersion"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
