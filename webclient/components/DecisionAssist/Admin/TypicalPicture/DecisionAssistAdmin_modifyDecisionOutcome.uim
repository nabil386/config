<!-- This page allows the user to modify outcome details.                   -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyDecisionOutcome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="TypicalPicture"
    NAME="TP_OUTCOMELIST"
    OPERATION="readAllActiveOutcomes"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="TypicalPicture"
    NAME="READ_DECISION_OUTCOME"
    OPERATION="readDecisionOutcome"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="tpDecisionOutcomeID"
    />
    <TARGET
      NAME="READ_DECISION_OUTCOME"
      PROPERTY="tpKey$key$tpDecisionOutcomeID"
    />
  </CONNECT>
  <PAGE_PARAMETER NAME="typicalPictureVersionID"/>
  <PAGE_PARAMETER NAME="tpDecisionOutcomeID"/>
  <PAGE_PARAMETER NAME="name"/>
  <PAGE_PARAMETER NAME="versionNumber"/>
  <PAGE_PARAMETER NAME="questionnaireVersionID"/>
  <SERVER_INTERFACE
    CLASS="TypicalPicture"
    NAME="TP_ACTION"
    OPERATION="modifyDecisionOutcome"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="tpDecisionOutcomeID"
    />
    <TARGET
      NAME="TP_ACTION"
      PROPERTY="dtls$dtls$tpDecisionOutcomeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="typicalPictureVersionID"
    />
    <TARGET
      NAME="TP_ACTION"
      PROPERTY="dtls$dtls$typicalPictureVersionID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- BEGIN, CR00198297, SS -->
  <!-- BEGIN, CR00198224, SS -->
  <CLUSTER
    LABEL_WIDTH="41"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.DecisionMatrix"
      WIDTH="80"
    >
      <!-- END, CR00198224 -->
      <!-- END, CR00198297 -->
      <CONNECT>
        <INITIAL
          NAME="READ_DECISION_OUTCOME"
          PROPERTY="result$dtls$dtls$decisionMatrixName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="READ_DECISION_OUTCOME"
          PROPERTY="result$dtls$dtls$decisionMatrixVersionID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="TP_ACTION"
          PROPERTY="decisionMatrixVersionID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00237410, AK -->
    <!-- BEGIN, CR00226313, PS -->
    <FIELD
      LABEL="Field.Label.Outcome"
      USE_BLANK="false"
      WIDTH="50"
    >
      <!-- END, CR00226313 -->
      <!-- END, CR00237410 -->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="outcomeID"
          NAME="TP_OUTCOMELIST"
          PROPERTY="outcomeName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="READ_DECISION_OUTCOME"
          PROPERTY="result$dtls$dtls$outcomeID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="TP_ACTION"
          PROPERTY="dtls$dtls$outcomeID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00198224, SS -->
    <!-- BEGIN, CR00417165, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00417165 -->
      <!-- END, CR00198224 -->
      <CONNECT>
        <SOURCE
          NAME="READ_DECISION_OUTCOME"
          PROPERTY="result$dtls$dtls$comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="TP_ACTION"
          PROPERTY="dtls$dtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    > </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
