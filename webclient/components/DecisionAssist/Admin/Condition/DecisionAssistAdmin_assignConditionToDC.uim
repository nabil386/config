<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to assign a condition to a determination     -->
<!-- configuration.                                                         -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_assignConditionToDC"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="conditionID"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="VIEW_CONDITION_ID_NAME"
    OPERATION="readConditionNameInfoAssociatedToDC"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="VIEW_CONDITION_ID_NAME"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="ASSIGN_CONDITIONS"
    OPERATION="assignConditionsToDC"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="ASSIGN_CONDITIONS"
      PROPERTY="dtls$key$key$key$determinationConfigID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="VIEW_DETERMINATION"
    OPERATION="readDeterminationConfig"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="VIEW_DETERMINATION"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- BEGIN, CR00215172, PS -->
  <CLUSTER SHOW_LABELS="false">
    <!-- END, CR00215172 -->
    <FIELD CONTROL="SKIP"/>
    <FIELD
      CONTROL="TRANSFER_LIST"
      HEIGHT="4"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$dtls$dtls$dtls$conditionID"
          NAME="VIEW_CONDITION_ID_NAME"
          PROPERTY="result$dtls$dtls$dtls$conditionName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ASSIGN_CONDITIONS"
          PROPERTY="dtls$conditionIDs"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>
</PAGE>
