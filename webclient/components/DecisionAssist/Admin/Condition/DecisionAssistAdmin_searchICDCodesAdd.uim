<!-- Description                                                            -->
<!-- This page allows the user to search for ICD Codes to add to condition. -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_searchICDCodesAdd"
  POPUP_PAGE="true"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="RELEASED_ICD_VERSION_NUMBER"
    OPERATION="readReleasedVersionNumbers"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="CURRENT_VERSION"
    OPERATION="viewICDCodeMaxVersionNumber"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="ICDCodes"
    NAME="ACTION"
    OPERATION="searchICDCode"
    PHASE="ACTION"
  />
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="result$messageDtls$dtls$message"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- BEGIN, CR00207167, SS -->
  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD
      LABEL="Field.Label.ICDCode"
      WIDTH="40"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$key$searchKey$ICDCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Version"
      WIDTH="25"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$versionDtls$versionNum$dtls$icdCodeVersionID"
          NAME="RELEASED_ICD_VERSION_NUMBER"
          PROPERTY="result$versionDtls$versionNum$dtls$versionNumber"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="CURRENT_VERSION"
          PROPERTY="result$versionKey$ICDCodeVersionID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$key$searchKey$ICDVersionID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.ICDText"
      WIDTH="80"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$key$searchKey$ICDText"
        />
      </CONNECT>
    </FIELD>
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Reset">
        <LINK
          PAGE_ID="DecisionAssistAdmin_searchICDCodesAdd"
          SAVE_LINK="false"
        />
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>
  <LIST
    
    TITLE="List.Title.SearchResults"
  >
    <!-- BEGIN, CR00207167, SS -->
    <CONTAINER
      LABEL="Container.Label.Action"
      SEPARATOR="Container.Separator"
      WIDTH="12"
    >
      <!-- END, CR00207167 -->
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Select"
        TYPE="DISMISS"
      >
        <LINK>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="result$listDtls$searchListDtls$dtls$ICDCodeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="value"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="result$listDtls$searchListDtls$dtls$ICDCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Title.ICDCode"
      WIDTH="14"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchListDtls$dtls$ICDCode"
        />
      </CONNECT>
      <!-- BEGIN, CR00184681, AK -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="DecisionAssistAdmin_viewICDCode"
      >
        <!-- END, CR00184681 -->
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$listDtls$searchListDtls$dtls$ICDCodeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="ICDCodeID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->


    <!-- BEGIN, CR00335911, MR -->
    <FIELD
      LABEL="Field.Title.ICDText"
      WIDTH="38"
    >
      <!-- END, CR00335911 -->


      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchListDtls$dtls$ICDText"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->


    <!-- BEGIN, CR00335911, MR -->
    <FIELD
      LABEL="Field.Title.StartDate"
      WIDTH="13"
    >
      <!-- END, CR00335911 -->


      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchListDtls$dtls$startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->


    <!-- BEGIN, CR00335911, MR -->
    <FIELD
      LABEL="Field.Title.EndDate"
      WIDTH="13"
    >
      <!-- END, CR00335911 -->


      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchListDtls$dtls$endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->


    <!-- BEGIN, CR00335911, MR -->
    <FIELD
      LABEL="Field.Title.Version"
      WIDTH="10"
    >
      <!-- END, CR00335911 -->


      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$listDtls$searchListDtls$dtls$ICDVersion"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!-- BEGIN, CR00246242, AK -->
  <ACTION_SET>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Close"
      TYPE="DISMISS"
    />
  </ACTION_SET>
  <!-- END, CR00246242 -->
</PAGE>
