<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to delete the determination package and      -->
<!-- condition association.                                                 -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_deleteDeterminationForConditionLink"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="conditionDCLinkID"/>
  <PAGE_PARAMETER NAME="determinationConfigID"/>
  <PAGE_PARAMETER NAME="conditionID"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="DELETE_DETERMINATION_FOR_CONDITION_ASSOCIATION"
    OPERATION="removeConditionDCAssociation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="READ_CONDITION"
    OPERATION="readCondition"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="READ_CONDITION"
      PROPERTY="key$dtls$dtls$conditionID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="DeterminationConfig"
    NAME="READ_DETERMINATION"
    OPERATION="readDeterminationConfig"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="determinationConfigID"
    />
    <TARGET
      NAME="READ_DETERMINATION"
      PROPERTY="key$key$key$determinationConfigID"
    />
  </CONNECT>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionDCLinkID"
    />
    <TARGET
      NAME="DELETE_DETERMINATION_FOR_CONDITION_ASSOCIATION"
      PROPERTY="key$dtls$dtls$conditionDCLinkID"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.ConfirmDeleteMessage"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00187368, AK -->
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.No"
      TYPE="ACTION"
    />
  </ACTION_SET>
  <!-- END, CR00187368 -->
</PAGE>
