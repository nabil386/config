<!-- This page allows the user to view the details of the condition.        -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_viewCondition"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SHORTCUT_TITLE ICON="condition.icon">
    <CONNECT>
      <SOURCE
        NAME="VIEW_CONDITION"
        PROPERTY="name"
      />
    </CONNECT>
  </SHORTCUT_TITLE>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="VIEW_CONDITION"
        PROPERTY="name"
      />
    </CONNECT>
  </PAGE_TITLE>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_viewCondition">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="conditionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="conditionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ICDCodes"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="DecisionAssistAdmin_listICDCodesForConditions">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="conditionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="conditionID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="VIEW_CONDITION"
    OPERATION="viewCondition"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="LIST_DETERMINATION_CONDITION"
    OPERATION="listDCsAssociatedToCondition"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="conditionID"/>
  <PAGE_PARAMETER NAME="conditionName"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="VIEW_CONDITION"
      PROPERTY="key$dtls$dtls$conditionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="LIST_DETERMINATION_CONDITION"
      PROPERTY="key$dtls$dtls$conditionID"
    />
  </CONNECT>
  <!-- BEGIN, CR00215172, PS -->
  <CLUSTER
    LABEL_WIDTH="24"
    NUM_COLS="2"
  >
    <!-- END, CR00215172 -->
    <FIELD LABEL="Field.Label.dateCreated">
      <CONNECT>
        <SOURCE
          NAME="VIEW_CONDITION"
          PROPERTY="result$dtls$dtls$dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ConditionGuidanceURL"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="VIEW_CONDITION"
          PROPERTY="result$dtls$dtls$guidanceURL"
        />
      </CONNECT>
      <LINK
        URI_SOURCE_NAME="VIEW_CONDITION"
        URI_SOURCE_PROPERTY="result$dtls$dtls$guidanceURL"
      />
    </FIELD>
    <FIELD LABEL="Field.Label.createdBy">
      <CONNECT>
        <SOURCE
          NAME="VIEW_CONDITION"
          PROPERTY="result$dtls$createdUserFullName"
        />
      </CONNECT>
      <!-- BEGIN, CR00198297, SS -->
      <!-- BEGIN, CR00184681, AK -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
        WINDOW_OPTIONS="width=625"
      >
        <!-- END, CR00184681 -->
        <!-- END, CR00198297 -->
        <CONNECT>
          <SOURCE
            NAME="VIEW_CONDITION"
            PROPERTY="result$dtls$dtls$createdBy"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </CLUSTER>
  <!-- BEGIN, CR00214609, PS -->
  <CLUSTER
    NUM_COLS="2"
    SHOW_LABELS="false"
  >
    <!-- END, CR00214609 -->
    <LIST TITLE="Cluster.Title.DeterminationConfigurations">
      <!-- BEGIN, CR00207167, SS -->
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <!-- END, CR00207167 -->
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Remove"
          TYPE="ACTION"
        >
          <!-- BEGIN, CR00226313, PS -->
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_deleteDeterminationForConditionLink"
            WINDOW_OPTIONS="width=500"
          >
            <!-- END, CR00226313 -->
            <CONNECT>
              <SOURCE
                NAME="LIST_DETERMINATION_CONDITION"
                PROPERTY="result$dtls$list$dtls$conditionDCLinkID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="conditionDCLinkID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DETERMINATION_CONDITION"
                PROPERTY="result$dtls$list$dtls$conditionID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="conditionID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="LIST_DETERMINATION_CONDITION"
                PROPERTY="result$dtls$list$dtls$determinationConfigID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="determinationConfigID"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <!-- BEGIN, CR00207167, SS -->
      </ACTION_SET>
      <FIELD
        LABEL="Field.Label.DeterminationName"
        WIDTH="60"
      >
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="LIST_DETERMINATION_CONDITION"
            PROPERTY="result$dtls$list$dtls$determinationName"
          />
        </CONNECT>
        <!-- BEGIN, CR00246866, AK -->
        <LINK PAGE_ID="DecisionAssistAdmin_viewDeterminationConfig">
          <CONNECT>
            <SOURCE
              NAME="LIST_DETERMINATION_CONDITION"
              PROPERTY="result$dtls$list$dtls$determinationConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="determinationConfigID"
            />
          </CONNECT>
        </LINK>
        <!-- END, CR00246866 -->
      </FIELD>
      <FIELD
        LABEL="Field.Label.DeterminationType"
        WIDTH="40"
      >
        <CONNECT>
          <SOURCE
            NAME="LIST_DETERMINATION_CONDITION"
            PROPERTY="result$dtls$list$dtls$type"
          />
        </CONNECT>
      </FIELD>
    </LIST>
    <LIST TITLE="Cluster.Title.ConditionNameTranslations">
      <!-- BEGIN, CR00207167, SS -->
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <!-- END, CR00207167 -->
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Edit"
          TYPE="ACTION"
        >
          <!-- BEGIN, CR00198884, SS -->
          <!-- BEGIN, CR00198297, SS -->
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_modifyConditionNameTranslation"
          >
            <!-- END, CR00198297 -->
            <!-- END, CR00198884 -->
            <CONNECT>
              <SOURCE
                NAME="VIEW_CONDITION"
                PROPERTY="result$transDtls$dtls$dtls$dtls$translationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="translationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_CONDITION"
                PROPERTY="result$dtls$dtls$name"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="conditionName"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <ACTION_CONTROL
          LABEL="ActionControl.Label.Delete"
          TYPE="ACTION"
        >
          <!-- BEGIN, CR00198297, SS -->
          <!-- BEGIN, CR00187459, SS -->
          <LINK
            OPEN_MODAL="true"
            PAGE_ID="DecisionAssistAdmin_deleteConditionNameTranslation"
          >
            <!-- END, CR00187459 -->
            <!-- END, CR00198297 -->
            <CONNECT>
              <SOURCE
                NAME="VIEW_CONDITION"
                PROPERTY="result$transDtls$dtls$dtls$dtls$translationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="translationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_CONDITION"
                PROPERTY="result$dtls$dtls$name"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="conditionName"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="VIEW_CONDITION"
                PROPERTY="result$transDtls$dtls$dtls$dtls$versionNo"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="versionNo"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
        <!-- BEGIN, CR00207167, SS -->
      </ACTION_SET>
      <FIELD
        LABEL="Field.Label.Language"
        WIDTH="30"
      >
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="VIEW_CONDITION"
            PROPERTY="result$transDtls$dtls$dtls$dtls$languageID"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00207167, SS -->
      <FIELD
        LABEL="Field.Label.Translation"
        WIDTH="70"
      >
        <!-- END, CR00207167 -->
        <CONNECT>
          <SOURCE
            NAME="VIEW_CONDITION"
            PROPERTY="result$transDtls$dtls$dtls$dtls$translationText"
          />
        </CONNECT>
      </FIELD>
    </LIST>
    <!-- BEGIN, CR00207167, SS -->
  </CLUSTER>
  <!-- END, CR00207167 -->


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.ConditionComments"
  >
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      HEIGHT="4"
      WIDTH="100"
    >
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="VIEW_CONDITION"
          PROPERTY="result$dtls$dtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
