<!-- This page allows you to create a new ICD codes for conditions.         -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_createICDCodesForConditions"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="conditionID"/>
  <PAGE_PARAMETER NAME="conditionName"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="LIST_ICD_CODES_CONDITION"
    OPERATION="listICDCodesAssociatedToCondition"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="CREATE_ICD_CODES_CONDITION"
    OPERATION="createICDCodeConditionAssociation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="VIEW_CONDITION"
    OPERATION="viewCondition"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="VIEW_CONDITION"
      PROPERTY="key$dtls$dtls$conditionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="LIST_ICD_CODES_CONDITION"
      PROPERTY="key$dtls$dtls$conditionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="conditionID"
    />
    <TARGET
      NAME="CREATE_ICD_CODES_CONDITION"
      PROPERTY="dtls$dtls$dtls$conditionID"
    />
  </CONNECT>
  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.ICDCode"
      WIDTH="70"
    >
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODES_CONDITION"
          PROPERTY="dtls$dtls$dtls$iCDCodeID"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="40"
    >
      <CONNECT>
        <TARGET
          NAME="CREATE_ICD_CODES_CONDITION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="RIGHT"
    BOTTOM="false"
  >


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    > 
        
       </ACTION_CONTROL>


  </ACTION_SET>
</PAGE>
