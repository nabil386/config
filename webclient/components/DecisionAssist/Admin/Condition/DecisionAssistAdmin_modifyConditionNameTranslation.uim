<!-- This page allows the user to modify the translation details            -->
<!-- for the condition.                                                     -->
<!-- BEGIN, CR00237410, AK -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyConditionNameTranslation"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END, CR00237410 -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="translationID"/>
  <PAGE_PARAMETER NAME="conditionName"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
    OPERATION="modifyConditionNameTranslation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="SI_VIEW_CONDITION_TRANSLATION"
    OPERATION="readConditionNameTranslation"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="translationID"
    />
    <TARGET
      NAME="SI_VIEW_CONDITION_TRANSLATION"
      PROPERTY="key$dtls$dtls$dtls$translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEW_CONDITION_TRANSLATION"
      PROPERTY="result$dtls$dtls$dtls$translationID"
    />
    <TARGET
      NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
      PROPERTY="dtls$dtls$dtls$dtls$translationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEW_CONDITION_TRANSLATION"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="SI_VIEW_CONDITION_TRANSLATION"
      PROPERTY="subjectID"
    />
    <TARGET
      NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
      PROPERTY="subjectID"
    />
  </CONNECT>
  <!-- BEGIN, CR00198884, SS -->
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="1"
  >
    <!-- BEGIN, CR00207167, SS -->
    <FIELD
      LABEL="Field.Label.Language"
      WIDTH="78"
    >
      <!-- END, CR00198297 -->
      <!-- END, CR00198884 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEW_CONDITION_TRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
          PROPERTY="languageID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207167, SS -->
    <!-- BEGIN, CR00198884, SS -->
    <!-- BEGIN, CR00198297, SS -->
    <FIELD
      LABEL="Field.Label.Translation"
      WIDTH="75"
    >
      <!-- END, CR00198297 -->
      <!-- END, CR00198884 -->
      <!-- END, CR00207167 -->
      <CONNECT>
        <SOURCE
          NAME="SI_VIEW_CONDITION_TRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="SI_MODIFY_CONDITION_NAME_TRANSLATION"
          PROPERTY="translationText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
