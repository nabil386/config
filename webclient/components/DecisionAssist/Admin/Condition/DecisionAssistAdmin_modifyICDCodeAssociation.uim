<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page allows you to modify the association details between         -->
<!-- the condition and the ICD code.                                        -->
<PAGE
  PAGE_ID="DecisionAssistAdmin_modifyICDCodeAssociation"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="iCDCodeConditionLinkID"/>
  <PAGE_PARAMETER NAME="conditionName"/>
  <PAGE_PARAMETER NAME="iCDText"/>
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="MODIFY_ICDCODE_CONDITION"
    OPERATION="modifyICDCodeConditionAssociation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="Condition"
    NAME="VIEW_ICDCODE_CONDITION"
    OPERATION="viewICDCodeConditionAssociation"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="iCDCodeConditionLinkID"
    />
    <TARGET
      NAME="VIEW_ICDCODE_CONDITION"
      PROPERTY="key$dtls$dtls$iCDCodeConditionLinkID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="VIEW_ICDCODE_CONDITION"
      PROPERTY="result$dtls$dtls$iCDCodeConditionLinkID"
    />
    <TARGET
      NAME="MODIFY_ICDCODE_CONDITION"
      PROPERTY="dtls$dtls$dtls$iCDCodeConditionLinkID"
    />
  </CONNECT>
  <!-- BEGIN, CR00198297, SS -->
  <CLUSTER
    LABEL_WIDTH="43"
    NUM_COLS="2"
  >


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="40"
    >
      <!-- END, CR00198297 -->
      <CONNECT>
        <SOURCE
          NAME="VIEW_ICDCODE_CONDITION"
          PROPERTY="startDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFY_ICDCODE_CONDITION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">


      <CONNECT>
        <SOURCE
          NAME="VIEW_ICDCODE_CONDITION"
          PROPERTY="endDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="MODIFY_ICDCODE_CONDITION"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    > </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>
</PAGE>
