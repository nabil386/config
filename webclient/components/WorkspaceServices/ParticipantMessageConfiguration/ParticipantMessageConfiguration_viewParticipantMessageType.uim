<PAGE PAGE_ID="ParticipantMessageConfiguration_viewParticipantMessageType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd" HIDE_CONDITIONAL_LINKS="false">

    <SERVER_INTERFACE CLASS="ParticipantMessageConfiguration" NAME="DISPLAY" OPERATION="listAllParticipantMessageConfiguration" />
    <ACTION_SET>
        <ACTION_CONTROL LABEL="ActionControl.Label.New">
            <LINK PAGE_ID="ParticipantMessageConfiguration_newAccountMessage" OPEN_MODAL="true" />
        </ACTION_CONTROL>
        <ACTION_CONTROL LABEL="ActionControl.Label.Reorder">
            <LINK PAGE_ID="ParticipantMessageConfiguration_reorder" OPEN_MODAL="true" />
        </ACTION_CONTROL>
    </ACTION_SET>
    <LIST>
        <ACTION_SET TYPE="LIST_ROW_MENU">
            <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
                
                <LINK OPEN_MODAL="true" PAGE_ID="ParticipantMessageConfiguration_modifyAccountMessage">
                    <CONNECT>
                        <SOURCE NAME="DISPLAY" PROPERTY="participantMessageConfigID" />
                        <TARGET NAME="PAGE" PROPERTY="participantMessageConfigID" />
                    </CONNECT>
                </LINK>
            </ACTION_CONTROL>
            
            <ACTION_CONTROL LABEL="ActionControl.Label.EditImage">
                <LINK OPEN_MODAL="true" PAGE_ID="ParticipantMessageConfiguration_modifyMessageIcon">
                    <CONNECT>
                        <SOURCE NAME="DISPLAY" PROPERTY="participantMessageConfigID"/>
                        <TARGET NAME="PAGE" PROPERTY="participantMessageConfigID"/>
                    </CONNECT>
                    <CONNECT>
                        <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
                        <TARGET NAME="PAGE" PROPERTY="versionNo"/>
                    </CONNECT>          
                </LINK>
            </ACTION_CONTROL>  
            <SEPARATOR/>            
            <ACTION_CONTROL LABEL="ActionControl.Label.Enable">
                <CONDITION>
                    <IS_TRUE NAME="DISPLAY" PROPERTY="enableIndicator" />
                </CONDITION>
                <LINK OPEN_MODAL="true" PAGE_ID="ParticipantMessageConfiguration_enableParticipantMessageType">
                    <CONNECT>
                        <SOURCE NAME="DISPLAY" PROPERTY="participantMessageConfigID" />
                        <TARGET NAME="PAGE" PROPERTY="participantMessageConfigID" />
                    </CONNECT>
                </LINK>
            </ACTION_CONTROL>
            <SEPARATOR/>
            <ACTION_CONTROL LABEL="ActionControl.Label.Disable">
                <CONDITION>
                    <IS_TRUE NAME="DISPLAY" PROPERTY="disableIndicator" />
                </CONDITION>
                <LINK OPEN_MODAL="true" PAGE_ID="ParticipantMessageConfiguration_disableParticipantMessageType">
                    <CONNECT>
                        <SOURCE NAME="DISPLAY" PROPERTY="participantMessageConfigID" />
                        <TARGET NAME="PAGE" PROPERTY="participantMessageConfigID" />
                    </CONNECT>
                </LINK>
            </ACTION_CONTROL>
            
        </ACTION_SET>
        <FIELD LABEL="Field.Title.MessageType">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="participantMessageType" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.Description">
	            <CONNECT>
	                <SOURCE NAME="DISPLAY" PROPERTY="description" />
	            </CONNECT>
        </FIELD>
        <!--FIELD >
		            <CONNECT>
		                <SOURCE NAME="DISPLAY" PROPERTY="image" />
		            </CONNECT>
        </FIELD-->
        <FIELD LABEL="Field.Title.Icon">
		            <CONNECT>
		                <SOURCE NAME="DISPLAY" PROPERTY="participantMsgImageXML" />
		            </CONNECT>
        </FIELD>
        
        <FIELD LABEL="Field.Title.Enabled">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="enabledInd" />
            </CONNECT>
        </FIELD>
    </LIST>
</PAGE>
