<!-- Included view used to locate a person's address on a map               -->
<PAGE
  PAGE_ID="Triage_defineGeocodingBias"
  WINDOW_OPTIONS="width=1025"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE
    CLASS="TriageConfig"
    NAME="DISPLAY"
    OPERATION="getBasicGoogleMap"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="TriageConfig"
    NAME="ACTION"
    OPERATION="saveGeocodeBoundary"
    PHASE="ACTION"
  />
  
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>  


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
  
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="xmlData"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
  <CLUSTER
  	NUM_COLS="2">	
  
  
  	<FIELD
  		LABEL="Field.Label.North">
  		<CONNECT>
				<TARGET
	        NAME="ACTION"
	        PROPERTY="northEast$latitude"
	      />  	
	    </CONNECT>
  	</FIELD>
  	
  	<FIELD
  		LABEL="Field.Label.South">
  		<CONNECT>
				<TARGET
					NAME="ACTION"
					PROPERTY="southWest$latitude"
				/>  	
			</CONNECT>
  	</FIELD>
  	
  	<FIELD
  		LABEL="Field.Label.East">
  		<CONNECT>
				<TARGET
					NAME="ACTION"
					PROPERTY="northEast$longitude"
				/>  	
			</CONNECT>
  	</FIELD>
  	
  	<FIELD
  		LABEL="Field.Label.West">
  		<CONNECT>
				<TARGET
					NAME="ACTION"
					PROPERTY="southWest$longitude"
				/>  	
			</CONNECT>
  	</FIELD>  	
  
  </CLUSTER>

</PAGE>
