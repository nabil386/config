<!-- Page to list Verification Categories .                                   -->
<PAGE
  PAGE_ID="VerificationAdmin_listItemUtilization1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="VerificationAdministration"
    NAME="DISPLAY"
    OPERATION="listVerificationItemUtilizationAndVersionNo"
    PHASE="DISPLAY"
  />
  <PAGE_PARAMETER NAME="verifiableDataItemID"/>
  <PAGE_PARAMETER NAME="verifiableDataItemName"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="verifiableDataItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$verifiableDataItemID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="VerificationAdmin_createItemUtilization"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="verifiableDataItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verifiableDataItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="verifiableDataItemName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verifiableDataItemName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="VerificationAdmin_modifyItemUtilization"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$verificationItemUtilizationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="verificationItemUtilizationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="Delete"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="VerificationAdmin_deleteVerificationItemUtilization1"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$verifiableDataItemID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="verifiableDataItemID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$verificationItemUtilizationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="verificationItemUtilizationID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$recordStatus"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="recordStatus"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$dtls$dtls$dtls$verificationItemID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="verificationItemID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$dtls$name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$dtls$dtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="VerificationAdmin_viewVerificationItemUtilizationFromList">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$dtls$dtls$verificationItemUtilizationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationItemUtilizationID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>
</PAGE>
