<!-- This page allows a user to cancel an verification item utilization.    -->
<PAGE PAGE_ID="VerificationAdmin_deleteVerificationItemUtilization1" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="DISPLAY" OPERATION="readVerificationItemPageContextDescription"/>
  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="ACTION" OPERATION="cancelVerificationItemUtilization" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="verifiableDataItemID"/>
  <PAGE_PARAMETER NAME="verificationItemUtilizationID"/>
  <PAGE_PARAMETER NAME="recordStatus"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="verificationItemID"/>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="result$dtls$dtls$message"/>
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verificationItemID"/>
    <TARGET NAME="DISPLAY" PROPERTY="verificationItemID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verificationItemUtilizationID"/>
    <TARGET NAME="ACTION" PROPERTY="verificationItemUtilizationID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="recordStatus"/>
    <TARGET NAME="ACTION" PROPERTY="recordStatus"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelVerificationItemUtilization"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>