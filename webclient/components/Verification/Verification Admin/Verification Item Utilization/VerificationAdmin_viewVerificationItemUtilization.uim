<!-- Page to View Verification Item Utilization.                            -->
<PAGE
  PAGE_ID="VerificationAdmin_viewVerificationItemUtilization"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  
  
  <INCLUDE FILE_NAME="VerificationAdmin_viewVerificationItemUtilizationView.vim"/>
  
  
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="VerificationAdmin_modifyItemUtilization"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="readDtls$verificationItemUtilizationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationItemUtilizationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL
      IMAGE="Delete"
      LABEL="ActionControl.Label.Delete"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="VerificationAdmin_deleteVerificationItemUtilization1"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="verifiableDataItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verifiableDataItemID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="readDtls$verificationItemUtilizationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationItemUtilizationID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="recordStatus"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="recordStatus"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="verificationItemID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationItemID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  
</PAGE>
