<!-- This page allows the user to create a Verification Item.               -->
<PAGE
  PAGE_ID="VerificationAdmin_createDependentDataItem"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="VerificationAdministration"
    NAME="ACTION"
    OPERATION="createDependantDataItem"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="verifiableDataItemID"/>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="VerificationAdmin_listDependentDataItem">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="verifiableDataItemID"
          />


          <TARGET
            NAME="PAGE"
            PROPERTY="verifiableDataItemID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="verifiableDataItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="dtls$dtls$createDtls$verifiableDataItemID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="30">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DataItem">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dataItem"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <!-- BEGIN, CR00463142, EC -->
    <FIELD
      HEIGHT="4"
      LABEL="Cluster.Title.Description"
    >
      <!-- END, CR00463142 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
