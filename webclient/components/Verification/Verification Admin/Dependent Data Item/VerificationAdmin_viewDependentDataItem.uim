<!-- Page to list Verification Categories .                                 -->
<PAGE PAGE_ID="VerificationAdmin_viewDependentDataItem" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <INCLUDE FILE_NAME="VerificationAdmin_viewDependentDataItemView.vim"/>
  
  
  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
      <LINK OPEN_MODAL="true" PAGE_ID="VerificationAdmin_modifyDependantDataItem">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="readDtls$dependentDataItemID"/>
          <TARGET NAME="PAGE" PROPERTY="dependentDataItemID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
      <LINK OPEN_MODAL="true" PAGE_ID="VerificationAdmin_deleteDependantDataItem" WINDOW_OPTIONS="width=400,height=150">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="readDtls$dependentDataItemID"/>
          <TARGET NAME="PAGE" PROPERTY="dependentDataItemID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="verifiableDataItemID"/>
          <TARGET NAME="PAGE" PROPERTY="verifiableDataItemID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  
</PAGE>