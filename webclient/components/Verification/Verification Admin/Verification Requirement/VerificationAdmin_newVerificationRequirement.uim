<!-- This page allows the user to create a Verification Requirement record.               -->
<PAGE PAGE_ID="VerificationAdmin_newVerificationRequirement" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="ACTION" OPERATION="newVerificationRequirement" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="verifiableDataItemID"/>
  <!--  <PAGE_PARAMETER NAME="displayUIM"/>
  <PAGE_PARAMETER NAME="displayRuleSetWithRuleClassName"/>-->


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="result$dtls$dtls$message"/>
    </CONNECT>
  </INFORMATIONAL>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verifiableDataItemID"/>
    <TARGET NAME="ACTION" PROPERTY="verifiableDataItemID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="VerificationAdmin_listVerificationRequirements">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="verifiableDataItemID"/>
          <TARGET NAME="PAGE" PROPERTY="verifiableDataItemID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.Details">
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.FromDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="fromDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MinimumItems" WIDTH="30">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="minimumItems"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ClientSupplied">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="clientSupplied"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Level" WIDTH="30">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="verificationLevel"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ToDate" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="toDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ReverificationMode">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="reverificationMode"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Mandatory">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="mandatory"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER DESCRIPTION="Cluster.Description.VerificationConditions" TITLE="Cluster.Title.VerificationDueConditions">
    <FIELD LABEL="Field.Label.RuleClass">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="ruleSetWithRuleClassName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.UIM">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="displayUIM"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DisplayRuleClass">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="displayRuleSetWithRuleClassName"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER DESCRIPTION="Cluster.Description.VerificationDueDetails" LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.VerificationDueDetails">
    <FIELD LABEL="Field.Label.DueDays" WIDTH="30">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dueDays"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DueDateFrom" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dueDateFrom"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DueDateEvent" USE_BLANK="true">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dueDateEventType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.WarningDays" WIDTH="30">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="warningDays"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ModifyDueDate">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="dueDateModifiableInd"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="45" NUM_COLS="2" TITLE="Cluster.Title.EventDetails">
    <FIELD LABEL="Field.Label.AddEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="vrAddedEventType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.UpdateEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="vrUpdatedEventType"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ValueChangedEvent">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="valueChangedEventType"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->


      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>