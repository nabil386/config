<!-- This page allows the user to view a Verification Requirement record.   -->
<PAGE
  PAGE_ID="VerificationAdmin_viewVerificationRequirementUsage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="verificationRequirementUsageID"/>


  <SERVER_INTERFACE
    CLASS="VerificationAdministration"
    NAME="DISPLAY"
    OPERATION="readVerificationRequirementUsage"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="verificationRequirementUsageID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$verificationRequirementUsageID"
    />
  </CONNECT>


  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="VerificationAdmin_modifyVerificationRequirementUsage"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="verificationRequirementUsageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationRequirementUsageID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    
    <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="VerificationAdmin_deleteVerificationRequirementUsage"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="verificationRequirementUsageID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationRequirementUsageID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$readDtls$recordStatus"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="recordStatus"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$readDtls$versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$dtls$readDtls$verificationRequirementID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="verificationRequirementID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- BEGIN, CR00050169, RPM -->
  <!-- BEGIN HARP 65057, NK-->
  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >
    <!-- END, HARP 65057-->
    <!-- END, CR00050169 -->


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00050169, RPM -->
    <!-- BEGIN HARP 65057, NK-->
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
  >
    <!-- END, HARP 65057-->
    <!-- END, CR00050169 -->
    <FIELD LABEL="Field.Label.AppliesTo">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedItemID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$description"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <FIELD
      HEIGHT="3"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$readDtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
