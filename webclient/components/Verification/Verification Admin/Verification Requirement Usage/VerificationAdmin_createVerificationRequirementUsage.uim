<!-- This page allows the user to create a Verification Item.               -->
<PAGE PAGE_ID="VerificationAdmin_createVerificationRequirementUsage" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="ACTION" OPERATION="createVerificationRequirementUsage" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verificationRequirementID"/>
    <TARGET NAME="ACTION" PROPERTY="dtls$dtls$createDtls$verificationRequirementID"/>
  </CONNECT>


  <PAGE_PARAMETER NAME="verificationRequirementID"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
      <LINK PAGE_ID="VerificationAdmin_viewVerificationRequirementUsage">
        <CONNECT>
          <SOURCE NAME="ACTION" PROPERTY="result$dtls$verificationRequirementUsageID"/>


          <TARGET NAME="PAGE" PROPERTY="verificationRequirementUsageID"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="27">


    <FIELD LABEL="Field.Label.Name" USE_BLANK="true" WIDTH="45">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Field.Label.AppliesTo">
      <FIELD LABEL="Field.Label" USE_BLANK="true" WIDTH="45">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="relatedItemID"/>
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label" USE_BLANK="true" WIDTH="45">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="relatedItemType"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="4" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->


      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>