<!-- This page allows a user to cancel an plan item.                           -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="VerificationAdmin_deleteVerificationRequirementUsage" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title" />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="DISPLAY" OPERATION="readVerificationRequirementUsagePageContextDescription" />
  <SERVER_INTERFACE CLASS="VerificationAdministration" NAME="ACTION" OPERATION="cancelVerificationRequirementUsage" PHASE="ACTION" />
  <PAGE_PARAMETER NAME="verificationRequirementUsageID" />
  <PAGE_PARAMETER NAME="recordStatus" />
  <PAGE_PARAMETER NAME="versionNo" />
  <PAGE_PARAMETER NAME="verificationRequirementID" />
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verificationRequirementUsageID" />
    <TARGET NAME="DISPLAY" PROPERTY="verificationRequirementUsageID" />
  </CONNECT>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="VerificationAdmin_viewVerificationRequirement">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="verificationRequirementID" />
          <TARGET NAME="PAGE" PROPERTY="verificationRequirementID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No" />
  </ACTION_SET>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="verificationRequirementUsageID" />
    <TARGET NAME="ACTION" PROPERTY="verificationRequirementUsageID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="recordStatus" />
    <TARGET NAME="ACTION" PROPERTY="recordStatus" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="versionNo" />
    <TARGET NAME="ACTION" PROPERTY="versionNo" />
  </CONNECT>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CancelVerificationRequirementUsage" />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>

