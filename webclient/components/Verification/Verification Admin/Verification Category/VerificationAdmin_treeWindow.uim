<PAGE
  PAGE_ID="VerificationAdmin_treeWindow"
  TYPE="SPLIT_WINDOW"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="verificationCategoryID"/>


  <SERVER_INTERFACE
    CLASS="VerificationAdministration"
    NAME="DISPLAY"
    OPERATION="getXMLStringForCategoryTree"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="verificationCategoryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$details$verificationCategoryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="verificationCategoryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$verificationCategoryID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$XMLData"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
