<PAGE
  PAGE_ID="VerificationAdmin_resolveVerificationWizard"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>    
    
    curam.omega3.request.RequestHandler 
    rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);    
    
    String context = request.getContextPath() + "/";
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    String wizardID = request.getParameter("wizardID");
    String url = "";
    
    if(wizardID == null)
    {
    core.ScriptletMissingParamException e = new
    core.ScriptletMissingParamException(-20002, "wizardID");
    System.out.println(e);
    throw e;
    }  
    
    curam.interfaces.MaintainVerificationGroupPkg.MaintainVerificationGroup_readWizardVGDetails_TH 
    th = new curam.interfaces.MaintainVerificationGroupPkg.MaintainVerificationGroup_readWizardVGDetails_TH();
    
    th.setFieldValue(th.key$wizardID_idx,wizardID);      
    th.callServer(); 
    int pointer = Integer.parseInt(th.getFieldValue(th.result$position_idx)); 
    String action = th.getFieldValue(th.result$actionString_idx);
    String verifiableDataItemID = th.getFieldValue(th.result$verifiableDataItemID_idx);
    int checkboxCount = Integer.parseInt(th.getFieldValue(th.result$itemsCheckCount_idx));     
    
    
    url = context + "VerificationAdmin_createVerificationItemUtilizationPage.do?wizardID="
    + curam.omega3.request.RequestUtils.escapeURL(wizardID);
    
    if(pointer == -1){
    if(action.equals("back")){
    url = context + "VerificationAdmin_createVerificationGroupPage.do?wizardID="
    + curam.omega3.request.RequestUtils.escapeURL(wizardID)+ "&amp;verifiableDataItemID="+curam.omega3.request.RequestUtils.escapeURL(verifiableDataItemID);
    }
    } else if(pointer == checkboxCount){
    if(action.equals("next")){
    url = context + "VerificationAdmin_verificationGroupSummaryPage.do?wizardID="
    + curam.omega3.request.RequestUtils.escapeURL(wizardID);
    }
    }
    url += "&amp;" + rh.getSystemParameters();  
    response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>

</PAGE>
