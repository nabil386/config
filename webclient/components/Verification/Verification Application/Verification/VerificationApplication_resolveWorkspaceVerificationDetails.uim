<!-- This page allows the user to view an the workspace of the verification -->
<!-- details.                         										-->
<PAGE
  PAGE_ID="VerificationApplication_resolveWorkspaceVerificationDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String caseID = request.getParameter("caseID");
      String evidenceDescriptorID = request.getParameter("evidenceDescriptorID");
      String dataItemName = request.getParameter("dataItemName");
      String vDIEDLinkID = request.getParameter("VDIEDLinkID");
      String contextDescription = request.getParameter("contextDescription");
      String url = "";
  	  
  	  if (Long.parseLong(caseID)!=0L) {
  	  url = context + "VerificationApplication_viewWorkspaceVerificationDetailsPage.do?caseID=" + curam.omega3.request.RequestUtils.escapeURL(caseID) + "&amp;evidenceDescriptorID=" + curam.omega3.request.RequestUtils.escapeURL(evidenceDescriptorID) + "&amp;dataItemName=" + curam.omega3.request.RequestUtils.escapeURL(dataItemName) + "&amp;VDIEDLinkID=" + curam.omega3.request.RequestUtils.escapeURL(vDIEDLinkID) + "&amp;contextDescription=" + curam.omega3.request.RequestUtils.escapeURL(contextDescription);  
		  
      } else {
        String participantID = request.getParameter("concernRoleID");
        url = context + "VerificationApplication_viewWorkspaceParticipantVerificationDetailsPage.do?concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(participantID) + "&amp;evidenceDescriptorID=" + curam.omega3.request.RequestUtils.escapeURL(evidenceDescriptorID) + "&amp;dataItemName=" + curam.omega3.request.RequestUtils.escapeURL(dataItemName) + "&amp;VDIEDLinkID=" + curam.omega3.request.RequestUtils.escapeURL(vDIEDLinkID) + "&amp;contextDescription=" + curam.omega3.request.RequestUtils.escapeURL(contextDescription);     
      }         

      url += "&amp;amp;" + rh.getSystemParameters();
      response.sendRedirect(response.encodeRedirectURL(url));
      
    
  </JSP_SCRIPTLET>
</PAGE>
