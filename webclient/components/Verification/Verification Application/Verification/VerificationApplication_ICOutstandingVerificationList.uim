<!-- Page to list Integrated Cases .                                        -->
<PAGE
  PAGE_ID="VerificationApplication_ICOutstandingVerificationList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>


    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>


  </PAGE_TITLE>
  <!--  BEGIN, EVDMGMT, GSP-->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="InPageNav.Label.VerificationOutstanding"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="VerificationApplication_ICOutstandingVerificationList"
        SAVE_LINK="false"
      >
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="InPageNav.Label.Verifications">
      <LINK
        PAGE_ID="VerificationApplication_IntegratedCaseVerificationList"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!--  END, EVDMGMT-->


  <SERVER_INTERFACE
    CLASS="VerificationApplication"
    NAME="DISPLAY"
    OPERATION="listOutstandingCaseVerificationDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$key$caseID"
    />
  </CONNECT>

  <INCLUDE FILE_NAME="VerificationApplication_ICOutstandingVerificationListView.vim"/>
</PAGE>
