<!-- This page allows a user to remove an verification waiver               -->
<PAGE PAGE_ID="VerificationApplication_cancelWaiver" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

    <!-- The page title for this page -->
    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
        </CONNECT>
    </PAGE_TITLE>

    <!-- Page parameters to this page -->
    <PAGE_PARAMETER NAME="verificationWaiverID"/>
    <PAGE_PARAMETER NAME="versionNo"/>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="verificationWaiverID"/>
        <TARGET NAME="ACTION" PROPERTY="verificationWaiverID"/>
    </CONNECT>
    <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="versionNo"/>
        <TARGET NAME="ACTION" PROPERTY="versionNo"/>
    </CONNECT>

    <!-- Declare the 'action' server bean -->
    <SERVER_INTERFACE NAME="ACTION" CLASS="VerificationApplication" OPERATION="cancelWaiver" PHASE="ACTION"/>

  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>

  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.RemoveVerificationWaiver"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>