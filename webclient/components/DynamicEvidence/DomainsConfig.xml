<dc:domains xmlns:dc="http://www.curamsoftware.com/curam/util/common/domain-config">
  <!--
  Marshal plugin for Dynamic UIM content. (See the
  plug-in configuration file for more details about the adaptor.)
  -->
  <dc:domain name="DYNAMIC_EVIDENCE_DATAXML">
    <dc:plug-in
      class="curam.dynamicevidence.plugin.marshal.DynamicEvidenceMarshal"
      name="marshal"
    />
  </dc:domain>


  <dc:domain name="DYNAMIC_ADDRESS_DATA">
    <dc:plug-in
      name="edit-renderer"
      purpose="curam-util-client::cef-edit-address"
    />
    <dc:plug-in
      name="view-renderer"
      purpose="curam-util-client::view-address"
    />
    <dc:plug-in
      name="marshal"
      purpose="curam-util-client::marshal-address"
    />
  </dc:domain>


</dc:domains>
