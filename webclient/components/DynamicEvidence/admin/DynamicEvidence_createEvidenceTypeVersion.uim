<!-- This page allows users to create a new Dynamic Evidence type           -->
<!--  metadada version                                                      -->
<PAGE
  PAGE_ID="DynamicEvidence_createEvidenceTypeVersion"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DynamicEvidenceAdmin"
    NAME="ACTION"
    OPERATION="createEvidenceTypeVersion"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="evidenceTypeID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceTypeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="evidenceTypeDefID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="35">
    <FIELD LABEL="Field.Label.EffectiveDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="effectiveFromDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
