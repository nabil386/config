<!-- This page allows users to create a new Dynamic Evidence type           -->
<PAGE PAGE_ID="DynamicEvidence_modifyEvidenceType" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="DynamicEvidenceAdmin" NAME="DISPLAY" OPERATION="readEvidenceType" PHASE="DISPLAY"/>


  <SERVER_INTERFACE CLASS="DynamicEvidenceAdmin" NAME="ACTION" OPERATION="modifyEvidenceType" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="evidenceTypeID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="evidenceTypeID"/>
    <TARGET NAME="DISPLAY" PROPERTY="evidenceTypeID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="evidenceTypeID"/>
    <TARGET NAME="ACTION" PROPERTY="evidenceTypeID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
    <TARGET NAME="ACTION" PROPERTY="versionNo"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT">
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="35" NUM_COLS="1">


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="evidenceTypeName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="evidenceTypeName"/>
      </CONNECT>
    </FIELD>

<!--  BEGIN, 182662  -->
    <FIELD LABEL="Field.Label.AutoEndDateInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="autoEndDateIndOpt"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="autoEndDateIndOpt"/>
      </CONNECT>
    </FIELD>
<!--  END, 182662  -->

	<FIELD LABEL="Field.Label.MaintenancePattern">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="maintenancePattern"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="maintenancePattern"/>
      </CONNECT>
    </FIELD>  
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Definition">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="5" LABEL="Cluster.Title.Definition">
      <!-- END, CR00463142 -->
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="definition"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="definition"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Description">
    <!-- BEGIN, CR00463142, EC -->
    <FIELD HEIGHT="5" LABEL="Cluster.Title.Description">
      <!-- END, CR00463142 -->
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="description"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="description"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>