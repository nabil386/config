<!-- This page allows users to create a new Dynamic Evidence type           -->
<PAGE
  PAGE_ID="DynamicEvidence_viewEvidenceType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.EvidenceTypeVersions"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="DynamicEvidence_listEvidenceTypeVersions"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceTypeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceTypeID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="userLocale"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userLocale"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceTypeName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceTypeName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Details"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="DynamicEvidence_viewEvidenceType"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceTypeID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceTypeID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="userLocale"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userLocale"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceTypeName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceTypeName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="DynamicEvidenceAdmin"
    NAME="DISPLAY"
    OPERATION="readEvidenceType"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="evidenceTypeID"/>
  <PAGE_PARAMETER NAME="userLocale"/>
  <PAGE_PARAMETER NAME="evidenceTypeName"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceTypeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="evidenceTypeID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="1"
    WIDTH="70"
  >


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="evidenceTypeName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.LogicalName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="logicalName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.SecurityGroupName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="securityGroupName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Definition"
  >


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="definition"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Description"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
