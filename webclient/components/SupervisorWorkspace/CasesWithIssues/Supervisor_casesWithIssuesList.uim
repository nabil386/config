<!-- Reads a Task's home page details. -->
<PAGE
  PAGE_ID="Supervisor_casesWithIssuesList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00294942, AKr -->
  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCasesWithIssues"
    NAME="DISPLAY"
    OPERATION="listCasesWithIssues"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCasesWithIssues"
    NAME="ACTION"
    OPERATION="listCasesWithIssuesByDate"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="week"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="week"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="registrationDate"
    />
  </CONNECT>
  <!-- END, CR00294942 -->


  <ACTION_SET>
    <ACTION_CONTROL LABEL="Field.StaticText.ReassignCases">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Supervisor_reassignCasesWithIssues"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="priorityDates$startDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="week"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="Field.StaticText.ViewHeatMap">
      <LINK PAGE_ID="Supervisor_casesWithIssuesHeatMap">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="priorityDates$startDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="week"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!-- BEGIN, CR00340529, MR -->
    <ACTION_CONTROL
      IMAGE="UpdateButton"
      LABEL="Action.Control.Update"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Supervisor_casesWithIssuesByDateList">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="chosenDate"
          />
          <!-- END, CR00294942 -->
          <TARGET
            NAME="PAGE"
            PROPERTY="recievedOnDate"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="week"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="week"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!-- END, CR00340529 -->
  </ACTION_SET>


  <CLUSTER
    NUM_COLS="3"
    SHOW_LABELS="false"
  >
    <CLUSTER
      LABEL_WIDTH="60"
      SHOW_LABELS="true"
    >
      <CONTAINER
        ALIGNMENT="LEFT"
        LABEL="Field.Title.CasesRecieved"
      >
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="priorityDates$startDate"
            />
          </CONNECT>


        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      LABEL_WIDTH="30"
      SHOW_LABELS="true"
    >
      <CONTAINER
        ALIGNMENT="LEFT"
        LABEL="Field.Title.ChooseDay"
      >
        <!-- BEGIN, CR00294942, AKr -->
        <FIELD
          LABEL="Field.Title.ChooseDay"
          WIDTH="45"
        >
          <CONNECT>
            <INITIAL
              HIDDEN_PROPERTY="chosenDayDetails$date"
              NAME="DISPLAY"
              PROPERTY="dateDescription"
            />
          </CONNECT>


          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="chosenDate"
            />
          </CONNECT>
        </FIELD>


      </CONTAINER>
    </CLUSTER>
    <CONTAINER
      ALIGNMENT="RIGHT"
      SEPARATOR="Container.Separator"
      WIDTH="50"
    >
      <ACTION_CONTROL LABEL="Action.Control.PreviousWeek">
        <LINK PAGE_ID="Supervisor_casesWithIssuesList">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="previousDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="week"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Action.Control.NextWeek">
        <CONDITION>
          <!-- BEGIN, CR00294942, AKr -->
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="allowNext"
          />
          <!-- END, CR00294942 -->
        </CONDITION>
        <LINK PAGE_ID="Supervisor_casesWithIssuesList">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="nextDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="week"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


  <LIST>


    <!-- BEGIN, CR00340529, MR -->
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="10"
    >
      <!-- END, CR00340529 -->
      <ACTION_CONTROL LABEL="ActionControl.Label.View">
        <LINK PAGE_ID="Supervisor_listIssueCasesByType">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="supervisor.type"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseIssuesViewOptionCode"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.CaseReference"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="caseReference"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_listIssueCasesByType">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.type"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseIssuesViewOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Product"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="productType"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00340529, MR -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <!-- END, CR00340529 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.NumOfIssues"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="issueCount"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <!-- BEGIN, CR00294942, AKr -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dtls$startDate"
        />
      </CONNECT>
      <!-- END, CR00294942 -->
    </FIELD>


  </LIST>


</PAGE>
