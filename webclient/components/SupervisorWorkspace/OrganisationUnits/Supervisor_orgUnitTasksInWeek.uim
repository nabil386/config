<!-- This page helps the supervisor manage the tasks due in the specified week.
    Tasks are dispalyed as assigned and reserved in a barchart representation. -->
<PAGE
  PAGE_ID="Supervisor_orgUnitTasksInWeek"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>


    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="deadlineDate"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorOrgUnits"
    NAME="DISPLAY"
    OPERATION="readOrgUnitTasksByUserDueByWeek"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="deadlineDate"/>
  <PAGE_PARAMETER NAME="orgUnitName"/>
  <PAGE_PARAMETER NAME="orgUnitID"/>
  <PAGE_PARAMETER NAME="orgStructureID"/>
  <PAGE_PARAMETER NAME="taskOptionCode"/>
  <PAGE_PARAMETER NAME="taskType"/>
  <PAGE_PARAMETER NAME="userName"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="deadlineDate"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$key$deadlineDate"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="orgUnitID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$key$organizationUnitID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="orgStructureID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$key$key$organizationStructureID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.TasksDueInWeek"
  >
    <FIELD CONFIG="OrgUnitTasksByWeek.BarChart.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="barchartXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
