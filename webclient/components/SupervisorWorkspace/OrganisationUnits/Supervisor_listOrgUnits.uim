<!-- This page displays the list Organisation Units for a supervisor.       -->
<PAGE
  PAGE_ID="Supervisor_listOrgUnits"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorOrgUnits"
    NAME="DISPLAY"
    OPERATION="listSupervisorOrgUnits"
    PHASE="DISPLAY"
  />


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Organization_viewOrgUnitDetailsFromList">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.OrgUnitName"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgUnitName"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_orgUnitWorkspace">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="orgUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="orgStructureID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgUnitStatusCode"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreationDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$unitDetails$orgUnitDetails$dtls$orgUnitCreationDate"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
