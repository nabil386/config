<PAGE PAGE_ID="Supervisor_forwardTasksReservedByUser" WINDOW_OPTIONS="width=1000,height=420" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="taskOptionCode"/>
  <PAGE_PARAMETER NAME="userFullName"/>


  <!--BEGIN, CR00351910, IBM-->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DEFERREDTASKSDISPLAY" PROPERTY="informationMsgTxt"/>
    </CONNECT>


    <!-- BEGIN, CR00369467, IBM -->
    <CONNECT>
      <SOURCE NAME="ACTION" PROPERTY="informationMsgTxt"/>
    </CONNECT>
    <!-- END, CR00369467 -->


  </INFORMATIONAL>


  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="DEFERREDTASKSDISPLAY" OPERATION="listReservedDeferredTasksByUser" PHASE="DISPLAY"/>


  <!-- BEGIN, CR00369467, IBM -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="OPENTASKSDISPLAY" OPERATION="listReservedOpenTasksByFiltered" PHASE="DISPLAY"/>
  <!-- END, CR00369467 -->


  <!--END, CR00351910 -->


  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="USERSDISPLAY" OPERATION="listSupervisorUsers" PHASE="DISPLAY"/>
  <SERVER_INTERFACE CLASS="MaintainSupervisorOrgUnits" NAME="ORGUNITDISPLAY" OPERATION="listSupervisorOrgUnits" PHASE="DISPLAY"/>


  <!-- BEGIN, CR00369467, IBM -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="ACTION" OPERATION="forwardTasksReservedByUserWithInformationalMessage" PHASE="ACTION"/>
  <!-- END, CR00369467 -->


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="DEFERREDTASKSDISPLAY" PROPERTY="key$key$userName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="ACTION" PROPERTY="details$details$userName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="OPENTASKSDISPLAY" PROPERTY="key$key$userName"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL DEFAULT="true" IMAGE="SaveButton" LABEL="ActionControl.SaveButton.label" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.CancelButton.label"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="30" NUM_COLS="2">
    <FIELD LABEL="Field.Label.MyUsers" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="userName" NAME="USERSDISPLAY" PROPERTY="fullName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="forwardToUser"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MyOrgUnits" USE_BLANK="true">
      <CONNECT>
        <INITIAL HIDDEN_PROPERTY="orgUnitID" NAME="ORGUNITDISPLAY" PROPERTY="orgUnitName"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="forwardToOrgUnit"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- BEGIN, CR00137796, SK -->
  <CLUSTER LABEL_WIDTH="15">
