    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="details$details$forwardTaskDetails$comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


