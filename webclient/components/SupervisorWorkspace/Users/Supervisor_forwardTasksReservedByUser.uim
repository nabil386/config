        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DEFERREDTASKSDISPLAY" PROPERTY="result$dtls$taskDetailsList$taskDetailsList$taskID"/>
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="details$details$deferredTaskIDList"/>
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="Field.Label.DeferredTaskID" WIDTH="10">
      <CONNECT>
        <SOURCE NAME="DEFERREDTASKSDISPLAY" PROPERTY="result$dtls$taskDetailsList$taskDetailsList$taskID"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DeferredSubject" WIDTH="30">
      <CONNECT>
        <SOURCE NAME="DEFERREDTASKSDISPLAY" PROPERTY="result$dtls$taskDetailsList$taskDetailsList$subject"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DeferredPriority" WIDTH="15">
      <CONNECT>
        <SOURCE NAME="DEFERREDTASKSDISPLAY" PROPERTY="result$dtls$taskDetailsList$taskDetailsList$priority"/>
