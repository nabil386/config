      </FIELD>
      <FIELD WIDTH="40">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="details$details$forwardTaskDetails$forwardToID"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <!-- END, CR00137796 -->
