<!-- This page allows a user,Organisation unit, position or a job to 
   unsubscribe from a Work Queue.                -->
<PAGE PAGE_ID="Supervisor_unsubscribeOrgObjectFromWorkQueue" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="workQueueID"/>
  <PAGE_PARAMETER NAME="workQueueName"/>
  <PAGE_PARAMETER NAME="subscriberID"/>
  <PAGE_PARAMETER NAME="subscriberType"/>
  <PAGE_PARAMETER NAME="unsubscribePageText"/>
  <PAGE_PARAMETER NAME="pageTitle"/>


  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="ACTION" OPERATION="unsubscribeMemberFromWorkQueue" PHASE="ACTION"/>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="ACTION" PROPERTY="key$userName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="workQueueID"/>
    <TARGET NAME="ACTION" PROPERTY="key$workQueueID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="subscriberID"/>
    <TARGET NAME="ACTION" PROPERTY="key$subscriberID"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="subscriberType"/>
    <TARGET NAME="ACTION" PROPERTY="key$subscriberType"/>
  </CONNECT>
  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="unsubscribePageText"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>