<!-- List all of the evidence approvals for a user.                         -->
<PAGE
  PAGE_ID="Supervisor_evidenceApprovals"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00293470, IBM -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="listTemporalEvidenceApprovalCheckByUser"
  />
  <!-- END, CR00293470 -->


  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="taskOptionCode"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userName"
    />
    <!-- BEGIN, CR00293470, IBM -->
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$userName"
    />
    <!-- END, CR00293470 -->
  </CONNECT>


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <!-- BEGIN, CR00293470, IBM -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_createTemporalEvidenceApprovalCheckForUser"
      >
        <!-- END, CR00293470 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <!-- BEGIN, CR00293470, IBM -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
        <!-- END, CR00293470 -->
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">


        <!-- BEGIN, CR00293470, IBM -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_modifyTemporalEvidenceApprovalCheckForUserFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="temporalEvApprovalCheckID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="temporalEvApprovalCheckID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
        <!-- END, CR00293470 -->


      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <!-- BEGIN, CR00293470, IBM -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Organization_cancelTemporalEvidenceApprovalCheckForUser"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="temporalEvApprovalCheckID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="temporalEvApprovalCheckID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
        <!-- END, CR00293470 -->
      </ACTION_CONTROL>


    </ACTION_SET>


    <!-- BEGIN, CR00293470, IBM -->
    <FIELD
      LABEL="Field.Label.EvidenceType"
      WIDTH="60"
    >
      <!-- BEGIN, CR00293865, IBM -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="evidenceType"
        />
      </CONNECT>
      <!-- END, CR00293470 -->
    </FIELD>
    <FIELD
      LABEL="Field.Label.Percentage"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="percentage"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <!-- BEGIN, CR00293470, IBM -->
      <INLINE_PAGE PAGE_ID="Organization_viewTemporalEvidenceApprovalCheckForUser">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="temporalEvApprovalCheckID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="temporalEvApprovalCheckID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$contextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="contextDescription"
          />
        </CONNECT>
      </INLINE_PAGE>
      <!-- END, CR00293470 -->
    </DETAILS_ROW>
  </LIST>


</PAGE>
