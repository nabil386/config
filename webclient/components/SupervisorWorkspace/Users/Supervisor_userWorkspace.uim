<!-- The home page of the supervisor user.                                  -->
<PAGE PAGE_ID="Supervisor_userWorkspace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.Home"/>
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="taskOptionCode"/>


  <!-- BEGIN, CR00280640, DJ -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <!-- BEGIN, CR00216252, MN -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="DISPLAY" OPERATION="readSupervisorWorkspaceContentDetails" PHASE="DISPLAY"/>
  <!-- END, CR00216252 -->


  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="ACTION" OPERATION="readSupervisorWorkspaceContentDetails" PHASE="ACTION"/>
  <!-- END, CR00280640 -->
  <!-- BEGIN, CR00050789, SS -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="DISPLAY1" OPERATION="listTaskAllocationBlockingHistoryForUser" PHASE="DISPLAY"/>
  <!-- END, CR00050789 -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorUsers" NAME="DISPLAY2" OPERATION="listAllWorkQueuesForUser"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$userName"/>
  </CONNECT>
  <!-- BEGIN, CR00050789, SS -->
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="DISPLAY1" PROPERTY="key$userName"/>
  </CONNECT>
  <!-- END, CR00050789 -->
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="DISPLAY2" PROPERTY="key$userName"/>
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskOptionCode"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$taskOption"/>
  </CONNECT>
  <!-- BEGIN, CR00280640, DJ -->
  <CONNECT>
    <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$supervisorDetails$userName"/>
    <TARGET NAME="ACTION" PROPERTY="key$userName"/>
  </CONNECT>
  <!-- END, CR00280640 -->


  <ACTION_SET>
    <ACTION_CONTROL LABEL="ActionControl.Label.TasksDueNextWeek" TYPE="SUBMIT">
      <LINK PAGE_ID="Supervisor_resolverUserWorkspace">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="userName"/>
          <TARGET NAME="PAGE" PROPERTY="userName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="CONSTANT" PROPERTY="supervisor.viewTaskOption"/>
          <TARGET NAME="PAGE" PROPERTY="taskOptionCode"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.TasksDueNextMonth" TYPE="SUBMIT">
      <LINK PAGE_ID="Supervisor_resolverUserWorkspace">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="userName"/>
          <TARGET NAME="PAGE" PROPERTY="userName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="CONSTANT" PROPERTY="supervisor.viewTaskOption2"/>
          <TARGET NAME="PAGE" PROPERTY="taskOptionCode"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER NUM_COLS="1" SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <!-- END, CR00123088 -->
    <!-- BEGIN, CR00280640, DJ -->
    <FIELD CONFIG="UserWorkspace.BarChart.Config">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$userWorkspaceChartXMLString"/>
      </CONNECT>
    </FIELD>
    <!-- END, CR00280640 -->
  </CLUSTER>


  <CLUSTER NUM_COLS="2" SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <LIST STYLE="cluster-margin-right" TITLE="Cluster.Title.WorkQueueSubscriptions">
      <ACTION_SET TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL LABEL="ActionControl.Label.Unsubscribe">
          <LINK OPEN_MODAL="true" PAGE_ID="Supervisor_unsubscribeOrgObjectFromWorkQueue" WINDOW_OPTIONS="width=400">
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$subscriberID"/>
              <TARGET NAME="PAGE" PROPERTY="subscriberID"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$subscriberType"/>
              <TARGET NAME="PAGE" PROPERTY="subscriberType"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$subscriberName"/>
              <TARGET NAME="PAGE" PROPERTY="userName"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$workQueueID"/>
              <TARGET NAME="PAGE" PROPERTY="workQueueID"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$pageTitle"/>
              <TARGET NAME="PAGE" PROPERTY="pageTitle"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$unsubscribePageText"/>
              <TARGET NAME="PAGE" PROPERTY="unsubscribePageText"/>
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>
      <FIELD LABEL="Field.Title.Name" WIDTH="50">
        <CONNECT>
          <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$workQueueName"/>
        </CONNECT>
        <LINK PAGE_ID="Supervisor_workQueueWorkspace">
          <CONNECT>
            <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$workQueueID"/>
            <TARGET NAME="PAGE" PROPERTY="workQueueID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="CONSTANT" PROPERTY="supervisor.viewTaskOption"/>
            <TARGET NAME="PAGE" PROPERTY="taskOptionCode"/>
          </CONNECT>
        </LINK>
      </FIELD>
      <FIELD LABEL="Field.Title.SubscriberType" WIDTH="50">
        <CONNECT>
          <SOURCE NAME="DISPLAY2" PROPERTY="result$dtls$subscriberType"/>
        </CONNECT>
      </FIELD>
    </LIST>
    <LIST STYLE="cluster-margin-left" TITLE="Cluster.Title.OrganizationUnitMembership">
      <!-- BEGIN, CR00039911, SS -->
      <!-- BEGIN, CR00280640, DJ -->
      <FIELD LABEL="Field.Label.OrgName" WIDTH="50">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$orgDetailsList$dtls$orgUnitName"/>
        </CONNECT>
        <LINK PAGE_ID="Supervisor_orgUnitWorkspace">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$orgDetailsList$dtls$orgUnitID"/>
            <TARGET NAME="PAGE" PROPERTY="orgUnitID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="CONSTANT" PROPERTY="supervisor.viewTaskOption"/>
            <TARGET NAME="PAGE" PROPERTY="taskOptionCode"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$orgDetailsList$dtls$orgStructureID"/>
            <TARGET NAME="PAGE" PROPERTY="orgStructureID"/>
          </CONNECT>
        </LINK>
      </FIELD>
      <FIELD LABEL="Field.Label.Position" WIDTH="50">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contentDetails$orgDetailsList$dtls$positionName"/>
        </CONNECT>
      </FIELD>
      <!-- END, CR00280640 -->


    </LIST>
  </CLUSTER>
</PAGE>