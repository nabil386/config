<!-- Reads a user skills details. -->
<PAGE
  PAGE_ID="Supervisor_viewUserSkill"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="viewUserSkill"
  />


  <PAGE_PARAMETER NAME="userSkillID"/>
  <PAGE_PARAMETER NAME="pageDescription"/>
  <!-- BEGIN, CR00118625, SK -->
  <PAGE_PARAMETER NAME="taskOptionCode"/>
  <!-- END, CR00118625 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="userSkillID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="userSkillKey$userSkillID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.SkillLevel">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="skillLevel"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.Description"
  >


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="details$description"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
