<PAGE PAGE_ID="Supervisor_subscribeUserToWorkQueues" WINDOW_OPTIONS="width=500,height=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE ACTION_ID_PROPERTY="actionIDProperty" CLASS="MaintainSupervisorWorkQueues" NAME="DISPLAY" OPERATION="listWorkQueuesByWorkQueueName" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="userName"/>
  <PAGE_PARAMETER NAME="userFullName"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$userName"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="35" NUM_COLS="1" TITLE="Cluster.Title.SearchCriteria">
    <FIELD LABEL="Field.Label.WorkQueue">
      <CONNECT>
        <TARGET NAME="DISPLAY" PROPERTY="key$name"/>
      </CONNECT>
    </FIELD>


    <ACTION_SET ALIGNMENT="CENTER" TOP="false">
      <ACTION_CONTROL ACTION_ID="SEARCH" DEFAULT="true" IMAGE="SearchButton" LABEL="ActionControl.Label.Search" TYPE="SUBMIT">
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL IMAGE="ResetButton" LABEL="ActionControl.Label.Reset">


        <LINK PAGE_ID="Supervisor_subscribeUserToWorkQueues">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="userName"/>
            <TARGET NAME="PAGE" PROPERTY="userName"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL ACTION_ID="SAVE" IMAGE="SaveButton" LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <LIST TITLE="List.Title.SearchResults">
    <CONTAINER ALIGNMENT="CENTER" LABEL="Container.Label.Subscribe" WIDTH="20">
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$workQueueID"/>
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET NAME="DISPLAY" PROPERTY="key$selectedQueueIDs"/>
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="Field.Label.WorkQueueName" WIDTH="80">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$dtls$name"/>
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>