<!-- Reads a Task's home page details. -->
<PAGE
  PAGE_ID="Supervisor_listCaseIssuesOfAType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCase"
    NAME="DISPLAY"
    OPERATION="listCaseIssuesOfAType"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCase"
    NAME="DISPLAYCHART"
    OPERATION="chartCaseIssuesByType"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="issueType"/>
  <PAGE_PARAMETER NAME="taskOptionCode"/>
  <PAGE_PARAMETER NAME="caseIssuesViewOptionCode"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$caseDtls$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="issueType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dtls$caseDtls$issueType"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAYCHART"
      PROPERTY="key$caseID$key$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="supervisor.type"
    />
    <TARGET
      NAME="DISPLAYCHART"
      PROPERTY="key$caseIssuesViewOption"
    />
  </CONNECT>


  <ACTION_SET>


    <ACTION_CONTROL LABEL="Action.Control.ReassignIssues">
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Supervisor_reassignCaseIssuesByType"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$dtls$caseDtls$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$dtls$caseDtls$issueType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="issueType"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$pageContext$description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="description"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.IssuesByType">
      <LINK PAGE_ID="Supervisor_listIssueCasesByType">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.type"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseIssuesViewOptionCode"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.IssuesByAge">
      <LINK PAGE_ID="Supervisor_caseIssuesByAge">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.age"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseIssuesViewOptionCode"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD CONFIG="IssueTypeCount.BarChart.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCHART"
          PROPERTY="result$dtls$chartXMLString"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST>
    <TITLE>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="List.Title.CaseIssuesByType"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="issueDesc"
        />
      </CONNECT>
    </TITLE>


    <CONTAINER
      LABEL="Container.Label.Case"
      SEPARATOR="Container.Separator"
      WIDTH="26"
    >
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$typeDetails$dtls$dtls$caseReference"
          />
        </CONNECT>
        <LINK PAGE_ID="IssueDelivery_home">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$typeDetails$dtls$dtls$caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </FIELD>


      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$issueDesc"
          />
        </CONNECT>
        <LINK PAGE_ID="IssueDelivery_home">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$typeDetails$dtls$dtls$caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Label.OwnerType"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$typeDetails$dtls$dtls$orgObjectType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Owner"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$typeDetails$dtls$dtls$owner"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.PrimaryClient"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$typeDetails$dtls$dtls$primaryClient"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.DateCreated"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$typeDetails$dtls$dtls$dateCreated"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="12"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$typeDetails$dtls$dtls$status"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
