<!-- Reads a Task's home page details. -->
<PAGE
  PAGE_ID="Supervisor_reassignCaseIssuesByAge"
  WINDOW_OPTIONS="width=800"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
       </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorUsers"
    NAME="DISPLAY"
    OPERATION="listSupervisorUsers"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCase"
    NAME="ACTION"
    OPERATION="reassignCaseIssuesByAge"
    PHASE="ACTION"
  />


  <CLUSTER LABEL_WIDTH="23">
    <CONTAINER LABEL="Container.Label.Owner">
      <FIELD WIDTH="28">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="organisationObject"
          />
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="49">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="newUserID"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <FIELD LABEL="Field.Label.Autoreassign">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="autoReassign"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <PAGE_PARAMETER NAME="description"/>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="caseIssuesViewOptionCode"/>


  <SERVER_INTERFACE
    CLASS="MaintainSupervisorCase"
    NAME="DISPLAYCASES"
    OPERATION="listCaseIssuesByAge"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAYCASES"
      PROPERTY="key$caseID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseIssuesViewOptionCode"
    />
    <TARGET
      NAME="DISPLAYCASES"
      PROPERTY="key$caseIssuesViewOption"
    />
  </CONNECT>


  <LIST TITLE="List.Title.Issues">


    <CONTAINER
      ALIGNMENT="CENTER"
      WIDTH="5"
    >
      <WIDGET TYPE="MULTISELECT">
        <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAYCASES"
              PROPERTY="result$caseIssueDtls$caseIssueByAgeDtls$dtls$caseID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="reassignCasesList"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <CONTAINER
      LABEL="Container.Label.Issue"
      SEPARATOR="Container.Separator"
      WIDTH="30"
    >
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCASES"
            PROPERTY="caseReference"
          />
        </CONNECT>
      </FIELD>


      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYCASES"
            PROPERTY="issueType"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>


    <FIELD
      LABEL="Field.Title.OwnerType"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="orgObjectType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Owner"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="owner"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Created"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYCASES"
          PROPERTY="caseStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
