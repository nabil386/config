<PAGE
  PAGE_ID="Supervisor_viewPosition"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      >
      </SOURCE>
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="positionName"
      >
      </SOURCE>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAY"
    OPERATION="viewPosition"
  />


  <PAGE_PARAMETER NAME="positionID"/>
  <PAGE_PARAMETER NAME="organisationStructureID"/>
  <PAGE_PARAMETER NAME="organisationUnitID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="positionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="orgStructureAndPositionKey$positionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organisationStructureID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="orgStructureAndPositionKey$organisationStructureID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organisationUnitID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="orgStructureAndPositionKey$organisationUnitID"
    />
  </CONNECT>


  <!--  Location Details -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAYLOCATIONDETAILS"
    OPERATION="viewLocationDetailsForPosition"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="positionID"
    />
    <TARGET
      NAME="DISPLAYLOCATIONDETAILS"
      PROPERTY="key$positionID"
    />
  </CONNECT>


  <!--Supervisor user details -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAYSUPERUSER"
    OPERATION="listSupervisorUserForPosition"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organisationStructureID"
    />
    <TARGET
      NAME="DISPLAYSUPERUSER"
      PROPERTY="key$orgStructureAndPositionKey$orgStructureAndPositionKey$organisationStructureID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="positionID"
    />
    <TARGET
      NAME="DISPLAYSUPERUSER"
      PROPERTY="orgStructureAndPositionKey$positionID"
    />
  </CONNECT>


  <!--Subordinate user -->
  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="DISPLAYSUBORDIANTEUSER"
    OPERATION="listSubordinateUserForPosition"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="positionID"
    />
    <TARGET
      NAME="DISPLAYSUBORDIANTEUSER"
      PROPERTY="key$orgStructureAndPositionKey$orgStructureAndPositionKey$positionID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="organisationStructureID"
    />
    <TARGET
      NAME="DISPLAYSUBORDIANTEUSER"
      PROPERTY="key$orgStructureAndPositionKey$orgStructureAndPositionKey$organisationStructureID"
    />
  </CONNECT>

<CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Label.Details"
  >
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="positionName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LeadPosition">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="leadPositionInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ReportsTo">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reportsToName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Job">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="jobName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OrgUnit">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="organisationUnitName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_orgUnitHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="viewPositionDetails$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="toDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!--Start Location Details -->


  <LIST TITLE="Cluster.Label.LocationDetails">
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="20"
    >
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove">
        <LINK
          PAGE_ID="Organization_RemoveLocationFromPosition"
          WINDOW_OPTIONS="width=400,height=150"
        >


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="viewPositionDetails$organisationUnitID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="organisationUnitID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="organisationStructureID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="organisationStructureID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAYLOCATIONDETAILS"
              PROPERTY="locationName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="name"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAYLOCATIONDETAILS"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAYLOCATIONDETAILS"
              PROPERTY="positionLocationLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="positionLocationLinkID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAYLOCATIONDETAILS"
              PROPERTY="positionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="positionID"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD
      LABEL="Field.Label.LocationName"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLOCATIONDETAILS"
          PROPERTY="locationName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.LocationStatus"
      WIDTH="40"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYLOCATIONDETAILS"
          PROPERTY="locationStatus"
        />
      </CONNECT>
    </FIELD>
  </LIST>
  <!--End Location Details -->
  <!--Begin super user for position -->


  <LIST TITLE="Cluster.Label.SupervisorUsers">


    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUPERUSER"
          PROPERTY="userForOrgUnitDetailsList$dtls$userFullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_viewUserDetails">


        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUPERUSER"
            PROPERTY="userForOrgUnitDetailsList$dtls$userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUPERUSER"
            PROPERTY="userForOrgUnitDetailsList$dtls$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.OrgUnit" WIDTH="20" >
      <!-- BEGIN, CR00096643, SD -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUPERUSER"
          PROPERTY="userForOrgUnitDetailsList$dtls$organisationUnitName"  />      
      </CONNECT>
      <!-- END, CR00096643 -->   
      <LINK PAGE_ID="Organization_orgUnitHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUPERUSER"
            PROPERTY="userForOrgUnitDetailsList$dtls$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="30"
    >


      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUPERUSER"
          PROPERTY="userStatusCode"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <!--End super user for position -->


  <!--Start subordinate user -->
  <LIST TITLE="Cluster.Label.SubordinateUsers">
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUBORDIANTEUSER"
          PROPERTY="userFullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_viewUserDetails">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUBORDIANTEUSER"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUBORDIANTEUSER"
            PROPERTY="dtls$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.OrgUnit"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUBORDIANTEUSER"
          PROPERTY="organisationUnitName"
        />
      </CONNECT>
      <LINK PAGE_ID="Organization_orgUnitHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYSUBORDIANTEUSER"
            PROPERTY="dtls$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSUBORDIANTEUSER"
          PROPERTY="userStatusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>
  <!--End subordinate user -->

  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Label.Comments"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER></PAGE>
