<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to enter search criteria -->
<!-- and search for registered users.-->
<PAGE
  PAGE_ID="Supervisor_userSearchDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>

  <!-- BEGIN, CR00282028, IBM -->
  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>
  <!-- END, CR00282028 -->


  <SERVER_INTERFACE
    CLASS="SupervisorApplicationPageContextDescription"
    NAME="DISPLAY"
    OPERATION="readLoggedInUserName"
    PHASE="DISPLAY"
  />


  <!-- BEGIN, CR00282028, IBM -->
  <SERVER_INTERFACE
    CLASS="SupervisorUserSearch"
    NAME="ACTION"
    OPERATION="searchUserDetails"
    PHASE="ACTION"
  />
  <!-- END, CR00282028 -->


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD LABEL="Field.Label.FirstName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="criteria$firstname"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OrganisationUnit">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="criteria$organisationUnitID"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.LastName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="criteria$surname"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Job">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="jobID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00414427, MR -->
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <!-- BEGIN, CR00248002, PM -->
        <LINK PAGE_ID="Supervisor_userSearchDetails"/>
        <!-- END, CR00248002 -->
      </ACTION_CONTROL>
    </ACTION_SET>
    <!-- END, CR00414427 -->
  </CLUSTER>
  <LIST TITLE="List.Title.SearchResults">
    <FIELD
      LABEL="Field.Title.FirstName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$fullName"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_userWorkspace">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$username"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Title.OrganisationUnit"
      WIDTH="22"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$organisationUnitName"
        />
      </CONNECT>
      <LINK PAGE_ID="Supervisor_orgUnitWorkspace">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="orgUnitID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="orgStructureID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="CONSTANT"
            PROPERTY="supervisor.viewTaskOption"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="taskOptionCode"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD
      LABEL="Field.Title.Position"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$positionName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewPositionDetails"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$positionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="positionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$organisationStructureID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationStructureID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$userSearchResultsDetails$dtls$userSearchDetailsReferenceList$organisationUnitID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="organisationUnitID"
          />
        </CONNECT>
      </LINK>
    </FIELD>
  </LIST>
</PAGE>
