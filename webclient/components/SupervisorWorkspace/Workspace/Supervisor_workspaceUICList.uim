<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page displays cases registered in an agency defined priority week, -->
<!-- that have associated issues. The list is sorted by Number Of Issues descending. -->
<PAGE
  PAGE_ID="Supervisor_workspaceUICList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="priorityWeekBegin"/>


  <SERVER_INTERFACE
    CLASS="SupervisorWorkspace"
    NAME="ACTION3"
    OPERATION="viewUICfilter"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="SupervisorWorkspace"
    NAME="DISPLAY1"
    OPERATION="listPriorityWeekCasesWithIssues"
    PHASE="DISPLAY"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="priorityWeekBegin"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$casesWithIssuesByDateKey$priorityDate"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="4"
    SHOW_LABELS="false"
  >
    <CONTAINER ALIGNMENT="LEFT">
      <ACTION_CONTROL LABEL="Action.Control.ReassignCases">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Supervisor_reassignPriorityWeekCaseIssues"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$pageContextDescription$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$priorityDates$startDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="regFromDate"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$priorityDates$endDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="regToDate"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <CONTAINER>
      <FIELD CONTROL="SKIP"/>
    </CONTAINER>
    <CONTAINER>
      <FIELD CONTROL="SKIP"/>
    </CONTAINER>
    <CONTAINER ALIGNMENT="RIGHT">
      <ACTION_CONTROL LABEL="Action.Control.ViewHeatMap">
        <LINK
          PAGE_ID="Supervisor_workspaceUIC"
          SAVE_LINK="true"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$pageContextDescription$description"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="description"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$priorityDates$startDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="regFromDate"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$priorityDates$endDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="regToDate"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="3"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Name"
  >
    <CLUSTER
      LABEL_WIDTH="75"
      NUM_COLS="1"
      SHOW_LABELS="true"
    >
      <CONTAINER
        ALIGNMENT="LEFT"
        LABEL="Field.Label.PriorityWeekBegin"
      >
        <FIELD LABEL="Field.Label.PriorityWeekBegin">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$priorityDates$startDate"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>
    <CLUSTER
      LABEL_WIDTH="40"
      NUM_COLS="1"
      SHOW_LABELS="true"
    >
      <CONTAINER
        ALIGNMENT="LEFT"
        LABEL="Field.Title.ChooseDay"
      >
        <FIELD LABEL="Field.Title.ChooseDay">
          <CONNECT>
            <INITIAL
              HIDDEN_PROPERTY="chooseDayList$date"
              NAME="DISPLAY1"
              PROPERTY="chooseDayList$dateDesc"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION3"
              PROPERTY="key$date$date"
            />
          </CONNECT>
        </FIELD>
        <ACTION_CONTROL
          IMAGE="UpdateButton"
          LABEL="Action.Control.Update"
          TYPE="SUBMIT"
        >
          <LINK PAGE_ID="Supervisor_casesWithIssuesByDateList">
            <CONNECT>
              <SOURCE
                NAME="ACTION3"
                PROPERTY="key$date$date"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="recievedOnDate"
              />
            </CONNECT>
          </LINK>
        </ACTION_CONTROL>
      </CONTAINER>
    </CLUSTER>
    <CONTAINER ALIGNMENT="RIGHT">
      <ACTION_CONTROL LABEL="Action.Control.PreviousWeek">
        <LINK PAGE_ID="Supervisor_workspaceUICList">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$previousDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="priorityWeekBegin"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Action.Control.NextWeek">
        <LINK PAGE_ID="Supervisor_workspaceUICList">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$dateDtls$nextDate"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="priorityWeekBegin"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>
  <LIST>
    <CONTAINER
      LABEL="Container.Label.Action"
      WIDTH="5"
    >
      <ACTION_CONTROL LABEL="ActionControl.Label.View">
        <LINK PAGE_ID="Supervisor_listIssueCasesByType">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY1"
              PROPERTY="result$casesWithIssuesUICDtlsList$casesWithIssuesDtlsList$casesWithIssueDetails$dtls$caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="supervisor.viewTaskOption"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taskOptionCode"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="supervisor.type"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseIssuesViewOptionCode"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD
      LABEL="Field.Label.CaseReference"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="caseReference"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Product"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="productType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.NumOfIssues"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="issueCount"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="dtls$startDate"
        />
      </CONNECT>
    </FIELD>
  </LIST>


</PAGE>
