<!-- This page allows the user to close a Task -->
<PAGE PAGE_ID="MaintainSupervisorTask_closeTask" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <!-- START CR CR00037467 NP -->
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="description"/>
  <!-- END CR CR00037467 NP -->
  <SERVER_INTERFACE CLASS="MaintainSupervisorTasks" NAME="ACTION" OPERATION="closeTask" PHASE="ACTION"/>
  <PAGE_PARAMETER NAME="taskID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="ACTION" PROPERTY="taskID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.CloseTask"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>