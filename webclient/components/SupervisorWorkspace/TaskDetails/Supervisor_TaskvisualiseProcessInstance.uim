<!-- Description -->
<!-- =========== -->
<!-- Displays a graphical view of the process instance associated with a Task. -->
<PAGE
  PAGE_ID="Supervisor_TaskvisualiseProcessInstance"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="TaskManagement"
    NAME="DISPLAY"
    OPERATION="visualiseProcessInstanceForTask"
  />
  <PAGE_PARAMETER NAME="taskID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="taskID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="taskKey$taskID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
  >
    <FIELD CONFIG="TaskUser.Config">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="workflowGraphXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
