<!-- Supervisor Task Reserve      -->
<PAGE PAGE_ID="Supervisor_reserveTask" WINDOW_OPTIONS="width=500" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="MaintainSupervisorTasks" NAME="ACTION" OPERATION="reserveTask" PHASE="ACTION"/>


  <SERVER_INTERFACE CLASS="MaintainSupervisorTasks" NAME="DISPLAY" OPERATION="listSupervisorUsers" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="taskID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="ACTION" PROPERTY="details$reserveDetails$taskID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="taskID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$taskID"/>
  </CONNECT>


  <CLUSTER LABEL_WIDTH="23">


    <CONTAINER LABEL="Container.Label.ReserveFor">
      <FIELD USE_BLANK="true" WIDTH="45">
        <CONNECT>
          <INITIAL HIDDEN_PROPERTY="result$usersList$dtls$userName" NAME="DISPLAY" PROPERTY="result$usersList$dtls$userFullName"/>
        </CONNECT>
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="details$reserveDetails$supervisorUserName"/>
        </CONNECT>
      </FIELD>
      <FIELD WIDTH="50">
        <CONNECT>
          <TARGET NAME="ACTION" PROPERTY="details$reserveDetails$userName"/>
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <!-- BEGIN, CR00417017, GK -->
    <FIELD HEIGHT="4" LABEL="Field.Label.Comments">
      <!-- END, CR00417017 -->
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="ReserveButton" LABEL="ActionControl.Label.Reserve" TYPE="SUBMIT"/>


    <ACTION_CONTROL IMAGE="CancelButton" LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>
</PAGE>