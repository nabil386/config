<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Displays all the assigments of a task.                                 -->
<PAGE
  PAGE_ID="Supervisor_viewTaskAssignmentList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>


  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="TaskManagement"
    NAME="DISPLAY"
    OPERATION="viewAssignmentsForTask"
  />
  <PAGE_PARAMETER NAME="taskID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="taskID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="taskKey$taskID"
    />
  </CONNECT>


  <LIST>
    <FIELD
      LABEL="Field.Title.AssignedTo"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$assignedTo"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Title.AssigneeType"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dtls$assigneeType"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
