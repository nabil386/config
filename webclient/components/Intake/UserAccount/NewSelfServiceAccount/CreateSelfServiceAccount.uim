<!-- Creates a Citizen Self Service Account.                                -->
<PAGE
  PAGE_ID="CreateSelfServiceAccount"
  WINDOW_OPTIONS="width=460"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="NewSelfServiceAccount"
    NAME="LINKDISPLAY"
    OPERATION="linkDynamicPage"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="NewSelfServiceAccount"
    NAME="ACTION"
    OPERATION="createSelfServiceAccountDetails"
    PHASE="ACTION"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <!-- BEGIN,CR00292037,AM -->
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      LABEL="ActionControl.Label.Cancel"
    />
    <!-- END,CR00292037,AM -->
  </ACTION_SET>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="55"
    NUM_COLS="1"
    STYLE="outer-cluster-borderless"
  >


    <FIELD
      LABEL="Field.Label.UserName"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="userName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Password"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="password"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Re-typePassword"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="retypePassword"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="55"
    NUM_COLS="1"
    TITLE="Cluster.Title.Details"
  >


    <FIELD
      LABEL="Field.Label.Question"
      WIDTH="70"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="secretQuestion"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Answer"
      WIDTH="50"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="secretAnswer"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="agreementRead"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    BEHAVIOR="NONE"
    SHOW_LABELS="false"
  >
    <CONTAINER ALIGNMENT="LEFT">
      <ACTION_CONTROL
        APPEND_ELLIPSIS="false"
        LABEL="ActionControl.Label.UserAgreement"
      >
        <LINK
          OPEN_MODAL="true"
          SAVE_LINK="false"
          URI_SOURCE_NAME="LINKDISPLAY"
          URI_SOURCE_PROPERTY="defaultAgreementPage"
        > </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>


</PAGE>
