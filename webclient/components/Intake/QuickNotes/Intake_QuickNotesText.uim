<!-- View the description of the Triage QuickNotes Text                       -->
<PAGE
  PAGE_ID="Intake_QuickNotesText"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="QuickNotes"
    NAME="DISPLAY"
    OPERATION="readQuickNotes"
  />


  <PAGE_PARAMETER NAME="quickNotesID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="quickNotesID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="quickNotesIDKey$quickNotesID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Field.Label.Sensitivity"
  >
    <FIELD
      LABEL="Field.Label.Sensitivity"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sensitivityCode"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  
   <!-- conditional cluster that shows an icon and message if the note is editable -->
	 <CLUSTER>
	  <CONDITION>
	    <IS_TRUE NAME="DISPLAY" PROPERTY="noteEditableInd"/>
	  </CONDITION>
	  <CONTAINER>
	          <IMAGE IMAGE="UnlockedIcon" LABEL="Icon.Editable"/>
	          <FIELD>
	                <CONNECT>
	                  <SOURCE NAME="DISPLAY" PROPERTY="editableNoteMessageOpt" />
	                </CONNECT>
	          </FIELD>
	        </CONTAINER>
	</CLUSTER>
	<!-- conditional cluster that shows a different icon and message if the note is locked for editing --> 
	<CLUSTER>
	  <CONDITION>
	    <IS_TRUE NAME="DISPLAY" PROPERTY="noteLockedInd" />
	  </CONDITION>
	  <CONTAINER>
	          <IMAGE IMAGE="LockedIcon" LABEL="Icon.Locked"/>
	          <FIELD>
	                <CONNECT>
	                  <SOURCE NAME="DISPLAY" PROPERTY="editableNoteMessageOpt" />
	                </CONNECT>
	          </FIELD>
	        </CONTAINER>
	</CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Cluster.Title.NoteHistory"
  >


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="notesText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
