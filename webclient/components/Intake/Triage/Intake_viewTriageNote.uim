<!-- This Page views a Note related to a particular triage -->
<PAGE
  PAGE_ID="Intake_viewTriageNote"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="key$triageNoteID"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IntakeTriage"
    NAME="DISPLAY"
    OPERATION="readTriageNote"
  />


  <PAGE_PARAMETER NAME="triageNoteID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="triageNoteID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$triageNoteID"
    />
  </CONNECT>


