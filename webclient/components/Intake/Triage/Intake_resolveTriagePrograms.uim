<!-- This page resolves between starting the screening or listing the       -->
<!-- programs to be selected for screening                                  -->
<PAGE
  PAGE_ID="Intake_resolveTriagePrograms"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    curam.omega3.request.RequestHandler
    rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
    String context = request.getContextPath() + "/";
    String concernRoleID = request.getParameter("concernRoleID");
    String programTypeIDs = request.getParameter("programTypeIDs");
   
    
    if(programTypeIDs == null) {
    core.ScriptletMissingParamException e = new
    core.ScriptletMissingParamException(-20002, "programTypeIDs");
    throw e;
    }
    
    programTypeIDs = programTypeIDs.replaceAll("\\t", ",");
    String url = "";
    
    curam.interfaces.IntakeScreeningPkg.IntakeScreening_getScreeningTypeByProgramID_TH
    th = new curam.interfaces.IntakeScreeningPkg.IntakeScreening_getScreeningTypeByProgramID_TH();
    th.setFieldValue(
    th.key$programTypeIDs_idx, programTypeIDs);
    th.callServer();
    String screeningTypeID = th.getFieldValue(th.result$screeningTypeID_idx);
      
      if (screeningTypeID == null) {
      core.ScriptletMissingParamException e = new
      core.ScriptletMissingParamException(-20002, "screeningTypeID");
      throw e;
      }
   
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(
    pageContext.getSession()).getLocale() + "/";
    url = context + "Intake_listProgramItemsForScreeningPage.do?" + "programTypeIDs=" + curam.omega3.request.RequestUtils.escapeURL(programTypeIDs);
    url += "&amp;" + "concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
    url += "&amp;" + "screeningTypeID=" + curam.omega3.request.RequestUtils.escapeURL(screeningTypeID);
    url += "&amp;o3frame=modal";
    
    url += "&amp;" + rh.getSystemParameters();
    response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
