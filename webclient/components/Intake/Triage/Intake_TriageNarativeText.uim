<!-- View the description of the Triage Narrative Text                       -->
<PAGE
  PAGE_ID="Intake_TriageNarativeText"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="IntakeTriage"
    NAME="DISPLAY"
    OPERATION="readTriageNote"
  />


  <PAGE_PARAMETER NAME="triageNoteID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="triageNoteID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$triageNoteID"
    />
  </CONNECT>


  <CLUSTER>


    <CLUSTER
      LABEL_WIDTH="28"
      NUM_COLS="2"
      STYLE="cluster-cpr-no-border"
    >


      <FIELD LABEL="Field.Label.Sensitivity">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="sensitivityCode"
          />
        </CONNECT>
      </FIELD>


    </CLUSTER>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Cluster.Title.NoteHistory"
  >


    <FIELD HEIGHT="4">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="notesText"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
