<PAGE
  PAGE_ID="Intake_ListTriageDelete"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <SERVER_INTERFACE
    CLASS="IntakeTriage"
    NAME="ACTION"
    OPERATION="cancelTriage"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="participantTriageID"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantTriageID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="participantTriageID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Yes"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="IntakeTriage_ListTriage"/>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
    </ACTION_SET>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.DeleteTriage"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
