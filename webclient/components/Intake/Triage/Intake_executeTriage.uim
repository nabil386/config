<!-- This page is configured and called when the IEG script is completed.   -->
<!-- It redirects the user to a Triage Result Recommendations page.         -->
<PAGE
  PAGE_ID="Intake_executeTriage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
      curam.omega3.request.RequestHandler
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      String executionID = request.getParameter("executionID");

       if (executionID == null) {
             core.ScriptletMissingParamException e = new
               core.ScriptletMissingParamException(-20002, "executionID");
             throw e;
      }

      curam.interfaces.IntakeTriagePkg.IntakeTriage_executeTriage_TH
            thExecuteTriage = new curam.interfaces.IntakeTriagePkg.IntakeTriage_executeTriage_TH();

      thExecuteTriage.setFieldValue(thExecuteTriage.key$executionID_idx, executionID);
      thExecuteTriage.callServer();

      String participantTriageID = thExecuteTriage.getFieldValue(thExecuteTriage.result$participantTriageID_idx);


      String url = "";
      url = context + "Intake_TriageServicesPage.do?" + "participantTriageID=" + curam.omega3.request.RequestUtils.escapeURL(participantTriageID);
      url += "&amp;" + rh.getSystemParameters();

      String encodeRedirectURL = response.encodeURL(url);
      response.sendRedirect(response.encodeRedirectURL(request.getContextPath() + "/ieg/CloseAndRedirect.jspx?redirect=" + encodeRedirectURL));

  </JSP_SCRIPTLET>
</PAGE>
