<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_listReopenings"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="programApplicationID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ReopenDetails"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Intake_listReopenings"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="programApplicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programApplicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.WithdrawalDetails"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Intake_listProgramWithdrawalRequests"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="programApplicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programApplicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="programApplicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programApplicationID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="DISPLAY"
    OPERATION="listReopenings"
  />


  <LIST>
    <FIELD LABEL="Field.Label.Reason">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reopenReason"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ReopenDateTime">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reopenDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreatedByUserName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createdByUserName"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
        WINDOW_OPTIONS="width=900"
      >


        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="createdByUserName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Intake_viewReopenComments">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="list$programAppStatusTransitionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programAppStatusTransitionID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Edit"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_modifyReopenDetails"
        >


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="list$programAppStatusTransitionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programAppStatusTransitionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>


    </ACTION_SET>


  </LIST>


</PAGE>
