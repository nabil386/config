<!-- Edit and approve Timer Extension                                       -->
<PAGE
  PAGE_ID="Intake_editAndApproveTimerExtension"
  WINDOW_OPTIONS="width=430"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="milestoneWaiverRequestID"/>


  <SERVER_INTERFACE
    CLASS="IntakeTimers"
    NAME="DISPLAY"
    OPERATION="getTimerExtensionDetail"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneWaiverRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$milestoneWaiverRequestID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneWaiverRequestID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$milestoneWaiverRequestID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="IntakeTimers"
    NAME="ACTION"
    OPERATION="editAndApproveTimerExtension"
    PHASE="ACTION"
  />


  <CLUSTER>
    <FIELD
      LABEL="Field.Label.DaysExtended"
      WIDTH="8"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$daysExtended"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$daysExtended"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Field.Label.Reason"
  >


    <FIELD
      HEIGHT="5"
      LABEL="Field.Label.Reason"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$approvalComments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="RIGHT">
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    
      
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


</PAGE>
