<!-- extend Timers                                                          -->
<PAGE
  PAGE_ID="Intake_extendTimers"
  WINDOW_OPTIONS="width=460"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="milestoneDeliveryID"/>


  <SERVER_INTERFACE
    CLASS="IntakeTimers"
    NAME="DISPLAY"
    OPERATION="showTimerName"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneDeliveryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$milestoneDeliveryID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="milestoneDeliveryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$milestoneDeliveryID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="IntakeTimers"
    NAME="ACTION"
    OPERATION="extendTimer"
    PHASE="ACTION"
  />


  <CLUSTER>
    <FIELD LABEL="Field.Label.Timer">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$timerDescription"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00279965, PC -->
    <FIELD
      LABEL="Field.Label.DaysExtended"
      USE_DEFAULT="false"
      WIDTH="8"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$daysExtended"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00279965 -->


  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Field.Label.Reason"
  >
    <FIELD HEIGHT="5">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$reason"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    
      
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


</PAGE>
