<!-- List Screening Narratives for a specified Screening Execution                          -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_listLifeEventQuickNotes"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="QuickNotes"
    NAME="DISPLAY"
    OPERATION="listQuickNotesByrelatedIDAndType"
  />


  <PAGE_PARAMETER NAME="participantLifeEventID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="participantLifeEventID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="quickNotesByRelatedIDAndType$relatedID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="LifeEvent.QuickNotesType.Value"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="type"
    />
  </CONNECT>


  <!-- New link  -->
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Intake_addLifeEventQuickNotes"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="participantLifeEventID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="relatedID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="type"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="quickNotesType"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>
  <!-- New link  -->


  <!-- Delete link  -->
  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="noteDeletedInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_deleteQuickNotes"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="quickNotesID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="quickNotesID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="participantLifeEventID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="participantLifeEventID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- Delete link  end-->


      <!-- Modify link -->
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Modify"
      >
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="noteDeletedInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_modifyLifeEventQuickNotes"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="quickNotesID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="quickNotesID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="participantLifeEventID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="relatedID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- Modify link end-->
    </ACTION_SET>


    <FIELD LABEL="Field.Label.Subject">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="subjectText"
        />
      </CONNECT>
    </FIELD>
    
    
    <!-- The following field gets mapped and displayed as an icon-->
    <FIELD WIDTH="5" >
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="latestNoteEditStatusOpt" />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.notesText">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="truncatedNoteText"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EnteredBy"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fullname"
        />
      </CONNECT>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Organization_viewUserDetails"
        WINDOW_OPTIONS="width=900"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="userName"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD
      LABEL="Field.Label.CreatedModifiedDateTime"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="modifiedDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Priority"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="priorityCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


    <!-- To show notes text -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="Intake_QuickNotesText">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="quickNotesID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="quickNotesID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- To show notes text end -->
  </LIST>
</PAGE>
