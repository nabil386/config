<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This is a search page which allows the user to search                  -->
<!-- for prospect persons and persons.                                      -->
<PAGE
  PAGE_ID="Search_results"
  SCRIPT_FILE="SearchResults.js"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="DISPLAY"
    OPERATION="searchPersonAndProspectPersons"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="ACTION"
    OPERATION="searchPersonAndProspectPersons"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="surname"/>
  <PAGE_PARAMETER NAME="forename"/>
  <PAGE_PARAMETER NAME="dateOfBirth"/>
  <PAGE_PARAMETER NAME="referenceNumber"/>
  <PAGE_PARAMETER NAME="middleName"/>
  <PAGE_PARAMETER NAME="informationMesgText"/>
  <PAGE_PARAMETER NAME="typeAction"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="surname"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$lastName"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="forename"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$firstName"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="middleName"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$initial"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="dateOfBirth"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$dateOfBirth"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="referenceNumber"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$referenceID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="informationMesgText"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$informationMsgText"
    />
  </CONNECT>


  <SCRIPT
    ACTION="fieldValidation()"
    EVENT="ONLOAD"
    SCRIPT_FILE="SearchResults.js"
  />


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <FIELD LABEL="Field.Label.ReferenceNumber">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="key$referenceID"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="key$referenceID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$referenceID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TITLE="Cluster.Title.AdditionalSearchCriteria"
  >


    <FIELD LABEL="Field.Label.FirstName">
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="key$firstName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="key$firstName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$firstName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.LastName"
      WIDTH="85"
    >
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="key$lastName"
        />
      </CONNECT>


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="key$lastName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$lastName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.DateOfBirth"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="key$dateOfBirth"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="key$dateOfBirth"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$dateOfBirth"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <FIELD
      LABEL="Field.Label.MiddleInitial"
      WIDTH="5"
      WIDTH_UNITS="CHARS"
    >
      <CONNECT>
        <INITIAL
          NAME="DISPLAY"
          PROPERTY="key$initial"
        />
      </CONNECT>


      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="key$initial"
        />
      </CONNECT>


      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$initial"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Gender"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$gender"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.PrimaryAddressData">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="addressData"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER>
    <ACTION_SET
      ALIGNMENT="CENTER"
      BOTTOM="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS">                             
        </LINK>
        <SCRIPT
          ACTION="deleteMyDivTag()"
          EVENT="ONCLICK"
          SCRIPT_FILE="SearchResults.js"
        />
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.Reset"
      >
        <LINK PAGE_ID="Search_results1">         
        </LINK>


      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="ResetButton"
        LABEL="ActionControl.Label.QuickRegister"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Quick_register"
        />
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <JSP_SCRIPTLET>   
    if (request.getParameter("typeAction")!=null) { 
    
  </JSP_SCRIPTLET>
  <INCLUDE FILE_NAME="SearchResultsActionView.vim"/>
  <JSP_SCRIPTLET>    
    } else 
       {   
  </JSP_SCRIPTLET>
  <INCLUDE FILE_NAME="SearchResultsDisplayView.vim"/>
  <JSP_SCRIPTLET>    
        }
  </JSP_SCRIPTLET>


</PAGE>
