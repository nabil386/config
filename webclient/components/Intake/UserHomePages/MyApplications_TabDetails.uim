              NAME="ACTION"
              PROPERTY="applicationSearchKey$processingDeadlineType"
            />
          </CONNECT>
        </FIELD>
      </CLUSTER>
      <CLUSTER
        SHOW_LABELS="false"
        TITLE="Container.Label.ApplicationType"
      >
        <FIELD
          CONTROL="CHECKBOXED_LIST"
          HEIGHT="5"
          LABEL="Container.Label.ApplicationType"
          USE_BLANK="false"
          WIDTH="100"
        >
          <CONNECT>
            <INITIAL
              HIDDEN_PROPERTY="result$dtls$applicationTypeCode"
              NAME="DISPLAYTYPE"
              PROPERTY="result$dtls$description"
            />
          </CONNECT>
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="applicationSearchKey$applicationType"
            />
          </CONNECT>
        </FIELD>
      </CLUSTER>
      <CLUSTER
        SHOW_LABELS="false"
