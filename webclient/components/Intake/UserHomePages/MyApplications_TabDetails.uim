          CONTROL="CHECKBOXED_LIST"
          HEIGHT="5"
          LABEL="Container.Label.ProcessingDeadline"
          USE_BLANK="false"
          WIDTH="100"
        >
          <CONNECT>
            <INITIAL
              HIDDEN_PROPERTY="result$dtls$processingDeadLineCode"
              NAME="DISPLAYDEADLINE"
              PROPERTY="result$dtls$description"
            />
          </CONNECT>
          <CONNECT>
