<PAGE
  PAGE_ID="WorkQueue_determinationTabDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="typeAction"/>


  <SERVER_INTERFACE
    CLASS="WorkQueueSearch"
    NAME="DISPLAY"
    OPERATION="searchWorkQueueForEligibilityWorker"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="IntakeApplicationSearch"
    NAME="DISPLAYDEADLINE"
    OPERATION="listProcessingDeadLine"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="WorkQueueSearch"
    NAME="ACTION"
    OPERATION="searchWorkQueueForEligibilityWorker"
    PHASE="ACTION"
  />


  <CLUSTER
    NUM_COLS="4"
    TITLE="Cluster.Title.SearchCriteria"
  >
    <CLUSTER TITLE="Container.Label.ProcessingDeadline">
      <FIELD HEIGHT="5">
        <CONNECT>
          <INITIAL
            HIDDEN_PROPERTY="result$dtls$processingDeadLineCode"
            NAME="DISPLAYDEADLINE"
            PROPERTY="result$dtls$description"
          />
        </CONNECT>
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="key$processDeadlineType"
          />
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>


  <CLUSTER>
    <ACTION_SET
      ALIGNMENT="LEFT"
      BOTTOM="false"
    >
      <ACTION_CONTROL
        IMAGE="SearchButton"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS"> </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


  <JSP_SCRIPTLET> if (request.getParameter("typeAction")!=null) { </JSP_SCRIPTLET>
  <INCLUDE FILE_NAME="WorkQueue_listResultsView.vim"/>
  <JSP_SCRIPTLET> } else { </JSP_SCRIPTLET>
  <INCLUDE FILE_NAME="WorkQueue_listResultsActionView.vim"/>
  <JSP_SCRIPTLET> } </JSP_SCRIPTLET>


</PAGE>
