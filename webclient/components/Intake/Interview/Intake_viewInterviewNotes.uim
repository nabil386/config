<!-- This Page allows to view the Notes of the  listed Interview            -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_viewInterviewNotes"
  WINDOW_OPTIONS="width=350,height=70"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="interviewID"/>
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DetailView"
      STYLE="in-page-link"
    >


      <LINK
        PAGE_ID="Intake_viewInterviewDetails"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.viewNotes"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Intake_viewInterviewNotes"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.viewAttendees"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Intake_viewAttendees"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="viewInterviewNote"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="interviewID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="interviewID$interviewID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <FIELD ALIGNMENT="LEFT">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewNotes"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
