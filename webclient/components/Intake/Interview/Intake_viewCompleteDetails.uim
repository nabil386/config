<!-- This page allows you to view interviews from Calendar.    -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_viewCompleteDetails"
  WINDOW_OPTIONS="width=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="ID"/>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="viewDetailsFromCalendar"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAYATTENDEES"
    OPERATION="viewInterviewAttendees"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="interviewID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="ID"
    />
    <TARGET
      NAME="DISPLAYATTENDEES"
      PROPERTY="interviewID"
    />
  </CONNECT>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.Subject"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewSubject"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Location"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewLocation"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.StartTime"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewStartDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndTime"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewEndDateTime"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.Priority"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewPriority"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="0"
    TITLE="Cluster.Title.Notes"
  >
    <FIELD
      ALIGNMENT="LEFT"
      HEIGHT="4"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="notes"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    BEHAVIOR="NONE"
    TITLE="Attendee.Title"
  >
    <LIST>
      <FIELD LABEL="Field.Label.Attendee">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYATTENDEES"
            PROPERTY="name"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Status">
        <CONNECT>
          <SOURCE
            NAME="DISPLAYATTENDEES"
            PROPERTY="invitationStatus"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>
  <ACTION_SET>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>
</PAGE>
