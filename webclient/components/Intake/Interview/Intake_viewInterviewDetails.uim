<!-- This page allows you to list the Interviews.    -->
<!-- Intervuew is used to schedule meeting with      -->
<!-- social agency.   -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_viewInterviewDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="interviewID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.DetailView"
      STYLE="in-page-current-link"
    >


      <LINK
        PAGE_ID="Intake_viewInterviewDetails"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.viewNotes"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Intake_viewInterviewNotes"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.viewAttendees"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Intake_viewAttendees"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="viewInterviewDetails"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="interviewID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="interviewID"
    />
  </CONNECT>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <FIELD
      LABEL="Field.Label.StartTime"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewStartDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EndTime"
      WIDTH="50"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="interviewEndDateTime"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="50"
    NUM_COLS="2"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="displayCancelationReason"
      />
    </CONDITION>


    <FIELD
      LABEL="Field.Label.CancelationReason"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="cancelationReason"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
</PAGE>
