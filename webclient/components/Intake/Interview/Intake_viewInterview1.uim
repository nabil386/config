<!-- This page allows you to view list of Interviews.    -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Intake_viewInterview1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="applicationID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ListView"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Intake_viewInterview1"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.CalendarView"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Intake_calendarView"
        SAVE_LINK="true"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="SI_DETAILS"
    OPERATION="listApplicationInterview1"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="SI_DETAILS"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
  >
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.NewInterview"
    >
      <CONDITION>
        <IS_FALSE
          NAME="SI_DETAILS"
          PROPERTY="disableMenuOpt"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Intake_createInterview"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
    <FIELD LABEL="Field.Label.Subject">
      <CONNECT>
        <SOURCE
          NAME="SI_DETAILS"
          PROPERTY="interviewSubject"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartTime">
      <CONNECT>
        <SOURCE
          NAME="SI_DETAILS"
          PROPERTY="interviewStartDateTime"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndTime">
      <CONNECT>
        <SOURCE
          NAME="SI_DETAILS"
          PROPERTY="interviewEndDateTime"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Location">
      <CONNECT>
        <SOURCE
          NAME="SI_DETAILS"
          PROPERTY="interviewLocation"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="SI_DETAILS"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="NextButton"
        LABEL="ActionControl.Label.EditInterview"
      >
        <CONDITION>
          <IS_TRUE
            NAME="SI_DETAILS"
            PROPERTY="result$interviewDetails1List$briefDetails1List$isEditable"
          />
        </CONDITION>


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_editInterview"
        >
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewVersionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="applicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="applicationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="NextButton"
        LABEL="ActionControl.Label.inviteAttendee"
      >
        <CONDITION>
          <IS_TRUE
            NAME="SI_DETAILS"
            PROPERTY="result$interviewDetails1List$briefDetails1List$isInvite"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_inviteAttendee"
        >
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewID"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="NextButton"
        LABEL="ActionControl.Label.completeInterview"
      >
        <CONDITION>
          <IS_TRUE
            NAME="SI_DETAILS"
            PROPERTY="result$interviewDetails1List$briefDetails1List$isCompleted"
          />


        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_completeInterview"
        >
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="NextButton"
        LABEL="ActionControl.Label.cancelInterview"
      >
        <CONDITION>
          <IS_TRUE
            NAME="SI_DETAILS"
            PROPERTY="result$interviewDetails1List$briefDetails1List$isCompleted"
          />
        </CONDITION>


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_cancelInterview"
        >
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="SI_DETAILS"
              PROPERTY="interviewVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="interviewVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Intake_viewInterviewDetails">
        <CONNECT>
          <SOURCE
            NAME="SI_DETAILS"
            PROPERTY="interviewID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="interviewID"
          />
        </CONNECT>


      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
