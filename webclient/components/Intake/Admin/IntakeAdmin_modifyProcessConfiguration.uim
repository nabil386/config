<!-- Modifies an Intake Process Configuration                    		    -->
<PAGE
  PAGE_ID="IntakeAdmin_modifyProcessConfiguration"
  WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="processID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$processID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="processID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="processID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="IntakeProcessAdmin"
    NAME="DISPLAY"
    OPERATION="getProcessConfiguration"
  />


  <!-- BEGING CR CR00282069 MN -->
  <SERVER_INTERFACE
    CLASS="IntakeProcessAdmin"
    NAME="ACTION"
    OPERATION="modifyProcessConfiguration1"
    PHASE="ACTION"
  />
  <!-- END CR CR00282069 MN -->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="IntakeAdmin_listUnpublishedProcesses"
        SAVE_LINK="false"
      />
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="35"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="70"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      CONTROL="CHECKBOXED_LIST"
      HEIGHT="5"
      LABEL="Field.Label.Applications"
      USE_BLANK="false"
      WIDTH="100"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$appDtls$dtls$applicationCode"
          NAME="DISPLAY"
          PROPERTY="result$appDtls$dtls$applicationCode"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$appDtls$selectedApplications"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="selectedApplications"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="35"
    NUM_COLS="1"
    TITLE="Cluster.Configuration.Title"
  >


    <CONTAINER LABEL="Field.Label.UploadedFile">


      <WIDGET TYPE="FILE_DOWNLOAD">
        <WIDGET_PARAMETER NAME="LINK_TEXT">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="processXMLFileName"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
        <WIDGET_PARAMETER NAME="PARAMS">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="processID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="processID"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <WIDGET
      LABEL="Field.Label.NewFile"
      TYPE="FILE_UPLOAD"
      WIDTH="50"
    >
      <WIDGET_PARAMETER NAME="CONTENT">


        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="processXML"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>


  </CLUSTER>


</PAGE>
