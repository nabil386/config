<!-- Creates a new Intake Process Configuration                    		    -->
<PAGE
  PAGE_ID="IntakeAdmin_newProcessConfiguration"
  WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IntakeProcessAdmin"
    NAME="DISPLAY"
    OPERATION="getApplicationCodeDetails"
  />


  <!-- BEGIN CR00282069 ,MN -->
  <SERVER_INTERFACE
    CLASS="IntakeProcessAdmin"
    NAME="ACTION"
    OPERATION="createProcessConfiguration1"
    PHASE="ACTION"
  />
  <!-- END CR00282069 ,MN -->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="IntakeAdmin_listUnpublishedProcesses"
        SAVE_LINK="false"
      />
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    BEHAVIOR="NONE"
    LABEL_WIDTH="35"
    NUM_COLS="1"
  >
    <FIELD
      LABEL="Field.Label.Name"
      WIDTH="70"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      CONTROL="CHECKBOXED_LIST"
      HEIGHT="5"
      LABEL="Field.Label.Applications"
      USE_BLANK="false"
      WIDTH="100"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$dtls$applicationCode"
          NAME="DISPLAY"
          PROPERTY="result$dtls$applicationCode"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="applicationCodes"
        />
      </CONNECT>
    </FIELD>


    <WIDGET
      LABEL="Field.Label.File"
      TYPE="FILE_UPLOAD"
    >
      <WIDGET_PARAMETER NAME="CONTENT">
        <CONNECT>
          <TARGET
            NAME="ACTION"
            PROPERTY="processXML"
          />
        </CONNECT>
      </WIDGET_PARAMETER>
    </WIDGET>


  </CLUSTER>


</PAGE>
