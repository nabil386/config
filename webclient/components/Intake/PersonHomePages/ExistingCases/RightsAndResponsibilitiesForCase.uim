<!-- Rights And Responsibility for case page -->
<PAGE
  PAGE_ID="RightsAndResponsibilitiesForCase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.RightsandResponsibilities"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00277619, JD -->
  <SERVER_INTERFACE
    CLASS="Application"
    NAME="ACTION"
    OPERATION="submitApplicationForCase1"
    PHASE="ACTION"
  />
  <!-- END, CR00277619 -->


  <ACTION_SET BOTTOM="false">


    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="QuitButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Intake_confirmSaveApplicationScriptExecutionForCaseSubmit"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      IMAGE="QuitButton"
      LABEL="ActionControl.Label.Back"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Intake_confirmSaveApplicationScriptExecutionForCaseSubmit"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="executionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="executionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Submit"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="ApplicationSummary"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="RightsAndResponsibilitiesView.vim"/>


</PAGE>
