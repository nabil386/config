<!-- This page allows user to list applications Types or programs configured-->
<!-- in the Application configuration. 	-->
<!-- BEGIN,CR00291376,AM 									-->
<PAGE
  PAGE_ID="Application_listApplicationTypesForExistingCase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END,CR00291376,AM -->


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="readStartApplicationForCase"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY1"
    OPERATION="readProgramAppTypesForCaseApplication"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="ACTION"
    OPERATION="validateSelectApplicationTypesForCase"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="ACTION1"
    OPERATION="validateSelectProgramTypesForCase"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="programTypeID"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleIDCaseIDKey$concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleIDCaseIDKey$caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="applicationForCaseTypeKey$caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY1"
      PROPERTY="result$applicationTypeCode"
    />
    <TARGET
      NAME="ACTION1"
      PROPERTY="programTypeKey$applicationTypeCode"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY1"
      PROPERTY="result$applicationTypeCode"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationTypeKey$applicationTypeCode"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="QuitButton"
      LABEL="ActionControl.Label.Quit"
      STYLE="screening-link"
    >
      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="Intake_listActionableIndividualExistingCasesByConcernRole"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
      STYLE="application-next-link"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Intake_resolveApplicationStartForCase">
        <CONNECT>
          <SOURCE
            NAME="ACTION1"
            PROPERTY="programTypeKey$applicationTypeCode"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="intakeApplicationTypeID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION1"
            PROPERTY="programTypeKey$programTypeCodes"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programTypeCodes"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false">
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="displayApplicationsInd"
      />
    </CONDITION>
    <LIST>
      <CONTAINER SEPARATOR="Container.Separator">
        <WIDGET
          ALIGNMENT="CENTER"
          TYPE="SINGLESELECT"
          WIDTH="100"
          WIDTH_UNITS="PERCENT"
        >
          <WIDGET_PARAMETER NAME="SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION"
                PROPERTY="applicationTypeKey$selectedProgramTypeCodes"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="result$list$applicationTypeCode"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$list$description"
          />
        </CONNECT>
      </FIELD>
      <CONTAINER
        LABEL="Field.Label.Description"
        WIDTH="80"
      >
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="TEXT"
              PROPERTY="Field.Label.ApplicationFor"
            />
          </CONNECT>
        </FIELD>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="listProgramsForApplicationType"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </LIST>
  </CLUSTER>


  <CLUSTER SHOW_LABELS="false">
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAY"
        PROPERTY="displayApplicationsInd"
      />
    </CONDITION>


    <LIST
      
      STYLE="screening-select-list"
    >
      <CONTAINER SEPARATOR="Container.Separator">
        <WIDGET
          ALIGNMENT="CENTER"
          TYPE="MULTISELECT"
          WIDTH="100"
          WIDTH_UNITS="PERCENT"
        >


          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION1"
                PROPERTY="programTypeKey$programTypeCodes"
              />
            </CONNECT>
          </WIDGET_PARAMETER>


          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY1"
                PROPERTY="result$list$programTypeCode"
              />
            </CONNECT>
          </WIDGET_PARAMETER>


          <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY1"
                PROPERTY="result$selectedProgramTypeCodes"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>
      <FIELD
        LABEL="Field.Label.Name"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY1"
            PROPERTY="result$list$description"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.Description"
        WIDTH="80"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY1"
            PROPERTY="result$list$programDescription"
          />
        </CONNECT>
      </FIELD>
    </LIST>
  </CLUSTER>
</PAGE>
