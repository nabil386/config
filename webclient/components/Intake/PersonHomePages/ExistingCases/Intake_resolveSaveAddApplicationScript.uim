<!-- This page resolves between select screening page and another resolver  -->
<!-- which resolves between selecting programs or starting the screening    -->
<PAGE
  PAGE_ID="Intake_resolveSaveAddApplicationScript"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    curam.omega3.request.RequestHandler
      rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

    String context = request.getContextPath() + "/";
    String executionID = request.getParameter("executionID");
    String concernRoleID = request.getParameter("concernRoleID");
    String code = request.getParameter("code");
    String url = "";

    if(code == null) {
	  core.ScriptletMissingParamException e = new
	    core.ScriptletMissingParamException(-20002, "code");
	  throw e;
	}

    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";

    if(code.equals("SAO19001")) {
      
      url = context + "Intake_listActionableIndividualExistingCasesByConcernRolePage.do" + "?concernRoleID=" 
        + curam.omega3.request.RequestUtils.escapeURL(concernRoleID); 
        
      String encodeRedirectURL = response.encodeURL(url);
      response.sendRedirect(response.encodeURL(request.getContextPath() + "/ieg/CloseAndRedirect.jspx?redirect=" + encodeRedirectURL));      
            
     
     } else if (code.equals("SAO19002")) {
       
       url = context + "Intake_listActionableIndividualExistingCasesByConcernRolePage.do" + "?concernRoleID=" 
       + curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
       
       String encodeRedirectURL = response.encodeURL(url);
       response.sendRedirect(response.encodeURL(request.getContextPath() + "/ieg/CloseAndRedirect.jspx?redirect=" + encodeRedirectURL));      
            
     } else if (code.equals("SAO19003")) {       
       
       url = context + "RightsAndResponsibilitiesForCasePage.do?concernRoleID="+ curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
       url+="&amp;executionID="+ curam.omega3.request.RequestUtils.escapeURL(executionID);

       url += "&amp;" + rh.getSystemParameters();
       response.sendRedirect(response.encodeRedirectURL(url));
     }
        


  </JSP_SCRIPTLET>
</PAGE>
