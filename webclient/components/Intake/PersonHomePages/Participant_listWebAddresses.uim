<!-- List all the web addresses for a person -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="Participant_listWebAddresses"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Addresses"
      STYLE="in-page-link"
    >


      <LINK
        PAGE_ID="Participant_listAllAddress"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.PhoneNumbers"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Participant_listPhoneNumbers"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.EmailAddresses"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Participant_listEmailAddresses"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.WebAddresses"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Participant_listWebAddresses"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <SERVER_INTERFACE
    CLASS="Participant"
    NAME="WEBADDRESS"
    OPERATION="listWebAddress"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="WEBADDRESS"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <!-- Link to page displaying editable details of the web address-->


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Participant_modifyWebAddressFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="WEBADDRESS"
              PROPERTY="concernRoleWebAddressID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleWebAddressID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Delete"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Participant_cancelWebAddress"
        >
          <!-- Map parameters in order to delete correct Web Address -->
          <CONNECT>
            <SOURCE
              NAME="WEBADDRESS"
              PROPERTY="concernRoleWebAddressID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleWebAddressID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="WEBADDRESS"
              PROPERTY="webAddressVersionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="webAddressVersionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <DETAILS_ROW>


      <INLINE_PAGE PAGE_ID="Participant_viewWebAddress">
        <CONNECT>
          <SOURCE
            NAME="WEBADDRESS"
            PROPERTY="concernRoleWebAddressID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleWebAddressID"
          />
        </CONNECT>
      </INLINE_PAGE>


    </DETAILS_ROW>


    <FIELD
      LABEL="Field.Label.WebPrimary"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="primaryWebAddressInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.WebAddress"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="webAddress"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.WebAddressType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="typeCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.WebAddressFrom"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.WebAddressTo"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="WEBADDRESS"
          PROPERTY="statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <!--BEGIN, CR00260856, KG -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Participant_createWebAddress"
      >
        <!--END, CR00260856 -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
