<PAGE
  PAGE_ID="Individual_resolveHomePage"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <JSP_SCRIPTLET>
        
        
        curam.omega3.request.RequestHandler rh = 
        curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
        
        String context = request.getContextPath() + "/";
        String concernRoleID = request.getParameter("concernRoleID");
        context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(
        pageContext.getSession()).getLocale() + "/"; 
        
        curam.interfaces.IndividualPkg.Individual_getIndividualHomePage_TH th = new curam.interfaces.IndividualPkg.Individual_getIndividualHomePage_TH();
        th.setFieldValue(th.key$concernRoleID_idx, concernRoleID);
        
        th.callServer();
        
        String homePage = th.getFieldValue(th.result$contextPanelPage_idx);
        
        String url = context+ curam.omega3.request.RequestUtils.escapeURL(homePage) +"Page.do?concernRoleID="+  curam.omega3.request.RequestUtils.escapeURL(concernRoleID) +
        "&amp;" + rh.getSystemParameters();
        
        response.sendRedirect(response.encodeRedirectURL(url));
        
        
    </JSP_SCRIPTLET>


</PAGE>
