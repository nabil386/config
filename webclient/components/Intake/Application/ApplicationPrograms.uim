<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="ApplicationPrograms"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="DISPLAY"
    OPERATION="listProgramApplications1"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="RIGHT">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.AddProgramRequest"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="Intake_addProgramApplication"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>


    <FIELD LABEL="Field.Label.ProgramName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="programName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ProgramRequestDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="programRequestDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Disposition">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="status"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DispositionDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="disposedDateTime"
        />
      </CONNECT>
    </FIELD>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!--BEGIN,CR00264222,ZT -->
      <ACTION_CONTROL LABEL="ActionControl.Label.EditDetails">
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="editDetailsInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Application_editProgramApplicationDetails"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="programApplicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!--END,CR00264222,ZT -->


      <ACTION_CONTROL LABEL="ActionControl.Label.Withdraw">


        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="withdrawInd"
          />
        </CONDITION>


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_createProgramWithdrawalRequest"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="programApplicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="programName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programName"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="NewButton"
        LABEL="ActionControl.Label.ConfirmRequest"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="confirmRequestInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_confirmProgramWithdrawalRequest"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="programApplicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        APPEND_ELLIPSIS="false"
        IMAGE="NewButton"
        LABEL="ActionControl.Label.ViewWithdrawalRequests"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="viewWithdrawalRequestsInd"
          />
        </CONDITION>
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_viewProgramWithdrawalRequest"
          SAVE_LINK="false"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="programApplicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programApplicationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </LIST>


</PAGE>
