<!-- View the client details                     -->
<PAGE
  PAGE_ID="Application_ClientDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="intakeApplicantID"/>


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="DISPLAY"
    OPERATION="readClientDetails"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="intakeApplicantID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$intakeApplicantID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.Gender">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$gender"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00302240, JD-->
    <FIELD LABEL="Cluster.Label.Address">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$address"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00302240-->


    <FIELD LABEL="Field.Label.MaritalStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$maritalStatus"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Nationality">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$nationality"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00302240, JD-->
    <FIELD LABEL="Field.Label.PhoneNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$phoneNumber"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00302240-->


    <FIELD LABEL="Field.Label.DateAdded">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$dateAdded"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="20"
    NUM_COLS="1"
  >


    <FIELD LABEL="Field.Label.ProgramsRequested">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programsRequested"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
