<!-- View the Comments for a Withdrawal Request record                      -->
<PAGE
  PAGE_ID="Intake_viewWithdrawalRequestComments"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="programWithdrawalRequestID"/>


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="DISPLAY"
    OPERATION="readWithdrawalRequestCommentDetails"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="programWithdrawalRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programWithdrawalRequestID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="RemoveFieldPadding"
    TITLE="Cluster.Title.WithdrawalComments"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
