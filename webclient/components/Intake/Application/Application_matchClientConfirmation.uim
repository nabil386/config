<!--Page to Match client confirmation.                  -->
<PAGE
  PAGE_ID="Application_matchClientConfirmation"
  WINDOW_OPTIONS="width=800,height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="originalConcernRoleID"/>
  <PAGE_PARAMETER NAME="duplicateConcernRoleID"/>
  <PAGE_PARAMETER NAME="intakeApplicantID"/>
  <PAGE_PARAMETER NAME="applicationID"/>


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="DISPLAYWIZARD"
    OPERATION="readMatchClientWizardDetails"
    PHASE="DISPLAY"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="ACTION"
        PROPERTY="result$dtls$informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="NAMEMERGEDISPLAY"
    OPERATION="validateAlternateNameMerge"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="NAMEMERGEDISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="NAMEMERGEDISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="REGISDISPLAY"
    OPERATION="readParticipantDetailsForMerge"
  />
  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="DUPLIDISPLAY"
    OPERATION="readParticipantDetailsForMerge"
  />


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="ACTION"
    OPERATION="matchClient"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="REGISDISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="DUPLIDISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="intakeApplicantID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="intakeApplicantID"
    />
  </CONNECT>


  <SERVER_INTERFACE
