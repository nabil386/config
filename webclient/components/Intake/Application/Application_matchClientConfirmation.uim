  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="ADDRESSMERGEDISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="ADDRESSMERGEDISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <!-- BEGIN,CR00292037,AM -->
    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <!--END,CR00292037  -->
      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="PERSONMERGEDISPLAY"
              PROPERTY="confirmationMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="NAMEMERGEDISPLAY"
          PROPERTY="isAlternateNameMergeRequired"
        />
      </CONDITION>
      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="NAMEMERGEDISPLAY"
              PROPERTY="confirmationNameMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="CLIENTREFMERGEDISPLAY"
          PROPERTY="isMergeClientReferenceRequired"
        />
      </CONDITION>
      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="CLIENTREFMERGEDISPLAY"
              PROPERTY="confirmationClientMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="PERSONMERGEDISPLAY"
          PROPERTY="isGenderMergeRequired"
        />
      </CONDITION>
      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="PERSONMERGEDISPLAY"
              PROPERTY="confirmationGenderMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="PERSONMERGEDISPLAY"
          PROPERTY="isMaritalMergeRequired"
        />
      </CONDITION>


      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="PERSONMERGEDISPLAY"
              PROPERTY="confirmationMaritalMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="PERSONMERGEDISPLAY"
          PROPERTY="isNationalityMergeRequired"
        />
      </CONDITION>


      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="PERSONMERGEDISPLAY"
              PROPERTY="confirmationNationalityMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="PERSONMERGEDISPLAY"
          PROPERTY="isCountryOfBirthMergeRequired"
        />
      </CONDITION>
      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="PERSONMERGEDISPLAY"
              PROPERTY="confirmationCountryMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="ADDRESSMERGEDISPLAY"
          PROPERTY="isAddressMergeRequired"
        />
      </CONDITION>


      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="ADDRESSMERGEDISPLAY"
              PROPERTY="confirmationAddressMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CLUSTER
      SHOW_LABELS="false"
      STYLE="outer-cluster-borderless"
    >
      <CONDITION>
        <IS_TRUE
          NAME="ADDRESSMERGEDISPLAY"
          PROPERTY="isPhoneNumberMergeRequired"
        />
      </CONDITION>


      <CONTAINER>
        <FIELD>
          <CONNECT>
            <SOURCE
              NAME="ADDRESSMERGEDISPLAY"
              PROPERTY="confirmationPhoneMessageOpt"
            />
          </CONNECT>
        </FIELD>
      </CONTAINER>
    </CLUSTER>


    <CONTAINER>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="Conformation.Text.Final"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


