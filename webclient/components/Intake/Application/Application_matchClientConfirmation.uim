  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="duplicateConcernRoleID"
    />
    <TARGET
      NAME="CLIENTREFMERGEDISPLAY"
      PROPERTY="duplicateConcernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="originalConcernRoleID"
    />
    <TARGET
      NAME="CLIENTREFMERGEDISPLAY"
      PROPERTY="originalConcernRoleID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="PERSONMERGEDISPLAY"
    OPERATION="validatePersonDetailsForMerge"
