<!-- Application Smart panel page -->
<PAGE PAGE_ID="AdvisorSmartPanel">


  <SERVER_INTERFACE
    CLASS="Advisor"
    NAME="DISPLAY"
    OPERATION="getAdvice"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Label.AdviceList"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$adviceXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
