<!-- It allows the user to select denied program application(s) to be appealed  -->
<PAGE
  PAGE_ID="Appeal_selectProgramDenialsForJudicialReview"
  WINDOW_OPTIONS="width=760,height=480"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="ApplicationAppeal"
    NAME="DISPLAY"
    OPERATION="listDeniedProgramApplicationsForJudicialReview"
    PHASE="DISPLAY"
  />


  <ACTION_SET
    ALIGNMENT="RIGHT"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="Continue"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Appeal_resolveJudicialReviewObjectAppealLevel"
        SAVE_LINK="FALSE"
      >
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="implCaseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$appealObjString"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="appealObjectsDelimitedList"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="Appeal_selectProgramDenialsView.vim"/>


</PAGE>
