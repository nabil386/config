<!-- This page displays Income Support In progess  preview panel        -->
<PAGE
  PAGE_ID="ApplicationISInprogressPreviewPanel"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="getApplicationListPreviewPanelDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <!-- BEGIN,CR00292037,AM -->
  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.Clients">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="clients"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="programs"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.LastSavedDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="lastSavedDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MethodofReceipt">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="received"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.PreferredContact">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="preferedContact"
        />
      </CONNECT>
    </FIELD>
    <FIELD CONTROL="SKIP"/>


  </CLUSTER>
  <!-- END,CR00292037 -->


</PAGE>
