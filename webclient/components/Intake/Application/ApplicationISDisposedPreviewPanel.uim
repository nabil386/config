<!--This page displays Income Support disposed preview panel       -->
<PAGE
  PAGE_ID="ApplicationISDisposedPreviewPanel"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="getApplicationListPreviewPanelDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.Clients">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="clients"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="programs"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.DisposedDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="disposedDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ApplicationDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="applicationDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.SubmittedDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="submittedDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MethodofReceipt">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="received"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PreferredContact">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="preferedContact"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.RelatedCase">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedCase"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.OutstandingVerifications">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="verificationsOutstanding"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Issues">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="issues"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Expedited">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="expedited"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Emergency">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="emergency"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
