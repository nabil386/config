<!-- This page decides whether the select application type page should be   -->
<!-- displayed or bypassed if there is only one application type, therefore -->
<!-- only one choice, available.												                    -->
<PAGE
  PAGE_ID="Intake_resolveStartApplication"
  WINDOW_OPTIONS="width=1200,height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    curam.omega3.request.RequestHandler
    rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
    String context = request.getContextPath() + "/";
    String concernRoleID = request.getParameter("concernRoleID");
    String url = "";
    
    if(concernRoleID == null) {
       core.ScriptletMissingParamException e = new
       core.ScriptletMissingParamException(-20002, "concernRoleID");
       throw e;
    }
    
    curam.interfaces.ApplicationPkg.Application_resolveStartApplication_TH
    th = new curam.interfaces.ApplicationPkg.Application_resolveStartApplication_TH();
    
    th.setFieldValue(th.key$concernRoleID_idx, concernRoleID);
    th.callServer();
       
    String invokeSelectApplicationPage = th.getFieldValue(th.result$invokeSelectApplicationTypePage_idx);
    boolean invokeSelectApplicationPageInd = Boolean.valueOf(invokeSelectApplicationPage).booleanValue();
    
    String invokeResolveApplicationType = th.getFieldValue(th.result$invokeResolveApplicationType_idx);
    boolean invokeResolveApplicationTypeInd = Boolean.valueOf(invokeResolveApplicationType).booleanValue();
     
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    
    if(invokeResolveApplicationTypeInd) {
    
      url = context + "ResolveApplicationProgramsForApplicationPage.do?" + "intakeApplicationTypeID=" + curam.omega3.request.RequestUtils.escapeURL(th.getFieldValue(th.result$applicationTypeCode_idx));
      url += "&amp;" + "concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
    
    } else if(invokeSelectApplicationPageInd) {
    
      url = context + "Intake_listApplicationTypesPage.do?";
      url += "&amp;" + "concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
    
    } 
    //BEGIN CR00291523 MN
    else
    {
    url = context + "Intake_noApplicationDefinedPage.do?";
    }
    //END CR00291523
    
    url += "&amp;" + rh.getSystemParameters();
    response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
