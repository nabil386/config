<!-- Display the application existing case preview panel.                   -->
<PAGE
  PAGE_ID="Application_listExistingCaseMembers"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseIDKey$caseID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="ApplicationExistingCases"
    NAME="DISPLAY"
    OPERATION="getApplicationExistingCasePreviewPanelDetails"
  />


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.CaseDetails"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Application_viewExistingCasePreviewPanel"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Programs"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Application_listExistingCasePrograms"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.CaseMembers"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Application_listExistingCaseMembers"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <!-- BEGIN,CR00291376,AM-->
  <LIST SORTABLE="true">
    <!-- END,CR00291376,AM-->
    <CONTAINER
      LABEL="Field.title.casemembers"
      WIDTH="100"
    >
      <FIELD
        LABEL="Field.title.casemembers"
        WIDTH="100"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$memberDetails$memberList$memberFullName"
          />
        </CONNECT>
        <LINK PAGE_ID="Individual_resolveConcernRoleTypeHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$memberDetails$memberList$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </CONTAINER>
  </LIST>


</PAGE>
