<!-- Display the application existing case preview panel.                   -->
<PAGE
  PAGE_ID="Application_listExistingCasePrograms"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseIDKey$caseID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="ApplicationExistingCases"
    NAME="DISPLAY"
    OPERATION="getApplicationExistingCasePreviewPanelDetails"
  />


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.CaseDetails"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Application_viewExistingCasePreviewPanel"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Programs"
      STYLE="in-page-current-link"
    >
      <LINK
        PAGE_ID="Application_listExistingCasePrograms"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.CaseMembers"
      STYLE="in-page-link"
    >
      <LINK
        PAGE_ID="Application_listExistingCaseMembers"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <LIST>
    <FIELD
      LABEL="Field.Title.CaseReference"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programDetails$progamList$caseReference"
        />
      </CONNECT>
      <LINK
        URI_SOURCE_NAME="DISPLAY"
        URI_SOURCE_PROPERTY="result$programDetails$progamList$programPageURL"
      />
    </FIELD>


    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="16"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programDetails$progamList$productType"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.PrimaryClient"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programDetails$progamList$primaryClientNameAndAge"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.ExpectedEndDate"
      WIDTH="24"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programDetails$progamList$expectedEndDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Status"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$programDetails$progamList$statusCode"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
