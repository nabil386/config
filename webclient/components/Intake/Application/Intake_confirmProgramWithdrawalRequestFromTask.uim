<!-- This Page views a particular program withdrawal request and conform   -->
<PAGE
  PAGE_ID="Intake_confirmProgramWithdrawalRequestFromTask"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="programWithdrawalRequestID"/>
  <PAGE_PARAMETER NAME="applicationID"/>


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="DISPLAY"
    OPERATION="readWithdrawalRequestDetailsForConfirmation"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="ACTION"
    OPERATION="confirmWithdrawalRequest"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="programWithdrawalRequestID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programWithdrawalRequestID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="programWithdrawalRequestID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="programWithdrawalRequestID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >


      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="ApplicationPrograms1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <INCLUDE FILE_NAME="Intake_confirmProgramWithdrawalRequestView.vim"/>


</PAGE>
