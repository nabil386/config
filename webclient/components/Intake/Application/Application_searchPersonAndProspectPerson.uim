  />


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="DISPLAY"
    OPERATION="searchPersonAndProspectPersonsForMerge"
  />


  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="actionControlID"
