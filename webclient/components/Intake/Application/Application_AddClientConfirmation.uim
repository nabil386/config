<!--Page to add the client for the application                  -->
<PAGE
  PAGE_ID="Application_AddClientConfirmation"
  WINDOW_OPTIONS="width=800,height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <MENU MODE="WIZARD_PROGRESS_BAR">
    <CONNECT>
      <SOURCE
        NAME="DISPLAYWIZARD"
        PROPERTY="wizardMenu"
      />
    </CONNECT>
  </MENU>


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="DISPLAYWIZARD"
    OPERATION="readAddClientWizardDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="applicationID"/>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MatchClient"
    NAME="DISPLAY"
    OPERATION="readParticipantDetailsForMerge"
  />


  <SERVER_INTERFACE
    CLASS="ApplicationClient"
    NAME="ACTION"
    OPERATION="addClient"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Conformation.Text.Initial"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN,CR00292037,AM -->
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Conformation.Text.Second"
        />
      </CONNECT>
    </FIELD>
    <!-- END,CR00292037 -->
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="13"
    STYLE="outer-cluster-borderless"
  >
    <FIELD
      LABEL="Field.Title.DateAdded"
      WIDTH="25"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dateAdded"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
    TAB_ORDER="ROW"
    TITLE="Cluster.Title.ClientDetails"
  >


    <FIELD LABEL="Field.Title.FirstName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="firstName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Title.LastName"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="lastName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.DateOfBirth">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="dateOfBirth"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.ReferenceNumber"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reference"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Gender">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="gender"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Title.MaritalStatus"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="maritalStatus"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
    TAB_ORDER="ROW"
    TITLE="Cluster.Title.Contact"
  >


    <FIELD LABEL="Field.Label.PrimaryAddressData">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="address"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.PhoneNumber"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="phoneNumber"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <ACTION_SET TOP="false">


    <ACTION_CONTROL
      ALIGNMENT="LEFT"
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Back"
    >
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Application_registerPersonAndAddAsClient"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="applicationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="applicationID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      ALIGNMENT="RIGHT"
      IMAGE="RegisterButton"
      LABEL="ActionControl.Label.Finish"
      TYPE="SUBMIT"
    >
      
    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
