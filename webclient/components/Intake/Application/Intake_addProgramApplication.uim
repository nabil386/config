<!-- This Page creates a program to a particular application                -->
<PAGE
  PAGE_ID="Intake_addProgramApplication"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAYTYPE"
    OPERATION="getConfiguredProgramTypesForApplicationType"
  />


  <SERVER_INTERFACE
    CLASS="ProgramApplication"
    NAME="ACTION"
    OPERATION="addProgram"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAYTYPE"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="45">


    <FIELD LABEL="Field.Label.Program">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$list$programTypeCode"
          NAME="DISPLAYTYPE"
          PROPERTY="result$list$programDescription"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="programType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ProgramRequestDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="programRequestDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
