<!-- Modifies the application details -->
<PAGE
  PAGE_ID="Application_editApplicationDetails"
  WINDOW_OPTIONS="width=550"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="applicationID"/>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00291216, PC -->
  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="readModifyApplicationSpecificDetails"
  />


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="ACTION"
    OPERATION="modifyApplicationSpecificDetails"
    PHASE="ACTION"
  />
  <!-- END, CR00291216 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="35"
    STYLE="outer-cluster-borderless"
  >


    <!-- BEGINING OF CR00273532 Manu-->
    <FIELD
      LABEL="Field.Label.ApplicationDate"
      WIDTH="90"
    >
      <!-- END OF CR00273532 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="applicationDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="applicationDate"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGINING OF CR00273532 Manu-->
    <FIELD
      LABEL="Field.Label.ApplicationMethod"
      WIDTH="90"
    >
      <!-- END OF CR00273532 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="applicationMethod"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="applicationMethod"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGINING OF CR00273532 Manu-->
    <FIELD
      LABEL="Field.Label.PreferredContact"
      WIDTH="90"
    >
      <!-- END OF CR00273532 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="preferredContact"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="preferredContact"
        />
      </CONNECT>
    </FIELD>
    <!--BEGIN, CR00302884,JD  -->
    <FIELD
      LABEL="Field.label.language"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="90"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSP"
          PROPERTY="interpreterLanguage"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTIONSP"
          PROPERTY="interpreterLanguage"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="ACTIONSP"
    OPERATION="editSpecialRequirements"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAYSP"
    OPERATION="readSpecialRequirements"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAYSP"
      PROPERTY="applicationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTIONSP"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CLUSTER
    BEHAVIOR="NONE"
    LAYOUT_ORDER="FIELD"
    SHOW_LABELS="false"
    TITLE="Field.label.editSplRequirement"
  >
    <FIELD HEIGHT="5">
      <CONNECT>
        <SOURCE
          NAME="DISPLAYSP"
          PROPERTY="specialRequirement"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTIONSP"
          PROPERTY="specialRequirements"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- END, CR00302884-->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
        </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
            
        </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
