<!-- This page displays the default Application preview panel               -->
<PAGE
  PAGE_ID="ApplicationDefaultPreviewPanel"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Application"
    NAME="DISPLAY"
    OPERATION="getApplicationListPreviewPanelDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$applicationID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="15"
    NUM_COLS="1"
  >
    <FIELD LABEL="Field.Label.Clients">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="clients"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="programs"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.MethodOfReceipt">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="received"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
