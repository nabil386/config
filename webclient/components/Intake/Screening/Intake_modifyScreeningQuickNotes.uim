<!-- Modifies the Screening Note Details.                     -->
<PAGE
  PAGE_ID="Intake_modifyScreeningQuickNotes"
  WINDOW_OPTIONS="width=680,height=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>
  
  <SERVER_INTERFACE
    CLASS="QuickNotes"
    NAME="DISPLAY_STATUS"
    OPERATION="readNoteLockedStatus"
  />

  <CONNECT>
    <SOURCE  NAME="PAGE"  PROPERTY="quickNotesID" />
    <TARGET  NAME="DISPLAY_STATUS" PROPERTY="quickNotesID" />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="FALSE"
  >
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <CONDITION>
        <IS_FALSE NAME="DISPLAY_STATUS" PROPERTY="noteLockedInd" />
      </CONDITION>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <INCLUDE FILE_NAME="Intake_modifyNoteView.vim"/>


</PAGE>
