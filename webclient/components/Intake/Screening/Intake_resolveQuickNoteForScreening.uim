<PAGE
  PAGE_ID="Intake_resolveQuickNoteForScreening"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    curam.omega3.request.RequestHandler
    rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
    
    String context = request.getContextPath() + "/";
    String relatedID = request.getParameter("relatedID");
    String quickNotesType = request.getParameter("quickNotesType");
    String url = "";
    
    if(relatedID == null) {
    core.ScriptletMissingParamException e = new
    core.ScriptletMissingParamException(-20002, "relatedID");
    throw e;
    }
    
    curam.interfaces.IntakeScreeningPkg.IntakeScreening_isScreeningCompleted_TH
    th = new curam.interfaces.IntakeScreeningPkg.IntakeScreening_isScreeningCompleted_TH();
    
    th.setFieldValue(th.key$screeningExecutionID_idx, relatedID);
    th.callServer();
    
     String screeningCompl = th.getFieldValue(th.result$isScreeningStatusComplete_idx);
	boolean screeningComplInd = Boolean.valueOf(screeningCompl).booleanValue();
    
    context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
    if(screeningComplInd) {
    url = context + "Intake_createScreeningNotePage.do?" + "relatedID=" + curam.omega3.request.RequestUtils.escapeURL(relatedID);
    url += "&amp;" + "quickNotesType=" + curam.omega3.request.RequestUtils.escapeURL(quickNotesType);
     }
     else {
      url = context + "Intake_validateScreeningStatusPage.do?";
     }
 
    url += "&amp;" + rh.getSystemParameters();
    response.sendRedirect(response.encodeRedirectURL(url));
    
  </JSP_SCRIPTLET>
</PAGE>
