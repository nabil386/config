<!-- This page allows you to resume In Progress Screening or to cancel      -->
<!-- In Progress Screening And Start New Screening.                         -->
<PAGE
  PAGE_ID="Intake_confirmStartNewScreening"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE STYLE="screening-title">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="ACTION"
    OPERATION="validateResolveStartNewScript"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="DISPLAY"
    OPERATION="getOptionsForOverrideScreening"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="concernRoleID"/>
  <PAGE_PARAMETER NAME="fullName"/>
  <PAGE_PARAMETER NAME="screeningTypeID"/>
  <PAGE_PARAMETER NAME="programTypeIDs"/>
  <PAGE_PARAMETER NAME="screeningExecutionID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="screeningExecutionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$screeningExecutionID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="screening-image-cluster"
  >
    <CONTAINER>
      <IMAGE
        IMAGE="Icon.Screening.PreviouslySaved"
        LABEL="Icon.Screening.New"
      />
      <FIELD>
        <LABEL>
          <CONNECT>
            <SOURCE
              NAME="TEXT"
              PROPERTY="New.Screening.Text"
            />
          </CONNECT>
        </LABEL>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="New.Screening.Text"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.SaveScreening"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST STYLE="screening-select-list">
    <CONTAINER WIDTH="5">
      <WIDGET
        ALIGNMENT="CENTER"
        TYPE="SINGLESELECT"
        WIDTH="100"
        WIDTH_UNITS="PERCENT"
      >
        <WIDGET_PARAMETER NAME="SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="details$code"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$list$code"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$description"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <ACTION_SET
      ALIGNMENT="RIGHT"
      TOP="false"
    >


      <ACTION_CONTROL
        IMAGE="NextButton"
        LABEL="ActionControl.Label.Next"
        STYLE="screening-next-link"
        TYPE="SUBMIT"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Intake_resolveConfirmStartNewScreeningScript"
          WINDOW_OPTIONS="width=1200,height=600"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="invokeResumeInd"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="invokeResumeInd"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="executionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="executionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="fullName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fullName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="screeningTypeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="screeningTypeID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="programTypeIDs"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programTypeIDs"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>


</PAGE>
