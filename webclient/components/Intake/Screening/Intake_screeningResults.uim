<!-- This page displays the screening results.                               -->
<PAGE
  PAGE_ID="Intake_screeningResults"
  WINDOW_OPTIONS="width=1200,height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="screeningExecutionID"/>


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="DISPLAY"
    OPERATION="generateResults"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="screeningExecutionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$screeningExecutionID"
    />
  </CONNECT>


  <CONTAINER>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$alertMessagesList"
        />
      </CONNECT>
    </FIELD>
  </CONTAINER>


  <!-- BEGIN, CR00261034, KG -->
  <LIST
    PAGINATED="false"
    TITLE="Cluster.Title.Eligible"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="hasInternalAgencyProgramsInd"
      />
    </CONDITION>
    <FIELD
      LABEL="Field.Title.ProgramName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="internalAgencyProgramList$list$programName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Result">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="internalAgencyProgramList$list$summaryText"
        />
      </CONNECT>
    </FIELD>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.Apply"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ResolveScreeningProgramsForApplication"
          WINDOW_OPTIONS="width=1200,height=600"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="internalAgencyProgramList$list$programTypeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programTypeID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="screeningExecutionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="screeningExecutionID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
  </LIST>


  <LIST
    PAGINATED="false"
    TITLE="Cluster.Title.Ineligible"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="hasExternalAgencyProgramsInd"
      />
    </CONDITION>
    <FIELD
      LABEL="Field.Title.ProgramName"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="externalAgencyProgramList$list$programName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Title.Result">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="externalAgencyProgramList$list$summaryText"
        />
      </CONNECT>
    </FIELD>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        IMAGE="DeleteButton"
        LABEL="ActionControl.Label.CreateRefferal"
      >


        <LINK
          OPEN_MODAL="true"
          PAGE_ID="SearchForExternalAgencyBranchOffering"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="externalAgencyProgramList$list$programTypeID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="programType"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="Constant.NoValue"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="state"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="Constant.NoValue"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="city"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="CONSTANT"
              PROPERTY="Constant.NoValue"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="zip"
            />
          </CONNECT>
        </LINK>


      </ACTION_CONTROL>
    </ACTION_SET>


  </LIST>
  <!-- END, CR00261034 -->


</PAGE>
