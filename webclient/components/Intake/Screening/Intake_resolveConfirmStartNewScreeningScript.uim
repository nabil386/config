<PAGE
  PAGE_ID="Intake_resolveConfirmStartNewScreeningScript"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <JSP_SCRIPTLET>

      curam.omega3.request.RequestHandler
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      String screeningTypeID = request.getParameter("screeningTypeID");
      String programTypeIDs = request.getParameter("programTypeIDs");
      String concernRoleID = request.getParameter("concernRoleID");
      String fullName = request.getParameter("fullName");
      String executionID = request.getParameter("executionID");
      boolean invokeResumeInd = Boolean.valueOf(request.getParameter("invokeResumeInd")).booleanValue();
      programTypeIDs = programTypeIDs.replaceAll("\\t", ",");
      String url = "";


      if (invokeResumeInd) {
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      url = context + "Intake_resumeScreeningPage.do?" + "executionID=" + curam.omega3.request.RequestUtils.escapeURL(executionID);
          url += "&amp;o3frame=modal";

        }
        else
        {

   if (screeningTypeID == null) {
   core.ScriptletMissingParamException e = new
   core.ScriptletMissingParamException(-20002, "screeningTypeID");
   throw e;
   }

   curam.interfaces.IntakeScreeningPkg.IntakeScreening_startScriptForSelectedPrograms_TH
   th = new curam.interfaces.IntakeScreeningPkg.IntakeScreening_startScriptForSelectedPrograms_TH();

   th.setFieldValue(th.startScreeningScriptKey$screeningTypeID_idx, screeningTypeID);
   th.setFieldValue(th.startScreeningScriptKey$programTypeIDs_idx, programTypeIDs);
   th.setFieldValue(th.concernRoleKey$concernRoleID_idx, concernRoleID);
   th.callServer();

   executionID = th.getFieldValue(th.result$executionID_idx);
   url = context + "ieg/Screening.do?" + "executionID=" + curam.omega3.request.RequestUtils.escapeURL(executionID);
   url += "&amp;" + "fullName=" + curam.omega3.request.RequestUtils.escapeURL(fullName);


      }

      url += "&amp;" + rh.getSystemParameters();
      response.sendRedirect(response.encodeRedirectURL(url));

  </JSP_SCRIPTLET>


</PAGE>
