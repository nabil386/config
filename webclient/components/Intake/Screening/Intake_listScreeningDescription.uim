<!-- View the description of the Screening Summary Text  for Screening Execution    -->
<PAGE
  PAGE_ID="Intake_listScreeningDescription"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_PARAMETER NAME="executionID"/>
  <PAGE_PARAMETER NAME="screeningExecutionID"/>
  <PAGE_PARAMETER NAME="screenedPrograms"/>


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="DISPLAY"
    OPERATION="getEligibleAndInEligiblePrograms"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="executionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$iegExecutionID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="screeningExecutionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$screeningExecutionID"
    />
  </CONNECT>


  <CONTAINER>
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$alertMessagesList"
        />
      </CONNECT>
    </FIELD>
  </CONTAINER>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="1"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="isCompletedInd"
      />
    </CONDITION>


    <FIELD LABEL="Field.Label.Eligible">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$eligiblePrograms"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.InEligible">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$inEligiblePrograms"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="1"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="result$isInProgressInd"
      />
    </CONDITION>


    <FIELD LABEL="Field.Label.Programs">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="screenedPrograms"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="isCancelledInd"
      />
    </CONDITION>


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.Screening.Cancelled"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
