<!-- This page allows to save the screening.                                -->
<PAGE
  PAGE_ID="Intake_confirmSaveScriptExecution"
  WINDOW_OPTIONS="width=1200,height=600"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE STYLE="screening-title">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="ACTION"
    OPERATION="quitScreening"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="IntakeScreening"
    NAME="DISPLAY"
    OPERATION="getOptionsForSaveScreening"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="executionID"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="executionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="iegExecutionID"
    />
  </CONNECT>


  <LIST STYLE="screening-select-list">
    <CONTAINER WIDTH="5">
      <WIDGET
        ALIGNMENT="CENTER"
        TYPE="SINGLESELECT"
        WIDTH="100"
        WIDTH_UNITS="PERCENT"
      >
        <WIDGET_PARAMETER NAME="SELECT_TARGET">
          <CONNECT>
            <TARGET
              NAME="ACTION"
              PROPERTY="details$code"
            />
          </CONNECT>
        </WIDGET_PARAMETER>


        <WIDGET_PARAMETER NAME="SELECT_SOURCE">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$list$code"
            />
          </CONNECT>
        </WIDGET_PARAMETER>
      </WIDGET>
    </CONTAINER>


    <FIELD LABEL="Description.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$list$description"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="true"
        PAGE_ID="Intake_listScreenings"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Cancel"
    >


      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="Intake_resumeScreening"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="executionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="executionID"
          />
        </CONNECT>
      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
