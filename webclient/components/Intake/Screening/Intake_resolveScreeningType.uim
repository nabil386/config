<!-- This page resolves between starting the screening or listing the       -->
<!-- programs to be selected for screening                                  -->
<PAGE
  PAGE_ID="Intake_resolveScreeningType"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    curam.omega3.request.RequestHandler
      rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

    String context = request.getContextPath() + "/";
    String screeningTypeID = request.getParameter("screeningTypeID");
    String concernRoleID = request.getParameter("concernRoleID");

    String programTypeIDs = "";
    String url = "";

    if(screeningTypeID == null) {
      core.ScriptletMissingParamException e = new
        core.ScriptletMissingParamException(-20002, "screeningTypeID");
      throw e;
    }

    curam.interfaces.IntakeScreeningPkg.IntakeScreening_startScript_TH
      th = new curam.interfaces.IntakeScreeningPkg.IntakeScreening_startScript_TH();

    th.setFieldValue(th.key$screeningTypeID_idx, screeningTypeID);
    th.setFieldValue(th.concernRoleKey$concernRoleID_idx, concernRoleID);
    th.callServer();

    String invokeStartScript = th.getFieldValue(th.result$invokeStartScript_idx);
	boolean invokeStartScriptInd = Boolean.valueOf(invokeStartScript).booleanValue();

    String invokeSelectProgramsPage = th.getFieldValue(th.result$invokeSelectProgramsPage_idx);
	boolean invokeSelectProgramsPageInd = Boolean.valueOf(invokeSelectProgramsPage).booleanValue();

    String executionID = th.getFieldValue(th.result$executionID_idx);

    if(invokeSelectProgramsPageInd) {

      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
      url = context + "Intake_listProgramItemsForScreeningPage.do?" + "screeningTypeID=" + curam.omega3.request.RequestUtils.escapeURL(screeningTypeID);
      url += "&amp;" + "concernRoleID=" + curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
      url += "&amp;" + "programTypeIDs=" + curam.omega3.request.RequestUtils.escapeURL(programTypeIDs);

    } else if(invokeStartScriptInd){

     url = context + "ieg/Screening.do?" + "executionID=" + curam.omega3.request.RequestUtils.escapeURL(executionID);
    }

    url += "&amp;" + rh.getSystemParameters();
    response.sendRedirect(response.encodeRedirectURL(url));

  </JSP_SCRIPTLET>
</PAGE>
