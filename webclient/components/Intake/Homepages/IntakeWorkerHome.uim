<!-- Displays the list of pods - Find Client, My Appointments, My BookMarks,My Team    -->
<!-- Today's Tasks, Online Application Work Queue-->
<PAGE
  COMPONENT_STYLE="page-omit-sidebar"
  PAGE_ID="IntakeWorkerHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="Organization"
    NAME="ANNOUNCEMENTDISPLAY"
    OPERATION="getAnnouncementsAsXML"
  />


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="announcements-cluster"
    WIDTH="100"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="ANNOUNCEMENTDISPLAY"
          PROPERTY="result$contentXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="IntakeWorkerHome"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageID$pageID"
    />
  </CONNECT>


  <INCLUDE FILE_NAME="PodContainer.vim"/>


</PAGE>
