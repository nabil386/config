<!-- Views a Case PCR record.                                       -->
<PAGE
  PAGE_ID="Intake_viewCasePCR"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="PCR"
    NAME="DISPLAY"
    OPERATION="viewCasePCRDetails"
  />


  <PAGE_PARAMETER NAME="casePCRID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="casePCRID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="casePCRID"
    />
  </CONNECT>


  <!-- BEGINING OF CR00273585 MN -->
  <LIST
    PAGINATED="false"
    
  >
    <!-- END OF CR00273585 -->
    <CONDITION>
      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="creoleExecutionInd"
      />
    </CONDITION>
    <FIELD
      LABEL="Field.Title.Assessment"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="assessment"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Reason"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reason"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Rating"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="rate"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <!-- BEGIN, CR00274042 MN -->
  <LIST
    PAGINATED="false"
    
  >
    <!-- END, CR00274042 MN -->
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAY"
        PROPERTY="creoleExecutionInd"
      />
    </CONDITION>


    <FIELD
      LABEL="Field.Title.Assessment"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="assessment"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.Reason"
      WIDTH="80"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="reason"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</PAGE>
