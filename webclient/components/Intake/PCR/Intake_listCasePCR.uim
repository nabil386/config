<!-- Lists PCR records.                                       -->
<PAGE
  PAGE_ID="Intake_listCasePCR"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="PCR"
    NAME="DISPLAY"
    OPERATION="listCasePCR"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="PCR"
    NAME="ACTION"
    OPERATION="assess"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="AddButton"
      LABEL="ActionControl.Label.Assess"
      TYPE="SUBMIT"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="manualAssessInd"
        />
      </CONDITION>


      <LINK PAGE_ID="Intake_listCasePCR"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="AddButton"
      LABEL="ActionControl.Label.EnterPCR"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="enterPCRInd"
        />
      </CONDITION>
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="PCR_manualCasePCR"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <FIELD ALIGNMENT="CENTER">
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="report"
      />
    </CONNECT>
  </FIELD>


  <CLUSTER TITLE="Cluster.Title.Assessments">
    <!-- BEGIN,CR00261060 -->
    <LIST PAGINATED="true">


      <!-- END,CR00261060 -->
      <FIELD
        LABEL="Field.Title.Date"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="executionDateTime"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Title.Priority"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="priorityClassification"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Title.Complexity"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="complexityClassification"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Title.Risk"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="riskClassification"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Title.AssessedBy"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="assessedByFullName"
          />
        </CONNECT>


      </FIELD>


      <DETAILS_ROW>
        <INLINE_PAGE PAGE_ID="Intake_viewCasePCR">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="casePCRID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="casePCRID"
            />
          </CONNECT>
        </INLINE_PAGE>
      </DETAILS_ROW>


    </LIST>
  </CLUSTER>


</PAGE>
