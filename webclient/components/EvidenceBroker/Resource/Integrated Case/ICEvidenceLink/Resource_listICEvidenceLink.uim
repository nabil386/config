<!-- This page lists the Evidence types associated with the selected        -->
<!-- integrated case.                                                       -->
<PAGE PAGE_ID="Resource_listICEvidenceLink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="IntegratedCase" NAME="DISPLAY" OPERATION="listICEvidenceLink" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="adminIntegratedCaseID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="adminIntegratedCaseID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="LEFT" BOTTOM="false" TOP="true">


    <ACTION_CONTROL IMAGE="NewButton" LABEL="ActionControl.Label.New">


      <LINK OPEN_MODAL="true" PAGE_ID="Resource_createEvidenceMetadataAndICEvidenceLink">


        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
          <TARGET NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
        </CONNECT>


      </LINK>


    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="AddExisting" LABEL="ActionControl.Label.AddExisting">


      <LINK OPEN_MODAL="true" PAGE_ID="Resource_addExistingEvidenceMetadata">


        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
          <TARGET NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
        </CONNECT>


      </LINK>


    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Remove" TYPE="ACTION">
        <LINK OPEN_MODAL="true" PAGE_ID="Resource_cancelICEvidenceLink" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="adminICEvidenceLinkID"/>
            <TARGET NAME="PAGE" PROPERTY="adminICEvidenceLinkID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
            <TARGET NAME="PAGE" PROPERTY="adminIntegratedCaseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="evidenceType"/>
            <TARGET NAME="PAGE" PROPERTY="evidenceType"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.Type" WIDTH="60">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="evidenceType"/>
      </CONNECT>
    </FIELD>


    <!--BEGIN, CR00119038 , NP-->
    <FIELD LABEL="Field.Label.SharedInd">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="shareableInd"/>
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Field.Label.Shareable">


      <ACTION_CONTROL LABEL="ActionControl.Label.Set">
        <CONDITION>
          <IS_FALSE NAME="DISPLAY" PROPERTY="shareableInd"/>
        </CONDITION>


        <LINK OPEN_MODAL="true" PAGE_ID="ICEvidenceLink_setShareable" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="adminICEvidenceLinkID"/>
            <TARGET NAME="PAGE" PROPERTY="adminICEvidenceLinkID"/>
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Unset">
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="shareableInd"/>
        </CONDITION>
        <LINK OPEN_MODAL="true" PAGE_ID="ICEvidenceLink_unsetShareable" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="adminICEvidenceLinkID"/>
            <TARGET NAME="PAGE" PROPERTY="adminICEvidenceLinkID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <!--END, CR00119038-->


  </LIST>
</PAGE>