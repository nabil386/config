<!-- This page is invoked when the supervisor selects the 'synchronization  -->
<!-- complete' link on the case participant creation evidence  -->
<!-- synchronization workflow supervisor activity.  -->
<PAGE
  PAGE_ID="WorkflowEvents_supervisorCaseParticipantCreationSynchronizationComplete"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="contextDescription"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="EvidenceBroker"
    NAME="ACTION"
    OPERATION="supervisorCaseParticipantCreationSynchronizeTaskComplete"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    >     
      </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.No">
      </ACTION_CONTROL>


  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false">


    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.ConfirmMessage"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
