<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Identical evidence comparison screen                                   -->
<PAGE
  PAGE_ID="EvidenceBroker_selectIdenticalEvidenceForComparison"
  WINDOW_OPTIONS="width=1000"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="evidenceDescriptorID"/>
  <PAGE_PARAMETER NAME="wizardID"/>


  <SERVER_INTERFACE
    CLASS="EvidenceBroker"
    NAME="DISPLAY"
    OPERATION="getIdenticalEvidencesForComparison"
  />


  <SERVER_INTERFACE
    CLASS="EvidenceBroker"
    NAME="DISPLAY1"
    OPERATION="getContextDetailsForIdenticalEvidenceComparison"
  />


  <SERVER_INTERFACE
    ACTION_ID_PROPERTY="actionString"
    CLASS="EvidenceBroker"
    NAME="ACTION"
    OPERATION="getSelectedIdenticalEvidencesToWizard"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceDescriptorID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$evidenceDescriptorID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceDescriptorID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$evidenceDescriptorID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="wizardID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="wizardID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <TITLE>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="evidenceTypeCode"
        />
      </CONNECT>
    </TITLE>
    <FIELD CONTROL="SKIP">        
      </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >


    <LIST>
      <TITLE>
        <CONNECT>
          <SOURCE
            NAME="TEXT"
            PROPERTY="List.Label.EvidenceSelectionOpen"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY1"
            PROPERTY="numberOfEvidences"
          />
        </CONNECT>
      </TITLE>


      <CONTAINER
        ALIGNMENT="CENTER"
        LABEL="Container.Label.Action"
      >
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="evidenceID"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_INITIAL">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY1"
                PROPERTY="preSelectedEvidencesList"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION"
                PROPERTY="selectedEvidencesList"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD LABEL="Field.Label.CaseMember">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="caseMemberName"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.Description">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="summary"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.ChangeSummary">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="changeDescription"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.Updates">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="updates"
          />
        </CONNECT>
      </FIELD>


      <FIELD LABEL="Field.Label.LatestActivity">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="latestActivity"
          />
        </CONNECT>
      </FIELD>


    </LIST>


  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      ACTION_ID="next"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <LINK
        DISMISS_MODAL="false"
        OPEN_MODAL="false"
        PAGE_ID="EvidenceBroker_compareSelectedIdenticalEvidence"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="evidenceDescriptorID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceDescriptorID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$wizardID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="wizardID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CloseButton"
      LABEL="ActionControl.Label.Close"
    />
  </ACTION_SET>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


</PAGE>
