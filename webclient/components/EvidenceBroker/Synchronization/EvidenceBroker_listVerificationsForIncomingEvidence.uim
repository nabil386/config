<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- This page is used to display a list of Verification items for  a      -->
<!-- incoming evidence.                                                    -->
<PAGE HIDE_CONDITIONAL_LINKS="false" PAGE_ID="EvidenceBroker_listVerificationsForIncomingEvidence" WINDOW_OPTIONS="width=700,height=600" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="evidenceType"/>
  <PAGE_PARAMETER NAME="targetCaseID"/>
  <PAGE_PARAMETER NAME="evidenceDescriptorID"/>
  <PAGE_PARAMETER NAME="targetEvidenceType"/>


  <SERVER_INTERFACE CLASS="EvidenceBroker" NAME="DISPLAY" OPERATION="listVerificationsForIncomingEvidence"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="caseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetEvidenceType"/>
    <TARGET NAME="DISPLAY" PROPERTY="targetEvidenceType"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="targetCaseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="targetCaseID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="evidenceDescriptorID"/>
    <TARGET NAME="DISPLAY" PROPERTY="evidenceDescriptorID"/>
  </CONNECT>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="evidenceType"/>
    <TARGET NAME="DISPLAY" PROPERTY="evidenceType"/>
  </CONNECT>


  <LIST>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="EvidenceBroker_listVerificationItemsProvided">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="vDIEDLinkID"/>
          <TARGET NAME="PAGE" PROPERTY="vDIEDLinkID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="sourceEvidenceDescriptorID"/>
          <TARGET NAME="PAGE" PROPERTY="evidenceDescriptorID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <FIELD LABEL="List.Title.VerificationItem" WIDTH="40">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dataItemName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.Title.Applicable" WIDTH="25">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="isApplicable"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="List.Title.DueDate" WIDTH="35">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="dueDate"/>
      </CONNECT>
    </FIELD>


  </LIST>


  <ACTION_SET ALIGNMENT="RIGHT" TOP="false">
    <ACTION_CONTROL IMAGE="CloseButton" LABEL="ActionControl.Label.Close"/>
  </ACTION_SET>


</PAGE>