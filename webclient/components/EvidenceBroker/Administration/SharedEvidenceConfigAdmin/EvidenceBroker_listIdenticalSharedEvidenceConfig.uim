<!-- This page lists shared evidence configurations for identical evidence  -->
<!-- sharing.                                                               -->
<?curam-deprecated Since Curam 7.0.2.0, this functionality has been replaced with the Advanced Evidence Sharing component.?>
<PAGE
  PAGE_ID="EvidenceBroker_listIdenticalSharedEvidenceConfig"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00205401, GYH -->
  <SERVER_INTERFACE
    CLASS="EvidenceBrokerAdmin"
    NAME="DISPLAY"
    OPERATION="listIdenticalEvidenceSharingConfig"
    PHASE="DISPLAY"
  />


  <ACTION_SET
    ALIGNMENT="LEFT"
    BOTTOM="false"
    TOP="true"
  >
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="EvidenceBroker_selectSourceAndTargetForIdentical"
      >
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Edit"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="EvidenceBroker_modifyIdenticalSharedEvidenceConfigFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="evidenceBrokerConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="evidenceBrokerConfigID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Delete"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="EvidenceBroker_cancelIdenticalSharedEvidenceConfig"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="evidenceBrokerConfigID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="evidenceBrokerConfigID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Label.SourceSystem"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sourceSystemName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Source"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sourceCategoryTypeDesc"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.TargetSystem"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="targetSystemName"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Target"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="targetCategoryTypeDesc"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.EvidenceType"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sourceEvidenceDesc"
        />
      </CONNECT>
    </FIELD>


    <!-- END, CR00205401 -->


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="EvidenceBroker_viewIdenticalSharedEvidenceConfig">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="evidenceBrokerConfigID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="evidenceBrokerConfigID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>
</PAGE>
