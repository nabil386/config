<!-- This Page allows users to select sharing source and target for         -->
<!-- evidence sharing.                                                      -->
<?curam-deprecated Since Curam 7.0.2.0, this functionality has been replaced with the Advanced Evidence Sharing component.?>

<PAGE
  PAGE_ID="EvidenceBroker_selectSourceAndTargetForIdentical"
  WINDOW_OPTIONS="width=1025"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <INCLUDE FILE_NAME="EvidenceBroker_selectSourceAndTarget.vim"/>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00205401, GYH -->
      <LINK
        DISMISS_MODAL="false"
        PAGE_ID="EvidenceBroker_createIdenticalSharedEvidenceConfig"
      >
        <!-- END, CR00205401 -->


        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="sourceDetails$sharingPartyID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="sourceID"
          />
        </CONNECT>


        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="sourceDetails$sharingPartyCaseType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="sourceType"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="sourceDetails$partyName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="sourceName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="targetDetails$sharingPartyID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="targetID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="targetDetails$sharingPartyCaseType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="targetType"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="targetDetails$partyName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="targetName"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK PAGE_ID="EvidenceBroker_listIdenticalSharedEvidenceConfig">
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


</PAGE>
