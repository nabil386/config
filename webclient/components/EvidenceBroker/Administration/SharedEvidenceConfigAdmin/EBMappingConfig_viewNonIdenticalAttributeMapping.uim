<!-- This page lists evidence types shared for non-identical       -->
<!-- evidence sharing.                                             -->
<?curam-deprecated Since Curam 7.0.2.0, this functionality has been replaced with the Advanced Evidence Sharing component.?>
<PAGE HIDE_CONDITIONAL_LINKS="false" PAGE_ID="EBMappingConfig_viewNonIdenticalAttributeMapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
  </PAGE_TITLE>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="informationMsgTxt"/>
    </CONNECT>
  </INFORMATIONAL>


  <SERVER_INTERFACE CLASS="MaintainEBMappingConfig" NAME="DISPLAY" OPERATION="listNonIdenticalAttributeMapping" PHASE="DISPLAY"/>


  <PAGE_PARAMETER NAME="evidenceBrokerAttributeID"/>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="evidenceBrokerAttributeID"/>
    <TARGET NAME="DISPLAY" PROPERTY="nonIdenticalAttributeMappingKey$ebAttributeConfigID"/>
  </CONNECT>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.AttributesMapped">


    <LIST>


      <ACTION_SET ALIGNMENT="RIGHT" TYPE="LIST_ROW_MENU">
        <ACTION_CONTROL LABEL="ActionControl.Label.New" TYPE="ACTION">
          <CONDITION>
            <IS_TRUE NAME="DISPLAY" PROPERTY="hasCodeTableDataType"/>
          </CONDITION>
          <LINK OPEN_MODAL="true" PAGE_ID="EBMappingConfig_addCodeTableMapping">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="ebAttributeMappingID"/>
              <TARGET NAME="PAGE" PROPERTY="ebAttributeMappingID"/>
            </CONNECT>


          </LINK>
        </ACTION_CONTROL>
        <!-- BEGIN, CR00440623, SSK -->
        <ACTION_CONTROL LABEL="ActionControl.Label.RemoveCodeTableValues" TYPE="ACTION">
          <CONDITION>
            <IS_TRUE NAME="DISPLAY" PROPERTY="enableCodetableValueView"/>
          </CONDITION>


          <LINK OPEN_MODAL="true" PAGE_ID="EBMappingConfig_removeCodeTableMappings" WINDOW_OPTIONS="width=700,height=300">
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="ebAttributeMappingID"/>
              <TARGET NAME="PAGE" PROPERTY="ebAttributeMappingID"/>
            </CONNECT>


            <CONNECT>
              <SOURCE NAME="PAGE" PROPERTY="evidenceBrokerAttributeID"/>
              <TARGET NAME="PAGE" PROPERTY="evidenceBrokerAttributeID"/>
            </CONNECT>


          </LINK>


        </ACTION_CONTROL>
        <!-- END, CR00440623 -->
        <ACTION_CONTROL LABEL="ActionControl.Label.Delete" TYPE="ACTION">


          <!-- BEGIN, CR00439996, GK -->
          <LINK OPEN_MODAL="true" PAGE_ID="EBMappingConfig_deleteNonIdenticalAttributeMapping" SAVE_LINK="true">
            <!-- END, CR00439996 -->


            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="ebAttributeMappingID"/>
              <TARGET NAME="PAGE" PROPERTY="ebAttibuteMappingID"/>
            </CONNECT>
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="versionNo"/>
              <TARGET NAME="PAGE" PROPERTY="ebAttibuteMappingVersionNo"/>
            </CONNECT>
            <!-- BEGIN, CR00439996, GK -->
            <CONNECT>
              <SOURCE NAME="DISPLAY" PROPERTY="enableCodetableValueView"/>
              <TARGET NAME="PAGE" PROPERTY="enableCodetableValueView"/>
            </CONNECT>
            <!-- END, CR00439996 -->


          </LINK>
        </ACTION_CONTROL>
      </ACTION_SET>
      <FIELD LABEL="Field.Label.SourceAttribute" WIDTH="80">
        <!-- BEGIN, CR00439996, GK -->
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="lhsAttributeLabel"/>
        </CONNECT>
        <!-- END, CR00439996 -->
      </FIELD>
      <FIELD LABEL="Field.Label.TargetAttribute" WIDTH="80">
        <!-- BEGIN, CR00439996, GK -->
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="rhsAttributeLabel"/>
        </CONNECT>
        <!-- END, CR00439996 -->
      </FIELD>
      <DETAILS_ROW>
        <CONDITION>
          <IS_TRUE NAME="DISPLAY" PROPERTY="enableCodetableValueView"/>
        </CONDITION>


        <INLINE_PAGE PAGE_ID="EBMappingConfig_viewAttributeCodetTableMapping">


          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="ebAttributeMappingID"/>
            <TARGET NAME="PAGE" PROPERTY="ebAttributeMappingID"/>
          </CONNECT>


          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="evidenceBrokerAttributeID"/>
            <TARGET NAME="PAGE" PROPERTY="evidenceBrokerAttributeID"/>
          </CONNECT>


        </INLINE_PAGE>
      </DETAILS_ROW>
    </LIST>


  </CLUSTER>


</PAGE>