<?curam-deprecated Since Curam 7.0.2.0, this functionality has been replaced with the Advanced Evidence Sharing component.?>
<PAGE
  PAGE_ID="EvidenceBroker_viewNonIdenticalSharedEvidenceConfig"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="EvidenceBrokerAdmin"
    NAME="DISPLAY"
    OPERATION="readEvidenceSharingConfigDetails"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="evidenceBrokerConfigID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceBrokerConfigID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$evidenceBrokerConfigID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="25"
    NUM_COLS="2"
  >


    <FIELD LABEL="Field.Label.ShareVerifications">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="shareVerificationsOption"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.SharingLimited">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="sharingLimitDesc"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
