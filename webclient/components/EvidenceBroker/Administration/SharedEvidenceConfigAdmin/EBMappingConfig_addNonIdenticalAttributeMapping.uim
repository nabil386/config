<!-- This page lists evidence types shared for non-identical       -->
<!-- evidence sharing.                                             -->
<?curam-deprecated Since Curam 7.0.2.0, this functionality has been replaced with the Advanced Evidence Sharing component.?>
<PAGE
  PAGE_ID="EBMappingConfig_addNonIdenticalAttributeMapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainEBMappingConfig"
    NAME="ACTION"
    OPERATION="addNonIdenticalAttributeMapping"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="MaintainEBMappingConfig"
    NAME="DISPLAY"
    OPERATION="listNonIdenticalUnmappedAttributeMapping"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="evidenceBrokerAttributeID"/>
  <PAGE_PARAMETER NAME="sourceEvidenceType"/>
  <PAGE_PARAMETER NAME="targetEvidenceType"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sourceEvidenceType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$lhsEvidenceType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="targetEvidenceType"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$rhsEvidenceType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceBrokerAttributeID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$ebAttributeConfigID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="evidenceBrokerAttributeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="nonIdenticalAttributeMappingDetails$dtls$ebAttributeConfigID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="sourceEvidenceType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="nonIdenticalAttributeMappingDetails$dtls$lhsEvidenceType"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="targetEvidenceType"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="nonIdenticalAttributeMappingDetails$dtls$rhsEvidenceType"
    />
  </CONNECT>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.AttributesMapped"
  >


    <!-- BEGIN, CR00441283, GK -->
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Field.StaticText.MapAttributes"
        />
      </CONNECT>
    </FIELD>


    <CLUSTER
      NUM_COLS="2"
      STYLE="cluster-cpr-no-border"
    >
      <LIST>
        <CONTAINER LABEL="Container.Label.Action">
          <!-- END, CR00441283 -->
          <WIDGET TYPE="SINGLESELECT">
            <WIDGET_PARAMETER NAME="SELECT_SOURCE">


              <CONNECT>
                <SOURCE
                  NAME="DISPLAY"
                  PROPERTY="sourceDtls$dtls$dtls$lhsAttribute"
                />
              </CONNECT>


            </WIDGET_PARAMETER>
            <WIDGET_PARAMETER NAME="SELECT_TARGET">


              <CONNECT>
                <TARGET
                  NAME="ACTION"
                  PROPERTY="nonIdenticalAttributeMappingDetails$dtls$lhsAttribute"
                />
              </CONNECT>


            </WIDGET_PARAMETER>
          </WIDGET>
        </CONTAINER>
        <!-- BEGIN, CR00441283, GK -->
        <FIELD
          LABEL="Field.Label.Source"
          WIDTH="100"
        >
          <!-- END, CR00441283 -->
          <!-- BEGIN, CR00439996, GK -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="sourceDtls$dtls$lhsAttributeLabel"
            />
          </CONNECT>
          <!-- END, CR00439996 -->
        </FIELD>
      </LIST>


      <LIST>


        <!-- BEGIN, CR00441283, GK -->
        <CONTAINER LABEL="Container.Label.Action">
          <!-- END, CR00441283 -->
          <WIDGET TYPE="SINGLESELECT">
            <WIDGET_PARAMETER NAME="SELECT_SOURCE">


              <CONNECT>
                <SOURCE
                  NAME="DISPLAY"
                  PROPERTY="targetDtls$dtls$dtls$rhsAttribute"
                />
              </CONNECT>


            </WIDGET_PARAMETER>
            <WIDGET_PARAMETER NAME="SELECT_TARGET">


              <CONNECT>
                <TARGET
                  NAME="ACTION"
                  PROPERTY="nonIdenticalAttributeMappingDetails$dtls$rhsAttribute"
                />
              </CONNECT>
            </WIDGET_PARAMETER>
          </WIDGET>
        </CONTAINER>
        <!-- BEGIN, CR00441283, GK -->
        <FIELD
          LABEL="Field.Label.Target"
          WIDTH="100"
        >
          <!-- END, CR00441283 -->
          <!-- BEGIN, CR00439996, GK -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="targetDtls$dtls$rhsAttributeLabel"
            />
          </CONNECT>
          <!-- END, CR00439996 -->
        </FIELD>


      </LIST>
    </CLUSTER>
  </CLUSTER>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="EvidenceBroker_listNonIdenticalEvidenceMappingConfig"
        SAVE_LINK="false"
      >
            </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="SaveAndAddButton"
      LABEL="ActionControl.Label.SaveAndAdd"
      TYPE="SUBMIT"
    >
      <LINK
        PAGE_ID="EBMappingConfig_addNonIdenticalAttributeMapping"
        SAVE_LINK="false"
      >
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
      <LINK
        PAGE_ID="EvidenceBroker_listNonIdenticalEvidenceMappingConfig"
        SAVE_LINK="false"
      >
            </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>
</PAGE>
