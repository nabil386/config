<!-- This page allows the user to select the type of person to be screened. -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="IncomeScreening_selectPersonType" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
  </PAGE_TITLE>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.CreateCaseFor">


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.RegisteredPerson">
        <LINK PAGE_ID="IncomeScreening_searchPerson1" SAVE_LINK="false" />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.NonRegisteredPerson">
        <LINK PAGE_ID="IncomeScreening_duplicatePersonRegistrationCheck" SAVE_LINK="false" OPEN_MODAL="true" />
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER>
      <ACTION_CONTROL LABEL="Field.StaticText.Prospect">
        <LINK PAGE_ID="IncomeScreening_registerProspect" />
      </ACTION_CONTROL>
    </CONTAINER>


  </CLUSTER>


</PAGE>
