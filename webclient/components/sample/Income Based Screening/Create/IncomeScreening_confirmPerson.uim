<!-- This page includes the view that allows the user to confirm the        -->
<!-- selected client. Pressing the next button creates the screening case.  -->
<PAGE
  PAGE_ID="IncomeScreening_confirmPerson"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK PAGE_ID="IncomeScreening_searchPerson1"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="NextButton"
      LABEL="ActionControl.Label.Next"
    >
      <!--BEGIN CR00085879, GBA-->
      <LINK PAGE_ID="IncomeScreening_createCase">
        <!--END CR00085879-->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    >
      <LINK PAGE_ID="IncomeScreening_selectPersonType"/>
    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="Screening_confirmPersonView.vim"/>


</PAGE>
