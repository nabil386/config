<!-- Description --><!-- =========== -->
<!-- This page allows the user to confirm details of the appointment  -->
<!-- scheduled with the caseworker.  -->
<PAGE xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PAGE_ID="IncomeScreening_confirmAppointment" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="pageDescription" />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="IncomeScreening" NAME="ACTION" OPERATION="scheduleAppointment" PHASE="ACTION" />


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL IMAGE="YesButton" LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="IncomeScreening_home" OPEN_MODAL="true">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID" />
          <TARGET NAME="PAGE" PROPERTY="caseID" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="NoButton" LABEL="ActionControl.Label.No">
      <LINK PAGE_ID="IncomeScreening_scheduleAppointment" OPEN_MODAL="true">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID" />
          <TARGET NAME="PAGE" PROPERTY="caseID" />
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="pageDescription" />
          <TARGET NAME="PAGE" PROPERTY="pageDescription" />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>


  <PAGE_PARAMETER NAME="pageDescription" />
  <PAGE_PARAMETER NAME="caseID" />
  <PAGE_PARAMETER NAME="userName" />
  <PAGE_PARAMETER NAME="date" />
  <PAGE_PARAMETER NAME="fullUserName" />


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID" />
    <TARGET NAME="ACTION" PROPERTY="caseID" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="userName" />
    <TARGET NAME="ACTION" PROPERTY="userName" />
  </CONNECT>
  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="date" />
    <TARGET NAME="ACTION" PROPERTY="appointmentDate" />
  </CONNECT>


  <CLUSTER LABEL_WIDTH="20" TITLE="Cluster.Title.ConfirmDetails">


    <FIELD LABEL="Field.Label.UserName">
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="fullUserName" />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Date">
      <CONNECT>
        <SOURCE NAME="PAGE" PROPERTY="date" />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
