<!-- Description -->
<!-- =========== -->
<!-- This is a person search that goes back to the -->
<!-- IncomeScreening_viewRegisteredHosehold page after a person has been selected. -->
<PAGE
  PAGE_ID="IncomeScreening_createHouseholdMember"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="concernRoleName"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="IncomeScreening"
    NAME="DISPLAY"
    OPERATION="getRegisteredMemberDetails"
  />


  <SERVER_INTERFACE
    CLASS="IncomeScreening"
    NAME="ACTION"
    OPERATION="insertHouseholdEvidence"
    PHASE="ACTION"
  />


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />


  </ACTION_SET>


  <PAGE_PARAMETER NAME="caseEvidenceTypeID"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseEvidenceTypeID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="caseEvidenceTypeID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="concernRoleID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="concernRoleID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="55"
    NUM_COLS="2"
    TITLE="Cluster.Tilte.Details"
  >


    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="concernRoleName"
        />
      </CONNECT>
      <LINK
        PAGE_ID="Participant_resolveRoleHome"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="concernRoleType"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantType"
          />
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Label.PrepareMeals">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="prepareMealsInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Disabled">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="disabledInd"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Relationship">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="rshipToHOH"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Citizen">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="citizenshipCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Striker">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="strikerInd"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>