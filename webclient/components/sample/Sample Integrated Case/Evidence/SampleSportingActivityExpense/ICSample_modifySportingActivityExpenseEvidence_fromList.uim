<!-- Modify Sporting Activity Expense evidence details when linked from the -->
<!-- list page.                                                             -->
<PAGE
  PAGE_ID="ICSample_modifySportingActivityExpenseEvidence_fromList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>


    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >

    </ACTION_CONTROL>


  </ACTION_SET>


  <INCLUDE FILE_NAME="ICSample_modifySportingActivityExpenseEvidence_content.vim"/>


</PAGE>
