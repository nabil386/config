<!-- Display sporting activity evidence details.                            -->
<PAGE
  PAGE_ID="ICSample_viewSportingActivityEvidenceCorrectionHistory"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>


  </PAGE_TITLE>
  
  <MENU MODE="IN_PAGE_NAVIGATION">
	  <ACTION_CONTROL STYLE="in-page-link" LABEL="ActionControl.Button.Label.SportingActivityEvidence">
	    <LINK PAGE_ID="ICSample_viewSportingActivityEvidence" DISMISS_MODAL="FALSE" OPEN_MODAL="TRUE" SAVE_LINK="FALSE">
	      <CONNECT>
	        <SOURCE PROPERTY="caseID" NAME="PAGE"/>
	        <TARGET PROPERTY="caseID" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="evidenceID" NAME="PAGE"/>
	        <TARGET PROPERTY="evidenceID" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="evidenceType" NAME="PAGE"/>
	        <TARGET PROPERTY="evidenceType" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="contextDescription" NAME="PAGE"/>
	        <TARGET PROPERTY="contextDescription" NAME="PAGE"/>
	      </CONNECT>
	    </LINK>
	  </ACTION_CONTROL>
	  <ACTION_CONTROL STYLE="in-page-current-link" LABEL="ActionControl.Button.Label.SportingActivityEvidenceCorrectionHistory">
	    <LINK PAGE_ID="ICSample_viewSportingActivityEvidenceCorrectionHistory" DISMISS_MODAL="FALSE" OPEN_MODAL="TRUE" SAVE_LINK="FALSE">
	      <CONNECT>
	        <SOURCE PROPERTY="caseID" NAME="PAGE"/>
	        <TARGET PROPERTY="caseID" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="evidenceID" NAME="PAGE"/>
	        <TARGET PROPERTY="evidenceID" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="evidenceType" NAME="PAGE"/>
	        <TARGET PROPERTY="evidenceType" NAME="PAGE"/>
	      </CONNECT>
	      <CONNECT>
	        <SOURCE PROPERTY="contextDescription" NAME="PAGE"/>
	        <TARGET PROPERTY="contextDescription" NAME="PAGE"/>
	      </CONNECT>
	    </LINK>
	  </ACTION_CONTROL>
  </MENU>
  
  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>

  <INCLUDE FILE_NAME="Evidence_listCorrectionHistory.vim"/>


</PAGE>
