<!-- Create new sporting activity evidence.                                 -->
<PAGE
  PAGE_ID="ICSample_createSportingActivityEvidence"
  WINDOW_OPTIONS="width=875"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="caseID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <!-- BEGIN, CR00022287, TV -->
  <PAGE_PARAMETER NAME="evidenceType"/>
  <!-- END CR00022287 -->


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    
    <ACTION_CONTROL
      IMAGE="SaveAndNewButton"
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ICSample_createSportingActivityEvidence"/>
    </ACTION_CONTROL>

    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>
  <INCLUDE FILE_NAME="ICSample_createSportingActivityEvidence_content.vim"/>
</PAGE>
