<!-- This is the site map for the Sample Sporting Grant Evidence.           -->
<PAGE
  PAGE_ID="Sample_sportingGrantProductSiteMap"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE DESCRIPTION="Page.Description">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="IntegratedCaseSampleSportingGrant"
    NAME="DISPLAY"
    OPERATION="readSiteMapDetails"
  />
  <PAGE_PARAMETER NAME="caseID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="caseID"
    />
  </CONNECT>


  <CLUSTER SHOW_LABELS="false">
    <CONTAINER>
      <ACTION_CONTROL
        IMAGE="ApplyEvidenceChanges"
        LABEL="Field.Label.ApplyChanges"
      >
        <!-- BEGIN, CR00223998, ELG -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Evidence_applyChanges1"
        >
          <!-- END, CR00223998 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="Field.Label.ApplyChanges">
        <!-- BEGIN, CR00223998, ELG -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="Evidence_applyChanges1"
        >
          <!-- END, CR00223998 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
  </CLUSTER>
  <CLUSTER SHOW_LABELS="false">
    <CONTAINER>
      <IMAGE
        IMAGE="SampleExpenseTab"
        LABEL="Tab.Title.Household"
      />
    </CONTAINER>
  </CLUSTER>
  <CLUSTER
    LABEL_WIDTH="100"
    NUM_COLS="3"
    SHOW_LABELS="false"
    WIDTH="80%"
  >
    <CONTAINER>
      <ACTION_CONTROL
        LABEL="Field.Label.SportingActivity"
        STYLE="siteMapLabel"
      >
        <LINK PAGE_ID="Sample_listSportingActivity">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="caseID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="caseID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="contextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="contextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="active"
          />
        </CONNECT>
      </FIELD>
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="inEdit"
          />
        </CONNECT>
      </FIELD>
    </CONTAINER>
  </CLUSTER>
</PAGE>
