<!-- Infrastructure pages used for validating evidence changes              -->
<PAGE
  PAGE_ID="Sample_validateSportingActivityExpenseEvidence1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="contextDescription"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="Evidence"
    NAME="DISPLAY"
    OPERATION="listAllForValidateChanges"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="Evidence"
    NAME="ACTION"
    OPERATION="validateChanges1"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="parentEvidenceID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="parentEvidenceID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="parentEvidenceID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="LEFT">


    <ACTION_CONTROL
      IMAGE="ValidateButton"
      LABEL="ActionControl.Label.ApplyChanges"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    >
    </ACTION_CONTROL>
  </ACTION_SET>


  <INCLUDE FILE_NAME="Evidence_listEvidenceView1.vim"/>


</PAGE>
