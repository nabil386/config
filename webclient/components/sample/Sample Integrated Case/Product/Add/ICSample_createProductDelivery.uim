<!-- This page is used to create the product delivery case.                 -->
<PAGE
  PAGE_ID="ICSample_createProductDelivery"
  WINDOW_OPTIONS="width=650"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >


    <ACTION_CONTROL
      IMAGE="PreviousButton"
      LABEL="ActionControl.Label.Previous"
    >
      <LINK
        OPEN_MODAL="false"
        DISMISS_MODAL="false"
        SAVE_LINK="false"
        PAGE_ID="ICSample_existingCaseList"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="productID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="productID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="integratedCaseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <!--  BEGIN, CR00021249, SK -->
    <!--  BEGIN, HARP 65193, SK -->
    <ACTION_CONTROL
      IMAGE="CreateProductDelivery"
      LABEL="ActionControl.Label.CreateDelivery"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="Case_resolveCaseHome">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="result$caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
      <!-- END, HARP 65193 -->
      <!-- END, CR00021249 -->


    <ACTION_CONTROL
      IMAGE="ExitButton"
      LABEL="ActionControl.Label.Exit"
    />


  </ACTION_SET>


  <!-- BEGIN, CR00211698, MC -->
  <INCLUDE FILE_NAME="ICSample_createProductDeliveryContent.vim"/>
  <!-- END, CR00211698 -->


</PAGE>
