<!-- This page allows a user to view a daily schedule.  -->
<PAGE
  PAGE_ID="SampleCSSAppointments_view"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="SampleCSSAppointments"
    NAME="DISPLAY"
    OPERATION="readDailySchedule"
  />


  <PAGE_PARAMETER NAME="scheduleID"/>
  <PAGE_PARAMETER NAME="locationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="scheduleID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="dailyScheduleKey$scheduleID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <FIELD
      HEIGHT="4"
      WIDTH="100"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
