<!-- This script opens the sample evidence view pages.                      -->
<PAGE
  PAGE_ID="ProductDelivery_resolveViewEffectiveDatedEvidence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
            
      String evidenceID = request.getParameter("evidenceID");
      String approveInd = request.getParameter("approveInd");
      String versionNo = request.getParameter("versionNo");
      String pageDescription = request.getParameter("pageDescription");
      String evidenceGroupNameCode = request.getParameter("evidenceGroupNameCode");
      String url = "";

      if (evidenceID == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "evidenceID");
       System.out.println(e);
       throw e;
       // END, CR00110216
      } 
      if (approveInd == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "approveInd");
       System.out.println(e);
       throw e;
       // END, CR00110216
      } 
      if (versionNo == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "versionNo");
       System.out.println(e);
       throw e;
       // END, CR00110216
      } 
      if (pageDescription == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "pageDescription");
       System.out.println(e);
       throw e;
       // END, CR00110216
      } 
      if (evidenceGroupNameCode == null) {
        // BEGIN, CR00110216, SK
       core.ScriptletMissingParamException e = new
                core.ScriptletMissingParamException(-20002, "evidenceGroupNameCode");
       System.out.println(e);
       throw e;
       // END, CR00110216
      } 
      if (evidenceGroupNameCode.equals("SIMGroup")) {
        if (approveInd.equals("false")) {
          url = context + "BenefitSample_viewEffectiveDatedEvidencePage.do?evidenceID=" + 
          curam.omega3.request.RequestUtils.escapeURL(evidenceID) + "&amp;pageDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);
        } else {
          url = context + "BenefitSample_viewEffectiveDatedEvidencePageApprove.do?evidenceID=" + 
          curam.omega3.request.RequestUtils.escapeURL(evidenceID) + "&amp;versionNo=" + curam.omega3.request.RequestUtils.escapeURL(versionNo) + "&amp;pageDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);
        }
      } else if (evidenceGroupNameCode.equals("SIMLGroup")) {
        if (approveInd.equals("false")) {
          url = context + "LiabilitySample_viewEffectiveDatedEvidencePage.do?evidenceID=" + 
          curam.omega3.request.RequestUtils.escapeURL(evidenceID) + "&amp;pageDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);
        } else {
          url = context + "LiabilitySample_viewEffectiveDatedEvidencePageApprove.do?evidenceID=" + 
          curam.omega3.request.RequestUtils.escapeURL(evidenceID) + "&amp;versionNo=" + curam.omega3.request.RequestUtils.escapeURL(versionNo) + "&amp;pageDescription=" + curam.omega3.request.RequestUtils.escapeURL(pageDescription);
        }
      }
      
      url += "&amp;" + rh.getSystemParameters();  
      response.sendRedirect(response.encodeRedirectURL(url));

    
  </JSP_SCRIPTLET>
</PAGE>
