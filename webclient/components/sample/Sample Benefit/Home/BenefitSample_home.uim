<!-- Sample Benefit home page.                                              -->
<PAGE PAGE_ID="BenefitSample_home" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00113328, ELG -->
  <SHORTCUT_TITLE ICON="productdelivery.icon">
    <CONNECT>
      <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
    </CONNECT>
  </SHORTCUT_TITLE>
  <!-- END, CR00113328 -->


  <SERVER_INTERFACE CLASS="ProductDelivery" NAME="DISPLAY" OPERATION="readHomePageDetails1"/>


  <MENU>
    <ACTION_CONTROL LABEL="ActionControl.Label.NewTask">
      <LINK OPEN_MODAL="true" PAGE_ID="Case_createTask">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.NewActivity">
      <LINK OPEN_MODAL="true" PAGE_ID="Case_createStandardActivity">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL IMAGE="EditButton" LABEL="ActionControl.Label.Edit">
      <!-- BEGIN, CR00238896, GD -->
      <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_modifyHeader1" WINDOW_OPTIONS="width=875">
      <!-- END, CR00238896 -->
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


    <ACTION_CONTROL IMAGE="ManageButton" LABEL="ActionControl.Label.Manage">
      <LINK PAGE_ID="BenefitSample_manage">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="ReassessButton" LABEL="ActionControl.Label.Reassess">
      <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_reassessBenefit" WINDOW_OPTIONS="width=825">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="CheckEligibilityButton" LABEL="ActionControl.Label.CheckEligibility">
      <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_checkEligibility" WINDOW_OPTIONS="width=1200,height=500">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="RemoveBookmarkButton" LABEL="ActionControl.Label.RemoveBookmark">
      <CONDITION>
        <IS_TRUE NAME="DISPLAY" PROPERTY="caseIsUserBookmarkInd"/>
      </CONDITION>
      <LINK OPEN_MODAL="true" PAGE_ID="Organization_cancelCaseBookmark" WINDOW_OPTIONS="width=400,height=150">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL IMAGE="AddBookmarkButton" LABEL="ActionControl.Label.AddBookmark">
      <CONDITION>
        <IS_FALSE NAME="DISPLAY" PROPERTY="caseIsUserBookmarkInd"/>
      </CONDITION>
      <LINK OPEN_MODAL="true" PAGE_ID="Organization_createCaseBookmark" WINDOW_OPTIONS="width=400,height=150">
        <CONNECT>
          <SOURCE NAME="PAGE" PROPERTY="caseID"/>
          <TARGET NAME="PAGE" PROPERTY="caseID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
          <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="caseID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$caseID"/>
  </CONNECT>


  <CLUSTER NUM_COLS="2" TITLE="Cluster.Title.Details">


    <FIELD LABEL="Field.Label.ProductName">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="productName"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.PrimaryClient">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="primaryClientName"/>
      </CONNECT>
      <!-- BEGIN, CR00103528, AK -->
      <LINK PAGE_ID="Case_resolveParticipantHome">
        <!-- END, CR00103528 -->
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="primaryClientID"/>
          <TARGET NAME="PAGE" PROPERTY="concernRoleID"/>
        </CONNECT>
      </LINK>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseStartDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="casePriorityCode"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.OwnerType">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseOwnerType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Objective">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="objectiveCode"/>
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Field.Label.CertificationFrequency">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="certificationFrequency"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL LABEL="ActionControl.Label.Change">
        <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_modifyCertificationFrequency" WINDOW_OPTIONS="width=650,height=200">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <CONTAINER LABEL="Field.Label.OverrideMDR">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="overrideMDRInd"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL LABEL="ActionControl.Label.Set" APPEND_ELLIPSIS="false">
        <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_setOverrideMDR" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="homePageDetails$caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Unset" APPEND_ELLIPSIS="false">
        <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_unsetOverrideMDR" WINDOW_OPTIONS="width=400,height=150">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="homePageDetails$caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Label.CaseReference">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$closureDtls$caseReference"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="caseStatusCode"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.ExpectedEndDate">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="expectedEndDate"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Classification">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="classificationCode"/>
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Field.Label.Owner">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="caseOwnerName"/>
        </CONNECT>
        <LINK OPEN_MODAL="true" PAGE_ID="Case_resolveOrgObjectTypeHome" WINDOW_OPTIONS="width=900,height=300">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="userName"/>
            <TARGET NAME="PAGE" PROPERTY="userName"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="orgObjectReference"/>
            <TARGET NAME="PAGE" PROPERTY="orgObjectReference"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="caseOwnerType"/>
            <TARGET NAME="PAGE" PROPERTY="orgObjectType"/>
          </CONNECT>
        </LINK>
      </FIELD>
      <ACTION_CONTROL LABEL="ActionControl.Label.Change" APPEND_ELLIPSIS="false">
        <LINK OPEN_MODAL="true" PAGE_ID="Case_modifyCaseOwner" WINDOW_OPTIONS="width=750,height=280">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="outcomeCode"/>
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Field.Label.DeliveryPattern">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="deliveryPattern"/>
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL LABEL="ActionControl.Label.Change" APPEND_ELLIPSIS="false">
        <LINK OPEN_MODAL="true" PAGE_ID="ProductDelivery_modifyDeliveryPattern" WINDOW_OPTIONS="width=620,height=200">
          <CONNECT>
            <SOURCE NAME="PAGE" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="productID"/>
            <TARGET NAME="PAGE" PROPERTY="productID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$contextDescription$description"/>
            <TARGET NAME="PAGE" PROPERTY="pageDescription"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


    <FIELD LABEL="Field.Label.DateReceived">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="receivedDate"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER SHOW_LABELS="false" TITLE="Cluster.Title.Comments">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$closureDtls$comments"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.RecentChanges">


    <FIELD LABEL="Field.Label.EventType">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$transactionLogDtls$dtlsList$eventTypeDesc"/>
      </CONNECT>
      <!--BEGIN CR00108134, GBA-->
      <LINK OPEN_MODAL="true" PAGE_ID="Case_resolveViewTransactionLogRecord">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="relatedID"/>
          <TARGET NAME="PAGE" PROPERTY="relatedID"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="transactionDescription"/>
          <TARGET NAME="PAGE" PROPERTY="transactionDescription"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="transactionDateTime"/>
          <TARGET NAME="PAGE" PROPERTY="transactionDateTime"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="result$transactionLogDtls$dtlsList$userFullName"/>
          <TARGET NAME="PAGE" PROPERTY="userFullName"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="eventTypeDesc"/>
          <TARGET NAME="PAGE" PROPERTY="eventType"/>
        </CONNECT>
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="pageID"/>
          <TARGET NAME="PAGE" PROPERTY="pageID"/>
        </CONNECT>
      </LINK>
    </FIELD>
    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="transactionDescription"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.TransactionDateTime">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$transactionLogDtls$dtlsList$transactionDateTime"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.UserFullname">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$transactionLogDtls$dtlsList$userFullName"/>
      </CONNECT>
      <LINK OPEN_MODAL="true" PAGE_ID="Organization_viewUserDetails" WINDOW_OPTIONS="width=900,height=300">
        <CONNECT>
          <SOURCE NAME="DISPLAY" PROPERTY="username"/>
          <TARGET NAME="PAGE" PROPERTY="userName"/>
        </CONNECT>
      </LINK>
      <!--END CR00108134-->
    </FIELD>
  </LIST>


</PAGE>