<!-- ===========                                                            -->
<!-- This page is used to display a list of financial instructions for      -->
<!-- the benefit sample.                                                    -->
<PAGE
  HIDE_CONDITIONAL_LINKS="false"
  PAGE_ID="LiabilitySample_listFinInstruction1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="ProductDelivery"
    NAME="DISPLAY"
    OPERATION="listCaseFinancialInstruction"
  />


  <PAGE_PARAMETER NAME="caseID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="caseID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$caseID"
    />
  </CONNECT>


  <INFORMATIONAL>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="result$warnings$informationalMsgDtlsList$dtls$informationMsgTxt"
      />
    </CONNECT>
  </INFORMATIONAL>


  <!-- BEGIN, CR00184236, MC -->
  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuTransactions"
      STYLE="in-page-current-link"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="unprocessedILIsInd"
        />
      </CONDITION>
      <LINK
        PAGE_ID="LiabilitySample_listFinInstruction1"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.MenuUnprocessed"
      STYLE="in-page-link"
    >
      <CONDITION>
        <IS_TRUE
          NAME="DISPLAY"
          PROPERTY="unprocessedILIsInd"
        />
      </CONDITION>
      <LINK
        PAGE_ID="LiabilitySample_listUnprocessedILIs"
        SAVE_LINK="false"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>
  <!-- END, CR00184236 -->


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="GenerateLiability"
      LABEL="ActionControl.Label.GenerateLiability"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="ProductDelivery_generateLiability"
        WINDOW_OPTIONS="width=500,height=150"
      >
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="caseID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="caseID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="description"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


  <!-- BEGIN, CR00219579, MC -->
  <INCLUDE FILE_NAME="Financial_listFinInstructionView1.vim"/>
  <!-- END, CR00219579 -->


</PAGE>
