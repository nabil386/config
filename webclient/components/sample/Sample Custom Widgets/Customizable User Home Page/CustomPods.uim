<!-- This page allows the user to view a list of Web Addresses -->
<!-- for a Prospect -->
<!-- The page allows a user to add, modify or view Web Address details.-->
<PAGE
  COMPONENT_STYLE="page-omit-sidebar"
  PAGE_ID="CustomPods"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="DisplayWidget"
    NAME="ANNOUNCEMENTDISPLAY"
    OPERATION="getAnnouncements"
  />


  <CLUSTER
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="ANNOUNCEMENTDISPLAY"
          PROPERTY="result$contentXML"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CONNECT>
    <SOURCE
      NAME="CONSTANT"
      PROPERTY="CustomPods"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageID$pageID"
    />
  </CONNECT>


  <INCLUDE FILE_NAME="PodContainer.vim"/>


</PAGE>
