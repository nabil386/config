<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Allows a user to select from a list of programs and run program        -->
<!-- recommendation for those programs.                                     -->
<PAGE
  PAGE_ID="CREOLEProgramRecommendation_selectProgramsForApplication"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CREOLEProgramRecommendation"
    NAME="DISPLAY"
    OPERATION="listProgramDetailsForApplication"
  />


  <SERVER_INTERFACE
    CLASS="CREOLEProgramRecommendation"
    NAME="ACTION"
    OPERATION="runProgramRecommendationForApplication"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="applicationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="applicationID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="applicationID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$applicationID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Run"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER
    LABEL_WIDTH="95"
    LAYOUT_ORDER="FIELD"
  >


    <FIELD
      ALIGNMENT="LEFT"
      LABEL="Field.Label.UseActiveEvidence"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="useActiveEvidence"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <CLUSTER>


    <!-- BEGINING OF CR00273434 -->
    <LIST >
      <!-- ENG OF CR00273434 -->
      <CONTAINER
        ALIGNMENT="CENTER"
        LABEL="Container.Label.Apply"
      >
        <WIDGET TYPE="MULTISELECT">
          <WIDGET_PARAMETER NAME="MULTI_SELECT_SOURCE">
            <CONNECT>
              <SOURCE
                NAME="DISPLAY"
                PROPERTY="result$list$productID"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
          <WIDGET_PARAMETER NAME="MULTI_SELECT_TARGET">
            <CONNECT>
              <TARGET
                NAME="ACTION"
                PROPERTY="details$selectedPrograms"
              />
            </CONNECT>
          </WIDGET_PARAMETER>
        </WIDGET>
      </CONTAINER>


      <FIELD
        LABEL="Field.Label.Program"
        WIDTH="60"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$list$programName"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.EligibilityStartDate"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$list$eligibilityStartDate"
          />
        </CONNECT>
      </FIELD>


      <FIELD
        LABEL="Field.Label.ProgramRequested"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$list$programRequested"
          />
        </CONNECT>
      </FIELD>


    </LIST>


  </CLUSTER>


</PAGE>
