<!-- This page allows the user to view fund configuration details for a fund -->
<PAGE
  PAGE_ID="FundPM_viewFundConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY1"
        PROPERTY="name"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainFundConfiguration"
    NAME="DISPLAY"
    OPERATION="viewFundConfiguration"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainProgramFund"
    NAME="DISPLAY1"
    OPERATION="readProgramFund"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$programFundID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="obligationConfigurationID"/>
  <PAGE_PARAMETER NAME="fundID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="obligationConfigurationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$fundConfigurationID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00426143, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00426143 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
