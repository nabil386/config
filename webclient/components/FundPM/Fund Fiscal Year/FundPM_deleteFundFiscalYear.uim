<!-- This page allows the user to logically delete a fund fiscal year line  -->
<!-- item record.	                                                        -->
<PAGE
  PAGE_ID="FundPM_deleteFundFiscalYear"
  WINDOW_OPTIONS="width=300"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="fundFiscalYearID"/>
  <PAGE_PARAMETER NAME="fundName"/>
  <PAGE_PARAMETER NAME="fiscalYearName"/>
  <PAGE_PARAMETER NAME="fundIDParam"/>


  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYear"
    NAME="ACTION"
    OPERATION="deleteFundFiscalYear"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYear"
    NAME="DISPLAY"
    OPERATION="readFundHomeForFiscalYear"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundFiscalYearID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$fundFiscalYearID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundFiscalYearID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="id"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <!-- BEGIN, CR00207190, PS -->
  <CLUSTER SHOW_LABELS="false">
    <!-- END, CR00207190 -->
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Cluster.Label.Text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Yes"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>
</PAGE>
