<!-- This page allows the user to view the fund home page.					-->
<PAGE
  PAGE_ID="FundPM_viewFundHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYear"
    NAME="DISPLAY"
    OPERATION="readAllFundFiscalYearsByFundID"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="MaintainFundConfiguration"
    NAME="DISPLAY1"
    OPERATION="listFundConfiguration"
  />


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="name"
      />
    </CONNECT>
  </PAGE_TITLE>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewFund"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="FundPM_viewFundHome">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <!-- BEGIN, CR00261860, GP -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programFundID"
          />
        </CONNECT>
        <!-- END, CR00261860 -->
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.FundConfiguration"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="FundPM_listFundConfiguration">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.FundAssociation"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="FundPM_listFundLinks">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>


  </MENU>


  <PAGE_PARAMETER NAME="fundIDParam"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programFundID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$programFundID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$programFundID"
    />
  </CONNECT>


  <!-- BEGIN, CR00227014, PS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00227014 -->
    <FIELD LABEL="Field.Lalel.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.label.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <!-- BEGIN, CR00207190, PS -->
  <CLUSTER
    LABEL_WIDTH="55"
    NUM_COLS="2"
    TITLE="Cluster.Title.ActiveFundConfiguration"
  >
    <!-- END, CR00207190 -->
    <FIELD LABEL="Field.Lalel.OverObligationIndCreateOblign">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="liveConfig$dtls$overObligationIndCreateOblign"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Lalel.WaitListAllowedInd">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="liveConfig$dtls$waitListAllowedInd"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Lalel.OverObligationIndAdjustOblign">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY1"
          PROPERTY="liveConfig$dtls$overObligationIndAdjustOblign"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="Cluster.Title.FiscalYears">
    <!-- BEGIN, CR00207190, PS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.DeleteFiscalYear">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_deleteFundFiscalYear"
          WINDOW_OPTIONS="width=450"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="fundFiscalYearID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundFiscalYearID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="name"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="yearName"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fiscalYearName"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="dtls$programFundID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <FIELD LABEL="Field.Label.YearName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="yearName"
        />
      </CONNECT>
      <LINK PAGE_ID="FundPM_viewFiscalYearFundHome">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="fundFiscalYearID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fiscalYearFundID"
          />
        </CONNECT>
        <!-- BEGIN, CR00227014, PS -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="fundFiscalYearID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundFiscalYearID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <!-- END, CR00227014 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="name"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="yearName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fiscalYearName"
          />
        </CONNECT>
      </LINK>
    </FIELD>
    <!-- END, CR00207190 -->
    <!--  BEGIN, CR00187494, AS -->
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.BudgetedTotal"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="allocatedTotal"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.RemainingBudgetBalance"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="remBudgetBalance"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      ALIGNMENT="RIGHT"
      LABEL="Field.Label.RemainingActualBalance"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="remainingActualBalance"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00187494  -->
  </LIST>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00426143, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <!-- END, CR00426143 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
