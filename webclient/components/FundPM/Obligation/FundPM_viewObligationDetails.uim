<!-- This page allows user to view Obligation details.     	    -->
<PAGE
  PAGE_ID="FundPM_viewObligationDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../WebContent/WEB-INF/CDEJ/schema/uim.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainObligateFund"
    NAME="DISPLAY"
    OPERATION="viewObligation"
  />


  <PAGE_PARAMETER NAME="obligationID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="obligationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$obligationID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Label.Details"
  >
    <FIELD LABEL="Field.Label.TransactionType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="transactionType"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Amount">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="amount"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreationDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="creationDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.AdjustmentType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="debitCreditType"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
