<!-- This page allows the user to create a budget adjustment.          	    -->
<PAGE
  PAGE_ID="FundPM_createBalanceLineItemDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYear"
    NAME="DISPLAY"
    OPERATION="readFundHomeForFiscalYear"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fiscalYearFundID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$fundFiscalYearID"
    />
  </CONNECT>
  <SERVER_INTERFACE
    CLASS="MaintainBudgetAdjustment"
    NAME="ACTION"
    OPERATION="createBudgetAdjustment"
    PHASE="ACTION"
  />
  <!-- BEGIN, CR00207915, PS -->
  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYearLineItem"
    NAME="DISPLAY1"
    OPERATION="viewLineItem"
    PHASE="DISPLAY"
  />
  <!-- END, CR00207915 -->


  <PAGE_PARAMETER NAME="fundFiscalYearLineItemID"/>
  <PAGE_PARAMETER NAME="fundName"/>
  <PAGE_PARAMETER NAME="fiscalYearName"/>
  <PAGE_PARAMETER NAME="fiscalYearFundID"/>


  <!-- BEGIN, CR00207915, PS -->
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundFiscalYearLineItemID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$key$fundFclYrLineItemID"
    />
  </CONNECT>
  <!-- END, CR00207915 -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundFiscalYearLineItemID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="relatedID"
    />
  </CONNECT>
  <CONNECT>
    <!-- BEGIN, CR00207915, PS -->
    <SOURCE
      NAME="DISPLAY1"
      PROPERTY="relatedType"
    />
    <!-- END, CR00207915 -->
    <TARGET
      NAME="ACTION"
      PROPERTY="relatedType"
    />
  </CONNECT>


  <!-- BEGIN, CR00227014, PS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00227014 -->
    <FIELD
      LABEL="Field.Label.Amount"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="dtls$amount"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.CreditDebitType"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="creditDebitType"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    NUM_COLS="1"
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <!-- BEGIN, CR00227014, PS -->
    <ACTION_CONTROL
      IMAGE="SaveAndNewButton"
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="FundPM_createBalanceLineItemDetails"/>
    </ACTION_CONTROL>
    <!-- END, CR00227014 -->
    <ACTION_CONTROL
      IMAGE="SaveButton"
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
	</ACTION_CONTROL>


    <ACTION_CONTROL
      IMAGE="CancelButton"
      LABEL="ActionControl.Label.Cancel"
    />
  </ACTION_SET>


</PAGE>
