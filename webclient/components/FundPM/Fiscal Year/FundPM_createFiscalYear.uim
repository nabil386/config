<!-- This page allows the user to create a fiscal year.						-->
<PAGE PAGE_ID="FundPM_createFiscalYear" WINDOW_OPTIONS="width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="MaintainFiscalYear" NAME="ACTION" OPERATION="createFiscalYear" PHASE="ACTION"/>
  <!-- BEGIN, CR00207190, PS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00207190 -->
    <FIELD LABEL="Field.Label.YearName" USE_BLANK="true" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="yearName"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate" USE_BLANK="true" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="startDate"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Category" USE_BLANK="true" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="category"/>
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate" USE_BLANK="true" USE_DEFAULT="false">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="endDate"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <!-- BEGIN, CR00227014, PS -->
    <ACTION_CONTROL LABEL="ActionControl.Label.SaveNew" TYPE="SUBMIT">
      <LINK PAGE_ID="FundPM_createFiscalYear"/>
    </ACTION_CONTROL>
    <!-- END, CR00227014 -->
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>
</PAGE>