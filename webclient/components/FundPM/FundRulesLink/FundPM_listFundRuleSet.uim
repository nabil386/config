<!-- This page allows the user to list the funding rule set details.   	 	-->
<PAGE
  PAGE_ID="FundPM_listFundRuleSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <!-- BEGIN, CR00198153, AS  -->
  <SERVER_INTERFACE
    CLASS="MaintainFundedItemRulesLink"
    NAME="DISPLAY"
    OPERATION="listFundedItemRulesAssociation"
  />
  <!-- END, CR00198153  -->


  <PAGE_PARAMETER NAME="fundedItemRulesLinkID"/>


  <!-- BEGIN, CR00207190, PS -->
  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.AssignRuleSet"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="FundPM_assignFundRuleSet"
      />
    </ACTION_CONTROL>
  </ACTION_SET>
  <!-- BEGIN, CR00227014, PS -->
  <LIST>
    <!-- END, CR00227014 -->


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207190 -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <!-- BEGIN, CR00198414, AS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_modifyFundRuleSetFromList"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="fundedItemRulesLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundedItemRulesLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
        <!-- END, CR00198414 -->
      </ACTION_CONTROL>
      <!-- BEGIN, CR00207190, PS -->
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_deleteFundedItemRuleSetLink"
          WINDOW_OPTIONS="width=500"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="fundedItemRulesLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundedItemRulesLinkID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <!-- END, CR00207190 -->


    <!-- BEGIN, CR00227014, PS -->
    <FIELD
      LABEL="Field.Label.FundedItem"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedTypeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.RuleSet"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="ruleName"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00227014 -->


    <FIELD
      LABEL="Field.Label.StartDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
