<!-- Description 															-->
<!-- =========== 															-->
<!-- This page allows user to view fund rule set.								-->
<PAGE
  PAGE_ID="FundPM_viewFundRulesSet"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>


  </PAGE_TITLE>


  <!-- BEGIN, CR00198153, AS  -->
  <SERVER_INTERFACE
    CLASS="MaintainFundedItemRulesLink"
    NAME="DISPLAY"
    OPERATION="readFundedItemRulesAssociation"
  />
  <!-- END, CR00198153  -->


  <PAGE_PARAMETER NAME="fundedItemRulesLinkID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundedItemRulesLinkID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$fundedItemRulesLinkID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="50"
    NUM_COLS="2"
    TITLE="Cluster.Title.Details"
  >
    <FIELD
      LABEL="Field.Label.Type"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedType"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.RuleSet"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="ruleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198153, AS  -->
    <FIELD
      LABEL="Field.Label.FundedItem"
      WIDTH="20"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedTypeName"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00198153  -->
    <FIELD
      LABEL="Field.Label.Status"
      WIDTH="10"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="recordStatus"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
      <!-- BEGIN, CR00198414, AS -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="FundPM_modifyFundRuleSetFromList"
      >
        <!-- BEGIN, CR00198153, AS  -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$fundedItemRulesLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundedItemRulesLinkID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$fundedItemRulesAssociationDtls$versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <!-- END, CR00198153  -->
      </LINK>
      <!-- END, CR00198414 -->
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
      <!-- BEGIN, CR00198414, AS -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="FundPM_deleteFundedItemRuleSetLink"
        WINDOW_OPTIONS="width=500"
      >
        <!-- BEGIN, CR00198153, AS  -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="key$fundedItemRulesLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundedItemRulesLinkID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$fundedItemRulesAssociationDtls$versionNo"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="versionNo"
          />
        </CONNECT>
        <!-- END, CR00198153  -->
      </LINK>
      <!-- END, CR00198414 -->
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Close">
      <LINK PAGE_ID="FundPM_listFundRuleSet">
        
      </LINK>
    </ACTION_CONTROL>


  </ACTION_SET>
</PAGE>
