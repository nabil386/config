<!-- Description -->
<!-- This will remove Wait List Entry for the fund  -->
<PAGE
  PAGE_ID="FundPM_removeWaitListEntry"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="PAGE"
        PROPERTY="contextDescription"
      />
    </CONNECT>


  </PAGE_TITLE>
  <PAGE_PARAMETER NAME="waitListEntryID"/>
  <PAGE_PARAMETER NAME="contextDescription"/>
  <PAGE_PARAMETER NAME="fundName"/>
  <PAGE_PARAMETER NAME="caseRefernceNo"/>
  <PAGE_PARAMETER NAME="createDate"/>
  <PAGE_PARAMETER NAME="participantName"/>
  <!-- BEGIN, CR00200567, AS -->
  <PAGE_PARAMETER NAME="fundFiscalYearID"/>
  <!-- END, CR00200567 -->


  <SERVER_INTERFACE
    CLASS="MaintainFundWaitList"
    NAME="DISPLAY"
    OPERATION="viewWaitListEntry"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="MaintainFundWaitList"
    NAME="ACTION"
    OPERATION="removeWaitListEntry"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waitListEntryID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$waitListEntryID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="waitListEntryID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="waitListEntryID"
    />
  </CONNECT>


  <!-- BEGIN, CR00200567, AS -->
  <!-- BEGIN, CR00207190, PS -->
  <CLUSTER SHOW_LABELS="false">
    <!-- END, CR00207190 -->
    <FIELD>
      <CONNECT>
        <SOURCE
          NAME="TEXT"
          PROPERTY="Cluster.Label.Text"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER>
    <!-- END, CR00200567 -->


    <FIELD
      LABEL="Field.Label.WaitListEntry.RemovalReason"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="removalReason"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="removalReason"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <!-- BEGIN, CR00200567, AS -->
    <ACTION_CONTROL
      LABEL="ActionControl.Yes.Label"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.No.Label">
    </ACTION_CONTROL>
    <!-- END, CR00200567 -->
  </ACTION_SET>
</PAGE>
