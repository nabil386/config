<!-- Description -->
<!-- This is view Wait List Entry for the fund fiscal year -->
<PAGE
  PAGE_ID="FundPM_searchWaitListEntry"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="fundFiscalYearID"/>
  <!-- BEGIN, CR00200567, AS -->
  <PAGE_PARAMETER NAME="waitListEntryID"/>
  <PAGE_PARAMETER NAME="fundFiscalYearName"/>
  <PAGE_PARAMETER NAME="participantName"/>
  <PAGE_PARAMETER NAME="position"/>
  <PAGE_PARAMETER NAME="priority"/>
  <PAGE_PARAMETER NAME="expiryDate"/>
  <PAGE_PARAMETER NAME="reviewDate"/>
  <!-- END, CR00200567 -->


  <SERVER_INTERFACE
    CLASS="MaintainFundFiscalYear"
    NAME="DISPLAY"
    OPERATION="listAllFundFiscalYear"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainFundWaitList"
    NAME="ACTION"
    OPERATION="searchWaitListDetails"
    PHASE="ACTION"
  />


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.SearchCriteria"
  >


    <FIELD
      LABEL="Field.Label.ClientName"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="personID"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00199618, AS -->
    <FIELD
      LABEL="Field.Label.Priority"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$priority"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00199618 -->


    <FIELD
      LABEL="Field.Label.ExpiryDays"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="expiryDays"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.FundFiscalYearName"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="fundFiscalYearID"
          NAME="DISPLAY"
          PROPERTY="fundFiscalYearName"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$fundFiscalYearID"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Label.Status"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="key$status"
        />
      </CONNECT>
    </FIELD>


    <ACTION_SET
      ALIGNMENT="CENTER"
      TOP="false"
    >
      <ACTION_CONTROL
        DEFAULT="true"
        LABEL="ActionControl.Label.Search"
        TYPE="SUBMIT"
      >
        <LINK PAGE_ID="THIS">
         </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Reset">


        <LINK PAGE_ID="FundPM_searchWaitListEntry">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="fundFiscalYearID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundFiscalYearID"
            />
          </CONNECT>


        </LINK>


      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Cancel">

      </ACTION_CONTROL>
    </ACTION_SET>
  </CLUSTER>
  <LIST TITLE="List.Title.SearchResults">


    <!-- BEGIN, CR00207190, PS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Edit.Label">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_updateWaitListEntry"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="waitListEntryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="waitListEntryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="dtls$fundFiscalYearID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundFiscalYearID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Remove.Label">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_removeWaitListEntry"
        >
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="waitListEntryID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="waitListEntryID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="ACTION"
              PROPERTY="dtls$fundFiscalYearID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundFiscalYearID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <!-- END, CR00207190 -->
    <FIELD LABEL="Field.Label.FundFiscalYearName">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$fundFiscalYearName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ParticipantName">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$participantName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.WaitListPosition">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="position"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$priority"
        />
      </CONNECT>
    </FIELD>


    <!--  BEGIN, CR00199275, AS -->
    <FIELD LABEL="Field.Label.ExpiryDate">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$expiryDate"
        />
      </CONNECT>
    </FIELD>
    <!--  END, CR00199275 -->


    <FIELD LABEL="Field.Label.ReviewDate">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="reviewDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Status">
      <CONNECT>
        <SOURCE
          NAME="ACTION"
          PROPERTY="dtls$status"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN, CR00200567, AS -->
    <DETAILS_ROW>


      <!-- BEGIN, CR00205389, AS-->
      <INLINE_PAGE PAGE_ID="FundPM_viewWaitListEntry">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="waitListEntryID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="waitListEntryID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="dtls$fundFiscalYearName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundFiscalYearName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="dtls$participantName"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="participantName"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="position"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="position"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="dtls$priority"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="priority"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="dtls$expiryDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="expiryDate"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="reviewDate"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="reviewDate"
          />
        </CONNECT>
      </INLINE_PAGE>
      <!-- END, CR00205389 -->
    </DETAILS_ROW>
    <!-- END, CR00200567 -->
  </LIST>
</PAGE>
