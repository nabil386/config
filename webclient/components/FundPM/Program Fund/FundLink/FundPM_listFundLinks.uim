<!-- Description 															-->
<!-- =========== 															-->
<!-- This page allows the user to lists all the fund associations.          -->
<PAGE
  PAGE_ID="FundPM_listFundLinks"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="fundIDParam"/>


  <!-- BEGIN, CR00198153, AS  -->
  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="DISPLAY"
    OPERATION="listFundAssociationDetails"
    PHASE="DISPLAY"
  />
  <!-- END, CR00198153  -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programFundID"
    />
  </CONNECT>


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="ActionControl.Label.ViewFund"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="FundPM_viewFundHome">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <!-- BEGIN, CR00261860, GP -->
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="programFundID"
          />
        </CONNECT>
        <!-- END, CR00261860 -->
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.FundConfiguration"
      STYLE="in-page-link"
    >
      <LINK PAGE_ID="FundPM_listFundConfiguration">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.FundAssociation"
      STYLE="in-page-current-link"
    >
      <LINK PAGE_ID="FundPM_listFundLinks">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <!-- BEGIN, CR00227014, PS -->
  <LIST>
    <!-- END, CR00227014 -->
    <!-- BEGIN, CR00207190, PS -->
    <ACTION_SET TYPE="LIST_ROW_MENU">
      <!-- END, CR00207190 -->
      <ACTION_CONTROL LABEL="ActionControl.Edit.Label">
        <LINK PAGE_ID="FundPM_modifyLinkType">
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="fundLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundLinkIDParam"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <!-- BEGIN, CR00207190, PS -->
      <ACTION_CONTROL
        LABEL="ActionControl.Label.Delete"
        TYPE="ACTION"
      >
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_deleteFundLink"
          WINDOW_OPTIONS="width=450"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="fundLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundLinkIDParam"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>
    <!-- END, CR00207190 -->


    <FIELD LABEL="Field.Name.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedTypeName"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00207190, PS -->
    <FIELD LABEL="Field.Label.AssociationType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="relatedType"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00207190 -->
    <!-- BEGIN, CR00227014, PS -->
    <FIELD LABEL="Field.Priority.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="priority"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00227014 -->
    <FIELD LABEL="Field.StartDate.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.EndDate.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00198153, AS  -->
    <FIELD LABEL="Field.Status.Label">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="fundAssociationDtls$recordStatus"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00198153  -->
    <!-- BEGIN, CR00207190, PS -->
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="FundPM_viewFundLink">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="fundLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundLinkIDParam"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="relatedID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="relatedID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
  </LIST>
</PAGE>
