<!-- Description 															-->
<!-- =========== 															-->
<!-- This page allows the user to modify an existing link between a         -->
<!-- fund and a related Type.                                               -->
<PAGE
  PAGE_ID="FundPM_modifyFundLink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Text1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="name"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title.Text1"
      />
    </CONNECT>
    <CONNECT>
      <SOURCE
        NAME="readFundLink"
        PROPERTY="relatedTypeName"
      />


    </CONNECT>


  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="fundIDParam"/>
  <PAGE_PARAMETER NAME="fundLinkIDParam"/>
  <PAGE_PARAMETER NAME="relatedTypeDesc"/>
  <PAGE_PARAMETER NAME="relatedTypeName"/>
  <PAGE_PARAMETER NAME="relatedIDParam"/>
  <PAGE_PARAMETER NAME="linkTypeParam"/>


  <SERVER_INTERFACE
    CLASS="MaintainProgramFund"
    NAME="DISPLAY"
    OPERATION="readProgramFund"
    PHASE="DISPLAY"
  />


  <!-- BEGIN, CR00198153, AS  -->
  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="readFundLink"
    OPERATION="viewFundAssociationDetails"
    PHASE="DISPLAY"
  />


  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="ACTION"
    OPERATION="modifyFundLink"
    PHASE="ACTION"
  />


  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="readAssociations"
    OPERATION="searchByFundAssociationRelatedID"
    PHASE="DISPLAY"
  />
  <!-- END, CR00198153  -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programFundID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="relatedIDParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="relatedID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="linkTypeParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="relatedType"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="readFundLink"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="programFundID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundLinkIDParam"
    />
    <TARGET
      NAME="readFundLink"
      PROPERTY="key$fundLinkID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundLinkIDParam"
    />
    <TARGET
      NAME="readAssociations"
      PROPERTY="key$fundLinkID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundLinkIDParam"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="fundLinkID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="relatedIDParam"
    />
    <TARGET
      NAME="readAssociations"
      PROPERTY="key$relatedID"
    />
  </CONNECT>


  <CLUSTER
    NUM_COLS="2"
    TITLE="Cluster.Title.FundAssociation"
  >
    <FIELD LABEL="Field.Label.AssociationType">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="relatedTypeDesc"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="readFundLink"
          PROPERTY="startDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="readFundLink"
          PROPERTY="priority"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="priority"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.FundedItem">
      <CONNECT>
        <SOURCE
          NAME="PAGE"
          PROPERTY="relatedTypeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="readFundLink"
          PROPERTY="endDate"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00426143, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <!-- END, CR00426143 -->
      <CONNECT>
        <SOURCE
          NAME="readFundLink"
          PROPERTY="comments"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.Funds">
    <CONTAINER LABEL="Container.Label">
      <ACTION_CONTROL LABEL="ActionControl.View.Label">
        <!-- BEGIN, CR00200427, RD -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="FundPM_viewFundLinkPopUp"
        >
          <CONNECT>
            <SOURCE
              NAME="readAssociations"
              PROPERTY="programFundID"
            />
            <!-- END, CR00200427 -->
            <TARGET
              NAME="PAGE"
              PROPERTY="fundIDParam"
            />
          </CONNECT>
          <!-- BEGIN, CR00198153, AS  -->
          <CONNECT>
            <SOURCE
              NAME="readAssociations"
              PROPERTY="fundAssociationDtls$fundLinkID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="fundLinkIDParam"
            />
          </CONNECT>
          <!-- END, CR00198153  -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="relatedIDParam"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="relatedID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>
    <FIELD LABEL="Field.Label.FundedItem">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="relatedTypeName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Fund">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="fundName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="priority"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET ALIGNMENT="CENTER">


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Previous"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="FundPM_modifyLinkType">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundLinkIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundLinkIDParam"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="FundPM_listFundLinks">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="FundPM_listFundLinks">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </ACTION_SET>


</PAGE>
