<!-- This page allows the user to view the fund link association details.   -->
<PAGE
  PAGE_ID="FundPM_viewFundLink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>


    <CONNECT>
      <SOURCE
        NAME="DISPLAY"
        PROPERTY="name"
      />
    </CONNECT>


  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="fundLinkIDParam"/>
  <PAGE_PARAMETER NAME="fundIDParam"/>
  <PAGE_PARAMETER NAME="relatedID"/>


  <SERVER_INTERFACE
    CLASS="MaintainProgramFund"
    NAME="DISPLAY"
    OPERATION="readProgramFund"
    PHASE="DISPLAY"
  />
  <!-- BEGIN, CR00198153, AS  -->
  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="readFundLink"
    OPERATION="viewFundAssociationDetails"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="MaintainFundLink"
    NAME="readAssociations"
    OPERATION="searchByFundAssociationRelatedID"
    PHASE="DISPLAY"
  >
  </SERVER_INTERFACE>
  <!-- END, CR00198153  -->


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundLinkIDParam"
    />
    <TARGET
      NAME="readFundLink"
      PROPERTY="key$fundLinkID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundIDParam"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$programFundID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="relatedID"
    />
    <TARGET
      NAME="readAssociations"
      PROPERTY="key$relatedID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="fundLinkIDParam"
    />
    <TARGET
      NAME="readAssociations"
      PROPERTY="key$fundLinkID"
    />
  </CONNECT>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00426143, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <!-- END, CR00426143 -->
      <CONNECT>
        <SOURCE
          NAME="readFundLink"
          PROPERTY="comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <LIST TITLE="List.Title.Funds">
    <!-- BEGIN, CR00207190, PS -->
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="priority"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="readAssociations"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="FundPM_viewFundLinkPopUp">
        <CONNECT>
          <SOURCE
            NAME="readAssociations"
            PROPERTY="result$fundAssociationDtlsList$fundAssociationDtls$programFundID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundIDParam"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="relatedID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="relatedID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="readAssociations"
            PROPERTY="result$fundAssociationDtlsList$fundAssociationDtls$fundLinkID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="fundLinkIDParam"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>
    <!-- END, CR00207190 -->
  </LIST>


</PAGE>
