<!-- Description 															-->
<!-- =========== 															-->
<!-- The page allows the user to create a program fund.   							-->
<PAGE
  PAGE_ID="FundPM_createProgramFund"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />


    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="MaintainProgramFund"
    NAME="insertFundBean"
    OPERATION="createProgramFund"
    PHASE="ACTION"
  />
  <!-- BEGIN, CR00227014, PS -->
  <CLUSTER
    LABEL_WIDTH="45"
    NUM_COLS="2"
  >
    <!-- END, CR00227014 -->
    <FIELD
      LABEL="Field.Label.Name"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.StartDate"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00227014, PS -->
    <FIELD
      LABEL="Field.Label.Type"
      USE_BLANK="true"
      USE_DEFAULT="false"
      WIDTH="50"
    >
      <!-- END, CR00227014 -->
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.EndDate"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="endDate"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <CLUSTER TITLE="Cluster.Label.FundConfigurationDetails">
    <!-- BEGIN, CR00227014, PS -->
    <FIELD
      LABEL="Field.Label.EffectiveDate"
      USE_BLANK="true"
      WIDTH="35"
    >
      <!-- END, CR00227014 -->
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="effectiveDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.OverObligationAllowedWhenCreatingObligation">
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="overObligationIndCreateOblign"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Lalel.OverObligationIndAdjustOblign">
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="overObligationIndAdjustOblign"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00227014, PS -->
    <FIELD LABEL="Field.Lalel.WaitListAllowedInd">
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="waitListAllowedInd"
        />
      </CONNECT>
    </FIELD>
    <!-- END, CR00227014 -->


  </CLUSTER>


  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >


    <!-- BEGIN, CR00426143, GK -->
    <FIELD
      HEIGHT="4"
      LABEL="Field.Label.Comments"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <!-- END, CR00426143 -->
      <CONNECT>
        <TARGET
          NAME="insertFundBean"
          PROPERTY="dtls$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <ACTION_SET
    ALIGNMENT="CENTER"
    BOTTOM="true"
    TOP="false"
  >
    <!-- BEGIN, CR00227014, PS -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00176311, ABS -->
      <!-- BEGIN, CR00198414, AS -->
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="FundPM_createProgramFund"
      />
      <!-- END, CR00176311 -->
      <!-- END, CR00198414 -->
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <!-- END, CR00227014 -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="FundPM_listProgramFund"/>
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
