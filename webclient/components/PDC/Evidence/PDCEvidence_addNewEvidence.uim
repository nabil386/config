<!-- Description                                                            -->
<!-- ===========                                                            -->
<!-- Infrastructure page containing a selection of custom evidence which    -->
<!-- may be created.                                                        -->
<PAGE PAGE_ID="PDCEvidence_addNewEvidence" WINDOW_OPTIONS="height=520,width=700" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE DESCRIPTION="Page.Title.Description">
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="Page.Title"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="concernRoleID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="concernRoleID"/>
    <TARGET NAME="DISPLAY" PROPERTY="key$concernRoleID"/>
  </CONNECT>


  <SERVER_INTERFACE CLASS="PDCPerson" NAME="DISPLAY" OPERATION="listPDCEvidenceTypeDetails" PHASE="DISPLAY"/>


  <LIST PAGINATED="false">


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL APPEND_ELLIPSIS="true" LABEL="ActionControl.Label.Add">
        <LINK DISMISS_MODAL="false" PAGE_ID="Evidence_resolveDynEvdCreateEvidencePage" SAVE_LINK="false">
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="caseID"/>
            <TARGET NAME="PAGE" PROPERTY="caseID"/>
          </CONNECT>
          <CONNECT>
            <SOURCE NAME="DISPLAY" PROPERTY="result$list$evidenceType"/>
            <TARGET NAME="PAGE" PROPERTY="evidenceType"/>
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD LABEL="List.Title.EvidenceType" WIDTH="33">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$list$evidenceType"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="List.Title.Description" WIDTH="67">
      <CONNECT>
        <SOURCE NAME="DISPLAY" PROPERTY="result$list$evidenceTypeDescription"/>
      </CONNECT>
    </FIELD>
  </LIST>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">


    <ACTION_CONTROL LABEL="ActionControl.Label.Close"/>


  </ACTION_SET>


</PAGE>