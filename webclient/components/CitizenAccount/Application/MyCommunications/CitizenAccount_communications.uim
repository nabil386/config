<!--BEGIN, CR00467332, MR-->
<PAGE
  PAGE_ID="CitizenAccount_communications"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!--END, CR00467332-->


  <PAGE_TITLE ICON="CitizenAccount_communication.Page.Icon">
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="page.title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CitizenAccount"
    NAME="DISPLAY"
    OPERATION="listCitizenCommunications"
  />


  <CLUSTER>


    <LIST SUMMARY="Table.Summary">
      <FIELD LABEL="Field.Label.From">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="userName"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Subject">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="subjectText"
          />
        </CONNECT>
      </FIELD>
      <FIELD LABEL="Field.Label.Date">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="communicationDate"
          />
        </CONNECT>
      </FIELD>
      <DETAILS_ROW>


        <INLINE_PAGE PAGE_ID="CitizenAccount_viewCommunication">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="communicationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="communicationID"
            />
          </CONNECT>
        </INLINE_PAGE>


      </DETAILS_ROW>


    </LIST>
  </CLUSTER>


</PAGE>
