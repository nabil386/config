<!-- Page used to link from the IEGTester page to the IEGScriptPlayer JSP.  -->
<!-- A check for validation errors is carried out first and if there are    -->
<!-- any present, the validation errors page is displayed.                  -->
<PAGE PAGE_ID="CitizenAccount_resolveResumeApplication">
	<JSP_SCRIPTLET>
    <![CDATA[
    // BEGIN 118764, PC	
      out.print("<script>top.CuramExternalApp.hideAppNavigation();</script>");
    // END 118764    
      javax.servlet.http.HttpServletRequest hsRequest = (javax.servlet.http.HttpServletRequest) request;
      javax.servlet.http.HttpSession httpSession = hsRequest.getSession();

      String url;
	  String contextPath = request.getContextPath();
	  String iegExecutionID = request.getParameter("iegExecutionID");
	  String playerID = request.getParameter("playerID");
      
	  url = contextPath + "/cw/ResumeIntakeIEGScript.jspx?iegExecutionID=" + curam.omega3.request.RequestUtils.escapeURL(iegExecutionID) + "&id=" + curam.omega3.request.RequestUtils.escapeURL(playerID);
    // BEGIN 118764, PC     
      out.print("<script>window.location.href='" + url + "'</script>");
    // END 118764  
    ]]>
  </JSP_SCRIPTLET>
</PAGE>
