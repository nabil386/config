<PAGE PAGE_ID="CitizenAccount_withdrawIntakeApplication" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    </CONNECT>
    <CONNECT>
      <SOURCE NAME="PAGE" PROPERTY="programName" />
    </CONNECT>
  </PAGE_TITLE>

  <SERVER_INTERFACE CLASS="CitizenAccount" NAME="ACTION" OPERATION="withdrawIntakeProgramApplication" PHASE="ACTION"/>

  <PAGE_PARAMETER NAME="intakeProgramApplicationID"/>
  <PAGE_PARAMETER NAME="programName"/>

  <CONNECT>
  	<SOURCE NAME="PAGE" PROPERTY="intakeProgramApplicationID"/>
  	<TARGET NAME="ACTION" PROPERTY="intakeProgramApplicationID"/>
  </CONNECT>
  
  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>

 <CLUSTER TITLE="Cluster.Title.Details">
    <FIELD  LABEL="Field.Title.WithdrawReason" USE_BLANK="true" USE_DEFAULT="false">
        <CONNECT>
          <TARGET NAME="ACTION"  PROPERTY="withdrawalRequestReason"/>
        </CONNECT>
    </FIELD>
    <FIELD  LABEL="Field.Title.OtherReason">
        <CONNECT>
          <TARGET NAME="ACTION"  PROPERTY="otherQReqReason"/>
        </CONNECT>
    </FIELD>
  </CLUSTER>

  <CLUSTER TITLE="Cluster.Title.SignatureDetails" DESCRIPTION="Cluster.Title.SignatureDetails.Description" STYLE="cluster-inner-tds-no-padding">
    <CLUSTER>
      <CLUSTER LAYOUT_ORDER="FIELD" LABEL_WIDTH="96%">
        <FIELD LABEL="Field.Title.ElectronicSignInd" ALIGNMENT="LEFT">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="electronicSigInd" />
          </CONNECT>
        </FIELD>
      </CLUSTER>
      <CLUSTER NUM_COLS="3" STYLE="cluster-no-border-all">
        <FIELD LABEL="Field.Title.FirstName">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="firstName" />
          </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.MiddleName">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="middleName" />
          </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.LastName">
          <CONNECT>
            <TARGET NAME="ACTION" PROPERTY="surname" />
          </CONNECT>
        </FIELD>
      </CLUSTER>
    </CLUSTER>
    <CLUSTER SHOW_LABELS="false" STYLE="cluster-no-border-all">
      <FIELD>
        <CONNECT>
          <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.WithdrawApplication"/>
        </CONNECT>
      </FIELD>
    </CLUSTER>
  </CLUSTER>

</PAGE>
