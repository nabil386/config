<!-- Page used to open a triage link in let us help you panel to the triage --> 
<!-- tab in citizen account home page.  -->
<PAGE PAGE_ID="CitizenAccount_goTriage">

    <PAGE_TITLE>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="page.title"/>
      </CONNECT>
    </PAGE_TITLE>
	
	  <JSP_SCRIPTLET>
	    <![CDATA[ 
	      out.println("<script>");
	      out.println("var url = window.location.href;");
	      out.println("var slashIndex = url.lastIndexOf(\"/\");");
	      out.println("var root = url.substring(0, slashIndex + 1);");
        out.println("var rand = Math.floor(Math.random()*100000);");
	      out.println("root += 'CitizenAccount_triagePage.do?o3ctx=4096&o3nocache='+rand");
	      out.println("setTimeout(\"window.location=root\",250);");
	      out.println("</script>");
	      ]]>
  </JSP_SCRIPTLET>
</PAGE>
