<PAGE PAGE_ID="CitizenAccount_lifeEventUpdates" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

    <SERVER_INTERFACE CLASS="CitizenLifeEvents" NAME="DISPLAY" OPERATION="listSubmittedLifeEvents"/>

    <PAGE_TITLE>
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1"/>
        </CONNECT>
    </PAGE_TITLE>
     
	<ACTION_SET>
        <ACTION_CONTROL LABEL="Field.Title.LifeEventName">
            <LINK PAGE_ID="CitizenAccount_listLifeEvents"/>
        </ACTION_CONTROL>
	</ACTION_SET>
	
	<CLUSTER STYLE="pageDescriptionStyle" SHOW_LABELS="false">
    	<FIELD >
			<CONNECT>
	            <SOURCE NAME="TEXT" PROPERTY="PageDescription.text"/>
	        </CONNECT>
		</FIELD> 
  	</CLUSTER>

    <LIST SUMMARY="Table.Summary">        
        <FIELD LABEL="Field.Title.LifeEventName">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="lifeEventName"/>
            </CONNECT>
        </FIELD>
        
        <FIELD LABEL="Field.Title.DateSubmitted">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="submittedDate"/>
            </CONNECT>
        </FIELD>
        
        <ACTION_CONTROL LABEL_ABBREVIATION="Container.Label.Action" LABEL="ActionControl.Label.ViewAnswers">
            <LINK PAGE_ID="CitizenAccount_resolveStartReviewLifeEventScript">
              <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="lifeEventID"/>
                <TARGET NAME="PAGE" PROPERTY="lifeEventID"/>
              </CONNECT>
            </LINK>
         </ACTION_CONTROL>
    </LIST>
</PAGE>