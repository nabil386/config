<PAGE 
	PAGE_ID="CitizenAccount_viewRecommendations" 
	SCRIPT_FILE="display-hide-div.js"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  
  	<PAGE_TITLE>
    	<CONNECT>
      	<SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
    	</CONNECT>
  	</PAGE_TITLE>
  	
  	<SERVER_INTERFACE CLASS="CitizenLifeEvents" NAME="DISPLAY" OPERATION="getRecommendations" />
  
  	<PAGE_PARAMETER NAME="lifeEventID" />
  	<PAGE_PARAMETER NAME="id" />
  	
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="lifeEventID" />
		<TARGET NAME="DISPLAY" PROPERTY="lifeEventID" />
	</CONNECT>    

	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="id" />
		<TARGET NAME="DISPLAY" PROPERTY="sessionID" />
	</CONNECT>    

	
  	<CLUSTER
  		STYLE="outer-cluster-borderless-nospace"
  		SHOW_LABELS="false"
  	>
    	<FIELD LABEL="Field.Header.Title" DESCRIPTION="Field.Header.Description">
			<CONNECT>
				<SOURCE NAME="DISPLAY" PROPERTY="header" />
			</CONNECT>
		</FIELD>      
  	</CLUSTER>		
  
  	<CLUSTER 
  		STYLE="outer-cluster-borderless-nospace"
  		SHOW_LABELS="false"
  	>
  		
			<FIELD>
				<CONNECT>
					<SOURCE NAME="DISPLAY" PROPERTY="recommendationsXML" />
				</CONNECT>
			</FIELD>  
  
  	</CLUSTER>
  
</PAGE>
