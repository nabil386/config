<PAGE 
  PAGE_ID="CitizenAccount_lifeEventsLanding" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">

    <PAGE_TITLE>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="page.title"/>
      </CONNECT>
    </PAGE_TITLE>
  
    <JSP_SCRIPTLET>
      <![CDATA[ 
        out.println("<script>");
        out.println("var url = window.location.href;");
        out.println("var slashIndex = url.lastIndexOf(\"/\");");
        out.println("var root = url.substring(0, slashIndex + 1);");
        out.println("root += 'CitizenAccount_resolveLifeEventsPage.do'");
        out.println("var questionIndex = url.lastIndexOf(\"?\");");
        out.println("var params = url.substring(questionIndex);");
        out.println("root += params");
        out.println("setTimeout(\"window.location=root\",250);");
        out.println("</script>");
        ]]>
    </JSP_SCRIPTLET>

</PAGE>
