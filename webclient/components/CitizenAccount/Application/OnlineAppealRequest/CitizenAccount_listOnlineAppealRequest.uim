<PAGE PAGE_ID="CitizenAccount_listOnlineAppealRequest" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
    <PAGE_TITLE ICON="CitizenAccount_listOnlineAppealRequest.Page.Icon">
        <CONNECT>
            <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText1" />
        </CONNECT>
    </PAGE_TITLE>
    
    <SERVER_INTERFACE CLASS="CitizenAccount" NAME="DISPLAY" OPERATION="listOnlineAppealRequests" />
    
    <ACTION_SET>
        <ACTION_CONTROL LABEL="ActionControl.Label.NewAppeal">
            <LINK PAGE_ID="CitizenWorkspace_appeals"/>
        </ACTION_CONTROL>
    </ACTION_SET>
    
    <LIST TITLE="List.Title.OnlineAppealRequest" DESCRIPTION="List.Description.OnlineAppealRequest">
		<CONTAINER LABEL="Field.Title.Request" WIDTH="25">
			<FIELD>
				<CONNECT>
					<SOURCE NAME="TEXT" PROPERTY="Page.StaticText1" />
				</CONNECT>
				<LINK URI_SOURCE_NAME="DISPLAY" URI_SOURCE_PROPERTY="documentLink" />
			</FIELD>
            <ACTION_CONTROL	IMAGE="PDF.Icon" LABEL="ActionControl.Label.PDF">
				<LINK URI_SOURCE_NAME="DISPLAY" URI_SOURCE_PROPERTY="documentLink" />
			</ACTION_CONTROL>
		</CONTAINER>
		
        <FIELD LABEL="Field.Title.PrimaryAppellant">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="primaryAppellant" />
            </CONNECT>
        </FIELD>
        <FIELD LABEL="Field.Title.DateSubmitted">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="submittedDateTime" />
            </CONNECT>
        </FIELD>		
        <FIELD LABEL="Field.Title.Status">
            <CONNECT>
                <SOURCE NAME="DISPLAY" PROPERTY="status" />
            </CONNECT>
        </FIELD>
    </LIST>
</PAGE>
