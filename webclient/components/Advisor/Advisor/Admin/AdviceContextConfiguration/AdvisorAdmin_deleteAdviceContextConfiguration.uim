<!-- Cancels an existing context record.                                    -->
<PAGE PAGE_ID="AdvisorAdmin_deleteAdviceContextConfiguration" WINDOW_OPTIONS="width=400" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE CLASS="AdvisorAdmin" NAME="ACTION" OPERATION="removeAdviceContextConfiguration" PHASE="ACTION"/>


  <PAGE_PARAMETER NAME="adviceContextConfigurationID"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="adviceContextConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="adviceContextConfigurationID"/>
  </CONNECT>


  <ACTION_SET TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Yes" TYPE="SUBMIT">
      <LINK PAGE_ID="AdvisorAdmin_listAdviceContextConfiguration"/>
    </ACTION_CONTROL>
    <ACTION_CONTROL LABEL="ActionControl.Label.No"/>
  </ACTION_SET>


  <CLUSTER SHOW_LABELS="false" STYLE="outer-cluster-borderless">
    <FIELD>
      <CONNECT>
        <SOURCE NAME="TEXT" PROPERTY="Field.StaticText.DeleteContext"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>