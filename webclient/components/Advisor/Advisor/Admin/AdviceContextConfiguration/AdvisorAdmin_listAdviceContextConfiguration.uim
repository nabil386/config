<!-- Lists advice context config records.                                   -->
<PAGE
  PAGE_ID="AdvisorAdmin_listAdviceContextConfiguration"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="AdvisorAdmin"
    NAME="DISPLAY"
    OPERATION="listAdviceContextConfiguration"
  />


  <ACTION_SET BOTTOM="false">
    <ACTION_CONTROL
      IMAGE="NewButton"
      LABEL="ActionControl.Label.New"
    >
      <LINK
        OPEN_MODAL="true"
        PAGE_ID="AdvisorAdmin_newAdviceContextConfiguration"
      />
    </ACTION_CONTROL>
  </ACTION_SET>


  <LIST>
    <ACTION_SET TYPE="LIST_ROW_MENU">


      <ACTION_CONTROL LABEL="ActionControl.Label.AddAdviceContextKey">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_newAdviceContextKey"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adviceContextConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adviceContextConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.AddRuleSet">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_newAdviceContextRules"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adviceContextConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adviceContextConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_modifyAdviceContextConfiguration"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adviceContextConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adviceContextConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_deleteAdviceContextConfiguration"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="adviceContextConfigurationID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="adviceContextConfigurationID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="60"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.DateCreated"
      WIDTH="15"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.CreatedBy"
      WIDTH="25"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createdBy"
        />
      </CONNECT>
    </FIELD>


    <DETAILS_ROW>
      <INLINE_PAGE PAGE_ID="AdvisorAdmin_listAdviceContextRules">
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="adviceContextConfigurationID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="adviceContextConfigurationID"
          />
        </CONNECT>
      </INLINE_PAGE>
    </DETAILS_ROW>


  </LIST>


</PAGE>
