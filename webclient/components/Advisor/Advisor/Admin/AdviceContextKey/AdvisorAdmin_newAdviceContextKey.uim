<!-- Creates a new advice context key for an advice context configuration               -->
<!-- BEGIN,CR00279018,AM-->
<!-- BEGIN, CR00350333, SS -->
<PAGE PAGE_ID="AdvisorAdmin_newAdviceContextKey" WINDOW_OPTIONS="width=500,height=130" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END, CR00350333 -->
  <!-- END,CR00279018,AM -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="adviceContextConfigurationID"/>
  <SERVER_INTERFACE CLASS="AdvisorAdmin" NAME="ACTION" OPERATION="createAdviceContextKey" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="adviceContextConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="adviceContextConfigurationID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN, CR00350333, SS -->
  <CLUSTER LABEL_WIDTH="50">
    <!-- END, CR00350333 -->
    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="name"/>
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="type"/>
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>