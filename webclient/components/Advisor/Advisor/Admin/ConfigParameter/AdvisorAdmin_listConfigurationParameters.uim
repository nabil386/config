<!-- Lists parameter records for a particular advice context key.                    -->
<PAGE
  PAGE_ID="AdvisorAdmin_listConfigurationParameters"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <SERVER_INTERFACE
    CLASS="AdvisorAdmin"
    NAME="DISPLAY"
    OPERATION="listConfigurationParameters"
  />


  <PAGE_PARAMETER NAME="adviceContextKeyID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="adviceContextKeyID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$adviceContextKeyID"
    />
  </CONNECT>


  <LIST>


    <ACTION_SET TYPE="LIST_ROW_MENU">
      <ACTION_CONTROL LABEL="ActionControl.Label.Edit">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_modifyConfigurationParameter"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="configurationParameterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="configurationParameterID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL LABEL="ActionControl.Label.Delete">
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="AdvisorAdmin_deleteConfigurationParameter"
        >
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="configurationParameterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="configurationParameterID"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
    </ACTION_SET>


    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN,CR00274001,AM-->
    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="15"
    >
      <!-- END,CR00274001,AM-->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>


    <FIELD
      LABEL="Field.Title.DateCreated"
      WIDTH="30"
    >
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="startDate"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN,CR00274001,AM-->
    <FIELD
      LABEL="Field.Title.CreatedBy"
      WIDTH="25"
    >
      <!-- END,CR00274001,AM-->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createdBy"
        />
      </CONNECT>
    </FIELD>


  </LIST>


</PAGE>
