<!-- Creates a new parameter for a page                                -->
<!-- BEGIN,CR00274001,CR00279018,AM-->
<PAGE
  PAGE_ID="AdvisorAdmin_newConfigurationParameter"
  WINDOW_OPTIONS="width=500,height=120"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END,CR00274001,CR00279018,AM-->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="adviceContextKeyID"/>


  <SERVER_INTERFACE
    CLASS="AdvisorAdmin"
    NAME="ACTION"
    OPERATION="createConfigurationParameter"
    PHASE="ACTION"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="adviceContextKeyID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="adviceContextKeyID"
    />
  </CONNECT>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN,CR00274001,AM-->
  <CLUSTER LABEL_WIDTH="35">


    <FIELD
      LABEL="Field.Title.Name"
      WIDTH="60"
    >
      <!-- END,CR00274001,AM-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <!-- BEGIN,CR00274001,AM-->
    <FIELD
      LABEL="Field.Title.Type"
      WIDTH="60"
    >
      <!-- END,CR00274001,AM-->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
