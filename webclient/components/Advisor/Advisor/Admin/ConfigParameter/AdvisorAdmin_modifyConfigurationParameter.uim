<!-- Modifies an existing parameter.                                             -->
<!-- BEGIN,CR00274001,AM-->
<PAGE
  PAGE_ID="AdvisorAdmin_modifyConfigurationParameter"
  WINDOW_OPTIONS="width=500,height=150"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <!-- END,CR00274001,AM-->


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>


  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="AdvisorAdmin"
    NAME="DISPLAY"
    OPERATION="viewConfigurationParameter"
  />


  <SERVER_INTERFACE
    CLASS="AdvisorAdmin"
    NAME="ACTION"
    OPERATION="modifyConfigurationParameter"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="configurationParameterID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="configurationParameterID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$configurationParameterID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="configurationParameterID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="configurationParameterID"
    />
  </CONNECT>


  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="versionNo"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="versionNo"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="DISPLAY"
      PROPERTY="adviceContextKeyID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="adviceContextKeyID"
    />
  </CONNECT>
  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="1"
  >
    <FIELD LABEL="Field.Title.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="name"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="name"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Title.Type">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="type"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="type"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>
