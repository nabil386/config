<!-- Creates a new rule set for an advice context config                              -->
<!-- BEGIN,CR00274001,AM-->
<PAGE PAGE_ID="AdvisorAdmin_newAdviceContextRules" WINDOW_OPTIONS="width=500,height=120" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd">
  <!-- END,CR00274001,AM-->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE NAME="TEXT" PROPERTY="PageTitle.StaticText"/>
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE CLASS="CREOLERuleSetAdmin" NAME="DISPLAY" OPERATION="listAllPublishedRuleSets"/>
  <PAGE_PARAMETER NAME="adviceContextConfigurationID"/>
  <SERVER_INTERFACE CLASS="AdvisorAdmin" NAME="ACTION" OPERATION="createAdviceContextRules" PHASE="ACTION"/>


  <CONNECT>
    <SOURCE NAME="PAGE" PROPERTY="adviceContextConfigurationID"/>
    <TARGET NAME="ACTION" PROPERTY="adviceContextConfigurationID"/>
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER" TOP="false">
    <ACTION_CONTROL LABEL="ActionControl.Label.Save" TYPE="SUBMIT"/>
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <CLUSTER LABEL_WIDTH="30">


    <FIELD LABEL="Field.Title.RuleSet">
      <CONNECT>
        <INITIAL NAME="DISPLAY" PROPERTY="name"/>
      </CONNECT>
      <CONNECT>
        <TARGET NAME="ACTION" PROPERTY="creoleRulesetName"/>
      </CONNECT>
    </FIELD>


  </CLUSTER>


</PAGE>