<!-- This jsp redirects to the received pageID      -->
<PAGE
  PAGE_ID="PodContainer_redirect"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
    
      String pageID = request.getParameter("pageID");
      String concernRoleID = request.getParameter("concernRoleID");

      String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory
        .getUserPreferences(pageContext.getSession()).getLocale() + "/";

      String targetPage = curam.omega3.request.RequestUtils.escapeURL(pageID) + "Page.do";
      String url = context + targetPage;     

      curam.omega3.request.RequestHandler 
        rh = curam.omega3.request.
            RequestHandlerFactory.getRequestHandler(request);
            
      url += "?o3ctx=4096";
      
      //CR00445909, DM
      if (!((concernRoleID==null)||(concernRoleID.equals("")))){
      	url+="&amp;concernRoleID="+curam.omega3.request.RequestUtils.escapeURL(concernRoleID);
      }
      //END CR00445909
      
      response.sendRedirect(response.encodeRedirectURL(url));
    
    
  </JSP_SCRIPTLET>
</PAGE>
