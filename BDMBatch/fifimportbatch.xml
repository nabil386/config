<?xml version="1.0" encoding="UTF-8"?>
<project default="runFIFImport"  name="fifImportBatch">

    <property name="chunker" value="curam.ca.gc.bdm.batch.bdmimportfif.intf.BDMImportFIFFromFile.process"/>
    <property name="batch" value="fifImportBatch"/>
  
	<import file="BDMBatchBase.xml"/>
	
	<target name="runFIFImport">
			<sequential>
				<sleep seconds="10"/>
				<antcall target="BDMBatchBase.runChunker">
					<param name="batch.program" value="${chunker}"/>
					<param name="batch.parameters" value="${batch.parameters}"/>
					<param name="batch" value="${batch}"/>
				</antcall>
			</sequential>
		</target>
</project>
