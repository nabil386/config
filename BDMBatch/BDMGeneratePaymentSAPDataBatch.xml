<?xml version="1.0" encoding="UTF-8"?>
<project default="runBDMGeneratePaymentSAPDataBatch"  name="BDMGeneratePaymentSAPDataBatch">

    <property name="chunker" value="curam.ca.gc.bdm.batch.bdmgeneratepaymentstagingsapdata.intf.BDMGeneratePaymentSAPDataBatch.process"/>
    <property name="batch" value="BDMGeneratePaymentStagingDataBatch"/>
  
	<import file="BDMGeneratePaymentSAPDataBatch.xml"/>
	
	<target name="runBDMGeneratePaymentSAPDataBatch">
			<sequential>
				<sleep seconds="10"/>
				<antcall target="BDMBatchBase.runChunker">
					<param name="batch.program" value="${chunker}"/>
					<param name="batch.parameters" value="${batch.parameters}"/>
					<param name="batch" value="${batch}"/>
				</antcall>
			</sequential>
		</target>
</project>
