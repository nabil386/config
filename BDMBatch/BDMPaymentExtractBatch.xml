<?xml version="1.0" encoding="UTF-8"?>
<project default="runBDMGeneratePaymentStagingDataBatch"  name="BDMGeneratePaymentStagingDataBatch">

    <property name="chunker" value="curam.ca.gc.bdm.batch.bdmgeneratepaymentstagingdatabatch.intf.BDMGeneratePaymentStagingDataBatch.process"/>
    <property name="batch" value="BDMGeneratePaymentStagingDataBatch"/>
  
	<import file="BDMBatchBase.xml"/>
	
	<target name="runBDMGeneratePaymentStagingDataBatch">
			<sequential>
				<sleep seconds="10"/>
				<antcall target="BDMBatchBase.runChunker">
					<param name="batch.program" value="${chunker}"/>
					<param name="batch.parameters" value="${batch.parameters}"/>
					<param name="batch" value="${batch}"/>
				</antcall>
			</sequential>
		</target>
</project>
