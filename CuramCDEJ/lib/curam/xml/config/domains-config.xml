    <dc:plug-in name="converter"
      class="curam.util.client.domain.convert.SvrBlobConverter"/>
    <dc:plug-in name="comparator"
      class="curam.util.client.domain.compare.SvrBlobComparator"/>
    <dc:plug-in name="default"
      class="curam.util.client.domain.defaults.SvrBlobDefault"/>
    <dc:plug-in name="select-renderer"
                                  purpose="curam-util-client::select-value"/>
    <dc:plug-in name="edit-renderer" purpose="curam-util-client::edit-text"/>
    <dc:plug-in name="view-renderer" purpose="view-text"/>
    <dc:plug-in name="marshal" purpose="marshal-text"/>
  </dc:domain>

