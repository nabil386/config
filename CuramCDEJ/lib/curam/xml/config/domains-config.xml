  <!--
  =============================================================================
  Configuration for non-root domains that are recognized for special handling
  by the infrastructure.
  =============================================================================
  -->
  <dc:domain name="CODETABLE_CODE" plug-in-target="descendant">
    <!--
    This configuration applies to descendant domains of CODETABLE_CODE, but not
    to the CODETABLE_CODE domain itself; it will inherit its configuration from
    its parent.
    -->
    <dc:plug-in name="converter" purpose="convert-ct"/>
    <dc:plug-in name="comparator" purpose="compare-ct"/>
    <dc:plug-in name="default" purpose="default-value-ct"/>
    <dc:plug-in name="edit-renderer" 
                purpose="edit-code-table"/>
    <!-- Inherits the rest from SVR_STRING. -->
  </dc:domain>

  <dc:domain name="SHORT_CODETABLE_CODE" plug-in-target="descendant">
    <!--
    This configuration applies to descendant domains of SHORT_CODETABLE_CODE,
    but not to the SHORT_CODETABLE_CODE domain itself; it will inherit its
    configuration from its parent.
    -->
    <dc:plug-in name="converter" purpose="convert-ct"/>
    <dc:plug-in name="comparator" purpose="compare-ct"/>
    <dc:plug-in name="default" purpose="default-value-ct"/>
    <dc:plug-in name="edit-renderer"
      class="curam.util.client.domain.render.legacy.edit.LegacyCTRadioGroupEditRenderer"/>
    <!-- Inherits the rest from SVR_STRING. -->
  </dc:domain>

