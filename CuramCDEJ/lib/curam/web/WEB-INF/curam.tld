      <description>The field to set.</description>
      <name>targetField</name>
      <required>true</required>
    </attribute>
    <attribute>
      <description>The TextHelper bean instance name to get the source field value from</description>
      <name>sourceBean</name>
    </attribute>
    <attribute>
      <description>The field to get the source value from.</description>
      <name>sourceField</name>
    </attribute>
    <attribute>
      <description>The request parameter to get the source value from.</description>
      <name>sourceParameter</name>
    </attribute>
    <attribute>
      <description>The literal source value.</description>
      <name>value</name>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
