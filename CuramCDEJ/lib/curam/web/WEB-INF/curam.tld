  <tag>
    <description>Declares an instance of a server access TextHelper</description>
    <name>useTextHelper</name>
    <tag-class>curam.omega3.taglib.bean.UseTextHelperTag</tag-class>
    <tei-class>curam.omega3.taglib.bean.UseTextHelperTEI</tei-class>
    <body-content>empty</body-content>
    <attribute>
      <description>The instance name of the TextHelper. Used by other tags to access this instance.</description>
      <name>beanName</name>
      <required>true</required>
    </attribute>
    <attribute>
      <description>The fully-scoped Java class name of the TextHelper</description>
      <name>className</name>
      <required>true</required>
    </attribute>
    <attribute>
      <description>The JSP scope the TextHelper is stored at.</description>
      <name>scope</name>
    </attribute>
    <attribute>
      <description>If true, checks that user has been given privileges to access the server function. Defaults to false.</description>
      <name>securityEnabled</name>
    </attribute>
  </tag>
  <tag>
    <description>Executes the callServer() method of a TextHelper</description>
    <name>callServer</name>
    <tag-class>curam.omega3.taglib.bean.CallServerTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <description>The TextHelper bean instance name to execute the callServer() method of.</description>
      <name>beanName</name>
      <required>true</required>
    </attribute>
  </tag>
  <tag>
    <description>Retrieves the value of a TextHelper field and prints it.</description>
    <name>getField</name>
    <tag-class>curam.omega3.taglib.bean.GetFieldTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <description>The TextHelper bean instance name to get the field value from</description>
      <name>sourceBean</name>
      <required>true</required>
    </attribute>
    <attribute>
      <description>The field to get the value from</description>
      <name>sourceField</name>
      <required>true</required>
    </attribute>
    <attribute>
      <description>If true, the field is an array field and an index is retrived from an enclosing loop tag. Default is false.</description>
      <name>useLoop</name>
    </attribute>
    <attribute>
      <description>If true, no special action will be taken for an empty value. If false, a non-breaking space character and a HTML marker comment will be inserted in place of an empty value to ensure that table cells will have some content. The default is false, but for page titles, the value should be set to true to avoid displaying the HTML comment in the browser window's title bar.</description>
      <name>ignoreEmpty</name>
    </attribute>
  </tag>
  <tag>
    <description>Sets the value of a TextHelper field.</description>
    <name>setField</name>
    <tag-class>curam.omega3.taglib.bean.SetFieldTag</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <description>The TextHelper bean instance name containing the field to set.</description>
      <name>targetBean</name>
      <required>true</required>
    </attribute>
