    <attribute>
      <description>The title of the link</description>
      <name>title</name>
      <required>false</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <description>The title prop of the link</description>
      <name>titleProp</name>
