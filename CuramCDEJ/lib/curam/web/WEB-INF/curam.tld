      <name>external</name>
      <required>false</required>
    </attribute>
    <attribute>
      <description>Assigns an accesskey to this link.</description>
      <name>accesskey</name>
      <required>false</required>
    </attribute>
    <attribute>
      <description>Used to handle the mouseover event</description>
      <name>onmouseover</name>
      <required>false</required>
    </attribute>
    <attribute>
      <description>Used to handle the mouseout event</description>
      <name>onmouseout</name>
      <required>false</required>
    </attribute>
    <attribute>
