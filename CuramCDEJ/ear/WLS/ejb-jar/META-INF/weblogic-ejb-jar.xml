<weblogic-ejb-jar
  xmlns="http://www.bea.com/ns/weblogic/weblogic-ejb-jar/1.0/weblogic-ejb-jar.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.bea.com/ns/weblogic/weblogic-ejb-jar/1.0/weblogic-ejb-jar.xsd http://www.bea.com/ns/weblogic/weblogic-ejb-jar/1.0/weblogic-ejb-jar.xsd ">

   <weblogic-enterprise-bean>
      <ejb-name>ClientCacheInvalidationEventMDB</ejb-name>
      <message-driven-descriptor>
         <destination-jndi-name>jms/CuramCacheInvalidationTopic</destination-jndi-name>
         <connection-factory-jndi-name>jms/CuramTopicConnectionFactory</connection-factory-jndi-name>
         <jms-client-id>CuramClientCacheInvalidationClientID</jms-client-id>
      </message-driven-descriptor>
      <run-as-principal-name>$RUN_AS_USER_NAME$</run-as-principal-name>
   </weblogic-enterprise-bean>

</weblogic-ejb-jar>
