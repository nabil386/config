<!--
  Extract the Dynamic Evidence Configuration data.
-->
<project
  default="extract.dynamic.evidence.config"
  name="app_extractDynamicEvidenceConfiguratoin"
>


  <property environment="sysenv."/>
  <property
    name="component"
    value=""
  />
  <property
    name="etExtractionList"
    value=""
  />
  <property
    name="lowerKey"
    value=""
  />
  <property
    name="upperKey"
    value=""
  />
  <property
    name="preserveRangeKeys"
    value=""
  />
  <property
    name="extractAll"
    value=""
  />
  <property
    name="etCodePrefix"
    value=""
  />
  <property
    name="extractProductLinks"
    value=""
  />
  <property
    name="extractICLinks"
    value=""
  />
  <property
    name="extractCaseLinks"
    value=""
  />
  <property
    name="datamanagerDir"
    value=""
  />
  <property
    name="dmxDir"
    value=""
  />
  <property
    name="codetableDir"
    value=""
  />
  <property
    name="tabDir"
    value=""
  />


  <!--  ***************************  -->
  <!--  ***  Import Properties  ***  -->
  <!--  ***************************  -->
  <import file="${sysenv.CURAMSDEJ}/bin/app_properties.xml"/>


  <!--  ***********************************************************  -->
  <!--  ***      EXTRACT DYNAMICEVIDENCE CONFIGURATION          ***  -->
  <!--  ***********************************************************  -->
  <target
    description="Extracts Dynamic Evidence Configuration artefacts"
    name="extract.dynamic.evidence.config"
  >


    <!-- Call app_batchlauncher.xml to launch the Batch Launcher -->
    <ant
      antfile="${dir.sde.bin}/app_batchlauncher.xml"
      inheritall="false"
      target="main"
    >


      <property
        name="batch.program"
        value="curam.dynamicevidence.sl.util.configextractor.intf.DynamicEvidenceConfigurationExtractor.extractConfigArtefacts"
      />
      <property
        name="batch.parameters"
        value="serverDir=${sysenv.SERVER_DIR},component=${component},etExtractionList=${etExtractionList},lowerKey=${lowerKey},upperKey=${upperKey},preserveRangeKeys=${preserveRangeKeys},extractAll=${extractAll},etCodePrefix=${etCodePrefix},extractProductLinks=${extractProductLinks},extractICLinks=${extractICLinks},datamanagerDir=${datamanagerDir},dmxDir=${dmxDir},codetableDir=${codetableDir},tabDir=${tabDir},extractCaseLinks=${extractCaseLinks}"
      />
    </ant>


  </target>


</project>
