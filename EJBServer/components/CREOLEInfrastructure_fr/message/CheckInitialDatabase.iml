<messages package="curam.creole.message"> 
  <message name="ERR_SNAPSHOT_ENTRY_FOUND"> 
    <!--<locale language="en">CREOLERuleSetSnapshot table should be empty, but found a row with creoleRuleSetSnapshotID=%1n.</locale> -->  
    <locale language="fr">La table %1s devrait être vide, mais une ligne contenant %2s=%3n a été détectée.</locale> 
  </message>  
  <message name="ERR_RULE_SET_DEFINITION_BLANK"> 
    <!--<locale language="en">CREOLERuleSet row with creoleRuleSetID=%1n has no ruleSetDefinition value set (it should be populated).</locale> -->  
    <locale language="fr">La ligne %1s comprenant %2s=%3n ne possède aucune valeur %4s définie (elle doit être remplie).</locale> 
  </message>  
  <message name="ERR_RULE_SET_VERSION_NOT_BLANK"> 
    <!--<locale language="en">CREOLERuleSet row with creoleRuleSetID=%1n has a ruleSetVersion set (it should not be populated).</locale> -->  
    <locale language="fr">La ligne %1s comprenant %2s=%3n possède une valeur %4s définie (elle ne doit pas être remplie).</locale> 
  </message>  
  <message name="ERR_RULE_SETS_CONTAIN_VALIDATION_ERRORS"> 
    <locale language="fr">Les ensembles de règles contiennent des erreurs de validation.</locale> 
  </message>  
  <message name="ERR_RULE_SET_NAME_INCONSISTENCY"> 
    <!--<locale language="en">CREOLERuleSet row with creoleRuleSetID=%1n specifies the rule set name as '%2s', but the ruleSetDefinition specifies the name as '%3s'.</locale> -->  
    <locale language="fr">La ligne %1s comprenant %2s=%3n spécifie le nom d'ensemble de règles sous la forme '%4s', mais %5s spécifie le nom sous la forme '%6s'.</locale> 
  </message> 
</messages>
