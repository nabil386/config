<messages package="curam.creole.message"> 
  <message name="JAVA_CLASS_NOT_FOUND"> 
    <locale language="fr">La classe Java '%1s' est introuvable.</locale> 
  </message>  
  <message name="JAVA_CLASS_LOAD_ERROR"> 
    <locale language="fr">Problème lors du chargement de la classe Java '%1s' : %2s</locale> 
  </message>  
  <message name="MORE_THAN_ONE_PUBLIC_STATIC_METHOD_EXISTS_IN_JAVA_CLASS"> 
    <!-- <locale language="en">There is more than one public static method named '%1s' in Java class '%2s'.</locale> -->  
    <locale language="fr">Plusieurs méthodes %1s intitulées '%2s' se trouvent dans la classe %3s '%4s'.</locale> 
  </message>  
  <message name="PUBLIC_STATIC_METHOD_NOT_FOUND_IN_JAVA_CLASS"> 
    <!--<locale language="en">Could not find a public static method named '%1s' in Java class '%2s'.</locale> -->  
    <locale language="fr">La méthode %1s intitulée '%2s' est introuvable dans la classe %3s '%4s'.</locale> 
  </message>  
  <message name="JAVA_CLASS_METHOD_MUST_NOT_RETURN_VOID"> 
    <locale language="fr">La méthode '%1s' de la classe Java '%2s' ne doit pas être renvoyée vide.</locale> 
  </message>  
  <message name="JAVA_CLASS_METHOD_ARGUMENTS_INVALID"> 
    <locale language="fr">Le premier argument de la méthode '%1s' de la classe Java '%2s' doit accepter un ou une '%3s'.</locale> 
  </message>  
  <message name="JAVA_CLASS_METHOD_ARGUMENTS_SIZE_INVALID"> 
    <locale language="fr">La méthode '%1s' de la classe Java '%2s' attend %3n argument(s) calculé(s), mais cette expression transmet %4n argument(s).</locale> 
  </message>  
  <message name="ERROR_CALLING_CLASS_METHOD"> 
    <locale language="fr">Erreur lors de l'appel de la méthode '%1s' sur '%2s' :\t</locale> 
  </message> 
</messages>
