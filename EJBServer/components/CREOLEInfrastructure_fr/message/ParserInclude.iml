  <message name="INCLUDE_ROOT_SET_ROOT_ELEMENT_MUST_BE_RULESET_OR_CLASS"> 
    <!--<locale language="en">The root element of an included rule set must be 'RuleSet' or 'Class'.</locale> -->  
