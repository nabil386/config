<messages package="curam.creole.message"> 
  <message name="INCLUDING_RULE_SET_PUBLIC_ID_NOT_SET"> 
    <locale language="fr">L'ID public de l'ensemble de règles inclus n'est pas défini.</locale> 
  </message>  
  <message name="INCLUDE_ELEMENT_HAS_NO_CHILD_ELEMENT"> 
    <!--<locale language="en">This Include element ('%1s') has no child element.</locale> -->  
