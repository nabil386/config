<registrations>
  <event-registration
    filter="curam.application.impl.ActiveCaseHandler"
    handler="curam.application.impl.ActiveCaseHandler"
  >
    <event-classes>
      <event-class identifier="IntakeProgramApplication"/>
    </event-classes>
  </event-registration>


  <event-registration
    filter="curam.application.impl.WithdrawalRequestHandler"
    handler="curam.application.impl.WithdrawalRequestHandler"
  >
    <event-classes>
      <event-class identifier="WithdrawalRequest"/>
    </event-classes>
  </event-registration>


  <event-registration
    filter="curam.application.impl.ApplicationReceivedHandler"
    handler="curam.application.impl.ApplicationReceivedHandler"
  >
    <event-classes>
      <event-class identifier="IntakeApplication"/>
    </event-classes>
  </event-registration>


  <event-registration handler="curam.intaketimers.impl.MilestoneCreationEventHandler">
    <event-classes>
      <event-class identifier="IntakeMilestoneCreationEvent"/>
    </event-classes>
  </event-registration>


  <event-registration
    filter="curam.application.impl.StatusChangedHandler"
    handler="curam.application.impl.StatusChangedHandler"
  >
    <event-classes>
      <event-class identifier="ProgramApplicationStatusChanged"/>
    </event-classes>
  </event-registration>


  <event-registration
    filter="curam.application.impl.RegisterProspectPersonAsPersonListener"
    handler="curam.application.impl.RegisterProspectPersonAsPersonListener"
  >
    <event-classes>
      <event-class identifier="ProspectPerson"/>
    </event-classes>
  </event-registration>


</registrations>
