						<label id="Question.Label.NatureOfAbsencesIfApplicable">
							<![CDATA[Nature of Absences (if applicant indicated any)]]>
						</label>
					</question>						
				</cluster>
				
				
			<!-- BEGIN GIS -->
			<cluster>
				<title id="QuestionPage.Title.GIS"><![CDATA[Guaranteed Income Supplement]]></title>
