							<label id="PersonalInformation.DOB">
								<![CDATA[Date of birth]]>
							</label>                  
						</question>	
						<question id="isDiffResiAddressForPartner">
							<label id="Question.Label.IsDiffResiAddrForPartner">
								<![CDATA[Did the applicant provide a different residential address for spouse/common-law partner?]]>
							</label>                   
						</question>					
					</cluster>	

					<!-- Partner Residential Address -->
					
			
					<cluster>
					    <display-text id="AddressInformation.DisplayText"><![CDATA[<h3>Partner Residential address</h3>]]></display-text>
				
						<layout>
							<label-width>0</label-width>
						</layout>
					</cluster>
					
			
			<!-- Residential Address CA -->
			<!-- <condition expression="Person.isPrimaryParticipant==false and Person.residentialCountry==&quot;CA&quot;"> -->
		     
				<cluster entity="Person" criteria="isPrimaryParticipant==false">
				    <question id="tempPartResAddress">
						<label id="AddressInformation.ResAddr"><![CDATA[Residential Address]]></label>
					</question>
					
				</cluster>
			 						
					<cluster>
					    <display-text id="Cluster.Title.RetirementIncomeDetails"><![CDATA[<h3>Retirement Income details</h3>]]></display-text>	
						<layout>
							<label-width>0</label-width>
						</layout>						
					</cluster>
					
					
					<cluster entity="Person" criteria="isPrimaryParticipant==false">
						<question id="tempIsRetirdOrRetirng2Yrs">
							<label id="Question.Label.DidPartnerRetireOrExpctedIn2Years">
								<![CDATA[Did the applicant indicate that their spouse/common-law partner retired from work in the last two years, or that they expect to retire in the next two years?]]>
							</label>
						</question>						

							<question id="tempRetirementDate" >
								<label id="Question.Label.RetirementDate">
									<![CDATA[Retirement Date]]>
								</label>							
							</question>		

						<question id="tempIsPensionReduced2Yrs">
							<label id="Question.Label.DidPartnerPensionIncmRedcdOrExpectedIn2Yrs">
								<![CDATA[Has the applicant indicated that their spouse/common-law partner's pension income has been reduced in the last two years or that they expect it will be reduced in the next two years?]]>
							</label>
						</question>						
							<question id="tempReductionDate" >
								<label id="Question.Label.DateOfReduction">
									<![CDATA[Date of reduction]]>
								</label>							
							</question>		
					</cluster>			

					<cluster>
						<display-text id="Cluster.Title.ForeignIncomeDetails"><![CDATA[<h3>Foreign Income details</h3>]]></display-text>
						<layout>
							<label-width>0</label-width>
						</layout>						
					</cluster>
					
					<cluster entity="Person" criteria="isPrimaryParticipant==false">
						<question id="tempIsReceivingForeignIncome">
							<label id="Question.Label.DidPartnerReceiveForeignIncome">
								<![CDATA[Did the applicant indicate that their spouse/common-law partner receives income from another country?]]>
							</label>
						</question>						
					
							<question id="tempForeignIncomeType">
								<label id="Question.Label.TypeOfForeignIncome">
									<![CDATA[Type of Foreign Income]]>
								</label>
							</question>		

							<question id="tempAnnualForeignIncomeStr" >
								<label id="Question.Label.AnnualForeignIncome">
									<![CDATA[Annual Foreign Income Amount & Currency]]>
								</label>
							</question>		
					</cluster>						

					<cluster>
					    <display-text id="QuestionPage.Title.SponsorshipDetails"><![CDATA[<h3>Sponsorship Details</h3>]]></display-text>
						<layout>
							<label-width>0</label-width>
						</layout>						
					</cluster>
					
					<cluster entity="Person" criteria="isPrimaryParticipant==false">
						<question id="tempIsSpouseOrCLPartnrSponsored">
							<label id="Question.Label.IsSponsorsed">
								<![CDATA[Did applicant indicate that their spouse/common-law partner was admitted to Canada under a sponsorship agreement?]]>
							</label>
						</question>						
					</cluster>
					
				</condition>
					
					
					<cluster>
					    <title id="Cluster.Title.PaymentInformation"><![CDATA[Payment information]]></title>
					    <edit-link start-page="PaymentInformation" skip-to-summary="false"/>
						<display-text id="Cluster.Title.PaymentInformationDisplay"><![CDATA[<h3>Retirement Income details</h3>]]></display-text>	
						<layout>
							<label-width>0</label-width>
						</layout>						
					</cluster>
					
					<condition expression="Person.isPrimaryParticipant==true">
						<cluster entity="Person" criteria="isPrimaryParticipant==true">
							<question id="preferredPaymentMethod">
								<label id="Bank.preferredPaymentMethod.Label">How would the applicant like to receive their
									payments?
								</label>
							</question>
					
						</cluster>
					
						<cluster entity="Payment">
							<question id="preferredBankAccount">
								<label id="Bank.preferredBankAccount.Label">Did the applicant indicate they would like to use one
									of these existing direct deposit accounts or did they provide new
									direct deposit information?
								</label>
							</question>
						</cluster>
					
						<condition expression="Payment.preferredBankAccount==&quot;BDMBAC8001&quot; and Person.hasActiveBankAccounts">
							<cluster entity="PaymentBanks">
								<question id="preferredBankForProgram">
									<label id="Bank.preferredBankForProgram.Label"><![CDATA[Which bank account would you like to use?]]></label>
								</question>
							</cluster>
						</condition>
						<condition expression="(Person.preferredPaymentMethod==&quot;EFT&quot; and Person.hasActiveBankAccounts==false)  or Payment.preferredBankAccount==&quot;BDMBAC8000&quot;">
							<cluster entity="Payment">
								<question id="bankAccountName">
									<label id="Bank.bankAccountName.Label"><![CDATA[Name(s) on the account]]></label>
								</question>
								<question id="bankBranchNum">
									<label id="Bank.bankBranchNum.Label"><![CDATA[Branch Transit Number]]></label>
								</question>
								<question id="bankInstNum">
									<label id="Bank.BankInstNum.Label"><![CDATA[Financial Institution Number]]></label>
								</question>
								<question id="accountNumber">
									<label id="Bank.account.Label"><![CDATA[Account Number]]></label>
								</question>
							</cluster>
						</condition>
						
						<cluster entity="WorldIncome">
							<question id="yearNdThresholdAmt">
								<label id="Question.Label.SelectYearAndThresholdAmount">
									<![CDATA[Select the year and threshold amount as indicated on the form]]>
								</label>
													
							</question>			
							<question id="isNonResidntNdIncmLTThrshld">
								<label id="Question.Label.IsNRCAndWorldIncomeGTThreshold">
									<![CDATA[Did the applicant indicate that they are not a resident of Canada for tax purposes and their net world income is more than the threshold?]]>
								</label>
								
							</question>		
						</cluster>
						
						
					<cluster entity="TaxInfo">
						<question id="dedFedTax">
							<label id="Question.Label.MonthlyTaxDeduction">
								<![CDATA[Does the applicant want Federal income tax deducted from their monthly payment?]]>
							</label>
							
						</question>
					</cluster>	
					<condition expression="TaxInfo.dedFedTax==&quot;YN1&quot;">
						<cluster entity="TaxInfo">	
							<question id="dedMethod">
								<label id="Question.Label.TaxDedtnInPercentageOrDollar">
									<![CDATA[Does the applicant want a percentage or a dollar amount for deduction?]]>
								</label>
								
							</question>	
						</cluster>
					</condition>
	
					<condition expression="TaxInfo.dedMethod==&quot;BDMTDM8000&quot;">
						<cluster entity="TaxInfo">
							<question id="dollarAmount">
								<label id="Question.Label.DollarAmountDeduction">
									<![CDATA[Indicate a dollar amount applicant wants deducted for income tax (for example: 100.00).]]>
								</label>
								
							</question>	
						</cluster>
					</condition>
					<condition expression="TaxInfo.dedMethod==&quot;BDMTDM8001&quot;">
						<cluster entity="TaxInfo">
							<question id="dollarPerct">
								<label id="Question.Label.PercentageAmountDeduction">
									<![CDATA[Indicate a percentage applicant wants deducted for income tax (for example: 20).]]>
								</label>
								
							</question>
						</cluster>	
					</condition>
	
				</condition>
				
				
				<cluster>
					    <title id="Cluster.Title.DeclarationSign"><![CDATA[Payment information]]></title>
					    <edit-link start-page="DeclarationAndSignatureDetails" skip-to-summary="false"/>
						<display-text id="Cluster.Title.DeclarationSign.Description"><![CDATA[<h3>Retirement Income details</h3>]]></display-text>	
						<layout>
							<label-width>0</label-width>
						</layout>						
				</cluster>
					
				<condition expression="not(isNotNull(Declaration.didAplicntSignedWithMark)) or not(Declaration.didAplicntSignedWithMark==&quot;YN1&quot;)">
					<cluster entity="Declaration">
						<question id="didAplicntSigned">
							<label id="Question.Label.ApplicantSigned">
								<![CDATA[Did the applicant sign the declaration?]]>
							</label>
							
						</question>		
					</cluster>
				</condition>
							
				<condition expression="not(isNotNull(Declaration.didAplicntSigned)) or not(Declaration.didAplicntSigned==&quot;YN1&quot;)">
					<cluster entity="Declaration">		
						<question id="didAplicntSignedWithMark" >
							<label id="Question.Label.ApplicantSignedWithMark">
								<![CDATA[Did the applicant sign the declaration with a mark?]]>
							</label>
																	
						</question>			
					</cluster>
				</condition>
				
				<condition expression="(isNotNull(Declaration.didAplicntSigned) and Declaration.didAplicntSigned==&quot;YN1&quot;) or (isNotNull(Declaration.didAplicntSignedWithMark) and Declaration.didAplicntSignedWithMark==&quot;YN1&quot;)">
					<cluster entity="Declaration">		
						<question id="aplicntSignedDate">
							<label id="Question.Label.SignedDate">
								<![CDATA[Date signed]]>
							</label>
													
						</question>			
					</cluster>
				</condition>
	
				<condition expression="(isNotNull(MaritalStatusInfo.currentMaritalStatus) and MaritalStatusInfo.currentMaritalStatus!=&quot;MS1&quot;) or Person.spouseOrCLPDetailsEnteredInd">
					<cluster>
						<display-text id="Cluster.Title.SPCLDeclarationSign">
							<![CDATA[Spouse/Common-Law Partner's declaration and signature]]>
						</display-text>			
						<layout>
							<label-width>0</label-width>
						</layout>			
					</cluster>
				</condition>
	
					<condition expression="(isNotNull(MaritalStatusInfo.currentMaritalStatus) and MaritalStatusInfo.currentMaritalStatus!=&quot;MS1&quot;) or Person.spouseOrCLPDetailsEnteredInd">
						<cluster entity="Declaration">
							<question id="didSpouseOrCLPSigned" >
								<label id="Question.Label.SPCLSigned">
									<![CDATA[Did the applicant's spouse/common-law partner sign the declaration?]]>
								</label>
													
							</question>		
						</cluster>
					</condition>	
	
					<condition expression="(isNotNull(MaritalStatusInfo.currentMaritalStatus) and MaritalStatusInfo.currentMaritalStatus!=&quot;MS1&quot;) or Person.spouseOrCLPDetailsEnteredInd">
						<cluster entity="Declaration">		
							<question id="didSpouseOrCLPSignedWithMark" >
								<label id="Question.Label.SPCLSignedWithMark">
									<![CDATA[Did the applicant's spouse/common-law partner sign the declaration with a mark?]]>
								</label>	
																
							</question>			
						</cluster>
					</condition>				
				
	
					<condition expression="((isNotNull(Declaration.didSpouseOrCLPSigned) and Declaration.didSpouseOrCLPSigned==&quot;YN1&quot;) or (isNotNull(Declaration.didSpouseOrCLPSignedWithMark) and Declaration.didSpouseOrCLPSignedWithMark==&quot;YN1&quot;))">	
						<cluster entity="Declaration">		
							<question id="spouseOrCLPSignedDate">
								<label id="Question.Label.SignedDate">
									<![CDATA[Date signed]]>
								</label>
													
							</question>			
						</cluster>
					</condition>	
	
				<cluster entity="Declaration">
					<display-text id="Cluster.Title.WAPDeclarationSign">
						<![CDATA[Witness or Authorized Person's declaration and signature]]>
					</display-text>
					<layout>
							<label-width>0</label-width>
						</layout>
					<question id="didTPSignedAsWitness">
						<label id="Question.Label.ThirdPartySignedAsWitness">
							<![CDATA[Did the Third Party sign as the witness?]]>
						</label>
										
					</question>	
					
					<question id="didTPSignedBehalfOfAplcnt">
						<label id="Question.Label.ThirdPartySignedAsAPForApplicant">
							<![CDATA[Did the Third Party sign as the Authorized person on behalf of Applicant?]]>
						</label>
										
					</question>		
				</cluster>
				
		
				<condition expression="(isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;)">
					<cluster entity="Declaration">		
						<question id="thirdPartySignedDate">
							<label id="Question.Label.SignedDate">
								<![CDATA[Date signed]]>
							</label>
												
						</question>			
					</cluster>
				</condition>
				
				<cluster entity="Declaration">
					<question id="didTPSignedBehalfOfSpouse">
						<label id="Question.Label.ThirdPartySignedAsAPForSPCL">
							<![CDATA[Did the Third Party sign as the Authorized person on behalf of applicant's spouse/common-law partner?]]>
						</label>
										
					</question>		
				</cluster>
		
				<condition expression="isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;">
					<cluster entity="Declaration">		
						<question id="tpSignedDateBehalfOfSpouse">
							<label id="Question.Label.SignedDate">
								<![CDATA[Date signed]]>
							</label>
												
						</question>			
					</cluster>
				</condition>	
	
				<condition expression="(isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;)">
					<cluster entity="Declaration">
						<question id="thirdPartyName">
							<label id="Question.Label.Name">
								<![CDATA[Name]]>
							</label>
						</question>		
					</cluster>	
				</condition>
				
				<condition expression="(isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;)">
					<cluster entity="Declaration">
						<question id="relationshipToApplicant">
							<label id="Question.Label.RelationToApplicant">
								<![CDATA[Relationship to applicant]]>
							</label>
												
						</question>		
					</cluster>	
				</condition>
				
				<condition expression="((isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;)) and Declaration.mailingCountry==&quot;CA&quot;">				
				<cluster entity="MailingAddress" grouping-id="WitnessMailingAddress">
					<display-text id="AddressInformation.MailAddr.Title"><![CDATA[<h3>Witness or Authorized Person's Mailing Address</h3>]]></display-text>
					<layout>
						<label-width>0</label-width>
					</layout>
				</cluster>

				<!-- Mailing Address CA -->
				<list entity="Declaration" criteria="mailingCountry==&quot;CA&quot;">
							<list entity="MailingAddress" criteria="country==&quot;CA&quot;">
							<column id="addressLine3">
							<title id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></title>
						</column>
						<column id="suiteNum">
							<title id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></title>
						</column>
						<column id="addressLine1">
							<title id="AddressInformation.MailAddr.StreetNum"><![CDATA[Mailing street number]]></title>
						</column>
						<column id="addressLine2">
							<title id="AddressInformation.MailAddr.StreetName"><![CDATA[Mailing street name]]></title>
						</column>
						<column id="city">
							<title id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></title>
						</column>
						<column id="province">
							<title id="AddressInformation.MailAddr.Prov"><![CDATA[Mailing Province or Territory]]></title>
						</column>
						<column id="postalCode">
							<title id="AddressInformation.MailAddr.PostalCode"><![CDATA[Mailing postal code]]></title>
						</column>
						<column id="country">
							<title id="AddressInformation.MailAddr.Country"><![CDATA[ Mailing country]]></title>
						</column>
							</list>
							</list>
	
			</condition>
			
			<condition expression="((isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;)) and Declaration.mailingCountry==&quot;US&quot;">				
				<cluster entity="MailingAddress" grouping-id="WitnessMailingAddress">
					<display-text id="AddressInformation.MailAddr.Title"><![CDATA[<h3>Witness or Authorized Person's Mailing Address</h3>]]></display-text>
					<layout>
						<label-width>0</label-width>
					</layout>
				</cluster>

			<!-- Mailing Address US -->
				<list entity="Declaration" criteria="mailingCountry==&quot;US&quot;">
							<list entity="IntlMailAddress" criteria="country==&quot;US&quot;">
							<column id="addressLine3">
							<title id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></title>
						</column>
						<column id="suiteNum">
							<title id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></title>
						</column>
						<column id="addressLine1">
							<title id="AddressInformation.MailAddr.StreetNum"><![CDATA[Mailing street number]]></title>
						</column>
						<column id="addressLine2">
							<title id="AddressInformation.MailAddr.StreetName"><![CDATA[Mailing street name]]></title>
						</column>
						<column id="city">
							<title id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></title>
						</column>
						<column id="state">
							<title id="AddressInformation.MailAddr.State"><![CDATA[Mailing State]]></title>
						</column>
						<column id="zipCode">
							<title id="AddressInformation.MailAddr.ZipCode"><![CDATA[Mailing Zip Code]]></title>
						</column>
						<column id="country">
							<title id="AddressInformation.MailAddr.Country"><![CDATA[Mailing country]]></title>
						</column>
							</list>
							</list>

			</condition>			
			
			<condition expression="((isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;) or (isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;)) and (Declaration.mailingCountry!=&quot;CA&quot; and Declaration.mailingCountry!=&quot;US&quot;)">				
				<cluster entity="MailingAddress" grouping-id="WitnessMailingAddress">
					<display-text id="AddressInformation.MailAddr.Title"><![CDATA[<h3>Witness or Authorized Person's Mailing Address</h3>]]></display-text>
					<layout>
						<label-width>0</label-width>
					</layout>
				</cluster>	

				<!-- Mailing Address INTL -->
				<list entity="Declaration" criteria="mailingCountry!=&quot;CA&quot; and mailingCountry!=&quot;US&quot;">
							<list entity="IntlMailAddress" criteria="country!=&quot;CA&quot; and country!=&quot;US&quot;">
							<column id="addressLine3">
							<title id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></title>
						</column>
						<column id="suiteNum">
							<title id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></title>
						</column>
						<column id="addressLine1">
							<title id="AddressInformation.MailAddr.StreetNumOne"><![CDATA[Mailing street 1]]></title>
						</column>
						<column id="addressLine2">
							<title id="AddressInformation.MailAddr.StreetNameTwo"><![CDATA[Mailing street 2]]></title>
						</column>
						<column id="city">
							<title id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></title>
						</column>
						<column id="addressLine4">
							<title id="AddressInformation.MailAddr.Intl.State"><![CDATA[Mailing State/Province/Region]]></title>
						</column>
						<column id="zipCode">
							<title id="AddressInformation.MailAddr.ZipCode"><![CDATA[Mailing Zip Code]]></title>
						</column>
						<column id="country">
							<title id="AddressInformation.MailAddr.Country"><![CDATA[Mailing country]]></title>
						</column>
							</list>
							</list>	
								
			</condition>		
					

			<!-- END SpousalOrCLP Info  -->
				
			<!-- END DECLARATION AND SIGNATURE -->					
		</summary-page>
	</section>
</ieg-script>
