<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:D="http://www.curamsoftware.com/BaseDomains"
	elementFormDefault="qualified">

	<xsd:import namespace="http://www.curamsoftware.com/BaseDomains" />
    <xsd:include schemaLocation="IEGDomains"/>
    <xsd:include schemaLocation="CitizenPortalDomains"/>
    <xsd:include schemaLocation="BDMOASGISApplicationSchemaDomain"/>  

	<xsd:element name="Application">
        <xsd:complexType>
            <xsd:sequence minOccurs="0">
                <xsd:element ref="Person" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="TaxInfo" minOccurs="0" maxOccurs="unbounded" />
                <xsd:element ref="IntakeApplicationType" minOccurs="0" maxOccurs="1"/>
                <xsd:element ref="ScreeningType" minOccurs="0" maxOccurs="1"/>
                <xsd:element ref="IntakeProgram" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="ScreeningProgram" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="Submission" minOccurs="0" maxOccurs="unbounded"/>
             	<xsd:element ref="Declaration" minOccurs="0" maxOccurs="1"/>  
             	<xsd:element ref="Attestation" minOccurs="0" maxOccurs="1"/>              	          	                              
            </xsd:sequence>        
            <xsd:attribute name="userName" type="IEG_STRING" default=""/>
            <xsd:attribute name="userType" type="IEG_STRING" default=""/>
            <xsd:attribute name="externalUserType" type="IEG_STRING" default=""/>
            <xsd:attribute name="continueSIN" type="IEG_BOOLEAN"/>
            <xsd:attribute name="applicationChannel" type="IEG_STRING" default="CURAM"/>
 			<xsd:attribute name="receiptDate" type="IEG_DATE"/>		
 			<xsd:attribute name="applicationForm" type="CT_BDMOASAPPLICATIONFORM"/>	 				            
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="IntakeApplicationType">
        <xsd:complexType>
            <xsd:attribute name="intakeApplicationTypeID" type="D:SVR_INT64"/>
        </xsd:complexType>
    </xsd:element>
    
    <xsd:element name="ScreeningType">
        <xsd:complexType>
            <xsd:attribute name="screeningTypeID" type="D:SVR_INT64"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="IntakeProgram">
        <xsd:complexType>
            <xsd:attribute name="programTypeID" type="D:SVR_INT64"/>
            <xsd:attribute name="programTypeReference" type="IEG_STRING"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScreeningProgram">
        <xsd:complexType>
            <xsd:attribute name="programTypeID" type="D:SVR_INT64"/>
            <xsd:attribute name="programTypeReference" type="IEG_STRING"/>
        </xsd:complexType>
    </xsd:element>
        
	<xsd:element name="Person">
		<xsd:complexType>
			<xsd:sequence minOccurs="0">
				<xsd:element ref="Relationship" minOccurs="0" maxOccurs="unbounded" />
				<xsd:element ref="Address" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="AddressLink" minOccurs="0" maxOccurs="1"/>
				<xsd:element ref="ResidentialAddress" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="MailingAddress" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="IntlResAddress" minOccurs="0" maxOccurs="unbounded"/>
                <xsd:element ref="IntlMailAddress" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="SearchAddress" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="SearchCriteriaResi" minOccurs="0" maxOccurs="unbounded"/>		
				<xsd:element ref="SearchCriteriaMail" minOccurs="0" maxOccurs="unbounded"/>								
				<xsd:element ref="CommunicationPref" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="Payment" minOccurs="0" maxOccurs="1" />
				<xsd:element ref="PaymentBanks" minOccurs="0" maxOccurs="unbounded" />
				<xsd:element ref="CommunicationDetails" minOccurs="0" maxOccurs="unbounded" />   
            	<xsd:element ref="AlertPreferences" minOccurs="0" maxOccurs="1" />
            	<xsd:element ref="Incarceration" minOccurs="0" maxOccurs="unbounded" />
            	<xsd:element ref="Income" minOccurs="0" maxOccurs="unbounded" />
            	<xsd:element ref="Household" minOccurs="0" maxOccurs="1"/>

            	<xsd:element ref="OASPension" minOccurs="0" maxOccurs="1"/> 
            	<xsd:element ref="LegalStatus" minOccurs="0" maxOccurs="1"/> 
            	<xsd:element ref="GIS" minOccurs="0" maxOccurs="1"/>
            	<xsd:element ref="RetirementIncome" minOccurs="0" maxOccurs="1"/>
            	<xsd:element ref="ForeignIncome" minOccurs="0" maxOccurs="1"/> 
            	<xsd:element ref="MaritalStatusInfo" minOccurs="0" maxOccurs="1"/>            	            	                	           	
            	<xsd:element ref="SponsorshipInfo" minOccurs="0" maxOccurs="1"/> 
            	<xsd:element ref="WorldIncome" minOccurs="0" maxOccurs="1"/>   
                <xsd:element ref="ResidenceHistory" minOccurs="0" maxOccurs="unbounded"/>  
                <xsd:element ref="AbsenceDetails" minOccurs="0" maxOccurs="1"/>             	          	
            	
			</xsd:sequence>
			
			<xsd:attribute name="personID" type="D:SVR_KEY" />
			<xsd:attribute name="programTypeID" type="D:SVR_KEY" />
			<xsd:attribute name="isPrimaryParticipant" type="IEG_BOOLEAN" default="false"/>

			<xsd:attribute name="firstName" type="BDM_NAME" />
			<xsd:attribute name="middleName" type="BDM_NAME"/>
			<xsd:attribute name="middleInitial" type="IEG_STRING" default=""/>
			<xsd:attribute name="lastName" type="BDM_NAME" />

			<xsd:attribute name="gender" type="IEG_GENDER" />
			<xsd:attribute name="genderDropdown" type="CT_GENDER"/>
			<xsd:attribute name="dateOfBirth" type="IEG_DATE"/>

			<xsd:attribute name="countryOfBirth" type="CT_COUNTRY"/>		
			<xsd:attribute name="dateOfDeath" type="IEG_DATE"/>
							
			<xsd:attribute name="hasIncome" type="IEG_BOOLEAN" default="false"/>
   			<xsd:attribute name="hasForeignIncome" type="IEG_YES_NO" />			
			<xsd:attribute name="isBlind" type="IEG_BOOLEAN" default="false"/>
			<xsd:attribute name="isPregnant" type="IEG_BOOLEAN" default="false"/>
			<xsd:attribute name="maritalStatus" type="CT_MARITALSTATUS" />
			<xsd:attribute name="hasCommunications" type="IEG_STRING" default=""/>

			<xsd:attribute name="isResidentialAddressUpdate" type="IEG_YES_NO" default="YN2"/>
			<xsd:attribute name="isMailingAddressUpdate" type="IEG_YES_NO" default="YN2"/>
			<xsd:attribute name="atSameResidence" type="IEG_YES_NO"/>
			<xsd:attribute name="startLivingAtSameResidenceDate" type="IEG_DATE"/>
			<xsd:attribute name="schoolName" type="IEG_STRING" />
			<xsd:attribute name="primaryDependantPersonID" type="D:SVR_KEY" />
    							
			<xsd:attribute name="personTitle" type="CT_PERSONTITLE" />			
			<xsd:attribute name="oasAccntNum" type="BDMOAS_OAS_ACCNT_NUM" />
			<xsd:attribute name="SIN" type="BDM_SIN" />
			<xsd:attribute name="sinRaw" type="BDM_SIN_RAW"/>
			<xsd:attribute name="sinRawAP" type="BDM_SIN_RAW_AP"/>
			<xsd:attribute name="PreferredName" type="BDM_NAME" />
			<xsd:attribute name="LastNameAtBirth" type="BDM_NAME" />
			<xsd:attribute name="ParentLastName" type="BDM_NAME" />
			<xsd:attribute name="EducationLevel" type="CT_BDMEDUCATIONLEVEL" />
			<xsd:attribute name="IndigenousPerson" type="BDM_YES_NO_OPTIONAL" />
			<xsd:attribute name="MinorityGroup" type="BDM_YES_NO_OPTIONAL" />			
			<xsd:attribute name="preferredPaymentMethod" type="PAYMENT_METHOD"/>		
			
			<xsd:attribute name="localID" type="IEG_INT64"/>
		
            <xsd:attribute name="resMail" type="IEG_YES_NO" />
            <xsd:attribute name="residentialCountry" type="CP_COUNTRY" default="CA"/>
            <xsd:attribute name="mailingCountry" type="CP_COUNTRY" default="CA"/>
            <xsd:attribute name="wsResValidOrOverride" type="IEG_STRING" default="1"/>
            <xsd:attribute name="wsMailValidOrOverride" type="IEG_STRING" default="1"/>
            
            <xsd:attribute name="isDiffResiAddressForPartner" type="IEG_YES_NO"/>                   
            <xsd:attribute name="spouseOrCLPDetailsEnteredInd" type="IEG_BOOLEAN" default="false" />  
            <xsd:attribute name="maritalStatusSetInd" type="IEG_BOOLEAN" default="false" />       
            <xsd:attribute name="hasActiveResidAddress" type="IEG_BOOLEAN" default="false"/>   
            <xsd:attribute name="hasActiveMailingAddress" type="IEG_BOOLEAN" default="false"/>                            
			<xsd:attribute name="addOrUpdateResidAddress" type="IEG_BOOLEAN" default="false"/>
			<xsd:attribute name="addOrUpdateMailAddress" type="IEG_BOOLEAN" default="false"/>  	
            <xsd:attribute name="invalidAddressIsContinue" type="IEG_YES_NO"/>  					                                        
            			
			<!-- Begin: Standard Not used Required for App Submission -->
            <xsd:attribute name="homePhoneCode" type="IEG_STRING"/>
            <xsd:attribute name="homePhoneNumber" type="IEG_STRING"/>
            <xsd:attribute name="workPhoneCode" type="IEG_STRING"/>
            <xsd:attribute name="workPhoneNumber" type="IEG_STRING"/>
            <xsd:attribute name="cellPhoneCode" type="IEG_STRING"/>
            <xsd:attribute name="cellPhoneNumber" type="IEG_STRING"/>
            <xsd:attribute name="otherPhoneCode" type="IEG_STRING"/>
            <xsd:attribute name="otherPhoneNumber" type="IEG_STRING"/>
            <xsd:attribute name="email" type="IEG_STRING"/>
            <xsd:attribute name="ssn" type="IEG_NON_FORMATTED_NUMBER"/>
            <xsd:attribute name="ssnStatus" type="CP_SSN_STATUS"/>
            <xsd:attribute name="contactLanguage" type="CP_LANGUAGE"/>
            <xsd:attribute name="isUSCitizen" type="CP_YESNO"/>
            <xsd:attribute name="alienStatus" type="CP_ALIEN_STATUS"/>
            <xsd:attribute name="hispanicOrLatino" type="IEG_BOOLEAN"/>
            <xsd:attribute name="sameMailingAddress" type="IEG_BOOLEAN" default="false"/>
            <xsd:attribute name="hasActiveBankAccounts" type="IEG_BOOLEAN" default="false"/>
			<!-- End: Standard Not used Required for App Submission -->

			<xsd:attribute name="numDependants" type="BDM_NUM_DEPENDANTS"  default="0"/>

			<xsd:attribute name="alertFullPhoneNumber" type="IEG_STRING"  default=""/>
			<xsd:attribute name="alertEmailAdr" type="IEG_STRING"  default=""/>
			
			<xsd:attribute name="socialSecurityOrIdentificationNumber" type="BDMOAS_FRGN_COUNTRY_ID_NUMBER"/>
			<xsd:attribute name="socialSecurityOrIdentificationNumberCountry" type="CT_COUNTRY"/>
			<xsd:attribute name="otherFirstName" type="BDM_NAME" />
			<xsd:attribute name="otherLastName" type="BDM_NAME" />
			<xsd:attribute name="spouseOrCLPDeclarationSignInd" type="IEG_BOOLEAN" default="false" />
			
			<!-- Temp address attributes for partner for sumamry display -->
			<xsd:attribute name="tempPartResAddress" type="IEG_STRING"/>
            <!-- Temp address attributes for partner for sumamry display -->
            
            <xsd:attribute name="tempPartMailAddress" type="IEG_STRING"/>
            <!-- Temp address attributes for partner for sumamry display -->
            
            
            <xsd:attribute name="tempIsRetirdOrRetirng2Yrs" type="IEG_YES_NO" />		
			<xsd:attribute name="tempRetirementDate" type="IEG_DATE"/>	
   			<xsd:attribute name="tempIsPensionReduced2Yrs" type="IEG_YES_NO" />		
			<xsd:attribute name="tempReductionDate" type="IEG_DATE"/>				
   			<xsd:attribute name="tempIsReceivingForeignIncome" type="IEG_YES_NO" />	
			<xsd:attribute name="tempForeignIncomeType" type="BDMOAS_FOREIGN_INCOME_TYPE"/>
			<xsd:attribute name="tempAnnualForeignIncomeStr" type="BDMOAS_ANNUAL_FRGN_INCM_AND_CURNCY"/>		
            
            <xsd:attribute name="tempIsSpouseOrCLPartnrSponsored" type="IEG_YES_NO"/>			
			
		</xsd:complexType>
		<xsd:key name="PersonKey">
			<xsd:selector xpath="./Person" />
			<xsd:field xpath="@personID" />
		</xsd:key>
		<xsd:keyref name="RelationshipRef" refer="PersonKey">
		    <xsd:selector xpath="./Person/Relationship" />
		    <xsd:field xpath="@personID" />
		</xsd:keyref>
	</xsd:element>

    <xsd:element name="SearchCriteriaResi">
        <xsd:complexType>
            <xsd:attribute name="residCountry" type="CP_COUNTRY" default="CA"/>   
            <xsd:attribute name="residPostalCode" type="BDM_ADDRESS_POSTAL_CODE"/>    
            <xsd:attribute name="idResidentialAddress" type="IEG_STRING" default="-1" />                              
			<xsd:attribute name="caResiAddrSearchInd" type="IEG_BOOLEAN" default="false"/>         		              		 
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SearchCriteriaMail">
        <xsd:complexType> 
            <xsd:attribute name="mailCountry" type="CP_COUNTRY" default="CA"/>  
              <xsd:attribute name="mailPostalCode" type="BDM_ADDRESS_POSTAL_CODE"/>   
            <xsd:attribute name="idMailingAddress" type="IEG_STRING" default="-1" />                                        
			<xsd:attribute name="caMailAddrSearchInd" type="IEG_BOOLEAN" default="false"/>  			              		 
        </xsd:complexType>
    </xsd:element>    

    <xsd:element name="SearchAddress">
        <xsd:complexType>
        <xsd:sequence minOccurs="0">
				<xsd:element ref="oneLineAddress" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
            <xsd:attribute name="idResidentialAddress" type="IEG_STRING" default="-1" />
            <xsd:attribute name="queryResidentialAddress" type="BDM_ADDRESS_POSTAL_CODE"/>
            <xsd:attribute name="idMailingAddress" type="IEG_STRING" default="-1" />
            <xsd:attribute name="queryMailingAddress" type="BDM_ADDRESS_POSTAL_CODE"/>
            <xsd:attribute name="AddressData" type="IEG_STRING" />	
    		<xsd:attribute name="formatedAddressData" type="IEG_STRING" />
    		<xsd:attribute name="isMailingAddress" type="IEG_YES_NO" default="YN2"/>
    		<xsd:attribute name="isResidentialAddress" type="IEG_YES_NO" default="YN2"/>
    		<xsd:attribute name="isSelected" type="IEG_BOOLEAN" default="false"/>
    		<xsd:attribute name="isActive" type="IEG_YES_NO" default="YN2"/>
    		 <xsd:attribute name="country" type="CP_COUNTRY"/>
        </xsd:complexType>
    </xsd:element>
  	<xsd:element name="Address">
        <xsd:complexType>
            <xsd:sequence minOccurs="0">
                <xsd:element ref="TempAddressLink" minOccurs="0" maxOccurs="unbounded"/>                         
            </xsd:sequence>
            <xsd:attribute name="street1" type="IEG_STRING"/>
            <xsd:attribute name="street2" type="IEG_STRING"/>
            <xsd:attribute name="city" type="IEG_STRING"/>
            <xsd:attribute name="county" type="CP_COUNTY"/>
            <xsd:attribute name="state" type="CP_STATE"/>
            <xsd:attribute name="zipCode" type="IEG_NON_FORMATTED_NUMBER"/>
            <xsd:attribute name="zipCodePlusFour" type="IEG_STRING"/>
            <xsd:attribute name="addressData" type="IEG_STRING"/>
			<xsd:attribute name="addressID" type="D:SVR_KEY"/>			         
            <xsd:attribute name="country" type="CP_COUNTRY"/>
            <xsd:attribute name="postalCode" type="BDM_ADDRESS_POSTAL_CODE"/>
            <xsd:attribute name="province" type="CP_PROVINCE"/>
            <xsd:attribute name="suiteNum" type="IEG_STRING"/>

            <xsd:attribute name="addressEvID" type="D:SVR_KEY" default="0"/>
            <!-- Address Validation: Begins -->
            <xsd:attribute name="isValidated" type="IEG_YES_NO" default="YN2"/>
   			<xsd:attribute name="unableToConnectServiceInd" type="IEG_BOOLEAN" default="false"/>
   			<xsd:attribute name="noAddressFoundInd" type="IEG_BOOLEAN" default="false"/>
   			<xsd:attribute name="multipleAddressesInd" type="IEG_BOOLEAN" default="false"/>
   			<xsd:attribute name="ambiguousAddressesInd" type="IEG_BOOLEAN" default="false"/>
			<!-- Address Validation: Ends -->
        </xsd:complexType>
		<xsd:key name="AddressKey">
            <xsd:selector xpath="./Address"/>
            <xsd:field xpath="@addressID"/>
        </xsd:key>
    </xsd:element>
    	
		<xsd:element name="AddressLink">
    		<xsd:complexType>
    			<xsd:attribute name="personID" type="D:SVR_KEY" />
				<xsd:attribute name="addressID" type="D:SVR_KEY" />

    		    <xsd:attribute name="addressEvID" type="D:SVR_KEY" default="0"/>
    		    <xsd:attribute name="isMailingAddressSameAsHome" type="IEG_YES_NO" default=""/>
    		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="TempAddressLink">
    		<xsd:complexType>
    			<xsd:attribute name="personID" type="D:SVR_KEY" />
				<xsd:attribute name="addressID" type="D:SVR_KEY" />
    		    <xsd:attribute name="addressEvID" type="D:SVR_KEY" default="0"/>
    		    <xsd:attribute name="isMailingAddressSameAsHome" type="IEG_YES_NO" default=""/>
    		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="ResidentialAddress">
        <xsd:complexType>
            <xsd:attribute name="street1" type="BDM_ADDRESS_STREET_NUMBER"/>
            <xsd:attribute name="street2" type="BDM_ADDRESS_STREET_NAME"/>
            <xsd:attribute name="city" type="BDM_ADDRESS_CITY"/>
            <xsd:attribute name="state" type="CP_STATE"/>
            <xsd:attribute name="zipCode" type="IEG_STRING"/>
            <xsd:attribute name="wsAddressType" type="BDM_WS_ADDRESS_TYPE"/>
            <!-- Support for international addresses -->
            <xsd:attribute name="addressLine1" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine2" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine3" type="BDM_ADDRESS_LINE"/>
            <!-- Using addrLine4 for state ( International address not US) -->
            <xsd:attribute name="addressLine4" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine5" type="BDM_ADDRESS_LINE"/>           
            <xsd:attribute name="county" type="CP_COUNTY"/>
            <xsd:attribute name="country" type="CP_COUNTRY"/>
            <xsd:attribute name="postalCode" type="BDM_ADDRESS_POSTAL_CODE"/>
            <xsd:attribute name="province" type="CP_PROVINCE"/>
            <xsd:attribute name="suiteNum" type="BDM_ADDRESS_SUITE_NUMBER"/>
            <xsd:attribute name="oneLineAddress" type="IEG_STRING"/>
            <!-- Validate Status 0- Not Validated  1 - Valid, 2 - Invalid, 3 - Exception in Service -->
             <xsd:attribute name="validateStatus" type="IEG_STRING"/>
			<xsd:attribute name="isAddrSelectedInd" type="IEG_BOOLEAN" default="false"/>			             
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="MailingAddress">
        <xsd:complexType>
            <xsd:attribute name="street1" type="BDM_ADDRESS_STREET_NUMBER"/>
            <xsd:attribute name="street2" type="BDM_ADDRESS_STREET_NAME"/>
            <xsd:attribute name="city" type="BDM_ADDRESS_CITY"/>
            <xsd:attribute name="state" type="CP_STATE"/>
            <xsd:attribute name="zipCode" type="BDM_ADDRESS_ZIPCODE"/>
            <xsd:attribute name="wsAddressType" type="BDM_WS_ADDRESS_TYPE"/>
            <!-- Support for international addresses -->
            <xsd:attribute name="addressLine1" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine2" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine3" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine4" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="addressLine5" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="county" type="CP_COUNTY"/>
            <xsd:attribute name="country" type="CP_COUNTRY"/>
            <xsd:attribute name="postalCode" type="BDM_ADDRESS_POSTAL_CODE"/>
            <xsd:attribute name="province" type="CP_PROVINCE"/>
            <xsd:attribute name="suiteNum" type="BDM_ADDRESS_SUITE_NUMBER"/>

            <xsd:attribute name="oneLineAddress" type="IEG_STRING"/>
           <!-- Validate Status 0- Not Validated  1 - Valid, 2 - Invalid, 3 - Exception in Service -->
             <xsd:attribute name="validateStatus" type="IEG_STRING"/>
			<xsd:attribute name="isAddrSelectedInd" type="IEG_BOOLEAN" default="false"/>								             
        </xsd:complexType>
    </xsd:element>			
    <xsd:element name="oneLineAddress">
    		<xsd:complexType>
    			<xsd:attribute name="AddressData" type="IEG_STRING" />	
    			<xsd:attribute name="formatedAddressData" type="IEG_STRING" />			
    		</xsd:complexType>
	</xsd:element>

	<xsd:element name="IntlResAddress">
        <xsd:complexType>
            <xsd:attribute name="city" type="BDM_ADDRESS_CITY"/>
            <xsd:attribute name="state" type="CT_ADDRESS_STATE"/>
            <xsd:attribute name="zipCode" type="BDM_ADDRESS_ZIPCODE"/>
            <!-- addressLine1 matches to StreetNum -->
            <xsd:attribute name="addressLine1" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine2 matches to StreetName -->
            <xsd:attribute name="addressLine2" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine3 matches to poBox -->
            <xsd:attribute name="addressLine3" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine4 matches to intlState -->
            <xsd:attribute name="addressLine4" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="province" type="CP_PROVINCE"/>
            <xsd:attribute name="suiteNum" type="BDM_ADDRESS_SUITE_NUMBER"/>
            <xsd:attribute name="country" type="CP_COUNTRY"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="IntlMailAddress">
        <xsd:complexType>
            <xsd:attribute name="city" type="BDM_ADDRESS_CITY"/>
            <xsd:attribute name="state" type="CT_ADDRESS_STATE"/>
            <xsd:attribute name="zipCode" type="BDM_ADDRESS_ZIPCODE"/>
            <!-- addressLine1 matches to StreetNum -->
            <xsd:attribute name="addressLine1" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine2 matches to StreetName -->
            <xsd:attribute name="addressLine2" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine3 matches to poBox -->
            <xsd:attribute name="addressLine3" type="BDM_ADDRESS_LINE"/>
            <!-- addressLine4 matches to intlState -->
            <xsd:attribute name="addressLine4" type="BDM_ADDRESS_LINE"/>
            <xsd:attribute name="province" type="CP_PROVINCE"/>
            <xsd:attribute name="suiteNum" type="BDM_ADDRESS_SUITE_NUMBER"/>
            <xsd:attribute name="country" type="CP_COUNTRY"/>
        </xsd:complexType>
    </xsd:element>
	
	<xsd:element name="Relationship">
		<xsd:complexType>
			<xsd:attribute name="relationshipType" type="IEG_STRING" />
			<xsd:attribute name="isNonParentPrimaryCaretaker" type="IEG_BOOLEAN" default="false"/>
			<xsd:attribute name="personID" type="D:SVR_KEY" />
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="CommunicationPref">
		<xsd:complexType>
			<xsd:attribute name="languageW" type="CT_LANGUAGE" />
			<xsd:attribute name="languageS" type="CT_LANGUAGE" />
			<xsd:attribute name="languageSelected" type="IEG_BOOLEAN" />
			<xsd:attribute name="recieveCorrespondPref" type="BDM_CT_CORRESDEL" />
		</xsd:complexType>
	</xsd:element>
	<!-- COMMUNICATION PAGE - BEGIN -->
	<xsd:element name="CommunicationDetails">
    	<xsd:complexType>
			<xsd:attribute name="emailAdr" type="IEG_STRING" default=""/>
			<xsd:attribute name="emailType" type="CT_EMAILTYPE" />
			<xsd:attribute name="phoneNumber" type="BDM_PHONE_NUMBER" default=""/>
			<xsd:attribute name="phoneAreaCode" type="BDM_PHONE_AREA_CODE" />
			<xsd:attribute name="fullPhoneNumber" type="IEG_STRING" />
			<xsd:attribute name="phoneType" type="CT_PHONETYPE" />
			<xsd:attribute name="countryCode" type="CT_COUNTRYCODE"/>
			<xsd:attribute name="phoneTime" type="CT_PHONETIME"/>
			<xsd:attribute name="phoneExt" type="BDM_PHONE_EXT_CODE" />
			<xsd:attribute name="isMor" type="IEG_BOOLEAN" />
			<xsd:attribute name="isAftr" type="IEG_BOOLEAN" />
			<xsd:attribute name="isEve" type="IEG_BOOLEAN" />
			<!-- created to show blank value on summary page (display only) when client doesn't select select these on page -->
			<xsd:attribute name="isMorDisplayValue" type="IEG_STRING"/>
			<xsd:attribute name="isAftrDisplayValue" type="IEG_STRING"/>
			<xsd:attribute name="isEveDisplayValue" type="IEG_STRING"/>
			<xsd:attribute name="tempAttribute" type="IEG_STRING"/>
			
			<xsd:attribute name="isPrimaryCommunication" type="IEG_BOOLEAN" default="false"/>    
            <xsd:attribute name="alertPrefEmail" type="IEG_YES_NO" default="YN2"/>
            <xsd:attribute name="alertPrefPhone" type="IEG_YES_NO" default="YN2"/>
            
            <!-- localID required for life event element -->
