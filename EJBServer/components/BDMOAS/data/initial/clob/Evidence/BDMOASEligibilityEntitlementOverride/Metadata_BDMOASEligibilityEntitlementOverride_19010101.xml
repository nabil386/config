<?xml version="1.0" encoding="UTF-8"?>
<!-- Feature 70518: Manage Override Evidence -->
<EvidenceTypeVersion businessEndDate="toDate"
	businessStartDate="fromDate" correctionOnly="true" modifiable="true"
	relatedParticipant="participant"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../../../DynamicEvidenceMetadata.xsd">
	<Model>
		<Attributes>
			<Attribute>
				<RelatedCPAttribute cpRoleType="MEM" name="participant"
					participantType="Person">
					<CPSearchType code="PRI" />
					<CPSearchType code="MEM" />
				</RelatedCPAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="benefitType">
					<DomainType dataType="Codetable">
						<CodetableOptions
							codetableName="BDMOASOverrideBenefitType" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="overrideValue">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="BDMOASOverrideValue" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="numberAmount">
					<DomainType dataType="Integer">
						<NumericOptions minValue="0" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="dollarAmount">
					<DomainType dataType="Money" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="reason">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="BDMOASOverrideReason" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="reasonDescription">
					<DomainType dataType="String">
						<StringOptions maxSize="144" trimLeadingSpaces="true"
							trimTrailingSpaces="true" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="fromDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="toDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<CommentsAttribute name="comments" />
			</Attribute>
		</Attributes>
		<Relationships />
	</Model>
	<Validations>
		<PatternValidations>
			<Validator>
				<DependentAttributeValidator
					dependency="MustEnter" source="reason" sourceLiteral="Other"
					target="reasonDescription" unidirectional="true" useSourceLiteral="true">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_MUST_ENTER_DESCRIPTION" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</DependentAttributeValidator>
			</Validator>
			<Validator>
				<ComparisonValidator operator="LessThan" shallow="false"
					source="fromDate" target="toDate" useClause="false" useLiteral="false">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_FROM_DATE_AFTER_TO_DATE" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</ComparisonValidator>
			</Validator>
            <Validator>
                <DependentAttributeValidator
                    dependency="MustEnter" source="participant" target="overrideValue">
                    <ValidationMessageInformation>
                        <ValidationMessageMapping>
                            <ValidationMessage>
                                <ValidationMessageResource
                                    id="ERR_OVERRIDE_VALUE_MANDATORY" />
                            </ValidationMessage>
                        </ValidationMessageMapping>
                    </ValidationMessageInformation>
                </DependentAttributeValidator>
            </Validator>
		</PatternValidations>
		<CustomRulesValidation
			ruleSetName="BDMOASEligibilityEntitlementOverrideValidationRuleSet" />
	</Validations>
	<SummaryInformation>
		<SummaryRuleset
			ruleSetName="BDMOASEligibilityEntitlementOverrideSummaryRuleSet" />
	</SummaryInformation>
	<UserInterface>
		<Cluster>
			<RelCPCluster defaultToBlank="true"
				enableMultiMemberUpdates="false" fullCreateParticipant="false"
				isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
				modifyOption="no" showParticipantTypeFilter="true" useSingleName="false">
				<StandardField source="participant" />
				<ClusterName id="Related.Participant.Cluster.ID2" />
				<LabelName id="Name.Label.ID2" />
				<FirstName id="First.Name.Label.ID2" />
				<SecondName id="Second.Name.Label.ID2" />
			</RelCPCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true" labelWidth="40" numberOfColumns="2">
				<Title id="Cluster.Title.ID1" />
				<StandardClusterField>
					<StandardField modifiable="true" source="benefitType"
						useBlank="true" useDefault="false">
						<Label id="Field.Label.ID1" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="overrideValue"
						useBlank="true" useDefault="false">
						<Label id="Field.Label.ID2" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="numberAmount"
						useDefault="false">
						<Label id="Field.Label.ID3" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="dollarAmount"
						useDefault="false">
						<Label id="Field.Label.ID4" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="reason"
						useBlank="true" useDefault="false">
						<Label id="Field.Label.ID5" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="reasonDescription"
						useDefault="false">
						<Label id="Field.Label.ID6" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="fromDate"
						useDefault="false">
						<Label id="Field.Label.ID7" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="toDate"
						useDefault="false">
						<Label id="Field.Label.ID8" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<CommentsCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true">
				<Title id="Comments.Cluster.Title.ID3" />
				<StandardField source="comments" />
			</CommentsCluster>
		</Cluster>
	</UserInterface>
</EvidenceTypeVersion>