				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true"
						source="absenceReason" useDefault="false" useBlank="true">
						<Label id="AbsenceReason.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true"
						source="intentToReturnYesNo" useDefault="false" useBlank="true">
