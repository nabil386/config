<?xml version="1.0" encoding="UTF-8"?>
<!-- 64790: Residence Period -->
<EvidenceTypeVersion correctionOnly="true"
	enableSaveAndNew="true" modifiable="true"
	relatedParticipant="caseParticipantRole"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../../../DynamicEvidenceMetadata.xsd">
	<Model>
		<Attributes>
			<Attribute>
				<RelatedCPAttribute cpRoleType="MEM"
					name="caseParticipantRole" participantType="Person">
					<CPSearchType code="PRI" />
					<CPSearchType code="MEM" />
				</RelatedCPAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="country">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="Country" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="residenceType">
					<DomainType dataType="Codetable">
						<CodetableOptions
							codetableName="OASResidenceType" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="absenceReason">
					<DomainType dataType="Codetable">
						<CodetableOptions
							codetableName="OASAbsenceReason" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="intentToReturnYesNo">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="IEGYesNo" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="startDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="endDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<CommentsAttribute name="comments" />
			</Attribute>
		</Attributes>
		<Relationships />
	</Model>
	<Validations>
		<PatternValidations>
			<Validator>
				<ComparisonValidator source="endDate"
					target="startDate" operator="GreaterThan">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource id="ERR_END_DATE_AFTER_START_DATE" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</ComparisonValidator>
			</Validator>
		</PatternValidations>
		<CustomRulesValidation
			ruleSetName="OASResidencePeriodValidationRuleSet" />
	</Validations>
	<SummaryInformation>
		<SummaryRuleset
			ruleSetName="OASResidencePeriodSummaryRuleSet" />
	</SummaryInformation>
	<UserInterface>
		<Cluster>
			<RelCPCluster defaultToBlank="false"
				enableMultiMemberUpdates="false" fullCreateParticipant="false"
				isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
				modifyOption="no" showParticipantTypeFilter="true"
				useSingleName="false">
				<StandardField source="caseParticipantRole" />
				<ClusterName id="Participant.Cluster.Title" />
				<LabelName id="Participant.Field.Label" />
			</RelCPCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true" labelWidth="40"
				numberOfColumns="2">
				<Title id="ResidencePeriod.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true" source="country"
						useDefault="false" useBlank="true">
						<Label id="Country.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true"
						source="residenceType" useDefault="false" useBlank="true">
						<Label id="ResidenceType.Field.Label" />
