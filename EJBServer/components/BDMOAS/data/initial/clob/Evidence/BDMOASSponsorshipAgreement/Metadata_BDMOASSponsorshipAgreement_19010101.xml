<?xml version="1.0" encoding="UTF-8"?>
<!-- Task 61872 DEV: Implement Sponsorship Evidence -->
<EvidenceTypeVersion modifiable="true"
	businessStartDate="startDate" correctionOnly="true"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../../DynamicEvidenceMetadata.xsd">
	<Model>
		<Attributes>
			<Attribute>
				<DataAttribute name="startDate" mandatory="true">
					<DomainType dataType="Date">
						<DateOptions currentDate="LessThan">
							<ValidationMessageInformation>
								<ValidationMessageMapping>
									<ValidationMessage>
										<ValidationMessageResource
											id="ERR_START_DATE_ON_AFTER_CURRENT_DATE" />
									</ValidationMessage>
								</ValidationMessageMapping>
							</ValidationMessageInformation>
						</DateOptions>
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="endDate" mandatory="true">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="sponsorName">
					<DomainType dataType="String">
						<StringOptions maxSize="65" trimLeadingSpaces="true"
							trimTrailingSpaces="true" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="breakdownReason">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="BDMOASBreakdownReason" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="breakdownDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<CommentsAttribute name="comments" />
			</Attribute>
		</Attributes>
		<Relationships>
			<Relationship>
				<MandatoryParent evidenceTypeCode="OASDET0012"
					name="sponsorship" />
			</Relationship>
		</Relationships>
	</Model>
	<Validations>
		<PatternValidations>
			<Validator>
				<ComparisonValidator operator="GreaterThan"
					source="breakdownDate" target="startDate">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_BREAKDOWN_DATE_BETWEEN_START_END" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</ComparisonValidator>
			</Validator>
			<Validator>
				<ComparisonValidator operator="LessThan"
					source="breakdownDate" target="endDate">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_BREAKDOWN_DATE_BETWEEN_START_END_1" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</ComparisonValidator>
			</Validator>
			<Validator>
				<ComparisonValidator operator="GreaterThanOrEqualTo"
					source="endDate" target="startDate" />
			</Validator>
			<Validator>
				<DependentAttributeValidator
					dependency="MustEnter" source="breakdownReason" target="breakdownDate"
					unidirectional="true">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_BREAKDOWN_DATE_REQUIRED_WHEN_BREAKDOWN_REASON_IS_ENTERED" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</DependentAttributeValidator>
			</Validator>
			<Validator>
				<DependentAttributeValidator
					dependency="MustEnter" source="breakdownDate" target="breakdownReason"
					unidirectional="true">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_BREAKDOWN_REASON_REQUIRED_WHEN_BREAKDOWN_REASON_IS_ENTERED" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</DependentAttributeValidator>
			</Validator>
		</PatternValidations>
		<CustomRulesValidation
			ruleSetName="BDMOASSponsorshipAgreementValidationRuleSet" />
	</Validations>
	<SummaryInformation>
		<SummaryRuleset
			ruleSetName="BDMOASSponsorshipAgreementSummaryRuleSet" />
	</SummaryInformation>
	<UserInterface>
		<Cluster>
			<StandardCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true" labelWidth="40" numberOfColumns="2">
				<Title id="Sponsorship.Cluster.Title" />
				<StandardClusterField>
					<StandardField source="startDate" modifiable="true"
						useDefault="false" useBlank="true">
						<Label id="StartDate.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField source="endDate" modifiable="true"
						useDefault="false" useBlank="true">
						<Label id="EndDate.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField source="sponsorName" modifiable="true"
						useDefault="false" useBlank="true">
						<Label id="SponsorName.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<SkipField />
				</StandardClusterField>
				<StandardClusterField>
					<StandardField source="breakdownReason" modifiable="true"
						useDefault="false" useBlank="true">
						<Label id="BreakdownReason.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField source="breakdownDate" modifiable="true"
						useDefault="false" useBlank="true">
						<Label id="BreakdownDate.Field.Label" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<CommentsCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true">
				<Title id="Comments.Cluster.Title" />
				<StandardField source="comments" />
			</CommentsCluster>
		</Cluster>
	</UserInterface>
</EvidenceTypeVersion>