						<Label id="NetRentalIncome.Field.Label" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true"
				numberOfColumns="2" labelWidth="40">
				<Title id="Block7.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true"
						source="netEmploymentIncome" useDefault="false">
						<Label id="NetEmploymentIncome.Field.Label" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true"
				numberOfColumns="2">
				<Title id="Block8.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true"
