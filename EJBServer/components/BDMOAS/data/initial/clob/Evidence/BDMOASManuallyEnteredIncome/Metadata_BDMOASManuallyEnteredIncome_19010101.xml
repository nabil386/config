				numberOfColumns="2">
				<Title id="Block3.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true"
						source="employmentInsurance" useDefault="false">
						<Label id="EmploymentInsurance.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true"
						source="workersCompensationBenefits" useDefault="false">
						<Label id="WorkersCompensationBenefits.Field.Label" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true"
				numberOfColumns="2">
				<Title id="Block4.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true"
