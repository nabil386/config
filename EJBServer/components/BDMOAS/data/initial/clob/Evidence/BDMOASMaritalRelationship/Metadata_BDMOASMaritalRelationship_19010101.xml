<?xml version="1.0" encoding="UTF-8"?>
<!-- Task DEV 86193 : Marital Relationship and Living Apart for Reasons Beyond 
	Control Evidence -->
<EvidenceTypeVersion businessEndDate="endDate"
	businessStartDate="startDate" modifiable="true"
	relatedParticipant="caseParticipantRoleID"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../../DynamicEvidenceMetadata.xsd">
	<Model>
		<Attributes>
			<Attribute>
				<RelatedCPAttribute cpRoleType="MEM"
					name="caseParticipantRoleID" participantType="Person">
					<CPSearchType code="PRI" />
				</RelatedCPAttribute>
			</Attribute>
			<Attribute>
				<RelatedCPAttribute cpRoleType="MEM"
					name="relatedCaseParticipantRoleID" participantType="Person">
					<CPSearchType code="MEM" />
				</RelatedCPAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" volatile="true"
					name="relationshipStatus">
					<DomainType dataType="Codetable">
						<CodetableOptions
							codetableName="BDMOASMaritalRelation" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute name="relationshipChangeType"
					volatile="true">
					<DomainType dataType="Codetable">
						<CodetableOptions
							codetableName="BDMOASMaritalChangeType" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="startDate">
					<DomainType dataType="Date">
						<DateOptions currentDate="LessThanOrEqualTo">
							<ValidationMessageInformation>
								<ValidationMessageMapping>
									<ValidationMessage>
										<ValidationMessageResource
											id="ERR_START_DATE_MUST_BE_ON_OR_BEFORE_CURRENT_DATE" />
									</ValidationMessage>
								</ValidationMessageMapping>
							</ValidationMessageInformation>
						</DateOptions>
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="false" name="endDate">
					<DomainType dataType="Date" />
				</DataAttribute>
			</Attribute>
			<Attribute>
				<CommentsAttribute name="comments" />
			</Attribute>
		</Attributes>
		<Relationships />
	</Model>
	<Validations>
		<PatternValidations>
			<Validator>
				<DependentAttributeValidator
					dependency="MustEnter" source="endDate"
					target="relationshipChangeType" unidirectional="true">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_REATIONSHIP_CHANGE_TYPE_MUST_BE_ENTERED_IF_END_DATE_ENTERED" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</DependentAttributeValidator>
			</Validator>
			<Validator>
				<ComparisonValidator
					operator="GreaterThanOrEqualTo" shallow="false" source="endDate"
					target="startDate" useClause="false" useLiteral="false">
					<ValidationMessageInformation>
						<ValidationMessageMapping>
							<ValidationMessage>
								<ValidationMessageResource
									id="ERR_START_DATE_CAN_NOT_BE_AFTER_END_DATE" />
							</ValidationMessage>
						</ValidationMessageMapping>
					</ValidationMessageInformation>
				</ComparisonValidator>
			</Validator>
		</PatternValidations>

		<CustomRulesValidation
			ruleSetName="BDMOASMaritalRelationshipValidationRuleSet" />
	</Validations>
	<SummaryInformation>
		<SummaryRuleset
			ruleSetName="BDMOASMaritalRelationshipSummaryRuleSet" />
	</SummaryInformation>
	<UserInterface>
		<Cluster>
			<RelCPCluster defaultToBlank="true"
				enableMultiMemberUpdates="false" fullCreateParticipant="false"
				isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
				modifyOption="no" showParticipantTypeFilter="true"
				useSingleName="false">
				<StandardField source="caseParticipantRoleID" />
				<ClusterName id="Participant.Cluster.Label" />
				<LabelName id="Participant.Cluster.Label" />
				<RelCpPfx id="Participant.Description.ID1" />
				<FirstName id="First.Name.Label.ID1" />
				<SecondName id="Second.Name.Label.ID1" />
			</RelCPCluster>
		</Cluster>
		<Cluster>
			<RelCPCluster defaultToBlank="true"
				enableMultiMemberUpdates="false" fullCreateParticipant="false"
				isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
				modifyOption="single" showParticipantTypeFilter="true"
				useSingleName="false">
				<StandardField source="relatedCaseParticipantRoleID" />
				<ClusterName id="Related.Participant.Cluster.Label" />
				<LabelName id="Related.Participant.Name.Label" />
				<RelCpPfx id="Related.Participant.Description.ID2" />
				<FirstName id="Related.Participant.First.Name.Label.ID2" />
				<SecondName
					id="Related.Participant.Second.Name.Label.ID2" />
			</RelCPCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true" labelWidth="40"
				numberOfColumns="2">
				<Title id="RelationshipDetails.Cluster.Title" />
				<StandardClusterField>
					<StandardField modifiable="true"
						source="relationshipStatus" useDefault="false" useBlank="true">
						<Label id="RelationshipStatus.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true"
						source="relationshipChangeType" useDefault="false" useBlank="true">
						<Label id="RelationshipChangeType.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="startDate"
						useDefault="false" useBlank="true">
						<Label id="StartDate.Field.Label" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="endDate"
						useDefault="false" useBlank="true">
						<Label id="EndDate.Field.Label" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<CommentsCluster isOnCreatePage="true"
				isOnModifyPage="true" isOnViewPage="true">
				<Title id="Comments.Cluster.Title" />
				<StandardField source="comments" />
			</CommentsCluster>
		</Cluster>
	</UserInterface>
</EvidenceTypeVersion>