<?xml version="1.0" encoding="UTF-8"?>
<!-- 90159 DEV: Implement Benefit Suspension Evidence -->
<EvidenceTypeVersion
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    correctionOnly="true" modifiable="true"
    relatedParticipant="caseParticipantRole"
    businessStartDate="startDate"
    businessEndDate="endDate"
    xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/DynamicEvidenceSchema.xsd">
  <Model>
    <Attributes>
      <Attribute>
        <RelatedCPAttribute cpRoleType="MEM" mandatory="true" name="caseParticipantRole" participantType="Person">
          <CPSearchType code="PRI"/>
        </RelatedCPAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute mandatory="true" name="suspendedBenefit">
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="BDMOASSuspendedBenefit"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute defaultValue="SST1" mandatory="true" name="suspensionStatus">
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="BDMOASSuspensionStatus"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute mandatory="true" name="suspensionReason">
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="BDMOASSuspensionReason"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute name="suspensionRsnDesc">
          <DomainType dataType="String">
            <StringOptions maxSize="144"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute name="unsuspensionReason">
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="BDMOASUnsuspensionReason"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute name="unsuspensionRsnDesc">
          <DomainType dataType="String">
            <StringOptions maxSize="144"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <CommentsAttribute name="comments"/>
      </Attribute>
      <Attribute>
        <DataAttribute mandatory="true" name="startDate">
          <DomainType dataType="Date">
            <DateOptions currentDate="LessThanOrEqualTo">
                <ValidationMessageInformation>
                    <ValidationMessageMapping>
                        <ValidationMessage>
                            <ValidationMessageResource
                                id="ERR_START_DATE_MUST_BE_ON_OR_BEFORE_CURRENT_DATE" />
                        </ValidationMessage>
                    </ValidationMessageMapping>
                </ValidationMessageInformation>
            </DateOptions>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute name="endDate">
          <DomainType dataType="Date"/>
        </DataAttribute>
      </Attribute>
    </Attributes>
    <Relationships/>
  </Model>
  <Validations>
    <PatternValidations>
      <Validator>
        <ComparisonValidator operator="GreaterThan" shallow="false" source="endDate" target="startDate" useClause="false" useLiteral="false">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_END_DATE_AFTER_START_DATE"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </ComparisonValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="suspensionStatus" sourceLiteral="Unsuspended" target="unsuspensionReason" unidirectional="true" useSourceLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_MUST_ENTER_UNSUSPENSION_REASON"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="suspensionReason" sourceLiteral="Presumed or Potentially Deceased" target="suspendedBenefit" targetLiteral="All Benefits" unidirectional="true" useSourceLiteral="true" useTargetLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_SUSPENDED_BENEFIT_MUST_BE_ALL_IF_DECEASED"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="suspensionReason" sourceLiteral="Cannot Locate" target="suspendedBenefit" targetLiteral="All Benefits" unidirectional="true" useSourceLiteral="true" useTargetLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_SUSPENDED_BENEFIT_MUST_BE_ALL_IF_CANT_LOCATE"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="suspensionReason" sourceLiteral="Other" target="suspensionRsnDesc" unidirectional="true" useSourceLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_MUST_ENTER_SUSPENSION_RSN_DESC"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="suspensionRsnDesc" target="suspensionReason" targetLiteral="Other" unidirectional="true" useTargetLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_SUSPENSION_RSN_MUST_BE_OTHER"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="unsuspensionReason" sourceLiteral="Other" target="unsuspensionRsnDesc" unidirectional="true" useSourceLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="ERR_MUST_ENTER_UNSUSPENSION_RSN_DESC"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
      <Validator>
        <DependentAttributeValidator dependency="MustEnter" source="unsuspensionRsnDesc" target="unsuspensionReason" targetLiteral="Other" unidirectional="true" useTargetLiteral="true">
          <ValidationMessageInformation>
            <ValidationMessageMapping>
              <ValidationMessage>
                <ValidationMessageResource id="UNSUSPENSION_RSN_MUST_BE_OTHER"/>
              </ValidationMessage>
            </ValidationMessageMapping>
          </ValidationMessageInformation>
        </DependentAttributeValidator>
      </Validator>
    </PatternValidations>
    <CustomRulesValidation
        ruleSetName="BDMOASBenefitSuspensionValidationRuleSet" />
  </Validations>
  <SummaryInformation>
    <SummaryRuleset ruleSetName="BDMOASBenefitSuspensionSummaryRuleSet"/>
  </SummaryInformation>
  <UserInterface>
    <Cluster>
      <RelCPCluster defaultToBlank="false"
        enableMultiMemberUpdates="false" fullCreateParticipant="false"
        isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
        modifyOption="no" showParticipantTypeFilter="true"
        useSingleName="false">
        <StandardField source="caseParticipantRole"/>
        <ClusterName id="Related.Participant.Cluster.Title"/>
        <LabelName id="Related.Participant.Name.Label"/>
        <RelCpPfx id="Related.Participant.Description"/>
        <FirstName id="Related.Participant.First.Name.Label"/>
        <SecondName id="Related.Participant.Second.Name.Label"/>
      </RelCPCluster>
    </Cluster>
    <Cluster>
      <StandardCluster isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true" labelWidth="40" numberOfColumns="2">
        <Title id="SuspensionDetails.Cluster.Title"/>
        <StandardClusterField>
          <StandardField modifiable="true" source="suspendedBenefit" useBlank="true" useDefault="false">
            <Label id="SuspendedBenefits.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="suspensionStatus" useDefault="true">
            <Label id="SuspensionStatus.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="suspensionReason" useBlank="true" useDefault="false">
            <Label id="SuspensionReason.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="suspensionRsnDesc" useDefault="false">
            <Label id="SuspensionReasonDescription.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="unsuspensionReason" useBlank="true" useDefault="false">
            <Label id="UnsuspensionReason.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="unsuspensionRsnDesc" useDefault="false">
            <Label id="UnsupensionReasonDescription.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="startDate" useDefault="false">
            <Label id="StartDate.Field.Label"/>
          </StandardField>
        </StandardClusterField>
        <StandardClusterField>
          <StandardField modifiable="true" source="endDate" useDefault="false">
            <Label id="EndDate.Field.Label"/>
          </StandardField>
        </StandardClusterField>
      </StandardCluster>
    </Cluster>
    <Cluster>
      <CommentsCluster isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true">
        <Title id="Comments.Cluster.Title"/>
        <StandardField source="comments"/>
      </CommentsCluster>
    </Cluster>
  </UserInterface>
</EvidenceTypeVersion>