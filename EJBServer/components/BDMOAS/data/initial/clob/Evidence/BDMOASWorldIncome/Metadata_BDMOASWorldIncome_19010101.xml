<?xml version="1.0" encoding="UTF-8"?>
<!-- Feature 72660: Manage World Income Evidence -->
<EvidenceTypeVersion correctionOnly="true" modifiable="true"
	relatedParticipant="participant"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../../../../../../DynamicEvidenceMetadata.xsd">
	<Model>
		<Attributes>
			<Attribute>
				<RelatedCPAttribute cpRoleType="MEM" name="participant"
					participantType="Person">
					<CPSearchType code="PRI" />
				</RelatedCPAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="threshold">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="BDMOASTaxThreshold" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<DataAttribute mandatory="true" name="overThreshold">
					<DomainType dataType="Codetable">
						<CodetableOptions codetableName="BDMOASYesNoNA" />
					</DomainType>
				</DataAttribute>
			</Attribute>
			<Attribute>
				<CommentsAttribute name="comments" />
			</Attribute>
		</Attributes>
		<Relationships />
	</Model>
	<Validations>
		<PatternValidations>
			<Validator>
				<DuplicateValidator>
					<DuplicateAttributes>
						<DuplicateAttribute name="threshold" />
						<ValidationMessageInformation>
							<ValidationMessageMapping>
								<ValidationMessage>
									<parameter name="threshold" />
									<ValidationMessageResource
										id="ERR_RECORD_FOR_YEAR_EXISTS" />
								</ValidationMessage>
							</ValidationMessageMapping>
						</ValidationMessageInformation>
					</DuplicateAttributes>
				</DuplicateValidator>
			</Validator>
		</PatternValidations>
	</Validations>
	<SummaryInformation>
		<SummaryRuleset ruleSetName="BDMOASWorldIncomeSummaryRuleSet" />
	</SummaryInformation>
	<UserInterface>
		<Cluster>
			<RelCPCluster defaultToBlank="false"
				enableMultiMemberUpdates="false" fullCreateParticipant="false"
				isOnCreatePage="true" isOnModifyPage="true" isOnViewPage="true"
				modifyOption="no" showParticipantTypeFilter="true"
				useSingleName="true">
				<StandardField source="participant" />
				<ClusterName id="Related.Participant.Cluster.ID3" />
				<LabelName id="Name.Label.ID3" />
				<FirstName id="First.Name.Label.ID3" />
			</RelCPCluster>
		</Cluster>
		<Cluster>
			<StandardCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true" labelWidth="40" numberOfColumns="2">
				<Title id="Cluster.Title.ID1" />
				<StandardClusterField>
					<StandardField modifiable="true" source="threshold"
						useBlank="true" useDefault="false">
						<Label id="Field.Label.ID1" />
					</StandardField>
				</StandardClusterField>
				<StandardClusterField>
					<StandardField modifiable="true" source="overThreshold"
						useBlank="true" useDefault="false">
						<Label id="Field.Label.ID2" />
					</StandardField>
				</StandardClusterField>
			</StandardCluster>
		</Cluster>
		<Cluster>
			<CommentsCluster isOnCreatePage="true" isOnModifyPage="true"
				isOnViewPage="true">
				<Title id="Comments.Cluster.Title.ID2" />
				<StandardField source="comments" />
			</CommentsCluster>
		</Cluster>
	</UserInterface>
</EvidenceTypeVersion>