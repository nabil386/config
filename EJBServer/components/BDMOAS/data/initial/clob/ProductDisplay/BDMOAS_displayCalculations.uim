<?xml version="1.0" encoding="UTF-8"?>
<PAGE PAGE_ID="BDMOAS_displayCalculations">

	<PAGE_PARAMETER NAME="determinationID" />
	<PAGE_PARAMETER NAME="displayDate" />

	<SERVER_INTERFACE CLASS="CaseDetermination"
		NAME="DISPLAY" OPERATION="viewDecisionDisplayRulesCategoryXML" />

	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="determinationID" />
		<TARGET NAME="DISPLAY" PROPERTY="key$determinationID" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="displayDate" />
		<TARGET NAME="DISPLAY" PROPERTY="key$date" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="TEXT" PROPERTY="CategoryRef" />
		<TARGET NAME="DISPLAY" PROPERTY="key$categoryRef" />

	</CONNECT>
	
	<CLUSTER NUM_COLS="2" TITLE="Cluster.EntitlementOverride"
		WIDTH="50">
		<FIELD LABEL="Field.OverrideAmount">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/meetsResidenceReqOAS"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
	</CLUSTER>
	
	<CLUSTER NUM_COLS="2" TITLE="Cluster.CalculationUsed"
		WIDTH="50">
		<FIELD LABEL="Field.TotalAmount">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/meetsResidenceReqOAS"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.BaseOAS">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/meetsResidenceReqOAS"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.ActuarialAjustment">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/meetsResidenceReqOAS"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

</PAGE>
