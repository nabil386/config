<?xml version="1.0" encoding="UTF-8"?>
<PAGE PAGE_ID="BDMOAS_displaySummaryDropdownPane">

	<PAGE_TITLE>
		<CONNECT>
			<SOURCE NAME="TEXT" PROPERTY="Page.Title" />
		</CONNECT>
	</PAGE_TITLE>

	<PAGE_PARAMETER NAME="determinationID" />
	<PAGE_PARAMETER NAME="displayDate" />

	<SERVER_INTERFACE CLASS="CaseDetermination"
		NAME="DISPLAY" OPERATION="viewDecisionDisplayRulesCategoryXML" />

	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="determinationID" />
		<TARGET NAME="DISPLAY" PROPERTY="key$determinationID" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="PAGE" PROPERTY="displayDate" />
		<TARGET NAME="DISPLAY" PROPERTY="key$date" />
	</CONNECT>
	<CONNECT>
		<SOURCE NAME="TEXT" PROPERTY="CategoryRef" />
		<TARGET NAME="DISPLAY" PROPERTY="key$categoryRef" />
	</CONNECT>

	<CLUSTER NUM_COLS="2">
		<CONDITION>
			<IS_TRUE
				EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/displayOASPension"
				NAME="DISPLAY" PROPERTY="xmlData" />
		</CONDITION>
		<FIELD LABEL="Field.Label.OAS">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/oasPensionEligibilityResult"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
		<FIELD LABEL="Field.Label.Entitlement">
			<CONNECT>
				<SOURCE
					EXTENDED_PATH="/DecisionDetails/BDMOASSummaryCategory/oasPensionEntitlement"
					NAME="DISPLAY" PROPERTY="xmlData" />
			</CONNECT>
		</FIELD>
	</CLUSTER>

</PAGE>
