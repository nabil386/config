<?xml version="1.0" encoding="UTF-8"?>
 
<ieg-script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ieg-schema.xsd"
finish-page="CommonIntake_finishInternalIntakeScript" 
	quit-page="CommonIntake_quitInternalIntakeScript"		
	show-sections="true"
	show-progress-bar="true"
	validate-save-and-exit="false"
	highlight-validation="true"
	>
		
	<identifier id="BDMOASGISApplicationAP" scriptversionnumber="V1" type="Intake" />

	<section>
		<title id="Section.Title.Declaration">
			<![CDATA[Spouse/Common-Law Partner]]>
		</title>
		<icon enabled-image="sample_about_you"
			current-image="sample_about_you" />

		<callout expression="SetSpouseOrCLPDeclarationSignInd()" id="SetSpouseOrCLPDeclarationSignInd"/>
		<question-page 
			id="DeclarationAndSignatureDetails" 
			entity="Declaration"
			progress="0" 
			show-back-button="true" 
			show-next-button="true" 
			show-exit-button="true"			
			show-save-exit-button="true"
			>
			
			<title id="QuestionPage.Title.DeclarationSign">
				<![CDATA[Declaration and Signature]]>
			</title>
			
			<description id="QuestionPage.Description.MandatoryFields">
				<![CDATA[All fields marked with * are mandatory.]]>
			</description>			

	    	<validation expression="ValidateAplcntOrWitnessSigndDeclrn(Declaration.didAplicntSigned, Declaration.didTPSignedBehalfOfAplcnt)">
				<message id="Validation.Message.ApplicantDeclaration"><![CDATA[Either applicant or authorized person signature should be selected.]]></message>
	    	</validation>

	    	<validation expression="ValidateSpouseOrWitnessSigndDeclrn(Declaration.didSpouseOrCLPSigned, Declaration.didTPSignedBehalfOfSpouse)">
				<message id="Validation.Message.SpouseOrClpDeclaration"><![CDATA[Either applicant's spouse/common-law partner or Authorized person signature should be selected.]]></message>
	    	</validation>
	    	
	    	<validation expression="ValidateWitnessSigndDeclrn(Declaration.didAplicntSignedWithMark, Declaration.didSpouseOrCLPSignedWithMark, Declaration.didTPSignedAsWitness)">
				<message id="Validation.Message.WitnessDeclaration"><![CDATA[Witness can only sign the application if the applicant and/or Applicant's spouse/common'law partner sign the declaration with a mark.]]></message>
	    	</validation>	    	

			<cluster entity="Declaration">
				<title id="Cluster.Title.DeclarationSign">
					<![CDATA[Applicant's declaration and signature]]>
				</title>
				<skip-field/>						
			</cluster>
	
			<condition expression="not(isNotNull(Declaration.didAplicntSignedWithMark)) or not(Declaration.didAplicntSignedWithMark==&quot;YN1&quot;)">
				<cluster entity="Declaration">
					<question id="didAplicntSigned">
						<label id="Question.Label.ApplicantSigned">
							<![CDATA[Did the applicant sign the declaration?]]>
						</label>
						<layout>
							<width>50</width>							
						</layout>						
					</question>		
				</cluster>
			</condition>
						
			<condition expression="not(isNotNull(Declaration.didAplicntSigned)) or not(Declaration.didAplicntSigned==&quot;YN1&quot;)">
				<cluster entity="Declaration">		
					<question id="didAplicntSignedWithMark" >
						<label id="Question.Label.ApplicantSignedWithMark">
							<![CDATA[Did the applicant sign the declaration with a mark?]]>
						</label>
						<layout>
							<width>50</width>							
						</layout>											
					</question>			
				</cluster>
			</condition>
			
			<condition expression="Declaration.didAplicntSigned==&quot;YN1&quot; or Declaration.didAplicntSignedWithMark==&quot;YN1&quot;">
				<cluster entity="Declaration">		
					<question id="aplicntSignedDate" mandatory="true" show-field-help="true">
						<label id="Question.Label.SignedDate">
							<![CDATA[Date signed]]>
						</label>
						<layout>
							<type>date-picker</type>
							<!-- <class-names>wds-js-input-mask-yyyy-mm-dd</class-names> -->
						</layout>
						<help-text id="Question.Label.SignedDate.Help">
							<![CDATA[YYYY-MM-DD]]>
						</help-text>						
					</question>			
				</cluster>
			</condition>

			<condition expression="Person.spouseOrCLPDeclarationSignInd">
				<cluster entity="Declaration">
					<title id="Cluster.Title.SPCLDeclarationSign">
						<![CDATA[Spouse/Common-Law Partner's declaration and signature]]>
					</title>
					<skip-field/>							
				</cluster>
			</condition>

				<condition expression="(Person.spouseOrCLPDeclarationSignInd) and (not(isNotNull(Declaration.didSpouseOrCLPSignedWithMark)) or not(Declaration.didSpouseOrCLPSignedWithMark==&quot;YN1&quot;)) ">
					<cluster entity="Declaration">
						<question id="didSpouseOrCLPSigned">
							<label id="Question.Label.SPCLSigned">
								<![CDATA[Did the applicant's spouse/common-law partner sign the declaration?]]>
							</label>
							<layout>
								<width>50</width>							
							</layout>							
						</question>		
					</cluster>
				</condition>	

				<condition expression="(Person.spouseOrCLPDeclarationSignInd) and (not(isNotNull(Declaration.didSpouseOrCLPSigned)) or not(Declaration.didSpouseOrCLPSigned==&quot;YN1&quot;)) ">
					<cluster entity="Declaration">		
						<question id="didSpouseOrCLPSignedWithMark" >
							<label id="Question.Label.SPCLSignedWithMark">
								<![CDATA[Did the applicant's spouse/common-law partner sign the declaration with a mark?]]>
							</label>	
							<layout>
								<width>50</width>							
							</layout>											
						</question>			
					</cluster>
				</condition>				
			

				<condition expression="((isNotNull(Declaration.didSpouseOrCLPSigned) and Declaration.didSpouseOrCLPSigned==&quot;YN1&quot;) or (isNotNull(Declaration.didSpouseOrCLPSignedWithMark) and Declaration.didSpouseOrCLPSignedWithMark==&quot;YN1&quot;))">	
					<cluster entity="Declaration">		
						<question id="spouseOrCLPSignedDate" mandatory="true" show-field-help="true">
							<label id="Question.Label.SignedDate">
								<![CDATA[Date signed]]>
							</label>
							<layout>
								<type>date-picker</type>
								<!-- <class-names>wds-js-input-mask-yyyy-mm-dd</class-names> -->
							</layout>
							<help-text id="Question.Label.SignedDate.Help">
								<![CDATA[YYYY-MM-DD]]>
							</help-text>						
						</question>			
					</cluster>
				</condition>	

			<cluster entity="Declaration">
				<title id="Cluster.Title.WAPDeclarationSign">
					<![CDATA[Witness or Authorized Person's declaration and signature]]>
				</title>
				<question id="didTPSignedAsWitness">
					<label id="Question.Label.ThirdPartySignedAsWitness">
						<![CDATA[Did the Third Party sign as the witness?]]>
					</label>
					<layout>
						<width>50</width>							
					</layout>					
				</question>		
			</cluster>
			<cluster entity="Declaration">
				<question id="didTPSignedBehalfOfAplcnt">
					<label id="Question.Label.ThirdPartySignedAsAPForApplicant">
						<![CDATA[Did the Third Party sign as the Authorized person on behalf of Applicant?]]>
					</label>
					<layout>
						<width>50</width>							
					</layout>					
				</question>		
			</cluster>
	
			<condition expression="Declaration.didTPSignedAsWitness==&quot;YN1&quot; or Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot;">
				<cluster entity="Declaration">		
					<question id="thirdPartySignedDate" mandatory="true" show-field-help="true">
						<label id="Question.Label.SignedDate">
							<![CDATA[Date signed]]>
						</label>
						<layout>
							<type>date-picker</type>
							<!-- <class-names>wds-js-input-mask-yyyy-mm-dd</class-names> -->
						</layout>
						<help-text id="Question.Label.SignedDate.Help">
							<![CDATA[YYYY-MM-DD]]>
						</help-text>						
					</question>			
				</cluster>
			</condition>
			
			<cluster entity="Declaration">
				<question id="didTPSignedBehalfOfSpouse">
					<label id="Question.Label.ThirdPartySignedAsAPForSPCL">
						<![CDATA[Did the Third Party sign as the Authorized person on behalf of applicant's spouse/common-law partner?]]>
					</label>
					<layout>
						<width>50</width>							
					</layout>					
				</question>		
			</cluster>
	
			<condition expression="Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;">
				<cluster entity="Declaration">		
					<question id="tpSignedDateBehalfOfSpouse" mandatory="true" show-field-help="true">
						<label id="Question.Label.SignedDate">
							<![CDATA[Date signed]]>
						</label>
						<layout>
							<type>date-picker</type>
							<!-- <class-names>wds-js-input-mask-yyyy-mm-dd</class-names> -->
						</layout>
						<help-text id="Question.Label.SignedDate.Help">
							<![CDATA[YYYY-MM-DD]]>
						</help-text>						
					</question>			
				</cluster>
			</condition>	

			<condition expression="Declaration.didTPSignedAsWitness==&quot;YN1&quot; or Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot; or Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;">
				<cluster entity="Declaration">
					<question id="thirdPartyName" mandatory="true">
						<label id="Question.Label.Name">
							<![CDATA[Name]]>
						</label>
					</question>		
				</cluster>	
			</condition>
			
			<condition expression="Declaration.didTPSignedAsWitness==&quot;YN1&quot; or Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot; or Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;">
				<cluster entity="Declaration">
					<question id="relationshipToApplicant"  mandatory="true">
						<label id="Question.Label.RelationToApplicant">
							<![CDATA[Relationship to applicant]]>
						</label>
						<layout>
							<width>60</width>							
						</layout>						
					</question>		
				</cluster>	
			</condition>
		</question-page>

		<condition expression="isNotNull(Declaration.didTPSignedAsWitness) and Declaration.didTPSignedAsWitness==&quot;YN1&quot; or isNotNull(Declaration.didTPSignedBehalfOfAplcnt) and Declaration.didTPSignedBehalfOfAplcnt==&quot;YN1&quot; or isNotNull(Declaration.didTPSignedBehalfOfSpouse) and Declaration.didTPSignedBehalfOfSpouse==&quot;YN1&quot;">
			<!-- Call out to create witness address   --> 
			<callout expression="PrepopulateWitnessAddressDetails()" id="PrepopulateWitnessAddressDetails"/>	

			<question-page 
				id="WitnessMailAddrInfoSearch" 
				entity="Declaration"
				progress="0"
				show-back-button="true" 
				show-next-button="true" 
				show-exit-button="true"			
				show-save-exit-button="true"
				>
				
				<validation expression="Declaration.mailingCountry!=&quot;CA&quot; or (Declaration.mailingCountry==&quot;CA&quot; and not(isNotNull(SearchAddress.queryMailingAddress)) or validateCAPostalCode(SearchAddress.queryMailingAddress))">
					<message id="AddressInformation.MailAddr.PostalCode.Validation"><![CDATA[Please enter a valid postal code (e.g. V1N 4J4)]]></message>
				</validation>
	
				<title id="AddressInformation.MailAddr.Title">
					<![CDATA[Residential address]]>
				</title>
				
				<description id="MandatoryFieldsDescription">
					<![CDATA[<p>All fields marked as * are mandatory.</p>]]>
				</description>			
				
				<cluster>
					<display-text id="SearchAddress.MailAddr.Description">
						<![CDATA[Your residential address must be in Canada. PO boxes cannot be used.]]>
					</display-text>
					<layout>
						<label-width>0</label-width>
					</layout>					
				</cluster>
				<cluster entity="Declaration">
					<question id="mailingCountry" mandatory="true">
						<label id="AddressInformation.Country">
							<![CDATA[Country]]>
						</label>
						<layout>
							<width>70</width>							
						</layout>						
					</question>
				</cluster>
				<condition expression="Declaration.mailingCountry==&quot;CA&quot;">
					<cluster entity="SearchAddress">
						<question id="queryMailingAddress" mandatory="true" show-field-help="false">
							<label id="SearchAddress.MailAddr.PostalCode"><![CDATA[Please enter mailing postal code (A1A 1A1)]]></label>
							<hint-text id="SearchAddress.Hint"><![CDATA[If you cannot find a match to your address in the list, select "Address not found? Click here to enter an address manually"]]></hint-text>
							<combo-box
								key="idResidentialAddress"
								search-function="searchResidentialAddressByPostalCode"
								target-entity="ResidentialAddress"
								filter-items="true" />
						</question>
					</cluster>
				</condition>			
			</question-page>
			
			 <callout
	        expression="SearchWitnessAddress(Declaration.mailingCountry,&quot;Mailing&quot;,SearchAddress.queryMailingAddress)"
	        id="SearchWitnessAddress"
	      />

	        <!--  List Address Page/ skip if it is not CA -->
			<condition expression="Declaration.mailingCountry==&quot;CA&quot;">
				<question-page 
				id="WitnessMailAddrInfoAddressList" 
				entity="Declaration"
				progress="0"
				show-back-button="true" 
				show-next-button="true" 
				show-exit-button="true"			
				show-save-exit-button="true"
				>
	    
				<title id="AddressInformation.MailAddr.Title" />
		
					<cluster>      			
						<display-text id="RequiredField.Indicator"/>
						<layout>
							<label-width>0</label-width>
						</layout>						
						</cluster>

						<cluster>					
							<list-question
								entity="SearchAddress"
								id="isSelected"
								single-select="true"
								criteria="isMailingAddress==&quot;YN1&quot; and isActive==&quot;YN1&quot;"
								display="dropdown" 
								mandatory="true"	 													
							>
								<label id="AddressInformation.AddressProv.Mailing.Desc">
									<![CDATA[Please select the mailing address]]>
								</label>	
								<item-label>
									<label-element attribute-id="formatedAddressData"/>
								</item-label>
							</list-question>		  
					</cluster>		
				</question-page>
			</condition>
		
			<callout
				expression="UpdateWitnessAddress(&quot;Mailing&quot;,&quot;STEP1&quot;,&quot;Test&quot; )"
				id="UpdateWitnessAddress"
			  />

			<question-page 
				id="WitnessMailAddrInfoDetails" 
				entity="Declaration"
				progress="0"
				show-back-button="true" 
				show-next-button="true" 
				show-exit-button="true"			
				show-save-exit-button="true"
				>
				
				<validation expression="Declaration.mailingCountry!=&quot;CA&quot; or (Declaration.mailingCountry==&quot;CA&quot; and (not(isNotNull(MailingAddress.postalCode)) or validateCAPostalCode(MailingAddress.postalCode)))">
					<message id="AddressInformation.MailAddr.PostalCode.Validation"><![CDATA[Please enter a valid postal code (e.g. V1N 4J4)]]></message>
				</validation>

				<validation expression="Declaration.mailingCountry!=&quot;CA&quot; or ValidateMailingAddressFields(MailingAddress.addressLine3, MailingAddress.addressLine1, MailingAddress.addressLine2)">
					<message id="AddressInformation.MailAddr.pobox.Validation"><![CDATA[Please enter a Street name, Street Number, and Unit/Apt/Suite number if PO Box is not supplied.]]></message>
				</validation>
				
				<validation expression="Declaration.mailingCountry!=&quot;US&quot; or ValidateMailingAddressFields(IntlMailAddress.addressLine3, IntlMailAddress.addressLine1, IntlMailAddress.addressLine2)">
					<message id="AddressInformation.MailAddr.pobox.Validation"><![CDATA[Please enter a Street name, Street Number, and Unit/Apt/Suite number if PO Box is not supplied.]]></message>
				</validation>
				
				<validation expression="(Declaration.mailingCountry==&quot;CA&quot; or Declaration.mailingCountry==&quot;US&quot;) or ValidateMailingAddressFields(IntlMailAddress.addressLine3, IntlMailAddress.addressLine1, IntlMailAddress.addressLine2)">
					<message id="AddressInformation.MailAddr.poboxIntl.Validation"><![CDATA[Please enter a Street name, Street Number, and Unit/Apt/Suite number if PO Box is not supplied.]]></message>
				</validation>

		    	<validation expression="validateUSZipCodeLength(Declaration.mailingCountry,IntlMailAddress.zipCode)">
					<message id="AddressInformation.MailAddr.Ziplength.Validation"><![CDATA[Zip code is invalid when country is US]]></message>
	    		</validation>
				
				<title id="AddressInformation.MailAddr.Title">
					<![CDATA[Witness or Authorized Person's Mailing Address]]>
				</title>
				
				<description id="MandatoryFieldsDescription">
					<![CDATA[<p>All fields marked as * are mandatory.</p>]]>
				</description>
					
				<!-- Mailing Address fields when country is CA -->			
				<condition expression="Declaration.mailingCountry==&quot;CA&quot;">
					<cluster entity="MailingAddress">
						<question id="suiteNum" mandatory="false">
							<label id="AddressInformation.AptNum">
								<![CDATA[Unit/Apt/Suite number]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>								
						</question>
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNum">
								<![CDATA[Street Number]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>							
						</question>
						<question id="addressLine2" mandatory="false"
							read-only-expression="MailingAddress.isAddrSelectedInd" >
							<label id="AddressInformation.StreetName">
								<![CDATA[Street Name]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="addressLine3" mandatory="false">
							<label id="AddressInformation.POBox">
								<![CDATA[PO box Number]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="city" mandatory="true"
							read-only-expression="MailingAddress.isAddrSelectedInd" >
							<label id="AddressInformation.City">
								<![CDATA[City, Town or Village]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="province" mandatory="true"
							read-only-expression="MailingAddress.isAddrSelectedInd" >
							<label id="AddressInformation.Prov">
								<![CDATA[Province or Territory]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="postalCode" mandatory="true"
							read-only-expression="MailingAddress.isAddrSelectedInd" >
							<label id="AddressInformation.MailAddr.PostalCode">
								<![CDATA[Postal code (residential)]]>
							</label>
							<layout>
								<width>60</width>								
								<class-names>wds-js-input-mask-postal-code</class-names>
							</layout>
						</question>
					</cluster>
				</condition>
	
				<!-- COMMON Mailing Address fields for US and All else -->
				<condition expression="Declaration.mailingCountry!=&quot;CA&quot;" >
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine3" mandatory="false">
							<label id="AddressInformation.POBox">
								<![CDATA[PO box Number]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>							
						</question>
						<question id="suiteNum" mandatory="false">
							<label id="AddressInformation.AptNum">
								<![CDATA[Unit/Apt/Suite number]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>							
						</question>
					</cluster>
				</condition>
				<!-- Mailing Address fields when country is US-->
				<condition expression="Declaration.mailingCountry==&quot;US&quot;" >
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNum">
								<![CDATA[Street Number]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>							
						</question>
						<question id="addressLine2" mandatory="false">
							<label id="AddressInformation.StreetName">
								<![CDATA[Street Name]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="city" mandatory="true">
							<label id="AddressInformation.City">
								<![CDATA[City, Town or Village]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="state" mandatory="true">
							<label id="AddressInformation.State"><![CDATA[State]]></label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
					</cluster>
				</condition>
	
				<!-- Mailing Address fields when country is NOT US or CA-->	
				<condition expression="Declaration.mailingCountry!=&quot;CA&quot;
					and Declaration.mailingCountry!=&quot;US&quot;">
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNumOne"><![CDATA[Street 1]]></label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="addressLine2" mandatory="false">
							<label id="AddressInformation.StreetNameTwo"><![CDATA[Street 2]]></label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="city" mandatory="true">
							<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
						<question id="addressLine4" mandatory="true">
							<label id="AddressInformation.Intl.State">
								<![CDATA[State/Province/Region]]>
							</label>
							<layout>
								<width>70</width>							
							</layout>							
						</question>
					</cluster>
				</condition>
				
				<!-- Mailing Address fields when country not CA -->
				<condition expression="Declaration.mailingCountry!=&quot;CA&quot;" >
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="zipCode" mandatory="true">
							<label id="AddressInformation.ZipCode">
								<![CDATA[Zip Code]]>
							</label>
							<layout>
								<width>60</width>							
							</layout>							
						</question>
					</cluster>
				</condition>	

				<set-attribute expression="Declaration.mailingCountry" id="MailingAddress.country"/>
				<set-attribute expression="Declaration.mailingCountry" id="IntlMailAddress.country" />
			</question-page>
			
				<!-- Always VAlidate Address Manual or from WS, Firts Parameter is 3 -->			
				
				<callout  expression="ValidateWitnessMailingAddress(SearchAddress.idMailingAddress, 
					MailingAddress.suiteNum, 
					MailingAddress.addressLine1, 
					MailingAddress.addressLine2, 
					MailingAddress.city, 
					MailingAddress.province, 
					MailingAddress.postalCode, Declaration.mailingCountry)" id="ValidateWitnessMailingAddress"/>

			<!--  CALL STEP2 to set online Address  -->
			<callout
	        expression="UpdateWitnessAddress(&quot;Mailing&quot;,&quot;STEP2&quot;,&quot;Test&quot; )"
	        id="UpdateAddress"
	      />

		<!--  Same Page as Residential Address for invalid and Exception MEssage -->			
		<condition expression="MailingAddress.validateStatus==&quot;2&quot;">		
			<question-page 
				id="WitnessMailAddrInfoInvalidAddrMsg" 
				entity="Declaration"
				progress="0"
				show-back-button="true" 
				show-exit-button="true"
				>
			   
				<title id="AddressInformation.MailAddr.Title" />
				<!--description id="ResidentialAddressValidationPage.Description" / -->						
					<cluster entity="Declaration">
						<question id="invalidAddressIsContinue" mandatory="true" multi-select="false">
							<label id="ResAddressInformation.AddressMSG.Invalid">
								<![CDATA[The address as entered is not valid. Do you still want to continue?]]>
							</label>
							<layout>
								<type>radio</type>
							</layout>
						</question>
					</cluster>																
			</question-page>
		</condition>

		<condition expression="MailingAddress.validateStatus==&quot;3&quot;">		
			<question-page 
				id="WitnessMailAddrInfoExceptionAddrMsg" 
				entity="Person"
				progress="0"
				show-back-button="true" 
