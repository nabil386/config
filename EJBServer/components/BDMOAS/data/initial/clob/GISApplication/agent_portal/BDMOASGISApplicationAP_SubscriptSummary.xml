            <delete-link/>
            <column id="firstName">
              <title id="PI.ApplDet.FirstName"><![CDATA[<h4>First given name (as indicated on your SIN card/letter)<h4>]]></title>
            </column>
      
            <list            
              entity="ResidentialAddress" criteria="country==&quot;CA&quot;"
              show-icons="false"
            >

             <column id="suiteNum">
								<title id="AddressInformation.ResAddr.AptNum"><![CDATA[Residential unit/apt/suite number]]></title>
