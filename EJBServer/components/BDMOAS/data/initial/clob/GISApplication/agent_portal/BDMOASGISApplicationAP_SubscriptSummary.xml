            <delete-link/>
            <column id="firstName">
              <title id="PI.ApplDet.FirstName"><![CDATA[<h4>First given name (as indicated on your SIN card/letter)<h4>]]></title>
            </column>
      
            <list            
              entity="Income"
              show-icons="false"
            >
							<column id="capitalGainsAmount">
								<title id="Question.Label.Workerscompensationbenefits"><![CDATA[Capital Gains]]></title>
							</column>
							<column id="netRentalAmount">
								<title id="Question.Label.NetRentalIncome"><![CDATA[Net rental income]]></title>
							</column>
	
							<column id="netEmploymentAmount">
								<title id="Question.Label.NetEmploymentIncome"><![CDATA[Net employment income]]></title>
							</column>													
							<column id="netSelfEmploymentAmount">
								<title id="Question.Label.NetSelfEmploymentIncome"><![CDATA[Net self-employment income]]></title>
							</column>															
							<column id="incomeSource">
								<title id="Question.Label.Source"><![CDATA[Source]]></title>
