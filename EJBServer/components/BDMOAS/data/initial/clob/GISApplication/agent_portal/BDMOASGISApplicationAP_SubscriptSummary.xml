<?xml version="1.0" encoding="UTF-8"?>
<ieg-script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ieg-schema.xsd" finish-page="CommonIntake_finishInternalIntakeScript" quit-page="CommonIntake_quitInternalIntakeScript" show-sections="true" show-progress-bar="true" validate-save-and-exit="false">
	<identifier id="BDMOASGISApplicationAP" scriptversionnumber="V1" type="Intake"/>
	<!-- SUMMARY PAGE -->
	<section>
		<title id="Summary.Title"><![CDATA[Summary]]></title>
		<summary-page id="SummaryPage" entity="Person" show-back-button="true" show-exit-button="true" show-save-exit-button="true">
			<title id="SummaryPage.Title"><![CDATA[Summary]]></title>
			<next-button-label id="NextButton.Label"><![CDATA[Continue]]></next-button-label>
			<!-- PERSONAL INFO SUMMARY - BEGIN -->

			<cluster grouping-id="PersonalInfo">
				<title id="PI.Title"><![CDATA[Personal information]]></title>
				<edit-link start-page="PersonalInformation" skip-to-summary="false"/>
				<display-text id="PI.ApplDet"><![CDATA[<h3>Applicant Details</h3>]]></display-text>		
				<display-text id="PI.Cluster.IntakeDet"><![CDATA[<h3>Intake Details</h3>]]></display-text>				
				<layout>
					<label-width>0</label-width>
				</layout>				
			</cluster>

			<cluster entity="Application" grouping-id="PersonalInfo">		
				<question id="receiptDate">
					<label id="PI.IntakeDet.Question.ReceiptDate"><![CDATA[<h4>Receipt Date<h4>]]></label>
				</question>				
			</cluster>

			<cluster grouping-id="PersonalInfo">
				<layout>
					<label-width>0</label-width>
				</layout>
				<display-text id="PI.Cluster.PersonalInfoDet"><![CDATA[<h3>Personal Information</h3>]]></display-text>
			</cluster>

			<cluster entity="Person" grouping-id="PersonalInfo">		
				<question id="personTitle">
					<label id="PI.PersonalInfo.Question.Title"><![CDATA[<h4>Title<h4>]]></label>
				</question>				
				<question id="sinRaw">
					<label id="PI.ApplDet.SIN"><![CDATA[<h4>Social Insurance Number<h4>]]></label>
				</question>								
				<question id="oasAccntNum">
					<label id="PI.PersonalInfo.Question.OASAccntNum"><![CDATA[<h4>OAS Account Number<h4>]]></label>
				</question>				
				<question id="firstName">
					<label id="PI.ApplDet.FirstName"><![CDATA[<h4>First given name (as indicated on your SIN card/letter)<h4>]]></label>
				</question>
				<question id="middleName">
					<label id="PI.ApplDet.OGivenName"><![CDATA[<h4>Other given name(s)<h4>]]></label>
				</question>
				<question id="lastName">
					<label id="PI.ApplDet.FamName"><![CDATA[<h4>Last name (as indicated on your SIN card/letter)<h4>]]></label>
				</question>
		
				<question id="LastNameAtBirth">
					<label id="PI.ApplDet.LastNameBirth"><![CDATA[<h4>Last name at birth<h4>]]></label>
				</question>
			
				<question id="dateOfBirth">
					<label id="PI.ApplDet.DOB"><![CDATA[<h4>Date of birth<h4>]]></label>
				</question>
				<skip-field/>				
			</cluster>			
			<!-- PERSONAL INFO  SUMMARY - END -->	
			
			
			
			<!-- LANGUAGE PREF. - BEGIN -->
			<cluster entity="CommunicationPref" grouping-id="ContactInfo">
				<title id="ContactInfo.Title"><![CDATA[Contact information]]></title>			
				<display-text id="CommunicationPref.Title"><![CDATA[<h3>Preferred Contact Information</h3>]]></display-text>				
				<display-text id="WrittenComm.LabelTag"><![CDATA[<h3>Language preferences</h3>]]></display-text>
				<edit-link start-page="CommunicationPref" skip-to-summary="false"/>
				<layout>
					<label-width>0</label-width>
				</layout>
			</cluster>

			<cluster entity="CommunicationPref" grouping-id="ContactInfo">
				<question id="languageS">
					<label id="CommunicationDetails.SectionOne.SpokenLanguageQuestion"><![CDATA[<h4>Preferred Language of Communication?<h4>]]></label>
				</question>
			</cluster>
			<!-- LANGUAGE PREF. - END -->		
			
			<!-- COMMUNICATION DETAILS SUMMARY- BEGIN -->
			<cluster grouping-id="ContactInfo">
				<layout>
					<label-width>0</label-width>
				</layout>
				<display-text id="CommunicationDetails.SectionOne.PTelephoneTitle"><![CDATA[<h3>Preferred phone number</h3>]]></display-text>
			</cluster>
			
			<cluster entity="CommunicationDetails" grouping-id="ContactInfo">
				<question id="phoneType">
					<label id="CommunicationDetails.SectionOne.ATelephoneTypeTitle"/>
				</question>
				<question id="countryCode">
					<label id="CommunicationDetails.SectionOne.ATelephoneCCode"><![CDATA[Country code]]></label>
				</question>
				<question id="fullPhoneNumber">
					<label id="CommunicationDetails.SectionOne.ATelephoneNumber"><![CDATA[Phone number (5551212)]]></label>
				</question>
				<question id="phoneExt">
					<label id="CommunicationDetails.SectionOne.ATelephoneExt"><![CDATA[Extension]]></label>
				</question>
				<skip-field/>
			</cluster>
			
			<!-- ALTERNATE COMMUNICATION INFORMATION START -->
			<condition expression="isNotNull(CommunicationDetails.phoneNumber) and CommunicationDetails.phoneNumber!=&quot;&quot;">
				<cluster grouping-id="ContactInfo">
					<layout>
						<label-width>0</label-width>
					</layout>
					<display-text id="CommunicationDetails.Title.ContactInfoTitle"><![CDATA[<h3>Alternate contact information</h3>]]></display-text>
				</cluster>
				<cluster entity="CommunicationDetails" criteria="isPrimaryCommunication==false" grouping-id="ContactInfo">
					<question id="phoneType">
						<label id="CommunicationDetails.SectionOne.ATelephoneTypeTitle"/>
					</question>
					<question id="countryCode">
						<label id="CommunicationDetails.SectionOne.ATelephoneCCode"><![CDATA[Country code]]></label>
					</question>
					<question id="fullPhoneNumber">
						<label id="CommunicationDetails.SectionOne.ATelephoneNumber"><![CDATA[Phone number (5551212)]]></label>
					</question>
					<question id="phoneExt">
						<label id="CommunicationDetails.SectionOne.ATelephoneExt"><![CDATA[Extension]]></label>
					</question>
					<skip-field/>
				</cluster>
			</condition>
			<!-- COMMUNICATION DETAILS SUMMARY- END -->	
			
			<!-- MARITAL STATUS SUMMARY BEGIN -->
			<cluster entity="MaritalStatusInfo" grouping-id="MaritalInfo">
				<display-text id="Cluster.Title.MaritalStatus"><![CDATA[<h3>Marital Status</h3>]]></display-text>	
					<layout>
						<label-width>0</label-width>
					</layout>											
				</cluster>
				
				<cluster entity="MaritalStatusInfo" grouping-id="MaritalInfo">
					<question id="currentMaritalStatus">
						<label id="Question.Label.CurrentMaritalStatus">
							<![CDATA[Current Marital Status]]>
						</label>						
					</question>						
				</cluster>	
				
				<condition expression="isNotNull(MaritalStatusInfo.currentMaritalStatus) and MaritalStatusInfo.currentMaritalStatus!=&quot;MS1&quot;">
					<cluster entity="MaritalStatusInfo" grouping-id="MaritalInfo">
						<question id="maritalDate" mandatory="true">
							<label id="Question.Label.Date">
								<![CDATA[Date]]>
							</label>
						</question>		
					</cluster>
				</condition>
			<!--  MARITAL STATUS SUMMARY END -->	
			
			<!-- PARTNER PERSONAL INFO BEGIN  -->
			<cluster grouping-id="SpouseOrCLPInfo">
						<title id="QuestionPage.Title.SpouseOrCLPDetails"><![CDATA[Spouse/Common-Law Partner Details]]></title>
						<edit-link start-page="PartnerDetails" skip-to-summary="false"/>
						<layout>
							<label-width>0</label-width>
						</layout>
						<display-text id="Cluster.Title.SpouseOrCLPDetails"><![CDATA[<h3>Spouse/Common-Law Partner Details</h3>]]></display-text>	
					</cluster>

					<cluster entity="Person" criteria="isPrimaryParticipant==false" grouping-id="SpouseOrCLPInfo">
						<question id="personTitle">
							<label id="Question.Label.Title">
								<![CDATA[Title]]>
							</label>					
						</question>
						
						<question id="sinRawAP">
							<label id="PersonalInformation.SIN">
								<![CDATA[<span style="color: red;">*</span>Social Insurance Number (without spaces or hyphens)]]>
							</label>				
						</question>	

						<question id="oasAccntNum">
							<label id="PersonalInformation.OASAccntNum">
								<![CDATA[<span style="color: red;">*</span>OAS Account Number]]>
							</label>				
						</question>					
						
						<question id="firstName" mandatory="true">
							<label id="PersonalInformation.FName">
								<![CDATA[<span style="color: red;">*</span>First name (as indicated on your SIN card/letter)]]>
							</label>
						</question>	
						<question id="middleName" >
							<label id="PersonalInformation.OName">
								<![CDATA[Middle name(s)]]>
							</label>
						</question>	
						<question id="lastName" mandatory="true">
							<label id="PersonalInformation.FamName">
								<![CDATA[Last name (as indicated on your SIN card/letter)]]>
							</label>
						</question>	
		 
						<question id="LastNameAtBirth">
							<label id="PersonalInformation.LName">
								<![CDATA[Last name at birth]]>
							</label>
						</question>	
						<question id="dateOfBirth" mandatory="true">
							<label id="PersonalInformation.DOB">
								<![CDATA[Date of birth]]>
							</label>                  
						</question>	
						<question id="isDiffResiAddressForPartner">
							<label id="Question.Label.IsDiffResiAddrForPartner">
								<![CDATA[Did the applicant provide a different residential address for spouse/common-law partner?]]>
							</label>                   
						</question>					
					</cluster>		
			<!-- PARTNER PERSONAL INFO END -->
			
			
			<!--  BEGIN COMMON ENTITIES -->	
			
