		<condition expression="Application.applicationForm==&quot;AF3&quot;">			
		<list
            entity="Person"
            show-icons="false"
          >
            <title id="Cluster.Title.ApplicantsReportedIncome"><![CDATA[Manually Reported Income]]></title>                      
            <edit-link
              skip-to-summary="true"
