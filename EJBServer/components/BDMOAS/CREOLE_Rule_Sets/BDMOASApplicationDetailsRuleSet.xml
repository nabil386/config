<RuleSet name="BDMOASApplicationDetailsRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../build/svr/creole.gen/schema/RuleSet.xsd">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="BDMOASApplicationDetails">
		<Attribute name="requestedPensionStartDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="applicationStatus">
			<Annotations />
			<type>
				<codetableentry table="BDMOASApplicationStatus" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="withdrawalDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="benefitType">
			<Annotations />
			<type>
				<codetableentry table="BDMOASBenefitType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="methodOfApplication">
			<Annotations />
			<type>
				<codetableentry table="BDMOASMethodOfApplication" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="receiptDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="related_BDMOASBenefitCancellationRequest">
			<type>
				<javaclass name="java.util.List">
					<ruleclass name="BDMOASBenefitCancellationRequest"
						ruleset="BDMOASBenefitCancellationRequestRuleSet" />
				</javaclass>
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getChildList">
					<type>
						<javaclass name="java.util.List">
							<ruleclass name="BDMOASBenefitCancellationRequest"
								ruleset="BDMOASBenefitCancellationRequestRuleSet" />
						</javaclass>
					</type>
					<arguments>
						<this />
						<String value="BDMOASBenefitCancellationRequest" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
		<Attribute name="related_BDMOASBenefitCessation">
			<type>
				<javaclass name="java.util.List">
					<ruleclass name="BDMOASBenefitCessation"
						ruleset="BDMOASBenefitCessationRuleSet" />
				</javaclass>
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getChildList">
					<type>
						<javaclass name="java.util.List">
							<ruleclass name="BDMOASBenefitCessation"
								ruleset="BDMOASBenefitCessationRuleSet" />
						</javaclass>
					</type>
					<arguments>
						<this />
						<String value="BDMOASBenefitCessation" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
		<Attribute name="related_BDMOASDeemedDate">
			<type>
				<javaclass name="java.util.List">
					<ruleclass name="BDMOASDeemedDate"
						ruleset="BDMOASDeemedDateRuleSet" />
				</javaclass>
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getChildList">
					<type>
						<javaclass name="java.util.List">
							<ruleclass name="BDMOASDeemedDate"
								ruleset="BDMOASDeemedDateRuleSet" />
						</javaclass>
					</type>
					<arguments>
						<this />
						<String value="BDMOASDeemedDate" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>