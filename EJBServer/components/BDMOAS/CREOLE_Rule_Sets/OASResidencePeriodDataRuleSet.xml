<?xml version="1.0" encoding="UTF-8"?>
<!-- 64790: Residence Period -->
<RuleSet name="OASResidencePeriodDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../build/svr/creole.gen/schema/RuleSet.xsd">
	<Class extends="ActiveSuccessionSet"
		extendsRuleSet="PropagatorRuleSet" name="OASResidencePeriod">
        
        <Annotations>
            <SuccessionSetPopulation
                startDateAttribute="startDate" endDateAttribute="endDate" />
        </Annotations>
        
		<Attribute name="country">
			<Annotations />
			<type>
				<codetableentry table="Country" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="residenceType">
			<Annotations />
			<type>
				<codetableentry table="OASResidenceType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="endDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="absenceReason">
			<Annotations />
			<type>
				<codetableentry table="OASAbsenceReason" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="intentToReturnYesNo">
			<Annotations />
			<type>
				<codetableentry table="IEGYesNo" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="intentToReturnInd">
			<Annotations />
			<type>
				<javaclass name="java.lang.Boolean" />
			</type>
			<derivation>
				<equals>
					<reference attribute="intentToReturnYesNo" />
					<Code table="IEGYesNo">
						<String value="YN1" />
					</Code>
				</equals>
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>