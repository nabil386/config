<RuleSet name="BDMOASDeemedDateRuleSet">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="BDMOASDeemedDate">
		<Attribute name="deemedDateReason">
			<Annotations />
			<type>
				<codetableentry table="BDMOASDeemedDateReason" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="country">
			<Annotations />
			<type>
				<codetableentry table="BDMSourceCountry" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="otherDeemedDateReason">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="deemedDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="applicationDetails">
			<type>
				<ruleclass name="BDMOASApplicationDetails"
					ruleset="BDMOASApplicationDetailsRuleSet" />
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getParent">
					<type>
						<ruleclass name="BDMOASApplicationDetails"
							ruleset="BDMOASApplicationDetailsRuleSet" />
					</type>
					<arguments>
						<this />
						<String value="BDMOASApplicationDetails" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>