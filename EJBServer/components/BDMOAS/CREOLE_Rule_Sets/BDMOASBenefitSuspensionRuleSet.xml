<?xml version="1.0" encoding="UTF-8"?>
<!-- 90159 DEV: Implement Benefit Suspension Evidence -->
<RuleSet name="BDMOASBenefitSuspensionRuleSet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
    <Class extends="DynamicEvidence"
        extendsRuleSet="DynamicEvidenceRuleSet"
        name="BDMOASBenefitSuspension">
        <Attribute name="suspendedBenefit">
            <Annotations />
            <type>
                <codetableentry
                    table="BDMOASSuspendedBenefit" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="suspensionStatus">
            <Annotations />
            <type>
                <codetableentry
                    table="BDMOASSuspensionStatus" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="suspensionReason">
            <Annotations />
            <type>
                <codetableentry
                    table="BDMOASSuspensionReason" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="suspensionRsnDesc">
            <Annotations />
            <type>
                <javaclass name="java.lang.String" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="unsuspensionReason">
            <Annotations />
            <type>
                <codetableentry
                    table="BDMOASUnsuspensionReason" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="unsuspensionRsnDesc">
            <Annotations />
            <type>
                <javaclass name="java.lang.String" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="startDate">
            <Annotations />
            <type>
                <javaclass name="curam.util.type.Date" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="endDate">
            <Annotations />
            <type>
                <javaclass name="curam.util.type.Date" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="caseParticipantRole">
            <Annotations />
            <type>
                <javaclass name="java.lang.Number" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        <Attribute name="related_caseParticipantRole">
            <Annotations />
            <type>
                <ruleclass name="CaseParticipantRole"
                    ruleset="CaseEntitiesRuleSet" />
            </type>
            <derivation>
                <reference
                    attribute="caseParticipantRoleRecord">
                    <create
                        ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference
                            attribute="caseParticipantRole" />
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>