<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASWorldIncomeVerDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="ActiveInEditEvidenceRow"
		extendsRuleSet="PropagatorRuleSet" name="BDMOASWorldIncome">

		<Attribute name="participant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		
		<Attribute name="overThreshold">
			<Annotations />
			<type>
				<codetableentry table="BDMOASYesNoNA" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		 
		 <Attribute name="threshold">
			<Annotations />
			<type>
				<codetableentry table="BDMOASTaxThreshold" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		
		<Attribute name="related_participant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="participant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		
				
	</Class>
</RuleSet>