<?xml version="1.0" encoding="UTF-8"?>
<!-- Feature 70518: Manage Override Evidence -->
<RuleSet name="BDMOASEligibilityEntitlementOverrideDataRuleSet">
	<Class extends="ActiveSuccessionSet" extendsRuleSet="PropagatorRuleSet"
		name="BDMOASEligibilityEntitlementOverride">
		<Annotations>
			<SuccessionSetPopulation endDateAttribute="toDate"
				startDateAttribute="fromDate" />
		</Annotations>
		<Attribute name="fromDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="reason">
			<Annotations />
			<type>
				<codetableentry table="BDMOASOverrideReason" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="reasonDescription">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="dollarAmount">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="benefitType">
			<Annotations />
			<type>
				<codetableentry table="BDMOASOverrideBenefitType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="toDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="overrideValue">
			<Annotations />
			<type>
				<codetableentry table="BDMOASOverrideValue" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="numberAmount">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="participant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_participant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="participant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>