<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASSponsorshipRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="BDMOASSponsorship">
		<Attribute name="isSponsored">
			<Annotations />
			<type>
				<codetableentry table="BDMYesNo" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute
			name="related_BDMOASSponsorshipAgreement">
			<type>
				<javaclass name="java.util.List">
					<ruleclass
						name="BDMOASSponsorshipAgreement"
						ruleset="BDMOASSponsorshipAgreementRuleSet" />
				</javaclass>
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getChildList">
					<type>
						<javaclass name="java.util.List">
							<ruleclass
								name="BDMOASSponsorshipAgreement"
								ruleset="BDMOASSponsorshipAgreementRuleSet" />
						</javaclass>
					</type>
					<arguments>
						<this />
						<String value="BDMOASSponsorshipAgreement" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>