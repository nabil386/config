<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASForeignIncomeDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="ActiveSuccessionSet"
		extendsRuleSet="PropagatorRuleSet" name="BDMOASForeignIncome">
		<Attribute name="country">
			<Annotations />
			<type>
				<codetableentry table="BDMSourceCountry" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="amount">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="incomeType">
			<Annotations />
			<type>
				<codetableentry table="IncomeTypeCode" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="year">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="currency">
			<Annotations />
			<type>
				<codetableentry table="BDMCurrency" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="canadianAmount">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="otherDescription">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRoleID">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRoleID">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRoleID" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>