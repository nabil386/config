<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASCRABirthAndDeathDataRuleSet">
  <Class
    extends="ActiveSuccessionSet"
    extendsRuleSet="PropagatorRuleSet"
    name="BDMOASCRABirthAndDeath"
  >
    <Annotations>
      <SuccessionSetPopulation/>
    </Annotations>
    <Attribute name="dateOfBirth">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    
    <Attribute name="dateOfDeath">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
 
    <Attribute name="person">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Number"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="related_person">
      <Annotations/>
      <type>
        <ruleclass
          name="CaseParticipantRole"
          ruleset="CaseEntitiesRuleSet"
        />
      </type>
      <derivation>
        <reference attribute="caseParticipantRoleRecord">
          <create
            ruleclass="CaseParticipantRoleCalculator"
            ruleset="DynamicEvidenceRuleSet"
          >
            <reference attribute="person"/>
          </create>
        </reference>
      </derivation>
    </Attribute>
  </Class>
</RuleSet>
