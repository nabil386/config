<RuleSet name="BDMOASBenefitCessationRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../build/svr/creole.gen/schema/RuleSet.xsd">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="BDMOASBenefitCessation">
		<Attribute name="reinstatementDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="cessationDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="applicationDetails">
			<type>
				<ruleclass name="BDMOASApplicationDetails"
					ruleset="BDMOASApplicationDetailsRuleSet" />
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getParent">
					<type>
						<ruleclass name="BDMOASApplicationDetails"
							ruleset="BDMOASApplicationDetailsRuleSet" />
					</type>
					<arguments>
						<this />
						<String value="BDMOASApplicationDetails" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>