<?xml version="1.0" encoding="UTF-8"?>
<RuleSet
	name="BDMOASLivingApartforReasonsBeyondControlDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="ActiveSuccessionSet"
		extendsRuleSet="PropagatorRuleSet"
		name="BDMOASLivingApartforReasonsBeyondControl">
		<Annotations>
			<SuccessionSetPopulation
				endDateAttribute="endDate" startDateAttribute="startDate" />
		</Annotations>
		<Attribute name="reasonForLivingApart">
			<Annotations />
			<type>
				<javaclass name="curam.creole.value.Timeline">
					<codetableentry table="BDMOASLivingApartReason" />
				</javaclass>
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="livingApartReasonDescription">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="endDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="maritalRelationship">
			<Annotations>
				<relatedSuccessionSet relationship="parent" />
				<Label label-id="maritalRelationship" name="" />
			</Annotations>
			<type>
				<ruleclass name="BDMOASMaritalRelationship"
					ruleset="BDMOASMaritalRelationshipDataRuleSet" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
	</Class>
</RuleSet>