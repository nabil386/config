<?xml version="1.0" encoding="UTF-8"?>
<!-- Feature 72659: Manage Retirement/Pension Reduction Evidence -->
<RuleSet name="BDMOASRetirementPensionReductionRuleSet">
	<Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet"
		name="BDMOASRetirementPensionReduction">
		<Attribute name="eventType">
			<Annotations />
			<type>
				<codetableentry table="BDMOASOptionableEvent" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="eventDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="participant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_participant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="participant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>