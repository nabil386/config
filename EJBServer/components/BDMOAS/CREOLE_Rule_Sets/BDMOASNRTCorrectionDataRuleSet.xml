<?xml version="1.0" encoding="UTF-8"?>
<!-- BEGIN Feature 106244: Manage NRT  -->
<RuleSet name="BDMOASNRTCorrectionDataRuleSet">
	<Class extends="ActiveSuccessionSet" extendsRuleSet="PropagatorRuleSet"
		name="BDMOASNRTCorrection">
		<Attribute name="correctionReason">
			<Annotations />
			<type>
				<codetableentry table="BDMOASNRTCorrectionReason" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="country">
			<Annotations />
			<type>
				<codetableentry table="BDMSourceCountry" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="percentage">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="endDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRoleID">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRoleID" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>