<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASConsentToBeDisadvantagedRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet"
		name="BDMOASConsentToBeDisadvantaged">

		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="consentStatus">
			<Annotations />
			<type>
				<codetableentry table="BDMOASConsentDisStatus" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="livingApartforReasonsBeyondControl">
			<type>
				<ruleclass name="BDMOASLivingApartforReasonsBeyondControl"
					ruleset="BDMOASLivingApartforReasonsBeyondControlRuleSet" />
			</type>
			<derivation>
				<call
					class="curam.dynamicevidence.cer.impl.DynamicEvidenceStatics"
					method="getParent">
					<type>
						<ruleclass
							name="BDMOASLivingApartforReasonsBeyondControl"
							ruleset="BDMOASLivingApartforReasonsBeyondControlRuleSet" />
					</type>
					<arguments>
						<this />
						<String value="BDMOASLivingApartforReasonsBeyondControl" />
					</arguments>
				</call>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>