<?xml version="1.0" encoding="UTF-8"?>
<!-- 64796 DEV: Implement First Entry Into Canada Details -->
<RuleSet name="BDMOASFirstEntryIntoCanadaDetailsRuleSet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../build/svr/creole.gen/schema/RuleSet.xsd">
	<Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet" name="BDMOASFirstEntryIntoCanadaDetails">
		<Attribute name="arrivalDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="immigrationDoc">
			<Annotations />
			<type>
				<codetableentry table="BDMOASImmigrationDoc" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="otherImmigrationDocDetails">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="arrivalCity">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="arrivalProvince">
			<Annotations />
			<type>
				<codetableentry table="ProvinceType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator" ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>