<?xml version="1.0" encoding="UTF-8"?>
<RuleSet
	name="BDMOASConsentToBeDisadvantagedDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="ActiveSuccessionSet"
		extendsRuleSet="PropagatorRuleSet"
		name="BDMOASConsentToBeDisadvantaged">
		<Annotations>
			<SuccessionSetPopulation startDateAttribute="startDate" />
		</Annotations>
		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="consentStatus">
			<Annotations />
			<type>
				<javaclass name="curam.creole.value.Timeline">
					<codetableentry table="BDMOASConsentDisStatus" />
				</javaclass>
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRole">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="livingApartforReasonsBeyondControl">
			<Annotations>
				<relatedSuccessionSet relationship="parent" />
				<Label label-id="livingApartforReasonsBeyondControl" name="" />
			</Annotations>
			<type>
				<ruleclass name="BDMOASLivingApartforReasonsBeyondControl"
					ruleset="BDMOASLivingApartforReasonsBeyondControlDataRuleSet" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
	</Class>
</RuleSet>