<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMOASFirstEntryIntoCanadaDetailsVerDataRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">
	<Class extends="ActiveInEditEvidenceRow"
		extendsRuleSet="PropagatorRuleSet" name="BDMOASFirstEntryIntoCanadaDetails">

		<Attribute name="caseParticipantRole">
			<Annotations />
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		
		<Attribute name="arrivalDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="immigrationDoc">
			<Annotations />
			<type>
				<codetableentry table="BDMOASImmigrationDoc" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="otherImmigrationDocDetails">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="arrivalCity">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="arrivalProvince">
			<Annotations />
			<type>
				<codetableentry table="ProvinceType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="evidenceID">
            <type>
                <javaclass name="Number" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipantRoleID">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipantRole" />
					</create>
				</reference>
			</derivation>
		</Attribute>
				
	</Class>
</RuleSet>