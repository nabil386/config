<workflow-process category="PC2001" createdBy="admin" creationDate="20081023T094229" id="108" language-version="1.0" process-version="1" released="true"> 
  <!-- END CR00168004 -->  
  <name>IdenticalSharingEvidenceUpdate</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Mise à jour des informations collectées identiques impliquées dans un partage</locale>  
      <locale language="en">Identical Sharing Evidence Update</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <!-- BEGIN, CR00248295, NS -->  
  <description> 
    <localized-text> 
      <locale language="fr">Avertit le propriétaire d'un dossier que des informations collectées sont partagées depuis un autre dossier et doivent être examinées et acceptées.</locale>  
      <locale language="en">Notifies the case owner of a case that identical evidence is being shared from another case and needs to be reviewed and accepted.</locale>
    </localized-text> 
  </description>  
  <!-- END, CR00248295 -->  
