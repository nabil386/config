            <link-parameter name="evidenceDescriptorID"> 
              <wdo-attribute name="evidenceDescriptorID" wdo-name="IdenticalEvidenceSharingWDO"/> 
            </link-parameter> 
          </action>  
          <!--  BEGIN, CR00236468, GYH -->  
