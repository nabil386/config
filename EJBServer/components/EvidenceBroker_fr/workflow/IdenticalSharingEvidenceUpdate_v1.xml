                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="caseID"> 
              <wdo-attribute name="caseID" wdo-name="IdenticalEvidenceSharingWDO"/> 
            </link-parameter> 
          </action>  
          <!--  END, CR00236468 --> 
        </actions>  
        <allow-deadline-override>true</allow-deadline-override>  
        <allow-task-forwarding>true</allow-task-forwarding> 
