<workflow-process category="PC2001" createdBy="admin" creationDate="20140212T153103" id="20140211" language-version="1.0" process-version="1" released="true"> 
  <name>AutoAcceptFailedTask</name>  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Tâche relative à l'échec de l'acceptation des informations collectées</locale>  
      <locale language="en">Auto Acceptance of Evidence Failed Task</locale>
    </localized-text> 
  </process-display-name>  
  <description> 
    <localized-text> 
      <locale language="fr">Génère une tâche lorsque des informations collectées n'ont pas pu être acceptées automatiquement sur un dossier.</locale>  
      <locale language="en">Generates task when evidences could not be
                automatically accepted onto a case.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="caseID" struct-name="curam.core.struct.CaseHeaderKey"/>  
      <target-attribute name="caseID" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="caseReference" struct-name="curam.core.struct.CaseReference"/>  
      <target-attribute name="caseReference" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="reason" struct-name="curam.evidencebroker.sl.struct.EvidenceAutoActionFailureNotificationDetails"/>  
      <target-attribute name="reason" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>AutoAcceptEvidenceFailureTaskDetails</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">Détails de la tâche relative à l'échec de l'acceptation automatique des informations collectées</locale>  
          <locale language="en">Auto Accept Evidence Failure Task Details</locale>
        </localized-text> 
      </display-name>  
      <description> 
        <localized-text> 
          <locale language="fr">Détails requis pour informer le propriétaire de dossier de
l'échec.</locale>  
          <locale language="en">The details needed to inform the case owner of the
                        failure.</locale>
        </localized-text> 
      </description>  
      <attributes> 
        <attribute> 
          <attribute-name>caseID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">ID du dossier</locale>  
              <locale language="en">caseID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>caseReference</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">Référence du dossier</locale>  
              <locale language="en">caseReference</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>reason</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">motif</locale>  
              <locale language="en">reason</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute> 
      </attributes> 
    </wdo> 
  </wdos>  
  <activities> 
    <start-process-activity id="1"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité de processus de début</locale>  
          <locale language="en">Start Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de début</locale>  
          <locale language="en">The start process activity</locale>
        </localized-text> 
      </description>  
      <split type="none"> 
        <transition-id idref="1"/> 
      </split> 
    </start-process-activity>  
    <end-process-activity id="2"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">End Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">The end process activity</locale>
        </localized-text> 
      </description>  
      <join type="none"/> 
    </end-process-activity>  
    <manual-activity category="AC1" id="3"> 
      <name> 
        <localized-text> 
          <locale language="fr">Tâche relative à l'échec de l'acceptation des informations collectées</locale>  
          <locale language="en">Auto Acceptance of Evidence Failed Task</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Génère une tâche lorsque des informations collectées n'ont pas pu être acceptées automatiquement sur un dossier.</locale>  
          <locale language="en">Generates task when evidences could not be automatically accepted onto a case.</locale>
        </localized-text> 
      </description>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split>  
      <task> 
        <message> 
          <message-text> 
            <localized-text> 
              <locale language="fr">L'acceptation automatique des informations collectées à partir du dossier %1s a été ignorée. The error
                                was %2s.</locale>  
              <locale language="en">The auto-acceptance of evidence from the case %1s was skipped. The error
                                was %2s.</locale>
            </localized-text> 
          </message-text>  
          <message-parameters> 
            <wdo-attribute name="caseReference" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/>  
            <wdo-attribute name="reason" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/> 
          </message-parameters> 
        </message>  
        <allow-deadline-override>true</allow-deadline-override>  
        <allow-task-forwarding>true</allow-task-forwarding> 
      </task>  
      <allocation-strategy identifier="curam.core.sl.intf.WorkflowAllocationFunction.caseOwnerAllocationStrategy" type="function"> 
        <function-mappings> 
          <formal-parameters> 
            <formal-parameter index="0"> 
              <struct type="curam.core.sl.struct.CaseIDKey"> 
                <field name="caseID"> 
                  <base-type type="INT64"> 
                    <wdo-attribute name="caseID" wdo-name="AutoAcceptEvidenceFailureTaskDetails"/> 
                  </base-type> 
                </field> 
              </struct> 
            </formal-parameter> 
          </formal-parameters> 
        </function-mappings> 
      </allocation-strategy>  
      <event-wait wait-on-all-events="false"> 
        <events> 
          <event event-class="TASK" event-type="CLOSED" identifier="1"> 
            <event-match-attribute name="taskID" wdo-name="Context_Task"/> 
          </event> 
        </events> 
      </event-wait> 
    </manual-activity>  
    <route-activity id="4"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité d'acheminement</locale>  
          <locale language="en">Route Activity</locale>
        </localized-text> 
      </name>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="3"/> 
      </split> 
    </route-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="2" to-activity-idref="4"/>  
    <transition from-activity-idref="4" id="3" to-activity-idref="2"/> 
  </transitions> 
</workflow-process>
