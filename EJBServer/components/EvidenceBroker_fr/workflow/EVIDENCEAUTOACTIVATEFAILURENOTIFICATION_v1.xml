      <name> 
        <localized-text> 
          <locale language="fr">Informer le propriétaire de dossier de l'échec de l'activation automatique d'informations collectées</locale>  
          <locale language="en">Inform Case Owner Auto Activate Of Evidence Failed</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Génère une notification pour les informations collectées impossibles à activer automatiquement dans un dossier.</locale>  
          <locale language="en">Generates a notification for evidence that could not be automatically activated onto a case.</locale>
        </localized-text> 
      </description>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Erreur lors de l'activation d'informations collectées dans le dossier %1s.</locale>  
                <locale language="en">Error activating evidence onto Case %1s.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="caseReference" wdo-name="EvidenceAutoActivateFailureDetails"/> 
            </message-parameters> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Echec de l'activation automatique des informations collectées pour le dossier %1s en raison de %2s</locale>  
                <locale language="en">Evidence Auto Activation failed on case %1s because %2s</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="caseReference" wdo-name="EvidenceAutoActivateFailureDetails"/>  
              <wdo-attribute name="reason" wdo-name="EvidenceAutoActivateFailureDetails"/> 
            </message-parameters> 
          </message> 
        </body>  
        <allocation-strategy identifier="curam.core.sl.intf.NotificationAllocationFunction.defaultStrategy" type="function"> 
          <function-mappings> 
            <formal-parameters> 
              <formal-parameter index="0"> 
                <base-type type="STRING"> 
                  <wdo-attribute name="caseOwner" wdo-name="EvidenceAutoActivateFailureDetails"/> 
                </base-type> 
              </formal-parameter> 
            </formal-parameters> 
          </function-mappings> 
        </allocation-strategy>  
        <actions> 
          <action open-modal="false" page-id="EvidenceBroker_resolveAcceptedEvidence" principal-action="false"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Examiner les informations collectées acceptées</locale>  
                  <locale language="en">Review Accepted Evidence</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="caseID"> 
              <wdo-attribute name="caseID" wdo-name="EvidenceAutoActivateFailureDetails"/> 
            </link-parameter> 
          </action> 
        </actions> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split> 
    </route-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="2" to-activity-idref="2"/> 
