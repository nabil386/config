<workflow-process category="PC2001" createdBy="admin" creationDate="20120906T180426" id="1126999418470912" language-version="1.0" process-version="1" released="true"> 
  <name>EVIDENCEAUTOACTIVATEFAILURENOTIFICATION</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Notification d'échec de l'activation automatique des informations collectées</locale>  
      <locale language="en">Evidence Auto Activate Failure Notification</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <description> 
    <localized-text> 
      <locale language="fr">Génère des notifications pour les informations collectées impossibles à activer automatiquement dans un dossier.</locale>  
      <locale language="en">Generates notifications for evidence that could not be automatically activated onto a case.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="details.caseReference" struct-name="curam.evidencebroker.sl.struct.EvidenceAutoActionFailureNotificationDetails"/>  
      <target-attribute name="caseReference" wdo-name="EvidenceAutoActivateFailureDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="details.caseOwner" struct-name="curam.evidencebroker.sl.struct.EvidenceAutoActionFailureNotificationDetails"/>  
      <target-attribute name="caseOwner" wdo-name="EvidenceAutoActivateFailureDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="details.caseID" struct-name="curam.evidencebroker.sl.struct.EvidenceAutoActionFailureNotificationDetails"/>  
      <target-attribute name="caseID" wdo-name="EvidenceAutoActivateFailureDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="reason" struct-name="curam.evidencebroker.sl.struct.EvidenceAutoActionFailureNotificationDetails"/>  
      <target-attribute name="reason" wdo-name="EvidenceAutoActivateFailureDetails"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>EvidenceAutoActivateFailureDetails</wdo-name>  
