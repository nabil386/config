  </message>
  <message name="ERR_RULE_SETS_CONTAIN_VALIDATION_ERRORS">
    <locale language="en">Rule sets contain validation errors.</locale> 
  </message>
  <message name="ERR_RULE_SET_NAME_INCONSISTENCY">
    <!--<locale language="en">CREOLERuleSet row with creoleRuleSetID=%1n specifies the rule set name as '%2s', but the ruleSetDefinition specifies the name as '%3s'.</locale> -->
