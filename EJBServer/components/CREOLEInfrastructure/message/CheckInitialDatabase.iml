<messages package="curam.creole.message">
  <message name="ERR_SNAPSHOT_ENTRY_FOUND">
    <!--<locale language="en">CREOLERuleSetSnapshot table should be empty, but found a row with creoleRuleSetSnapshotID=%1n.</locale> -->
	<locale language="en">%1s table should be empty, but found a row with %2s=%3n.</locale> 
  </message>
  <message name="ERR_RULE_SET_DEFINITION_BLANK">
    <!--<locale language="en">CREOLERuleSet row with creoleRuleSetID=%1n has no ruleSetDefinition value set (it should be populated).</locale> -->
