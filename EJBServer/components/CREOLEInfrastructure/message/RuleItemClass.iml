<messages package="curam.creole.message">
  <message name="SUPERCLASS_NOT_FOUND">
    <locale language="en">Cannot find superclass '%1s'.</locale> 
  </message> 
  <message name="CIRCULAR_REFERENCE_IN_SUPERCLASS_HIERARCHY">
    <locale language="en">Circular reference detected amongst superclass hierarchy.</locale> 
  </message>  
  <message name="INVALID_ATTRIBUTE_TYPE">
    <locale language="en">Attribute returns a '%1s' but the attribute overridden from '%2s' requires a '%3s'.</locale> 
  </message>  
  <message name="CLASS_DECLARATION_MUST_BE_ABSTRACT_OR_ATTRIBUTE_CALCULATION_PROVIDED">
    <!--<locale language="en">Class should be declared 'abstract', or a calculation for attribute '%1s' must be provided.</locale> -->
	<locale language="en">Class should be declared '%1s', or a calculation for attribute '%2s' must be provided.</locale> 
  </message>  
  <message name="CLASS_DECLARATION_MUST_BE_ABSTRACT_OR_ATTRIBUTE_CALCULATION_PROVIDED_INHERITED_FROM_CLASS">
    <!--<locale language="en">Class should be declared 'abstract', or a calculation for attribute '%1s' (inherited from '%2s') must be provided.</locale> -->
	<locale language="en">Class should be declared '%1s', or a calculation for attribute '%2s' (inherited from '%3s') must be provided.</locale> 
  </message> 
  <message name="CLASS_DOES_NOT_SPECIFY_OR_INHERIT_DESCRIPTION_ATTRIBUTE">
    <locale language="en">The rule class '%1s' does not specify or inherit a '%2s' attribute.  Creating an explicit '%2s' attribute may help you understand the behavior of your rule set at runtime.</locale> 
  </message>  
  <message name="CLASS_CONTAINS_MORE_THAN_ONE_ATTRIBUTE_WITH_SAME_NAME">
    <locale language="en">There is more than one attribute with name '%1s' in class '%2s'.</locale> 
  </message>  
  <message name="RETURN_TYPE_COULD_NOT_BE_DETERMINED">
    <locale language="en">Could not determine return type for '%1s'.</locale> 
  </message>  
  <message name="CANNOT_OVERRIDE_INITIALIZED_ATTRIBUTE">
    <locale language="en">The attribute '%1s' attempts to override initialized attribute '%2s'.  Initialized attributes cannot be overridden.</locale> 
  </message>  
  <message name="EXTENDS_SPECIFIES_RULE_SET_BUT_NOT_RULE_CLASS">
    <locale language="en">The rule set of the superclass has been specified but not the rule class.  Either specify the rule class name of the superclass or remove the rule set name.</locale> 
  </message>  
</messages>