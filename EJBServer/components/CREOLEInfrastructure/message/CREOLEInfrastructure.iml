<messages package="curam.creole.message">
  <message name="ERR_CALCULATOR_INVALID_STATE_FOR_SPECIFYING_VALUE">
    <locale language="en">A value cannot be specified, as the current state of this calculator is '%1s'.</locale> 
  </message>  
  <message name="ERR_ATTRIBUTE_VALUE_RECALCULATION_PROHIBITED">
    <locale language="en">This action caused '%1s' to request recalculation, which is prohibited under the specified recalculation strategy.</locale> 
  </message>  
  <message name="VALUE_MUST_BE_SPECIFIED">
    <locale language="en">Value must be specified before it is used (it cannot be calculated).</locale> 
  </message>  
  <message name="ERR_XRV_NAME_UNIQUE">
    <locale language="en">This Name has already been defined for a CER Rule Set.</locale> 
  </message>  
  <message name="ERR_RULE_SET_UPLOAD">
    <locale language="en">%1s: %2s %3s (%4n, %5n) %6s</locale> 
  </message>  
</messages>