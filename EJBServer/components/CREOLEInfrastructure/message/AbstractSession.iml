<messages package="curam.creole.message">
  <message name="ERR_UNRECOVERABLE_SESSION_STACK_CORRUPTED">
    <locale language="en">Unrecoverable session error - calculation stack corrupted.</locale> 
  </message>    
  <message name="STATE_OF_CALCULATION_CANNOT_BE_RETRIEVED_WHILE_CALCULATION_IN_PROGRESS">
    <locale language="en">The state of a calculator may not be retrieved whilst a calculation is in progress.</locale> 
  </message>  
  <message name="RULE_SET_DOES_NOT_CONTAIN_RULE_CLASS">
    <locale language="en">Rule set '%1s' does not contain the rule class '%2s'.</locale> 
  </message>
  <message name="RULE_CLASS_IN_RULE_SET_IS_ABSTRACT_AND_CANNOT_BE_INSTANTIATED">
    <locale language="en">Rule set '%1s' in rule set '%2s' is abstract; only concrete rule classes can be instantiated.</locale> 
  </message>  
  <message name="ERR_VALUE_CHANGE_NOT_ALLOWED_ON_CREATION_ARGUMENT">
    <locale language="en">Value change not allowed on the attribute value '%1s' because this attribute value was used as a creation value for its rule object.</locale>
  </message>    
  <message name="ERR_RULE_OBJECT_NOT_FOUND">
    <locale language="en">No rule object found for rule object ID %1n.</locale>
  </message>    
