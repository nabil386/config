<messages package="curam.creole.message">
  <message name="ERR_TABLE_DMX_NOT_CREATED">
    <locale language="en">A DMX file for table %1s will not be created as the table contains no data.</locale>
  </message> 
  <message name="ERR_TABLE_ROW_DMX_NOT_CREATED">
    <locale language="en">A DMX file for rule set name '%1s' on table %2s will not be created as the row could not be found.</locale>
  </message> 
  <message name="ERR_IN_EDIT_TABLE_ROW_DMX_NOT_CREATED">
    <locale language="en">A DMX file for in edit rule set name '%1s' will not be created as the row could not be found.</locale>
  </message> 
  <message name="INF_EXTRACTING_TABLE">
      <locale language="en">Extracting the entire contents of the rule set table.</locale> 
  </message>
  <message name="INF_EXTRACTING_IN_EDIT_RULE_SET">
      <locale language="en">Extracting an in-edit version of the given rule set.</locale> 
  </message>
  <message name="INF_EXTRACTING_RULE_SET">
      <locale language="en">Extracting rule set name '%1s'.</locale> 
  </message>
  <message name="ERR_PARTIAL_FAILURE_EXTRACTING_RULE_SET">
      <locale language="en">Some of the table extract failed due to above exceptions.</locale> 
  </message>
  <message name="ERR_MISSING_CREOLE_RULE_SET_NAME">
      <locale language="en">Rule Set Name parameter has not been specified. A single rule set name must be specified when the INEDIT flag is set.</locale> 
  </message>  
  <message name="ERR_MULTIPLE_CREOLE_RULE_SET_NAMES">
      <locale language="en">Multiple Rule Set Name parameters have been specified. A single rule set name must be specified when the INEDIT flag is set.</locale> 
  </message>
  <message name="INF_EXTRACTING_CREOLE_RULE_SET_PROPERTIES">
      <locale language="en">Extracting rule set properties file name '%1s'</locale> 
  </message>
  <message name="ERR_EXTRACTING_CREOLE_RULE_SET_PROPERTIES">
    <locale language="en">The properties file name '%1s' could not be read or does not exist on the database.</locale> 
  </message>
  
</messages>