<?curam-deprecated Since Curam 6.0.4.0 replaced with workflow EVIDENCEAUTOACCEPTFAILEDNOTIFICATION
    because this workflow enacted for auto accept evidence failure notification 
    is not having the exact reason for the evidence auto acceptance failure. 
    To pass the exact reason for the evidence auto acceptance failure additional 
    enactment parameters are required, so a new workflow has been introduced. 
    See release note:CR00341649.
?>
<workflow-process
  category="PC2001"
  createdBy="admin"
  creationDate="20090924T153103"
  id="28420"
  language-version="1.0"
  process-version="1"
  released="true"
>
  <name>AUTOACCEPTEVIDENCEFAILEDNOTIFICATION</name>
  <!--BEGIN, CR00361282, CL -->
  <process-display-name>
    <localized-text>
      <locale language="en">Auto Accept Evidence Failed Notification</locale>
    </localized-text>
  </process-display-name>
  <!--END, CR00361282 -->
  <description>
    <localized-text>
      <locale language="en">Generates notifications for evidence that could not be
                automatically accepted onto a case.</locale>
    </localized-text>
  </description>
  <enactment-mappings>
    <mapping>
      <source-attribute
        name="caseReference"
        struct-name="curam.evidencebroker.sl.struct.AutoAcceptEvidenceFailureNotificationDetails"
      />
      <target-attribute
        name="caseReference"
        wdo-name="AutoAcceptEvidenceFailureDetails"
      />
    </mapping>
    <mapping>
      <source-attribute
        name="caseOwner"
        struct-name="curam.evidencebroker.sl.struct.AutoAcceptEvidenceFailureNotificationDetails"
      />
      <target-attribute
        name="caseOwner"
        wdo-name="AutoAcceptEvidenceFailureDetails"
      />
    </mapping>
    <mapping>
      <source-attribute
        name="caseID"
        struct-name="curam.evidencebroker.sl.struct.AutoAcceptEvidenceFailureNotificationDetails"
      />
      <target-attribute
        name="caseID"
        wdo-name="AutoAcceptEvidenceFailureDetails"
      />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo
      initialize-attributes="false"
      is-list-wdo="false"
    >
      <wdo-name>AutoAcceptEvidenceFailureDetails</wdo-name>
      <display-name>
        <localized-text>
          <locale language="en">Auto Accept Evidence Failure Details</locale>
        </localized-text>
      </display-name>
      <description>
        <localized-text>
          <locale language="en">The details needed to inform the case owner of the
                        failure.</locale>
        </localized-text>
      </description>
      <attributes>
        <attribute>
          <attribute-name>caseReference</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case Reference</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>caseOwner</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case Owner</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>caseID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
      </attributes>
    </wdo>
  </wdos>
  <activities>
    <start-process-activity id="1">
      <name>
        <localized-text>
          <locale language="en">Start Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The start process activity</locale>
        </localized-text>
      </description>
      <split type="none">
        <transition-id idref="1"/>
      </split>
    </start-process-activity>
    <end-process-activity id="2">
      <name>
        <localized-text>
          <locale language="en">End Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The end process activity</locale>
        </localized-text>
      </description>
      <join type="none"/>
    </end-process-activity>
    <route-activity
      category="AC1"
      id="3"
    >
      <name>
        <localized-text>
          <locale language="en">Inform Case Owner Auto Accept Of Evidence Failed</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">Generates a notification for evidence that could not be
                        automatically accepted onto a case.</locale>
        </localized-text>
      </description>
      <notification delivery-mechanism="DM1">
        <subject>
          <message>
            <message-text>
              <localized-text>
                <!--  BEGIN, CR00236468, GYH -->
                <locale language="en">Error accepting evidence onto Case %1s.</locale>
                <!--  END, CR00236468 -->
              </localized-text>
            </message-text>
            <message-parameters>
              <wdo-attribute
                name="caseReference"
                wdo-name="AutoAcceptEvidenceFailureDetails"
              />
            </message-parameters>
          </message>
        </subject>
        <body>
          <message>
            <message-text>
              <localized-text>
                <!--  BEGIN, CR00236468, GYH -->
                <locale language="en">An error occurred during the auto acceptance
                                    of incoming evidence onto Case %1s. You will need to examine and
                                    manually resolve any issues with this evidence.</locale>
                <!--  END, CR00236468 -->
              </localized-text>
            </message-text>
            <message-parameters>
              <wdo-attribute
                name="caseReference"
                wdo-name="AutoAcceptEvidenceFailureDetails"
              />
            </message-parameters>
          </message>
        </body>
        <allocation-strategy
          identifier="curam.core.sl.intf.NotificationAllocationFunction.defaultStrategy"
          type="function"
        >
          <function-mappings>
            <formal-parameters>
              <formal-parameter index="0">
                <base-type type="STRING">
                  <wdo-attribute
                    name="caseOwner"
                    wdo-name="AutoAcceptEvidenceFailureDetails"
                  />
                </base-type>
              </formal-parameter>
            </formal-parameters>
          </function-mappings>
        </allocation-strategy>
        <!--  BEGIN, CR00236468, GYH -->
        <actions>
          <action
            page-id="EvidenceBroker_resolveIncomingEvidence"
            principal-action="false"
          >
            <message>
              <message-text>
                <localized-text>
                  <locale language="en">Review Incoming Evidence</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="caseID">
              <wdo-attribute
                name="caseID"
                wdo-name="AutoAcceptEvidenceFailureDetails"
              />
            </link-parameter>
          </action>
        </actions>
        <!--  END, CR00236468 -->
      </notification>
      <join type="none"/>
      <split type="none">
        <transition-id idref="2"/>
      </split>
    </route-activity>
  </activities>
  <transitions>
    <transition
      from-activity-idref="1"
      id="1"
      to-activity-idref="3"
    />
    <transition
      from-activity-idref="3"
      id="2"
      to-activity-idref="2"
    />
  </transitions>
</workflow-process>
