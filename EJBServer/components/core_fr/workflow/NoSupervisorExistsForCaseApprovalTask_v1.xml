<!-- BEGIN, CR00228764, PB -->
<workflow-process category="PC3" createdBy="admin" creationDate="20091002T150747" id="150" language-version="1.0" process-version="1" released="true"> 
  <!-- END, CR00228764 -->  
  <name>NoSupervisorExistsForCaseApprovalTask</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Aucun superviseur n'existe pour la tâche d'approbation de dossier</locale>  
      <locale language="en">No Supervisor Exists For Case Approval Task</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <description> 
    <localized-text> 
      <locale language="fr">Ce processus permet d'envoyer une tâche à une file d'attente des travaux s'il n'existe aucun superviseur pour le dossier soumis pour approbation.</locale>  
      <locale language="en">This process is used to send a task to a work queue if there is no supervisor existing for the case submitted for approval.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="caseID" struct-name="curam.core.struct.CaseHeaderKey"/>  
      <target-attribute name="caseID" wdo-name="CaseDetailsWDO"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="caseReference" struct-name="curam.core.struct.CaseReference"/>  
      <target-attribute name="caseReference" wdo-name="CaseDetailsWDO"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="workQueueID" struct-name="curam.core.sl.struct.WorkQueueIDKey"/>  
      <target-attribute name="workQueueID" wdo-name="WorkQueueIDWDO"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>CaseDetailsWDO</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">CaseDetailsWDO</locale>  
          <locale language="en">CaseDetailsWDO</locale>
        </localized-text> 
      </display-name>  
      <description> 
        <localized-text> 
          <locale language="fr">Objet de données de flux de travaux pour l'ID de dossier et la référence de dossier utilisés pour construire le message pour la tâche.</locale>  
          <locale language="en">Workflow data object for case ID and case reference which are used to construct the message for task.</locale>
        </localized-text> 
      </description>  
      <attributes> 
        <attribute> 
          <attribute-name>caseID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">ID du dossier</locale>  
              <locale language="en">caseID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>caseReference</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">Référence du dossier</locale>  
              <locale language="en">caseReference</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute> 
      </attributes> 
    </wdo>  
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>WorkQueueIDWDO</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">WorkQueueIDWDO</locale>  
          <locale language="en">WorkQueueIDWDO</locale>
        </localized-text> 
      </display-name>  
      <description> 
        <localized-text> 
          <locale language="fr">Objet de données du flux de travaux pour l'ID de file d'attente des travaux utilisé pour transmettre les données à la fonction de stratégie d'allocation.</locale>  
          <locale language="en">Workflow data object for work queue ID which is used to pass to the allocation strategy function.</locale>
        </localized-text> 
      </description>  
      <attributes> 
        <attribute> 
          <attribute-name>workQueueID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">workQueueID</locale>  
              <locale language="en">workQueueID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute> 
      </attributes> 
    </wdo> 
  </wdos>  
  <activities> 
    <start-process-activity id="1"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité de processus de début</locale>  
          <locale language="en">Start Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de début</locale>  
          <locale language="en">The start process activity</locale>
        </localized-text> 
      </description>  
      <split type="none"> 
        <transition-id idref="1"/> 
      </split> 
    </start-process-activity>  
    <end-process-activity id="2"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">End Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">The end process activity</locale>
        </localized-text> 
      </description>  
      <join type="none"/> 
    </end-process-activity>  
    <manual-activity category="AC1" id="3"> 
      <name> 
        <localized-text> 
          <locale language="fr">Affecter la tâche à la file d'attente des travaux par défaut</locale>  
          <locale language="en">Assign Task To Default Work Queue</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Créer une tâche en réponse à un dossier soumis pour approbation, mais qui ne possède pas de superviseur. La tâche récemment créée est affectée à la file d'attente des travaux par défaut dans cette instance.</locale>  
          <locale language="en">Creates a task in response to a case submitted for approval but which does not have a supervisor. The newly created task is assigned to the default work queue in this instance.</locale>
        </localized-text> 
      </description>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split>  
      <task> 
        <message> 
          <message-text> 
            <localized-text> 
              <locale language="fr">Il n'existe aucun superviseur pour le dossier %1s soumis pour approbation.</locale>  
              <locale language="en">No Supervisor existing for Case %1s submitted for approval.</locale>
            </localized-text> 
          </message-text>  
          <message-parameters> 
            <wdo-attribute name="caseReference" wdo-name="CaseDetailsWDO"/> 
          </message-parameters> 
        </message>  
        <actions> 
          <action page-id="Case_resolveCaseHome" principal-action="false"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Page d'accueil du dossier</locale>  
                  <locale language="en">Case Home Page</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="caseID"> 
              <wdo-attribute name="caseID" wdo-name="CaseDetailsWDO"/> 
            </link-parameter> 
          </action>  
          <action open-modal="true" page-id="WorkflowEvents_confirmSupervisorExists" principal-action="true"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Confirmer que le superviseur existe ?</locale>  
                  <locale language="en">Confirm supervisor exists?</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="caseID"> 
              <wdo-attribute name="caseID" wdo-name="CaseDetailsWDO"/> 
            </link-parameter> 
          </action> 
        </actions>  
        <allow-deadline-override>false</allow-deadline-override>  
        <allow-task-forwarding>true</allow-task-forwarding> 
      </task>  
      <allocation-strategy identifier="curam.core.sl.intf.WorkflowAllocationFunction.workQueueTaskAllocationStrategy" type="function"> 
        <function-mappings> 
          <formal-parameters> 
            <formal-parameter index="0"> 
              <struct type="curam.core.sl.struct.WorkQueueIDKey"> 
                <field name="workQueueID"> 
                  <base-type type="INT64"> 
                    <wdo-attribute name="workQueueID" wdo-name="WorkQueueIDWDO"/> 
                  </base-type> 
                </field> 
              </struct> 
            </formal-parameter> 
          </formal-parameters> 
        </function-mappings> 
      </allocation-strategy>  
      <event-wait wait-on-all-events="false"> 
        <events> 
          <event event-class="CASE" event-type="NO_SUPERVISOR_EXISTS" identifier="1"> 
            <event-match-attribute name="caseID" wdo-name="CaseDetailsWDO"/> 
          </event> 
        </events> 
      </event-wait>  
      <biz-object-associations> 
        <biz-object-association biz-object-type="BOT1"> 
          <wdo-attribute name="caseID" wdo-name="CaseDetailsWDO"/> 
        </biz-object-association> 
      </biz-object-associations> 
    </manual-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="2" to-activity-idref="2"/> 
  </transitions> 
</workflow-process>
