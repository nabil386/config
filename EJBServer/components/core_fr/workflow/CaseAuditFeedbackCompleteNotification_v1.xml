<workflow-process category="PC3001" createdBy="admin" creationDate="20100128T170826" id="153" language-version="1.0" process-version="1" released="true"> 
  <!-- END, CR00228764 -->  
  <name>CaseAuditFeedbackCompleteNotification</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Notification de commentaires en retour d'audit de dossier terminés</locale>  
      <locale language="en">Case Audit Feedback Complete Notification</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <description> 
    <localized-text> 
      <locale language="fr">Envoie une notification à un auditeur pour l'informer que des commentaires en retour ont été fournis.</locale>  
      <locale language="en">Sends a notification to an auditor, informing them that feedback has been provided.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="caseReference" struct-name="curam.caseaudit.entity.struct.CaseAuditFeedbackCompleteNotificationDetails"/>  
      <target-attribute name="caseReference" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="caseAuditID" struct-name="curam.caseaudit.entity.struct.CaseAuditFeedbackCompleteNotificationDetails"/>  
      <target-attribute name="caseAuditID" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="auditor" struct-name="curam.caseaudit.entity.struct.CaseAuditFeedbackCompleteNotificationDetails"/>  
      <target-attribute name="auditor" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>CaseAuditFeedbackCompleteNotificationDetails</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">Détails de la notification de l'achèvement des commentaires en retour pour l'audit de dossier</locale>  
          <locale language="en">Case Audit Feedback Completion Notification Details</locale>
        </localized-text> 
      </display-name>  
      <description> 
        <localized-text> 
          <locale language="fr">Détails requis pour informer l'auditeur de l'achèvement des commentaires en retour.</locale>  
          <locale language="en">The details required to inform the auditor of the completed feedback.</locale>
        </localized-text> 
      </description>  
      <attributes> 
        <attribute> 
          <attribute-name>caseReference</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">Référence du dossier.</locale>  
              <locale language="en">Case Reference</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>caseAuditID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">ID d'audit de dossier</locale>  
              <locale language="en">Case Audit ID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>auditor</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">Auditeur</locale>  
              <locale language="en">Auditor</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute> 
      </attributes> 
    </wdo> 
  </wdos>  
  <activities> 
    <start-process-activity id="1"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité de processus de début</locale>  
          <locale language="en">Start Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de début</locale>  
          <locale language="en">The start process activity</locale>
        </localized-text> 
      </description>  
      <split type="none"> 
        <transition-id idref="1"/> 
      </split> 
    </start-process-activity>  
    <end-process-activity id="2"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">End Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">The end process activity</locale>
        </localized-text> 
      </description>  
      <join type="none"/> 
    </end-process-activity>  
    <route-activity category="AC1" id="3"> 
      <name> 
        <localized-text> 
          <locale language="fr">Informer l'auditeur de l'achèvement des commentaires en retour sur l'audit de dossier</locale>  
          <locale language="en">Inform Auditor Of Case Audit Feedback Completion</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Envoie une notification à un auditeur pour l'informer que des commentaires en retour ont été fournis.</locale>  
          <locale language="en">Sends a notification to an auditor, informing them that feedback has been provided.</locale>
        </localized-text> 
      </description>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Commentaires en retour reçus sur l'audit du dossier %1s.</locale>  
                <locale language="en">Audit feedback received for Case %1s.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="caseReference" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
            </message-parameters> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Des commentaires en retour ont été reçus pour l'audit de dossier %1s.</locale>  
                <locale language="en">Case Audit feedback has been received for Case %1s.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="caseReference" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
            </message-parameters> 
          </message> 
        </body>  
        <allocation-strategy identifier="curam.core.sl.intf.NotificationAllocationFunction.defaultStrategy" type="function"> 
          <function-mappings> 
            <formal-parameters> 
              <formal-parameter index="0"> 
                <base-type type="STRING"> 
                  <wdo-attribute name="auditor" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
                </base-type> 
              </formal-parameter> 
            </formal-parameters> 
          </function-mappings> 
        </allocation-strategy>  
        <actions> 
          <action page-id="CaseAudit_auditorViewCaseAudit" principal-action="false"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Accueil d'audit de dossier</locale>  
                  <locale language="en">Case Audit Home</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="caseAuditID"> 
              <wdo-attribute name="caseAuditID" wdo-name="CaseAuditFeedbackCompleteNotificationDetails"/> 
            </link-parameter> 
          </action> 
        </actions> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split> 
    </route-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="2" to-activity-idref="2"/> 
  </transitions> 
