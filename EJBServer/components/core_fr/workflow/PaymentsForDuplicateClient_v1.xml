<workflow-process category="PC10" createdBy="admin" creationDate="20080516T210301" id="79" language-version="1.0" process-version="1" released="true"> 
  <!-- END CR00168004 -->  
  <name>PaymentsForDuplicateClient</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Paiements pour client dupliqué</locale>  
      <locale language="en">Payments For Duplicate Client</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <!-- BEGIN, CR00248295, NS -->  
  <description> 
    <localized-text> 
      <locale language="fr">Avertit le propriétaire de dossier qu'un paiement a été exécuté à l'attention d'une personne qui est marquée comme doublon.</locale>  
      <locale language="en">Notifies the case owner of a case that a payment has been issued to a person who is marked as duplicate.</locale>
    </localized-text> 
  </description>  
  <!-- END, CR00248295 -->  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="concernRoleID" struct-name="curam.core.struct.ConcernRoleKey"/>  
      <target-attribute name="concernRoleID" wdo-name="ConcernRoleDetailsCaseRefAndFinInstruction"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="finInstructionID" struct-name="curam.core.struct.ILIFinInstructID"/>  
      <target-attribute name="instructID" wdo-name="ConcernRoleDetailsCaseRefAndFinInstruction"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
