<workflow-process category="PC1" createdBy="admin" creationDate="20121022T153051" id="1126999418470745" language-version="1.0" process-version="2" released="true"> 
  <name>RemoveOrgUnit</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Retirer l'unité d'organisation</locale>  
      <locale language="en">Remove Organisation Unit</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <description> 
    <localized-text> 
      <locale language="fr">Ce processus de flux de travaux assure le traitement lorsqu'un administrateur choisit de supprimer une unité d'organisation de la structure d'organisation. Si des tâches ou des dossiers sont affectés à l'unité d'organisation supprimée, une tâche est créée pour l'utilisateur principal de l'organisation afin de demander que la charge de travail soit réaffectée.</locale>  
      <locale language="en">This workflow process handles processing when an administrator chooses to delete an organisation unit from the organisation structure. If there are tasks or cases assigned to the deleted organisation unit, then a task is created for the organisation's lead user to request the workload be reassigned.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="name" struct-name="curam.core.sl.entity.struct.OrganisationUnitName"/>  
      <target-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="organisationStructKey.organisationStructureID" struct-name="curam.core.sl.struct.CancelOrgUnitKey"/>  
      <target-attribute name="organisationStructureID" wdo-name="RemoveOrgUnitWDO"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="organisationUnitKey.organisationUnitID" struct-name="curam.core.sl.struct.CancelOrgUnitKey"/>  
      <target-attribute name="organisationUnitID" wdo-name="RemoveOrgUnitWDO"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>RemoveOrgUnitWDO</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">RemoveOrgUnitWDO</locale>  
          <locale language="en">RemoveOrgUnitWDO</locale>
        </localized-text> 
      </display-name>  
      <attributes> 
        <attribute> 
          <attribute-name>isOrgUnitDeleted</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">isOrgUnitDeleted</locale>  
              <locale language="en">isOrgUnitDeleted</locale>
            </localized-text> 
          </display-name>  
          <type>BOOLEAN</type>  
          <required-at-enactment>false</required-at-enactment>  
          <process-output>false</process-output>  
          <initial-value>false</initial-value> 
        </attribute>  
        <attribute> 
          <attribute-name>orgUnitName</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">orgUnitName</locale>  
              <locale language="en">orgUnitName</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>areNonCaseAndTaskErrorsPresent</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">areNonCaseAndTaskErrorsPresent</locale>  
              <locale language="en">areNonCaseAndTaskErrorsPresent</locale>
            </localized-text> 
          </display-name>  
          <type>BOOLEAN</type>  
          <required-at-enactment>false</required-at-enactment>  
          <process-output>false</process-output>  
          <initial-value>false</initial-value> 
        </attribute>  
        <attribute> 
          <attribute-name>organisationStructureID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">organisationStructureID</locale>  
              <locale language="en">organisationStructureID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>organisationUnitID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">organisationUnitID</locale>  
              <locale language="en">organisationUnitID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>supervisorUser</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">supervisorUser</locale>  
              <locale language="en">supervisorUser</locale>
            </localized-text> 
          </display-name>  
          <type>STRING</type>  
          <required-at-enactment>false</required-at-enactment>  
          <process-output>false</process-output>  
          <constant-value>supervisor</constant-value> 
        </attribute> 
      </attributes> 
    </wdo> 
  </wdos>  
  <activities> 
    <start-process-activity id="1"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité de processus de début</locale>  
          <locale language="en">Start Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de début</locale>  
          <locale language="en">The start process activity</locale>
        </localized-text> 
      </description>  
      <split type="none"> 
        <transition-id idref="1"/> 
      </split> 
    </start-process-activity>  
    <end-process-activity id="2"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">End Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">The end process activity</locale>
        </localized-text> 
      </description>  
      <join type="none"/> 
    </end-process-activity>  
    <automatic-activity category="AC1" id="3"> 
      <name> 
        <localized-text> 
          <locale language="fr">Retirer l'unité d'organisation</locale>  
          <locale language="en">Remove Organisation Unit</locale>
        </localized-text> 
      </name>  
      <join type="none"/>  
      <split type="xor"> 
        <transition-id idref="3"/>  
        <transition-id idref="4"/>  
        <transition-id idref="10"/> 
      </split>  
      <bpo-mapping interface-name="curam.core.sl.intf.OrganisationUnit" method-name="cancelOrganisationInWF"> 
        <formal-parameters> 
          <formal-parameter index="0"> 
            <struct type="curam.core.sl.struct.CancelOrgUnitKey"> 
              <field name="organisationStructKey"> 
                <struct type="curam.core.sl.entity.struct.OrganisationStructureID"> 
                  <field name="organisationStructureID"> 
                    <base-type type="INT64"> 
                      <wdo-attribute name="organisationStructureID" wdo-name="RemoveOrgUnitWDO"/> 
                    </base-type> 
                  </field> 
                </struct> 
              </field>  
              <field name="organisationUnitKey"> 
                <struct type="curam.core.sl.entity.struct.OrganisationUnitKey"> 
                  <field name="organisationUnitID"> 
                    <base-type type="INT64"> 
                      <wdo-attribute name="organisationUnitID" wdo-name="RemoveOrgUnitWDO"/> 
                    </base-type> 
                  </field> 
                </struct> 
              </field> 
            </struct> 
          </formal-parameter> 
        </formal-parameters>  
        <return> 
          <struct> 
            <field name="isOrgUnitCancelled"> 
              <base-type> 
                <wdo-attribute name="isOrgUnitDeleted" wdo-name="RemoveOrgUnitWDO"/> 
              </base-type> 
            </field>  
            <field name="areNonCaseAndTaskErrorsPresent"> 
              <base-type> 
                <wdo-attribute name="areNonCaseAndTaskErrorsPresent" wdo-name="RemoveOrgUnitWDO"/> 
              </base-type> 
            </field> 
          </struct> 
        </return> 
      </bpo-mapping> 
    </automatic-activity>  
    <manual-activity category="AC1" id="4"> 
      <name> 
        <localized-text> 
          <locale language="fr">Réaffecter les tâches de l'unité d'organisation &amp; Dossiers</locale>  
          <locale language="en">Reassign Organisation Unit Tasks &amp; Cases</locale>
        </localized-text> 
      </name>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">L'unité d'organisation %1s comprend au moins une tâche ou un dossier.</locale>  
                <locale language="en">The Organisation Unit %1s has at least one task or case.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Impossible de retirer %1s, car des tâches ou des dossiers sont affectés à l'unité d'organisation. Une tâche a été créée et affectée au superviseur de l'unité d'organisation pour demander que la charge de travail soit réaffectée.</locale>  
                <locale language="en">%1s could not be removed because task(s) and/or case(s) are assigned to the Organisation Unit. A Task has been created and assigned to the Organisation Unit's Supervisor to request the workload be reassigned.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </body>  
        <allocation-strategy identifier="Supervisor" type="target"/> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="9"/> 
      </split>  
      <task> 
        <message> 
          <message-text> 
            <localized-text> 
              <locale language="fr">Réallouez toutes les tâches et tous les dossiers affectés à l'unité d'organisation %1s, car ils sont marqués pour retrait.</locale>  
              <locale language="en">Reallocate all the tasks and Cases assigned to organisation unit %1s as it is marked for removal.</locale>
            </localized-text> 
          </message-text>  
          <message-parameters> 
            <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
          </message-parameters> 
        </message>  
        <actions> 
          <action open-modal="true" page-id="Organization_closeRemoveOrgUnitTask" principal-action="true"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Confirmer la réallocation de la charge de travail</locale>  
                  <locale language="en">Confirm Workload Reallocation</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="orgUnitName"> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </link-parameter>  
            <link-parameter name="taskID"> 
              <wdo-attribute name="taskID" wdo-name="Context_Task"/> 
            </link-parameter> 
          </action> 
        </actions>  
        <allow-deadline-override>false</allow-deadline-override>  
        <allow-task-forwarding>true</allow-task-forwarding> 
      </task>  
      <allocation-strategy identifier="curam.core.sl.intf.NotificationAllocationFunction.parentOrgUnitSupervisorStrategy" type="function"> 
        <function-mappings> 
          <formal-parameters> 
            <formal-parameter index="0"> 
              <base-type type="INT64"> 
                <wdo-attribute name="organisationUnitID" wdo-name="RemoveOrgUnitWDO"/> 
              </base-type> 
            </formal-parameter>  
            <formal-parameter index="1"> 
              <base-type type="INT64"> 
                <wdo-attribute name="organisationStructureID" wdo-name="RemoveOrgUnitWDO"/> 
              </base-type> 
            </formal-parameter> 
          </formal-parameters> 
        </function-mappings> 
      </allocation-strategy>  
      <event-wait wait-on-all-events="false"> 
        <events> 
          <event event-class="TASK" event-type="CLOSED" identifier="1"> 
            <event-match-attribute name="taskID" wdo-name="Context_Task"/> 
          </event> 
        </events> 
      </event-wait> 
    </manual-activity>  
    <route-activity id="5"> 
      <name> 
        <localized-text> 
          <locale language="fr">Route 1</locale>  
          <locale language="en">Route 1</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Cette activité achemine le flux de la réaffectation de tâches d'unité d'organisation et d'activités de dossier ainsi que l'activité de notification d'erreur et de notification de suppression afin de supprimer l'activité de boucle d'unité d'organisation.</locale>  
          <locale language="en">This activity routes the flow from reassign organisation unit tasks and cases activity and route with error notification activity and route with delete notification activity to remove organisation unit loop activity.</locale>
        </localized-text> 
      </description>  
      <join type="xor"/>  
      <split type="none"> 
        <transition-id idref="7"/> 
      </split> 
    </route-activity>  
    <route-activity category="AC1" id="9"> 
      <name> 
        <localized-text> 
          <locale language="fr">Route avec notification d'erreur</locale>  
          <locale language="en">Route with Error Notification</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Cette activité envoie une notification d'erreur rencontrée lors de la suppression de l'unité d'organisation.</locale>  
          <locale language="en">This activity sends an error notification which are encountered while deleting the organisation unit.</locale>
        </localized-text> 
      </description>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Le système a rencontré des exceptions lors de la suppression de l'unité d'organisation %1s. Essayez de la supprimer de nouveau.</locale>  
                <locale language="en">System encountered some exceptions while deleting %1s organisation unit. Please try deleting again.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Essayez de supprimer l'unité d'organisation %1s de nouveau.</locale>  
                <locale language="en">Please try deleting %1s organisation unit again.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </body>  
        <allocation-strategy identifier="Supervisor" type="target"/> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="11"/> 
      </split> 
    </route-activity>  
    <route-activity category="AC1" id="10"> 
      <name> 
        <localized-text> 
          <locale language="fr">Route avec notification de suppression</locale>  
          <locale language="en">Route with delete notification</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Cette activité envoie une notification de suppression une fois l'unité d'organisation supprimée.</locale>  
          <locale language="en">This activity sends a delete notification after the organisation unit is removed.</locale>
        </localized-text> 
      </description>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">L'unité d'organisation %1s a été retirée avec succès.</locale>  
                <locale language="en">The Organisation Unit %1s has been removed successfully.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Le système a terminé avec succès le processus de retrait de l'unité d'organisation %1s.</locale>  
                <locale language="en">The system successfully completed the process of removing Organisation Unit %1s.</locale>
              </localized-text> 
            </message-text>  
            <message-parameters> 
              <wdo-attribute name="orgUnitName" wdo-name="RemoveOrgUnitWDO"/> 
            </message-parameters> 
          </message> 
        </body>  
        <allocation-strategy identifier="Supervisor" type="target"/> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="12"/> 
      </split> 
    </route-activity>  
    <loop-begin-activity id="7"> 
      <name> 
        <localized-text> 
          <locale language="fr">Boucle de retrait de l'unité d'organisation [Début]</locale>  
          <locale language="en">Remove Organisation Unit Loop [Begin]</locale>
        </localized-text> 
      </name>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split>  
      <loop-type name="while"/>  
      <condition> 
        <expression conjunction="and" data-item-lhs="RemoveOrgUnitWDO.isOrgUnitDeleted" data-item-rhs="false" id="1" operation="=="/>  
        <expression conjunction="and" data-item-lhs="RemoveOrgUnitWDO.areNonCaseAndTaskErrorsPresent" data-item-rhs="false" id="2" operation="=="/> 
      </condition>  
      <block-endpoint-ref activity-id="8"/> 
    </loop-begin-activity>  
    <loop-end-activity id="8"> 
      <name> 
        <localized-text> 
          <locale language="fr">Boucle de retrait de l'unité d'organisation [Fin]</locale>  
          <locale language="en">Remove Organisation Unit Loop [End]</locale>
        </localized-text> 
      </name>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="8"/> 
      </split>  
      <block-endpoint-ref activity-id="7"/> 
    </loop-end-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="7"/>  
    <transition from-activity-idref="7" id="2" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="3" to-activity-idref="4"> 
      <condition> 
        <expression conjunction="and" data-item-lhs="RemoveOrgUnitWDO.isOrgUnitDeleted" data-item-rhs="false" id="1" operation="=="/>  
        <expression conjunction="and" data-item-lhs="RemoveOrgUnitWDO.areNonCaseAndTaskErrorsPresent" data-item-rhs="false" id="2" operation="=="/> 
      </condition> 
    </transition>  
    <transition from-activity-idref="3" id="4" to-activity-idref="9"> 
      <condition> 
        <expression conjunction="and" data-item-lhs="RemoveOrgUnitWDO.areNonCaseAndTaskErrorsPresent" data-item-rhs="true" id="1" operation="=="/> 
      </condition> 
    </transition>  
    <transition from-activity-idref="5" id="7" to-activity-idref="8"/>  
    <transition from-activity-idref="8" id="8" to-activity-idref="2"/>  
    <transition from-activity-idref="4" id="9" to-activity-idref="5"/>  
    <transition from-activity-idref="3" id="10" to-activity-idref="10"/>  
    <transition from-activity-idref="9" id="11" to-activity-idref="5"/>  
    <transition from-activity-idref="10" id="12" to-activity-idref="5"/> 
  </transitions> 
</workflow-process>
