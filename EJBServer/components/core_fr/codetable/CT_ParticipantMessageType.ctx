<!-- This codetable lists the various types of Participamt Messages in the System. 
     Certain message types can be enabled, disabled for certain particpant types. This
     codetable also dictates the ordering of messages, using the sort order. -->
<codetables package="curam.participantmessages.codetable"> 
  <codetable java_identifier="ParticipantMessageType" name="ParticipantMessageType"> 
    <displaynames> 
      <locale language="fr">Type de message du participant</locale> 
    </displaynames>  
    <code default="false" java_identifier="SYSTEM" status="ENABLED" value="PMT2001"> 
      <locale language="fr" sort_order="1"> 
        <description>Système</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="VERIFICATION" status="ENABLED" value="PMT2002"> 
      <locale language="fr" sort_order="2"> 
        <description>Vérification</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="MEETING" status="ENABLED" value="PMT2003"> 
      <locale language="fr" sort_order="3"> 
        <description>Réunion</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="PAYMENT" status="ENABLED" value="PMT2004"> 
      <locale language="fr" sort_order="4"> 
        <description>Paiement</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="APPLICATION" status="ENABLED" value="PMT2005"> 
      <locale language="fr" sort_order="5"> 
        <description>Accusé de réception de la demande</description>  
        <annotation/> 
      </locale> 
    </code> 
  </codetable> 
</codetables>
