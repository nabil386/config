<messages package="curam.message"> 
  <message name="ERR_SS_BAD_QUERY"> 
    <locale language="fr">Requête non valide trouvée.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_TERMS"> 
    <locale language="fr">La requête ne contient aucun terme - requête non valide.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_VALID_TERMS"> 
    <locale language="fr">La requête ne contient aucun terme valide - requête non valide.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_FIELD"> 
    <locale language="fr">La requête contient des termes faisant référence à une zone nommée \'%1s\' qui est inconnue ou non enregistrée dans ce service de recherche.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNINDEXED_FIELD"> 
    <locale language="fr">La requête contient des termes faisant référence à une zone nommée \'%1s\' qui n'est pas indexée dans ce service de recherche.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_FIELDS"> 
    <locale language="fr">La requête ne contient aucune zone à renvoyer - requête non valide.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_RETURN_FIELD"> 
    <locale language="fr">La requête tente de renvoyer la zone inconnue \'%1s\'.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNTOKENIZED_WILDCARD"> 
    <locale language="fr">Les requêtes contenant des caractères génériques non marqués ne sont pas prises en charge.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_EMPTY_TERM"> 
    <locale language="fr">Terme vide trouvé.</locale> 
  </message>  
  <message name="ERR_SS_BAD_QUERY_MULTIPLE_TERM_TOKEN_FIELD"> 
    <locale language="fr">Une requête de préfixe contenant plusieurs marqueurs sur une zoné marquée a été trouvée.</locale> 
  </message> 
</messages>
