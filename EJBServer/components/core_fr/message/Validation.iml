<messages package="curam.util.persistence.message"> 
  <message name="RUN_ID_VALIDATION_NAME_INVALID"> 
    <locale language="fr">Erreur dans le nom de la validation personnalisable spécifié '%1s' :
le nom de validation n'est pas valide. Son format devrait être
packageName.ValidationInterfaceName.validateMethodName</locale> 
  </message>  
  <message name="RUN_ID_VALIDATION_INTF_NOT_FOUND"> 
    <locale language="fr">Erreur dans le nom de la validation personnalisable spécifié '%1s' :
l'interface de validation '%2s' est introuvable.</locale> 
  </message>  
  <message name="RUN_ID_VALIDATION_METHOD_NOT_FOUND"> 
    <locale language="fr">Erreur dans le nom de la validation personnalisable spécifié '%1s' :
l'interface de validation '%2s' ne contient pas de méthode '%3s'.</locale> 
  </message>  
  <message name="RUN_ID_VALIDATION_ON_CLASS"> 
    <locale language="fr">Erreur dans le nom de la validation personnalisable spécifié '%1s' :
la classe de validation '%2s' est une classe et non une interface.</locale> 
  </message>  
  <message name="RUN_ID_VALIDATION_ANNOTATION_NOT_FOUND"> 
    <locale language="fr">Avertissement lié au nom de la validation personnalisable spécifié '%1s' :
l'interface de validation '%2s' n'est pas annotée avec l'annotation '%3s'.</locale> 
  </message>  
  <message name="INF_VALIDATION_BOOTSTRAP_TRANSACTION"> 
    <locale language="fr">Transaction pour l'amorçage de la liste des validations
personnalisables désactivées</locale> 
  </message> 
