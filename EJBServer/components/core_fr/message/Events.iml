<messages package="curam.util.message"> 
  <message name="ERR_EVT_CLASS_IS_NOT_AN_EVENT_HANDLER"> 
    <locale language="fr">La classe '%1s' n'implémente pas l'interface curam.util.events.impl.EventHandler. Il ne s'agit donc pas d'un gestionnaire d'événements.</locale> 
  </message>  
  <message name="ERR_EVT_EVENT_HANDLER_CLASS_NOT_FOUND"> 
    <locale language="fr">La classe de gestionnaire d'événements '%1s' est introuvable.</locale> 
  </message>  
  <message name="ERR_EVT_EVENT_HANDLER_COULD_NOT_BE_INSTANTIATED"> 
    <locale language="fr">Le gestionnaire d'événements '%1s' n'a pas pu être instancié.</locale> 
  </message>  
  <message name="ERR_EVT_EVENT_FILTER_COULD_NOT_BE_INSTANTIATED"> 
    <locale language="fr">Le filtre d'événement '%1s' n'a pas pu être instancié.</locale> 
  </message>  
  <message name="ERR_EVT_EVENT_FILTER_CLASS_NOT_FOUND"> 
    <locale language="fr">La classe de filtre d'événement '%1s' est introuvable.</locale> 
  </message>  
  <message name="ERR_EVT_CLASS_IS_NOT_AN_EVENT_FILTER"> 
    <locale language="fr">La classe '%1s' n'implémente pas l'interface curam.util.events.impl.EventFilter. Il ne s'agit donc pas d'un filtre d'événement.</locale> 
  </message>  
  <message name="ERR_CANNOT_UNMARSHAL_XML"> 
    <locale language="fr">Une erreur s'est produite lors du chargement du document XML d'enregistrement du gestionnaire d'événements.</locale> 
  </message>  
  <message name="ERR_EVT_EVENT_CLASS_HAS_NO_HANDLER_SPECIFIED"> 
    <locale language="fr">La classe d'événement '%1s' doit posséder un gestionnaire d'événements associé.</locale> 
  </message> 
</messages>
