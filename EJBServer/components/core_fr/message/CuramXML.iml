<messages package="curam.util.message"> 
  <message name="ERR_PRINT_STREAM_NO_CONNECT"> 
    <locale language="fr">Impossible de se connecter au serveur XML.</locale> 
  </message>  
  <message name="ERR_PRINT_STREAM_TEMPLATE_RNF"> 
    <locale language="fr">Le modèle indiqué est introuvable.</locale> 
  </message>  
  <message name="ERR_PRINT_STREAM_BAD_SETUP"> 
    <locale language="fr">Le nom d'hôte ou le numéro de port du serveur XML n'est pas valide ou n'est pas défini.</locale> 
  </message>  
  <message name="ERR_PRINT_STREAM_NO_RESPONSE"> 
    <locale language="fr">Echec de l'obtention d'une réponse depuis le serveur XML.</locale> 
  </message>  
  <message name="ERR_PRINT_STREAM_BAD_RESPONSE"> 
    <locale language="fr">Une exception s'est produite dans le flux d'impression. Echec de l'obtention de la réponse attendue depuis le serveur XML : %1s</locale> 
  </message>  
  <message name="ERR_XML_OPERATION_UNSAVABLE"> 
    <locale language="fr">Impossible de sauvegarder le fichier XML.</locale> 
  </message>  
  <message name="ERR_XML_CLOSING_SOCKET"> 
    <locale language="fr">Un problème s'est produit lors de la fermeture du socket sur XMLServer.</locale> 
  </message>  
  <message name="ERR_XML_ILLEGAL_ENDPOINT_ARGUMENTS"> 
    <locale language="fr">La liste de noms d'hôte et de ports du serveur XML doit être égale.</locale> 
  </message>  
  <message name="ERR_ENCODING_NOT_SUPPORTED"> 
    <locale language="fr">Le codage "%1s" spécifié n'est pas pris en charge.</locale> 
  </message> 
</messages>
