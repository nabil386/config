<project name="RESTToolkitWrapper">
  <!-- Wrapper file to handle integrating with the howard stream. -->
  <property environment="sysenv."/>
  <property name="curam.rest.component.rest_build.xml" 
            value="${sysenv.SERVER_DIR}/components/Rest/script/rest_build.xml"/>

  <target name="rest">
    <ant antfile="${curam.rest.component.rest_build.xml}" target="rest" inheritALL="true"/>
  </target>

  <target name="restEAR">
    <ant antfile="${curam.rest.component.rest_build.xml}" target="restEAR" inheritALL="true"/>
  </target>
</project>