<weblogic-application xmlns="http://www.bea.com/ns/weblogic/90">
  <xml>
    <parser-factory>
      <transformer-factory>
        weblogic.apache.xalan.processor.TransformerFactoryImpl
      </transformer-factory>
    </parser-factory>
  </xml>
  
  <classloader-structure>
    <module-ref><module-uri>/Rest</module-uri></module-ref>
    <classloader-structure>
      <module-ref><module-uri>InvalidationModule.jar</module-uri></module-ref>
    </classloader-structure>
  </classloader-structure>

  <prefer-application-packages>
    <package-name>curam.util.*</package-name>
    <package-name>org.apache.commons.lang.*</package-name>
    <!-- contents of xalan.jar, serializer.jar -->
    <package-name>java_cup.runtime.*</package-name>
    <package-name>org.apache.bcel.*</package-name>
    <package-name>org.apache.regexp.*</package-name>
    <package-name>org.apache.xalan.*</package-name>
    <package-name>org.apache.xml.*</package-name>
    <package-name>org.apache.xpath.*</package-name>
    <package-name>org.w3c.dom.xpath.*</package-name>
    <!-- contents of xerces.jar -->
    <package-name>org.apache.html.dom.*</package-name>
    <package-name>org.apache.wml.*</package-name>
    <package-name>org.apache.xerces.dom3.*</package-name>
    <package-name>org.apache.xerces.impl.*</package-name>
    <package-name>org.apache.xerces.jaxp.*</package-name>
    <package-name>org.apache.xerces.parsers.*</package-name>
    <package-name>org.apache.xerces.util.*</package-name>
    <package-name>org.apache.xerces.xinclude.*</package-name>
    <package-name>org.apache.xerces.xni.*</package-name>
    <package-name>org.apache.xerces.xpointer.*</package-name>
    <package-name>org.apache.xerces.xs.*</package-name>
    <package-name>org.apache.xerces.dom.*</package-name>
    <package-name>org.apache.xml.serialize.*</package-name>
    <package-name>org.w3c.dom.html.*</package-name>
    <package-name>com.sun.jersey.*</package-name>
    <package-name>com.sun.research.ws.wadl.*</package-name>
    <package-name>com.sun.ws.rs.ext.*</package-name>
    <!-- Jackson-*.jar -->
    <package-name>org.codehaus.jackson.*</package-name>
    <package-name>com.fasterxml.jackson.*</package-name>
    <!-- jettison-*.jar -->
    <package-name>org.codehaus.jettison.*</package-name>
    <!-- asm.jar -->
    <package-name>org.objectweb.asm.*</package-name>
  </prefer-application-packages>
</weblogic-application>
