<!-- This codetable lists the various types of Participamt Messages in the System. 
     Certain message types can be enabled, disabled for certain particpant types. This
     codetable also dictates the ordering of messages, using the sort order. -->
<codetables package="curam.participantmessages.codetable">
  <codetable
    java_identifier="ParticipantMessageType"
    name="ParticipantMessageType"
  >
    <code
      default="false"
      java_identifier="NOMESSAGE"
      status="ENABLED"
      value="PMT2000"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>No Messages</description>
        <annotation/>
      </locale>
    </code>
    <code
      default="false"
      java_identifier="APPEAL"
      status="ENABLED"
      value="PMT30000"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Appeal</description>
        <annotation/>
      </locale>
    </code>    
  </codetable>
</codetables>
