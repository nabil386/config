<messages package="curam.message">
  <message name="ERR_FUND_FV_AMOUNT_ENTERED_IS_NEGATIVE">
    <locale language="en">Amount may not be negative.</locale>
  </message>
  <message name="ERR_FUND_FV_ATLEAST_ONE_FIELD_SHOULD_EXIST">
    <locale language="en">At least one of Service Offering, Product or Fund must be
      provided.</locale>
  </message>
  <message name="ERR_FUND_XFV_ENDDATE_AFTER_STARTDATE">
    <locale language="en">The End Date must be after Start Date.</locale>
  </message>
