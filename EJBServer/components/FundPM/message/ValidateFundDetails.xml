  </message>
  <message name="ERR_FUND_FV_PRODUCT_INACTIVE">
    <locale language="en">Product must be active.</locale>
  </message>
  <message name="ERR_FUND_FV_SPECIFIED_FUND_IS_INACTIVE">
    <locale language="en">Specified fund must be active.</locale>
  </message>
  <message name="ERR_FUND_FV_CASE_MUST_BE_ACTIVE">
    <locale language="en">Case must be active.</locale>
  </message>
  <message name="ERR_FUND_XRV_NO_PRIORS_FOUND_FOR_UPDATE">
    <locale language="en">Funded item has not been allocated before and cannot be updated.</locale>
  </message>
  <message name="ERR_FUND_XRV_PRIORS_FOUND_FOR_NEW">
    <locale language="en">Funded item has been allocated before and cannot be allocated as new.</locale>
  </message>
