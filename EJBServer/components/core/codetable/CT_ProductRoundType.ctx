<codetables package="curam.codetable">
  <codetable
    java_identifier="PRODUCTROUNDTYPE"
    name="ProductRoundType"
  >
    <displaynames>
      <locale language="en">Product Round Type</locale>
    </displaynames>
    <code
      default="true"
      java_identifier="NEARESTFIVE"
      status="ENABLED"
      value="N05"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Round to the nearest 5 cent</description>
        <annotation/>
      </locale>
    </code>
    <code
      default="false"
      java_identifier="NEARESTTEN"
      status="ENABLED"
      value="N10"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Round to the nearest 10 cent</description>
        <annotation/>
      </locale>
    </code>
    <code
      default="false"
      java_identifier="NOROUNDING"
      status="ENABLED"
      value="NON"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>No rounding required</description>
        <annotation/>
      </locale>
    </code>
  </codetable>
</codetables>
