  <!-- END CR00168004 -->
  <name>UnmarkDuplicateClient</name>


  <!--BEGIN, CR00361282, CL -->
  <process-display-name>
    <localized-text>
      <locale language="en">Unmark Duplicate Client</locale>
    </localized-text>
  </process-display-name>
  <!--END, CR00361282 -->


  <!-- BEGIN, CR00248295, NS -->
  <description>
    <localized-text>
      <locale language="en">Notifies the case owners of all cases in which a client that has been unmarked as a duplicate is serving as a case participant, to alert them to the fact that they may want to research the case to see if any action needs to be taken.</locale>
    </localized-text>
  </description>
  <!-- END, CR00248295 -->


  <enactment-mappings>
    <mapping>
      <source-attribute
        name="concernRoleID"
        struct-name="curam.core.struct.ConcernRoleKey"
      />
      <target-attribute
        name="concernRoleID"
        wdo-name="ConcernRoleDetailsAndCaseRef"
      />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo
      initialize-attributes="false"
      is-list-wdo="true"
    >
      <wdo-name>CaseIDList</wdo-name>
      <display-name>
        <localized-text>
          <locale language="en">CaseIDList</locale>
        </localized-text>
      </display-name>
      <attributes>
        <attribute>
          <attribute-name>caseID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">caseID</locale>
            </localized-text>
