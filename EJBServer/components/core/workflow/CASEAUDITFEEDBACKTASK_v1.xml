<workflow-process
  category="PC3001"
  createdBy="admin"
  creationDate="20090826T172502"
  id="28422"
  language-version="1.0"
  process-version="1"
  released="true"
>
  <name>CASEAUDITFEEDBACKTASK</name>
  <!--BEGIN, CR00361282, CL -->
  <process-display-name>
    <localized-text>
      <locale language="en">Case Audit Feedback Task</locale>
    </localized-text>
  </process-display-name>
  <!--END, CR00361282 -->


  <!-- BEGIN, CR00306684, AKr -->
  <description>
    <localized-text>
      <locale language="en">
         Creates a task for the case owner and case supervisor of a case that 
         has been audited, when the case audit of the case is complete and 
         feedback is required.        
      </locale>
    </localized-text>
  </description>
  <!-- END, CR00306684 -->
  <enactment-mappings>
    <mapping>
      <source-attribute
        name="caseAuditID"
        struct-name="curam.caseaudit.entity.struct.FeedbackRequiredTaskDetails"
      />
      <target-attribute
        name="caseAuditID"
        wdo-name="CaseAuditFeedbackDetails"
      />
    </mapping>
    <mapping>
      <source-attribute
        name="feedbackUser"
        struct-name="curam.caseaudit.entity.struct.FeedbackRequiredTaskDetails"
      />
      <target-attribute
        name="feedbackUser"
        wdo-name="CaseAuditFeedbackDetails"
      />
    </mapping>
    <mapping>
      <source-attribute
        name="caseAuditRef"
        struct-name="curam.caseaudit.entity.struct.FeedbackRequiredTaskDetails"
      />
      <target-attribute
        name="caseAuditRef"
        wdo-name="CaseAuditFeedbackDetails"
      />
    </mapping>
    <mapping>
      <source-attribute
        name="caseAuditFeedbackID"
        struct-name="curam.caseaudit.entity.struct.FeedbackRequiredTaskDetails"
      />
      <target-attribute
        name="caseAuditFeedbackID"
        wdo-name="CaseAuditFeedbackDetails"
      />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo
      initialize-attributes="false"
      is-list-wdo="false"
    >
      <wdo-name>CaseAuditFeedbackDetails</wdo-name>
      <display-name>
        <localized-text>
          <locale language="en">CaseAuditFeedbackDetails</locale>
        </localized-text>
      </display-name>
      <attributes>
        <attribute>
          <attribute-name>caseAuditID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case Audit ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>feedbackUser</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Feedback User</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>caseAuditRef</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case Audit Reference</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>caseAuditFeedbackID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Case Audit Feedback ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
      </attributes>
    </wdo>
  </wdos>
  <activities>
    <start-process-activity id="1">
      <name>
        <localized-text>
          <locale language="en">Start Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The start process activity</locale>
        </localized-text>
      </description>
      <split type="none">
        <transition-id idref="1"/>
      </split>
    </start-process-activity>
    <end-process-activity id="2">
      <name>
        <localized-text>
          <locale language="en">End Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The end process activity</locale>
        </localized-text>
      </description>
      <join type="none"/>
    </end-process-activity>
    <manual-activity
      category="AC1"
      id="3"
    >
      <name>
        <localized-text>
          <locale language="en">FeedbackTask</locale>
        </localized-text>
      </name>
      <join type="none"/>
      <split type="none">
        <transition-id idref="2"/>
      </split>
      <task>
        <message>
          <message-text>
            <localized-text>
              <locale language="en">Feedback must be provided on Case Audit %1s</locale>
            </localized-text>
          </message-text>
          <message-parameters>
            <wdo-attribute
              name="caseAuditRef"
              wdo-name="CaseAuditFeedbackDetails"
            />
          </message-parameters>
        </message>
        <actions>
          <action
            open-modal="true"
            page-id="CaseAudit_provideFeedback"
            principal-action="true"
          >
            <message>
              <message-text>
                <localized-text>
                  <locale language="en">Provide Feedback</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="caseAuditFeedbackID">
              <wdo-attribute
                name="caseAuditFeedbackID"
                wdo-name="CaseAuditFeedbackDetails"
              />
            </link-parameter>
          </action>
        </actions>
        <allow-deadline-override>false</allow-deadline-override>
        <allow-task-forwarding>false</allow-task-forwarding>
      </task>
      <allocation-strategy
        identifier="curam.core.sl.intf.NotificationAllocationFunction.defaultStrategy"
        type="function"
      >
        <function-mappings>
          <formal-parameters>
            <formal-parameter index="0">
              <base-type type="STRING">
                <wdo-attribute
                  name="feedbackUser"
                  wdo-name="CaseAuditFeedbackDetails"
                />
              </base-type>
            </formal-parameter>
          </formal-parameters>
        </function-mappings>
      </allocation-strategy>
      <event-wait wait-on-all-events="false">
        <events>
          <event
            event-class="CaseAudit"
            event-type="FeedbackReceived"
            identifier="1"
          >
            <event-match-attribute
              name="caseAuditFeedbackID"
              wdo-name="CaseAuditFeedbackDetails"
            />
          </event>
        </events>
      </event-wait>
    </manual-activity>
  </activities>
  <transitions>
    <transition
      from-activity-idref="1"
      id="1"
      to-activity-idref="3"
    />
    <transition
      from-activity-idref="3"
      id="2"
      to-activity-idref="2"
    />
  </transitions>
</workflow-process>
