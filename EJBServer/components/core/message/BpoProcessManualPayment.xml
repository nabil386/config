<messages package="curam.message">
  <message name="INF_OVERPAYMENT_OCCURRED">
    <locale language="en">An overpayment has occurred.</locale>
  </message>
  <message name="INF_PAYMENT_DUE_OVERPAID">
    <locale language="en">A manual payment of "%1s" was issued to "%2s" for case ID "%3s" on "%4s". A Manual Payment Overpayment of "%5s"has been created.</locale>
  </message>
  <message name="INF_PAYMENT_NOT_DUE">
    <locale language="en">There is no payment due to "%1s" for case ID "%2s". The Manual payment made on "%3s" has been recorded as an overpayment.</locale>
  </message>
</messages>