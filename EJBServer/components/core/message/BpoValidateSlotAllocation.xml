<messages package="curam.message">
  <message name="ERR_SA_FV_SLOTID_EMPTY">
    <locale language="en">A slot must be specified.</locale>
  </message>
  <message name="ERR_SA_FV_SLOTALLOCATIONDATE_EMPTY">
    <locale language="en">A date must be specified.</locale>
  </message>
  <message name="ERR_SA_FV_NUMBERWORKUNITS_EMPTY">
    <locale language="en">The number of work units must be specified.</locale>
  </message>
  <message name="ERR_SA_FV_SLOTAllOCATION_LESSTHANZERO">
    <locale language="en">The number of work units for a slot must be greater than zero.</locale>
  </message>
  <message name="ERR_S_FV_SLOTAllOCATION_GREATERTHANMAX">
    <locale language="en">The number of work units for a slot cannot be greater than the maximum value set for the slot.</locale>
  </message>
</messages>