<workflow-process category="PC5" createdBy="admin" creationDate="20090401T132038" id="28515" language-version="1.0" process-version="1" released="true"> 
  <name>SamplePlannedItemPostApprovalCriteria</name>  
  <!--BEGIN, CR00361282, CL -->  
  <process-display-name> 
    <localized-text> 
      <locale language="fr">Exemples de critères post-approbation d'élément planifié</locale>  
      <locale language="en">Sample Planned Item Post Approval Criteria</locale>
    </localized-text> 
  </process-display-name>  
  <!--END, CR00361282 -->  
  <description> 
    <localized-text> 
      <locale language="fr">Exemple de flux de travaux pour un critère post-approbation d'élément planifié.  Contient une activité d'acheminement qui consiste à envoyer une notification à l'utilisateur à l'origine de l'interaction.</locale>  
      <locale language="en">Sample workflow for a planned item post approval criteria.  Contains a route activity which sends a notification to the enacting user.</locale>
    </localized-text> 
  </description>  
  <enactment-mappings> 
    <mapping> 
      <source-attribute name="caseID" struct-name="curam.core.struct.CaseHeaderKey"/>  
      <target-attribute name="caseID" wdo-name="RequiredObjectIDs"/> 
    </mapping>  
    <mapping> 
      <source-attribute name="plannedItemIDKey.plannedItemID" struct-name="curam.serviceplans.sl.struct.PlannedItemIDKey"/>  
      <target-attribute name="plannedItemID" wdo-name="RequiredObjectIDs"/> 
    </mapping> 
  </enactment-mappings>  
  <wdos> 
    <wdo initialize-attributes="false" is-list-wdo="false"> 
      <wdo-name>RequiredObjectIDs</wdo-name>  
      <display-name> 
        <localized-text> 
          <locale language="fr">RequiredObjectIDs</locale>  
          <locale language="en">RequiredObjectIDs</locale>
        </localized-text> 
      </display-name>  
      <attributes> 
        <attribute> 
          <attribute-name>caseID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">ID du dossier</locale>  
              <locale language="en">caseID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute>  
        <attribute> 
          <attribute-name>plannedItemID</attribute-name>  
          <display-name> 
            <localized-text> 
              <locale language="fr">plannedItemID</locale>  
              <locale language="en">plannedItemID</locale>
            </localized-text> 
          </display-name>  
          <type>INT64</type>  
          <required-at-enactment>true</required-at-enactment>  
          <process-output>false</process-output> 
        </attribute> 
      </attributes> 
    </wdo> 
  </wdos>  
  <activities> 
    <start-process-activity id="1"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité de processus de début</locale>  
          <locale language="en">Start Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de début</locale>  
          <locale language="en">The start process activity</locale>
        </localized-text> 
      </description>  
      <split type="none"> 
        <transition-id idref="1"/> 
      </split> 
    </start-process-activity>  
    <end-process-activity id="2"> 
      <name> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">End Process Activity</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Activité du processus de fin</locale>  
          <locale language="en">The end process activity</locale>
        </localized-text> 
      </description>  
      <join type="none"/> 
    </end-process-activity>  
    <route-activity category="AC1" id="3"> 
      <name> 
        <localized-text> 
          <locale language="fr">Notification</locale>  
          <locale language="en">Notification</locale>
        </localized-text> 
      </name>  
      <description> 
        <localized-text> 
          <locale language="fr">Envoie une notification à l'utilisateur à l'origine de l'interaction.</locale>  
          <locale language="en">Sends notification to enacting user.</locale>
        </localized-text> 
      </description>  
      <notification delivery-mechanism="DM1"> 
        <subject> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Traitement post-approbation terminé</locale>  
                <locale language="en">Post Approval Processing Complete</locale>
              </localized-text> 
            </message-text> 
          </message> 
        </subject>  
        <body> 
          <message> 
            <message-text> 
              <localized-text> 
                <locale language="fr">Veuillez noter que le traitement post-approbation s'est finalisé.</locale>  
                <locale language="en">Please note that post approval processing has completed.</locale>
              </localized-text> 
            </message-text> 
          </message> 
        </body>  
        <allocation-strategy identifier="curam.core.sl.intf.NotificationAllocationFunction.caseOwnerStrategy" type="function"> 
          <function-mappings> 
            <formal-parameters> 
              <formal-parameter index="0"> 
                <base-type type="INT64"> 
                  <wdo-attribute name="caseID" wdo-name="RequiredObjectIDs"/> 
                </base-type> 
              </formal-parameter> 
            </formal-parameters> 
          </function-mappings> 
        </allocation-strategy>  
        <actions> 
          <action page-id="ServicePlanDelivery_resolveViewPlanItemPage" principal-action="false"> 
            <message> 
              <message-text> 
                <localized-text> 
                  <locale language="fr">Lien vers la page d'accueil de l'élément planifié</locale>  
                  <locale language="en">Link to Planned Item Home Page</locale>
                </localized-text> 
              </message-text> 
            </message>  
            <link-parameter name="plannedItemID"> 
              <wdo-attribute name="plannedItemID" wdo-name="RequiredObjectIDs"/> 
            </link-parameter> 
          </action> 
        </actions> 
      </notification>  
      <join type="none"/>  
      <split type="none"> 
        <transition-id idref="2"/> 
      </split> 
    </route-activity> 
  </activities>  
  <transitions> 
    <transition from-activity-idref="1" id="1" to-activity-idref="3"/>  
    <transition from-activity-idref="3" id="2" to-activity-idref="2"/> 
  </transitions> 
</workflow-process>
