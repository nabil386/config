  <property name="curam.supervisor.listSupervisorDirectlySubscribedWorkQueuesOnly" dynamic="yes" constant="ENV_SUPERVISOR_LISTSUPERVISORDIRECTLYSUBSCRIBEDWORKQUEUESONLY">
