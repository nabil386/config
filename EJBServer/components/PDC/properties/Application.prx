      </locale>
    </locales>
  </property>
  <property name="curam.batch.resetpdcconversiononmodify.chunkkeywaitinterval" dynamic="yes" constant="ENV_RESETPDCCONVERSIONONMODIFY_CHUNK_KEY_WAIT_INTERVAL">
    <type>INT32</type>
    <value>1000</value>
    <default-value>1000</default-value>
    <category>APP_BATCH</category>
    <locales>
      <locale language="en">
        <display-name>Reset PDC Conversion on Modify Batch Program Chunk Key Wait Interval</display-name>
        <description>The interval (in milliseconds) for which the Reset PDC Conversion on Modify
          batch program will wait before retrying when reading the chunk key table.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.batch.resetpdcconversiononmodify.unprocessedchunkwaitinterval" dynamic="yes" constant="ENV_RESETPDCCONVERSIONONMODIFY_UNPROCESSED_CHUNK_WAIT_INTERVAL">
    <type>INT32</type>
    <value>1000</value>
    <default-value>1000</default-value>
    <category>APP_BATCH</category>
    <locales>
      <locale language="en">
        <display-name>Reset PDC Conversion on Modify Batch Program Unprocessed Chunk Wait Interval</display-name>
        <description>The interval (in milliseconds) for which the Reset PDC Conversion on Modify
          batch program will wait before retrying when reading the chunk table.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.batch.resetpdcconversiononmodify.processunprocessedchunk" dynamic="yes" constant="ENV_RESETPDCCONVERSIONONMODIFY_PROCESS_UNPROCESSED_CHUNKS">
    <type>STRING</type>
    <value>NO</value>
    <default-value>NO</default-value>
    <category>APP_BATCH</category>
    <locales>
      <locale language="en">
        <display-name>Reset PDC Conversion on Modify Batch Program Process Unprocessed Chunks After Completion</display-name>
        <description>Should Reset PDC Conversion on Modify batch program process any unprocessed
          chunks found after all the streams have completed.</description>
      </locale>
    </locales>
  </property>
