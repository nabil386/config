<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="PDCIdentificationDataRuleSet">
  <Class
    extends="ActiveSuccessionSet"
    extendsRuleSet="PropagatorRuleSet"
    name="PDCIdentification"
  >
    <Annotations>
      <SuccessionSetPopulation
        endDateAttribute="toDate"
        startDateAttribute="fromDate"
      />
    </Annotations>
    <Attribute name="alternateID">
      <Annotations/>
      <type>
        <javaclass name="java.lang.String"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="fromDate">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="altIDType">
      <Annotations/>
      <type>
        <codetableentry table="ConcernRoleAlternateID"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="country">
        <Annotations />
        <type>
            <codetableentry table="BDMSourceCountry" />
        </type>
        <derivation>
            <specified />
        </derivation>
    </Attribute>
    <Attribute name="toDate">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="preferredInd">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Boolean"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    	<!-- BEGIN 57386 DEV: Implement Identification Evidence Changes -->
		<Attribute name="bdmReceivedFrom">
			<Annotations />
			<type>
				<codetableentry table="BDMReceivedFrom" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="bdmReceivedFromCountry">
			<Annotations />
			<type>
				<codetableentry table="BDMSourceCountry" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="bdmModeOfReceipt">
			<Annotations />
			<type>
				<codetableentry table="BDMModeOfReceipt" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
    	<!-- END 57386 DEV: Implement Identification Evidence Changes -->
    <Attribute name="description">
      <type>
        <javaclass name="curam.creole.value.Message"/>
      </type>
      <derivation>
        <ResourceMessage
          key="DynamicEvidenceSuccessionSet.description"
          resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules"
        >
          <reference attribute="successionID"/>
        </ResourceMessage>
      </derivation>
    </Attribute>
    <Attribute name="participant">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Number"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="related_participant">
      <Annotations/>
      <type>
        <ruleclass
          name="CaseParticipantRole"
          ruleset="CaseEntitiesRuleSet"
        />
      </type>
      <derivation>
        <reference attribute="caseParticipantRoleRecord">
          <create
            ruleclass="CaseParticipantRoleCalculator"
            ruleset="DynamicEvidenceRuleSet"
          >
            <reference attribute="participant"/>
          </create>
        </reference>
      </derivation>
    </Attribute>
  </Class>
</RuleSet>
