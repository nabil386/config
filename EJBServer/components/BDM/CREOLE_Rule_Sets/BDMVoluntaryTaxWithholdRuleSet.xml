<RuleSet name="BDMVoluntaryTaxWithholdRuleSet">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="BDMVoluntaryTaxWithhold">
		<Attribute name="amount">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="endDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="percentageValue">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="voluntaryTaxWithholdType">
			<Annotations />
			<type>
				<codetableentry table="BDMVTWType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>