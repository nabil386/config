<RuleSet name="IncomeRuleSet">
	<Class extends="DynamicEvidence"
		extendsRuleSet="DynamicEvidenceRuleSet" name="Income">
		<Attribute name="income">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="year">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<!-- BEGIN - User Story 21834 - add Attestation date -->
		<Attribute name="attestationDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<!-- END - User Story 21834 - add Attestation date -->
		<!-- BEGIN: Attestation Drop Down -->
		<Attribute name="bdmAgreeAttestation">
			<Annotations />
			<type>
				<codetableentry table="BDMAgreeAttestation" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<!-- END: Attestation Drop Down -->	
		<Attribute name="startDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<call class="curam.ca.gc.bdm.creole.impl.Statics" method="getIncomeStartDate">
					<type>
						<javaclass name="curam.util.type.Date" />
					</type>
					<arguments>
						<reference attribute="year"/>
					</arguments>
				</call>
			</derivation>
		</Attribute>
		<Attribute name="endDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<call class="curam.ca.gc.bdm.creole.impl.Statics" method="getIncomeEndDate">
					<type>
						<javaclass name="curam.util.type.Date" />
					</type>
					<arguments>
						<reference attribute="year"/>
					</arguments>
				</call>
			</derivation>
		</Attribute>
		 <!-- BEGIN: Attestee Cluster-->
		<Attribute name="attesteeCaseParticipant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_attesteeCaseParticipant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="attesteeCaseParticipant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		 <!-- END: Attestee Cluster-->
		<Attribute name="caseParticipant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>