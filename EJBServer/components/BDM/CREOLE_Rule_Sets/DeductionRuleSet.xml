<?xml version="1.0" encoding="UTF-8"?>
<!-- Feature 99661 : Implement Manage VTW Evidence -->
<RuleSet name="DeductionRuleSet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">

	<Class name="Deduction">
	
		<Attribute name="deductionID">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="deductionName">
			<type>
				<codetableentry table="DeductionName" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="maxAmount">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="maxPercentage">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="defaultAmount">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="defaultPercentage">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>

		<Attribute name="minAmount">
			<type>
				<javaclass name="Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		
		<Attribute name="recordStatus">
			<type>
				<codetableentry table="RecordStatus" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
	</Class>
</RuleSet>
    