<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMPDCBankAccountVerDataRuleSet">
  <Class
    extends="ActiveInEditEvidenceRow"
    extendsRuleSet="PropagatorRuleSet"
    name="PDCBankAccount"
  >
    <Attribute name="jointAccountInd">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Boolean"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="accountNumber">
      <Annotations/>
      <type>
        <javaclass name="java.lang.String"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="accountName">
      <Annotations/>
      <type>
        <javaclass name="java.lang.String"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="fromDate">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="accountStatus">
      <Annotations/>
      <type>
        <codetableentry table="BankAccountStatus"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="sortCode">
      <Annotations/>
      <type>
        <javaclass name="java.lang.String"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="accountType">
      <Annotations/>
      <type>
        <codetableentry table="BankAccountType"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="toDate">
      <Annotations/>
      <type>
        <javaclass name="curam.util.type.Date"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="preferredInd">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Boolean"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="description">
      <type>
        <javaclass name="curam.creole.value.Message"/>
      </type>
      <derivation>
        <ResourceMessage
          key="DynamicEvidenceSuccessionSet.description"
          resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules"
        >
          <reference attribute="successionID"/>
        </ResourceMessage>
      </derivation>
    </Attribute>
    <Attribute name="participant">
      <Annotations/>
      <type>
        <javaclass name="java.lang.Number"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="related_participant">
      <Annotations/>
      <type>
        <ruleclass
          name="CaseParticipantRole"
          ruleset="CaseEntitiesRuleSet"
        />
      </type>
      <derivation>
        <reference attribute="caseParticipantRoleRecord">
          <create
            ruleclass="CaseParticipantRoleCalculator"
            ruleset="DynamicEvidenceRuleSet"
          >
            <reference attribute="participant"/>
          </create>
        </reference>
      </derivation>
    </Attribute>
  </Class>
</RuleSet>
