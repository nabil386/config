<?xml version="1.0" encoding="UTF-8"?>
<RuleSet name="BDMConsentDeclarationRuleSet">
	<Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet"
		name="BDMConsentDeclaration">
		<Attribute name="signatureType">
			<Annotations />
			<type>
				<codetableentry table="BDMSignatureType" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="relationshipType">
			<Annotations />
			<type>
				<codetableentry table="BDMWitnessRelationship" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="witnessDateSigned">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="isWitness">
			<Annotations />
			<type>
				<javaclass name="java.lang.Boolean" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="isSigningOnBehalf">
			<Annotations />
			<type>
				<javaclass name="java.lang.Boolean" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="dateSigned">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="witness">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_witness">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="witness" />
					</create>
				</reference>
			</derivation>
		</Attribute>
		<Attribute name="participant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_participant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="participant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>