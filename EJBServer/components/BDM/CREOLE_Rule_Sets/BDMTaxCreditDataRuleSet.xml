<RuleSet name="BDMTaxCreditDataRuleSet">
	<Class extends="ActiveSuccessionSet"
		extendsRuleSet="PropagatorRuleSet" name="BDMTaxCredit">
		<Attribute name="toDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="incomeTaxOnlyPart">
			<Annotations />
			<type>
				<codetableentry table="BDMAlertsChoice" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="taxCredit">
			<Annotations />
			<type>
				<codetableentry table="BDMTaxSituation" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="numberOfdependant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="incomeTaxFromPay" >
			<Annotations />
			<type>
				<codetableentry table="BDMAlertsChoice" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="fromDate">
			<Annotations />
			<type>
				<javaclass name="curam.util.type.Date" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="nativeStatus">
			<Annotations />
			<type>
				<codetableentry table="BDMAlertsChoice" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
			<derivation>
				<ResourceMessage
					key="DynamicEvidenceSuccessionSet.description"
					resourceBundle="curam.dynamicevidence.cer.rules.InfrastructureRules">
					<reference attribute="successionID" />
				</ResourceMessage>
			</derivation>
		</Attribute>
		<Attribute name="participant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="participant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>