<?xml version="1.0" encoding="UTF-8"?>
<!-- 94324 DEV: Manage Third Party Evidence configuration -->
<RuleSet name="BDMThirdPartyContactRuleSet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">

    <Class name="BDMThirdPartyContact" extends="DynamicEvidence"
        extendsRuleSet="DynamicEvidenceRuleSet">

        <Attribute name="caseParticipantRole">
            <type>
                <javaclass name="java.lang.Number" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        
        <Attribute name="thirdPartyCaseParticipantRole">
            <type>
                <javaclass name="java.lang.Number" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="title">
            <type>
                <codetableentry table="PersonTitle" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        
        <Attribute name="individualWithinOrg">
            <type>
                <javaclass name="java.lang.String" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="program">
            <type>
                <javaclass name="java.lang.String" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="role">
            <type>
                <codetableentry table="BDMThirdPartyRole" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="roleType">
            <type>
                <codetableentry table="BDMThirdPartyRoleType" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="poaClassification">
            <type>
                <codetableentry table="BDMThirdPartyPOAClass" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="poaType">
            <type>
                <codetableentry table="BDMThirdPartyPOAType" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="relationshipToClient">
            <type>
                <codetableentry table="BDMThirdPartyRelationship" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="languagePreference">
            <type>
                <codetableentry table="BDMLanguage" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="from">
            <type>
                <javaclass name="curam.util.type.Date" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="to">
            <type>
                <javaclass name="curam.util.type.Date" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>
        
        <Attribute name="resolution">
            <type>
                <codetableentry table="BDMIssueResolution" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="bdmReceivedFrom">
            <type>
                <codetableentry table="BDMReceivedFrom" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="bdmReceivedFromCountry">
            <type>
                <codetableentry table="BDMSourceCountry" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="bdmModeOfReceipt">
            <type>
                <codetableentry table="BDMModeOfReceipt" />
            </type>
            <derivation>
                <specified />
            </derivation>
        </Attribute>

        <Attribute name="related_caseParticipantRole">
            <type>
                <ruleclass name="CaseParticipantRole"
                    ruleset="CaseEntitiesRuleSet" />
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="caseParticipantRole" />
                    </create>
                </reference>
            </derivation>
        </Attribute>

        <Attribute name="related_thirdPartyCaseParticipantRole">
            <type>
                <ruleclass name="CaseParticipantRole"
                    ruleset="CaseEntitiesRuleSet" />
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="thirdPartyCaseParticipantRole" />
                    </create>
                </reference>
            </derivation>
        </Attribute>

    </Class>

</RuleSet>