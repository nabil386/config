				<codetableentry table="BDMAgreeAttestation" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<!-- END: Attestation Drop Down -->
		<Attribute name="description">
			<type>
				<javaclass name="curam.creole.value.Message" />
			</type>
