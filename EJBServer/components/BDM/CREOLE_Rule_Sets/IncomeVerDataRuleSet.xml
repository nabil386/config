<RuleSet name="IncomeVerDataRuleSet">

	<Class extends="ActiveInEditEvidenceRow" extendsRuleSet="PropagatorRuleSet" name="Income">
		<Attribute name="income">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="year">
			<Annotations />
			<type>
				<javaclass name="java.lang.String" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="caseParticipant">
			<Annotations />
			<type>
				<javaclass name="java.lang.Number" />
			</type>
			<derivation>
				<specified />
			</derivation>
		</Attribute>
		<Attribute name="related_caseParticipant">
			<Annotations />
			<type>
				<ruleclass name="CaseParticipantRole"
					ruleset="CaseEntitiesRuleSet" />
			</type>
			<derivation>
				<reference attribute="caseParticipantRoleRecord">
					<create ruleclass="CaseParticipantRoleCalculator"
						ruleset="DynamicEvidenceRuleSet">
						<reference attribute="caseParticipant" />
					</create>
				</reference>
			</derivation>
		</Attribute>
	</Class>
</RuleSet>