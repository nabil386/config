<?xml version="1.0" encoding="UTF-8"?>
<EvidenceTypeVersion
  modifiable="true"
  relatedParticipant="participant"
>
  <Model>
    <Attributes>
      <Attribute>
        <RelatedCPAttribute
          cpRoleType="MEM"
          name="participant"
          participantType="Person"
        >
          <CPSearchType code="PRI"/>
          <CPSearchType code="MEM"/>
        </RelatedCPAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          name="title"
          volatile="true"
        >
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="PersonTitle"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          mandatory="true"
          name="firstName"
          volatile="true"
        >
          <DomainType dataType="String">
            <StringOptions
              maxSize="65"
              trimLeadingSpaces="true"
              trimTrailingSpaces="true"
            />
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          name="middleName"
          volatile="true"
        >
          <DomainType dataType="String">
            <StringOptions maxSize="65"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          mandatory="false"
          name="lastName"
          volatile="true"
        >
          <DomainType dataType="String">
            <StringOptions
              maxSize="65"
              trimLeadingSpaces="true"
              trimTrailingSpaces="true"
            />
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          name="suffix"
          volatile="true"
        >
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="PersonSuffix"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          name="initials"
          volatile="true"
        >
          <DomainType dataType="String">
            <StringOptions maxSize="10"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <DataAttribute
          mandatory="true"
          name="nameType"
          volatile="true"
        >
          <DomainType dataType="Codetable">
            <CodetableOptions codetableName="AlternateNameType"/>
          </DomainType>
        </DataAttribute>
      </Attribute>
      <Attribute>
        <CommentsAttribute name="comments"/>
      </Attribute>
    </Attributes>
    <Relationships/>
  </Model>
