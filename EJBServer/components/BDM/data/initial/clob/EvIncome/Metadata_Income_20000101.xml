              </ValidationMessageMapping>
            </ValidationMessageInformation>
          </DuplicateAttributes>
        </DuplicateValidator>
      </Validator>
    </PatternValidations>
   <CustomRulesValidation ruleSetName="BDMIncomeValidationRuleSet"/>
