  <PatternValidations>
      <Validator>
        <DuplicateValidator>
          <DuplicateAttributes conjunction="AND">
            <DuplicateAttribute name="caseParticipant"/>
            <DuplicateAttribute name="year"/>
            <ValidationMessageInformation>
              <ValidationMessageMapping>
                <ValidationMessage>               
                <parameter name="year"/>
                  <ValidationMessageResource id="ERR_INCOME_YEAR_DUPLICATE"/>                                  	
