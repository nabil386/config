<?xml version="1.0" encoding="UTF-8"?>

<ieg-script xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ieg-schema.xsd"
	finish-page="cw/FinishIEGScript.jspx"
	quit-page="cw/ExitIEGScript.jspx"
	show-sections="true"
	show-progress-bar="true"
	validate-save-and-exit="false"
	config-properties="bdm-app-config"
	>

	<identifier id="BDMLifeEventAddressUpdate" scriptversionnumber="V1" type="LifeEvent" />

	<section>
		<title id="AddressInformation.Title"><![CDATA[Address Information]]></title>
		<icon enabled-image="sample_about_you" current-image="sample_about_you" />
		<!-- BEGIN Residential ADDRESS -->
		<question-page 
			id="AddressInformation" 
			entity="Person"
			progress="0"
			show-back-button="false"
			show-exit-button="true"
			show-next-button="true" 
			show-save-exit-button="false"
			>
			<validation expression="not(isNotNull(ResidentialAddress.postalCode)) or validateCAPostalCode(ResidentialAddress.postalCode)">
				<message id="AddressInformation.ResAddr.PostalCode.Validation"><![CDATA[Please enter a valid residential postal code. For example: A1A 1A1]]></message>
			</validation>
			<set-attribute expression="Person.residentialCountry" id="ResidentialAddress.country"/>
			<set-attribute expression="Person.residentialCountry" id="IntlResAddress.country" />
			<validation expression="ValidateResidentialAddress(SearchAddress.idResidentialAddress, 
				ResidentialAddress.suiteNum, 
				ResidentialAddress.addressLine1, 
				ResidentialAddress.addressLine2, 
				ResidentialAddress.city, 
				ResidentialAddress.province, 
				ResidentialAddress.postalCode,
				Person.residentialCountry)">
				<message id="AddressInformation.ResAddr.Validation"><![CDATA[The address provided does not appear to be valid. Please review the address to confirm it is correct and click 'Next' to continue.]]></message>
			</validation>
	    	<validation expression="validateUSZipCodeLength(Person.residentialCountry,IntlResAddress.zipCode)">
				<message id="AddressInformation.ResAddr.Ziplength.Validation"><![CDATA[Zip code is invalid when country is US]]></message>
	    	</validation>
			
			<title id="AddressInformation.ResAddr.Title"><![CDATA[Residential address]]></title>
			<description id="MandatoryFieldsDescription"><![CDATA[<p>All fields are mandatory unless specified as optional.</br>Please specify your residential address information below</br></p>]]></description>
			<cluster>
				<question id="residentialCountry" mandatory="true">
					<label id="AddressInformation.Country"><![CDATA[Country]]></label>
				</question>
			</cluster>
			
			<condition expression="Person.residentialCountry==&quot;CA&quot;">
				<cluster entity="SearchAddress">
					<question id="queryResidentialAddress" mandatory="true" show-field-help="false">
						<label id="SearchAddress.ResAddr.PostalCode"><![CDATA[Please enter your residential postal (A1A 1A1) code and select your address]]></label>
						<hint-text id="SearchAddress.Hint"><![CDATA[If you cannot find a match to your address in the list, select "Address not found? Click here to enter an address manually"]]></hint-text>
						<combo-box
							key="idResidentialAddress"
							search-function="searchResidentialAddressByPostalCode"
							target-entity="ResidentialAddress"
							filter-items="true" />
					</question>
				</cluster>
			</condition>

			<!-- Residential Address fields when country is CA -->			
			<condition expression="Person.residentialCountry==&quot;CA&quot;
				and SearchAddress.idResidentialAddress!=&quot;idAddress&quot;">
				<cluster entity="ResidentialAddress">
					<question id="suiteNum" mandatory="false">
						<label id="AddressInformation.AptNum"><![CDATA[Unit/Apt/Suite number]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [10]}]]></mask-format></layout>
					</question>
					<question id="addressLine1" mandatory="true">
						<label id="AddressInformation.StreetNum"><![CDATA[Street Number]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
					<question id="addressLine2" mandatory="true"
						read-only-expression="SearchAddress.idResidentialAddress!=&quot;-2&quot;">
						<label id="AddressInformation.StreetName"><![CDATA[Street Name]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="city" mandatory="true"
						read-only-expression="SearchAddress.idResidentialAddress!=&quot;-2&quot;">
						<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="province" mandatory="true"
						read-only-expression="SearchAddress.idResidentialAddress!=&quot;-2&quot;">
						<label id="AddressInformation.Prov"><![CDATA[Province or Territory]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="postalCode" mandatory="true" 
						read-only-expression="SearchAddress.idResidentialAddress!=&quot;-2&quot;">
						<label id="AddressInformation.ResAddr.PostalCode"><![CDATA[Postal code (residential)]]></label>
						<layout><class-names>wds-js-input-mask-postal-code</class-names></layout>
					</question>
				</cluster>
			</condition>

			<!-- COMMON Residential Address fields for US and All else -->
			<condition expression="Person.residentialCountry!=&quot;CA&quot;" >
				<cluster entity="IntlResAddress" grouping-id="resAddressGroup">
					<question id="suiteNum" mandatory="false">
						<label id="AddressInformation.AptNum"><![CDATA[Unit/Apt/Suite number]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [10]}]]></mask-format></layout>
					</question>
				</cluster>
			</condition>
			
			<!-- Residential Address fields when country is US-->
			<condition expression="Person.residentialCountry==&quot;US&quot;" >
				<cluster entity="IntlResAddress" grouping-id="resAddressGroup">
					<question id="addressLine1" mandatory="true">
						<label id="AddressInformation.StreetNum"><![CDATA[Street Number]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
					<question id="addressLine2" mandatory="true">
						<label id="AddressInformation.StreetName"><![CDATA[Street Name]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="city" mandatory="true">
						<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="state" mandatory="true">
						<label id="AddressInformation.State"><![CDATA[State]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="zipCode" mandatory="true">
						<label id="AddressInformation.ResAddr.ZipCode"><![CDATA[Zip Code]]></label>
						<layout><mask-format><![CDATA[{"delimiter":"-", "delimiterLazyShow":"true", "blocks":[5,4]}]]></mask-format></layout>
					</question>
				</cluster>
			</condition>

			<!-- Residential Address fields when country is NOT US or CA-->	
			<condition expression="Person.residentialCountry!=&quot;CA&quot;
				and Person.residentialCountry!=&quot;US&quot;
				and SearchAddress.idResidentialAddress!=&quot;idAddress&quot;" >
				<cluster entity="IntlResAddress" grouping-id="resAddressGroup">
					<question id="addressLine1" mandatory="true">
						<label id="AddressInformation.StreetNumOne"><![CDATA[Street 1]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
					<question id="addressLine2" mandatory="false">
						<label id="AddressInformation.StreetNameTwo"><![CDATA[Street 2]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="city" mandatory="true">
						<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="addressLine4" mandatory="true">
						<label id="AddressInformation.Intl.State"><![CDATA[State/Province/Region]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="zipCode" mandatory="true">
						<label id="AddressInformation.ResAddr.ZipCode"><![CDATA[Zip Code]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [10]}]]></mask-format></layout>
					</question>
				</cluster>
			</condition>

			<condition expression="(Person.residentialCountry==&quot;CA&quot; 
				and SearchAddress.idResidentialAddress!=&quot;idAddress&quot;)
				or Person.residentialCountry!=&quot;CA&quot;" >
				<cluster entity="Person">
					<question id="resMail" mandatory="true">
						<label id="AddressInformation.ResAddr.MailingRed"><![CDATA[Is your mailing address different from your residential address?]]></label>
						<layout><type>radio</type></layout>
					</question>
				</cluster>
			</condition>
		</question-page>
		<!-- END Residential ADDRESS -->

		<!-- BEGIN MAILING ADDRESS -->
		<condition expression="Person.resMail==&quot;YN1&quot;">
			<question-page 
				id="AddressMailingInformation" 
				entity="Person"
				progress="0"
				show-back-button="true" 
				show-exit-button="true"
				>
				
				<set-attribute expression="Person.mailingCountry" id="MailingAddress.country"/>
				<set-attribute expression="Person.mailingCountry" id="IntlMailAddress.country"/>
				<validation expression="not(isNotNull(MailingAddress.postalCode)) or validateCAPostalCode(MailingAddress.postalCode)">
					<message id="AddressInformation.MailAddr.PostalCode.Validation"><![CDATA[Please enter a valid mailing postal code.]]></message>
				</validation>
				<validation expression="ValidateMailingAddress(Person.mailingCountry, MailingAddress.addressLine3, MailingAddress.addressLine1, MailingAddress.addressLine2, MailingAddress.suiteNum, IntlMailAddress.addressLine3, IntlMailAddress.addressLine1, IntlMailAddress.addressLine2, IntlMailAddress.suiteNum)">
					<message id="AddressInformation.MailAddr.pobox.Validation"><![CDATA[Please enter a Street name, Street Number, and Unit/Apt/Suite number if PO Box is not supplied.]]></message>
				</validation>
				<validation expression="ValidateAddress(SearchAddress.idMailingAddress, MailingAddress.suiteNum, MailingAddress.addressLine1,
					MailingAddress.addressLine2, MailingAddress.city, MailingAddress.province, MailingAddress.postalCode, Person.mailingCountry)">
					<message id="AddressInformation.MailAddr.Validation"><![CDATA[The address provided does not appear to be valid. Please review the address to confirm it is correct and click 'Next' to continue.]]></message>
				</validation>
		    	<validation expression="validateUSZipCodeLength(Person.mailingCountry,IntlMailAddress.zipCode)">
					<message id="AddressInformation.MailAddr.Ziplength.Validation"><![CDATA[Zip code is invalid when country is US]]></message>
				</validation>
				
				<title id="AddressInformation.MailAddr.Title"><![CDATA[Mailing address]]></title>
				<description id="MandatoryFieldsDescription"><![CDATA[<p>All fields are mandatory unless specified as optional.</br>Please specify your mailing address information below</p>]]></description>
				<cluster>
					<question id="mailingCountry" mandatory="true" >
						<label id="AddressInformation.Country"><![CDATA[Country]]></label>
					</question>
				</cluster>

				<condition expression="Person.mailingCountry==&quot;CA&quot;">
					<cluster entity="SearchAddress">
						<question id="queryMailingAddress" mandatory="true" show-field-help="false">
							<label id="SearchAddress.MailAddr.PostalCode"><![CDATA[Please enter your mailing postal code (A1A 1A1) and select your address]]></label>
							<hint-text id="SearchAddress.Hint"><![CDATA[If you cannot find a match to your address in the list, select "Address not found? Click here to enter an address manually"]]></hint-text>
							<combo-box
								key="idMailingAddress"
								search-function="searchMailingAddressByPostalCode"
								target-entity="MailingAddress"
								filter-items="true"/>
						</question>
					</cluster>
				</condition>

				<!-- Mailing Address fields when country is CA -->			
				<condition expression="Person.mailingCountry==&quot;CA&quot;
					and SearchAddress.idMailingAddress!=&quot;idAddress&quot;">
					<cluster entity="MailingAddress">
						<question id="addressLine3" mandatory="false">
							<label id="AddressInformation.POBox"><![CDATA[PO Box/Rural Route/General Delivery]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="suiteNum" mandatory="false">
							<label id="AddressInformation.AptNum"><![CDATA[Unit/Apt/Suite number]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [10]}]]></mask-format></layout>
					</question>
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNum"><![CDATA[Street Number]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
						<question id="addressLine2" mandatory="false"
							read-only-expression="SearchAddress.idMailingAddress!=&quot;-2&quot;" >
							<label id="AddressInformation.StreetName"><![CDATA[Street Name]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="city" mandatory="true"
							read-only-expression="SearchAddress.idMailingAddress!=&quot;-2&quot;" >
							<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="province" mandatory="true"
							read-only-expression="SearchAddress.idMailingAddress!=&quot;-2&quot;" >
							<label id="AddressInformation.Prov"><![CDATA[Province or Territory]]></label>
					</question>
						<question id="postalCode" mandatory="true"
							read-only-expression="SearchAddress.idMailingAddress!=&quot;-2&quot;" >
							<label id="AddressInformation.MailAddr.PostalCode"><![CDATA[Postal code (residential)]]></label>
							<layout><class-names>wds-js-input-mask-postal-code</class-names></layout>
					</question>
					</cluster>
				</condition>
	
				<!-- COMMON Mailing Address fields for US and All else -->
				<condition expression="Person.mailingCountry!=&quot;CA&quot;" >
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine3" mandatory="false">
							<label id="AddressInformation.POBox"><![CDATA[PO Box/Rural Route/General Delivery]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="suiteNum" mandatory="false">
							<label id="AddressInformation.AptNum"><![CDATA[Unit/Apt/Suite number]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [10]}]]></mask-format></layout>
					</question>
					</cluster>
				</condition>
				
				<!-- Mailing Address fields when country is US-->
				<condition expression="Person.mailingCountry==&quot;US&quot;" >
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNum"><![CDATA[Street Number]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
						<question id="addressLine2" mandatory="false">
							<label id="AddressInformation.StreetName"><![CDATA[Street Name]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="city" mandatory="true">
							<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="state" mandatory="true">
							<label id="AddressInformation.State"><![CDATA[State]]></label>
					</question>
					<question id="zipCode" mandatory="true">
						<label id="AddressInformation.ZipCode"><![CDATA[Zip Code]]></label>
						<layout><mask-format><![CDATA[{"delimiter":"-", "delimiterLazyShow":"true", "blocks":[5,4]}]]></mask-format></layout>
					</question>
					</cluster>
				</condition>
	
				<!-- Mailing Address fields when country is NOT US or CA-->	
				<condition expression="Person.mailingCountry!=&quot;CA&quot;
					and Person.mailingCountry!=&quot;US&quot;
					and SearchAddress.idMailingAddress!=&quot;idAddress&quot;">
					<cluster entity="IntlMailAddress" grouping-id="100">
						<question id="addressLine1" mandatory="false">
							<label id="AddressInformation.StreetNumOne"><![CDATA[Street 1]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [6]}]]></mask-format></layout>
					</question>
						<question id="addressLine2" mandatory="false">
							<label id="AddressInformation.StreetNameTwo"><![CDATA[Street 2]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="city" mandatory="true">
							<label id="AddressInformation.City"><![CDATA[City, Town or Village]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
						<question id="addressLine4" mandatory="true">
							<label id="AddressInformation.Intl.State"><![CDATA[State/Province/Region]]></label>
							<layout><mask-format><![CDATA[{"delimiter": "", "blocks": [30]}]]></mask-format></layout>
					</question>
					<question id="zipCode" mandatory="true">
						<label id="AddressInformation.ZipCode"><![CDATA[Zip Code]]></label>
						<layout><mask-format><![CDATA[{"delimiter": "", "blocks":[10]}]]></mask-format></layout>
					</question>
					</cluster>
				</condition>
			</question-page>
		</condition>
	</section>
	<!-- END Address Change page -->

	<!-- BEGIN SUMMARY PAGE -->
	<section>
		<title id="Summary.Title"><![CDATA[Summary of changes]]></title>
		<summary-page id="SummaryPage" 
			entity="Person" 
			show-back-button="true"
			show-exit-button="true"
			show-next-button="true" 
			show-save-exit-button="false"
			>
			<next-button-label id="NextButton.Label"><![CDATA[Submit]]></next-button-label>
			<!-- ADDRESS INFO SUMMARY - BEGIN -->
			<cluster entity="ResidentialAddress" grouping-id="Address">
				<edit-link start-page="AddressInformation" />
				<display-text id="AddressInformation.ResAddr.AddrTitle"><![CDATA[<h3>Residential address</h3>]]></display-text>
            </cluster>
			<!-- Residential Address CA -->
			<condition expression="ResidentialAddress.country==&quot;CA&quot;">
				<cluster entity="ResidentialAddress" grouping-id="Address">
					<question id="suiteNum" ><label id="AddressInformation.ResAddr.AptNum"><![CDATA[Residential unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.ResAddr.StreetNum"><![CDATA[Residential street number]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.ResAddr.StreetName"><![CDATA[Residential street name]]></label></question>
					<question id="city" ><label id="AddressInformation.ResAddr.City"><![CDATA[Residential City, Town or Village]]></label></question>
					<question id="province" ><label id="AddressInformation.ResAddr.Prov"><![CDATA[Residential Province or Territory]]></label></question>
	                <question id="postalCode" ><label id="AddressInformation.ResAddr.PostalCode"><![CDATA[Residential postal code]]></label></question>
					<question id="country" ><label id="AddressInformation.ResAddr.Country"><![CDATA[Residential country]]></label></question>				
				</cluster>
			</condition>
			<!-- Residential Address US -->
			<condition expression="ResidentialAddress.country==&quot;US&quot;">
				<cluster entity="IntlResAddress" grouping-id="Address">
					<question id="suiteNum" ><label id="AddressInformation.ResAddr.AptNum"><![CDATA[Residential unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.ResAddr.StreetNum"><![CDATA[Residential street number]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.ResAddr.StreetName"><![CDATA[Residential street name]]></label></question>
					<question id="city" ><label id="AddressInformation.ResAddr.City"><![CDATA[Residential City, Town or Village]]></label></question>
					<question id="state" ><label id="AddressInformation.ResAddr.State"><![CDATA[Residential State]]></label></question>
					<question id="zipCode" ><label id="AddressInformation.ResAddr.ZipCode"><![CDATA[Residential Zip Code]]></label></question>
					<question id="country" ><label id="AddressInformation.ResAddr.Country"><![CDATA[Residential country]]></label></question>
				</cluster>
			</condition>
			<!-- Residential Address INTL -->
			<condition expression="ResidentialAddress.country!=&quot;CA&quot; and ResidentialAddress.country!=&quot;US&quot;">
				<cluster entity="IntlResAddress" grouping-id="Address">
					<question id="suiteNum" ><label id="AddressInformation.ResAddr.AptNum"><![CDATA[Residential unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.ResAddr.StreetNumOne"><![CDATA[Residential street 1]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.ResAddr.StreetNameTwo"><![CDATA[Residential street 2]]></label></question>
					<question id="city" ><label id="AddressInformation.ResAddr.City"><![CDATA[Residential City, Town or Village]]></label></question>
					<question id="addressLine4" ><label id="AddressInformation.ResAddr.Intl.State"><![CDATA[Residential State/Province/Region]]></label></question>
					<question id="zipCode" ><label id="AddressInformation.ResAddr.ZipCode"><![CDATA[Residential Zip Code]]></label></question>
					<question id="country" ><label id="AddressInformation.ResAddr.Country"><![CDATA[Residential country]]></label></question>
				</cluster>
			</condition>
			<cluster entity="Person" grouping-id="Address">
				<question id="resMail" ><label id="AddressInformation.ResAddr.MailingRed"><![CDATA[Is your mailing address different from your residential address?]]></label></question>
			</cluster>
			
			<!-- Mailing Address-->
			<condition expression="Person.resMail==&quot;YN1&quot;">
				<cluster entity="MailingAddress" grouping-id="MailingAddress">
					<display-text id="AddressInformation.MailAddr.AddrTitle"><![CDATA[<h3>Mailing address</h3>]]></display-text>
	             </cluster>
	        </condition>
	        <!-- Mailing Address CA -->
			<condition expression="Person.resMail==&quot;YN1&quot; and Person.mailingCountry==&quot;CA&quot;">
				<cluster entity="MailingAddress" grouping-id="MailingAddress">
					<question id="addressLine3" ><label id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></label></question>
					<question id="suiteNum" ><label id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.MailAddr.StreetNum"><![CDATA[Mailing street number]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.MailAddr.StreetName"><![CDATA[Mailing street name]]></label></question>
					<question id="city" ><label id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></label></question>
					<question id="province" ><label id="AddressInformation.MailAddr.Prov"><![CDATA[Mailing Province or Territory]]></label></question>
	                <question id="postalCode" ><label id="AddressInformation.MailAddr.PostalCode"><![CDATA[Mailing postal code]]></label></question>
	                <question id="country" ><label id="AddressInformation.MailAddr.Country"><![CDATA[ Mailing country]]></label></question>
				</cluster>
			</condition>
			<!-- Mailing Address US -->
			<condition expression="Person.resMail==&quot;YN1&quot; and Person.mailingCountry==&quot;US&quot;">
				<cluster entity="IntlMailAddress" grouping-id="MailingAddress">
					<question id="addressLine3" ><label id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></label></question>
					<question id="suiteNum" ><label id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.MailAddr.StreetNum"><![CDATA[Mailing street number]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.MailAddr.StreetName"><![CDATA[Mailing street name]]></label></question>
					<question id="city" ><label id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></label></question>
					<question id="state" ><label id="AddressInformation.MailAddr.State"><![CDATA[Mailing State]]></label></question>
					<question id="zipCode" ><label id="AddressInformation.MailAddr.ZipCode"><![CDATA[Mailing Zip Code]]></label></question>
					<question id="country" ><label id="AddressInformation.MailAddr.Country"><![CDATA[Mailing country]]></label></question>
				</cluster>
			</condition>
			<!-- Mailing Address INTL -->
			<condition expression="Person.resMail==&quot;YN1&quot; and Person.mailingCountry!=&quot;CA&quot; and Person.mailingCountry!=&quot;US&quot;">
				<cluster entity="IntlMailAddress" grouping-id="MailingAddress">
					<question id="addressLine3" ><label id="AddressInformation.MailAddr.POBox"><![CDATA[Mailing PO Box/Rural Route/General Delivery]]></label></question>
					<question id="suiteNum" ><label id="AddressInformation.MailAddr.AptNum"><![CDATA[Mailing unit/apt/suite number]]></label></question>
					<question id="addressLine1" ><label id="AddressInformation.MailAddr.StreetNumOne"><![CDATA[Mailing street 1]]></label></question>
					<question id="addressLine2" ><label id="AddressInformation.MailAddr.StreetNameTwo"><![CDATA[Mailing street 2]]></label></question>
					<question id="city" ><label id="AddressInformation.MailAddr.City"><![CDATA[Mailing City, Town or Village]]></label></question>
					<question id="addressLine4" ><label id="AddressInformation.MailAddr.Intl.State"><![CDATA[Mailing State/Province/Region]]></label></question>
					<question id="zipCode" ><label id="AddressInformation.MailAddr.ZipCode"><![CDATA[Mailing Zip Code]]></label></question>
					<question id="country" ><label id="AddressInformation.MailAddr.Country"><![CDATA[Mailing country]]></label></question>
				</cluster>
			</condition>
			<!-- ADDRESS INFO SUMMARY - END -->
		</summary-page>
	</section>
	<!-- END SUMMARY PAGE -->

</ieg-script>