    page-id="Organization_cancelCaseBookmark" open-as="modal" />

  <mc:menu-separator id="sep5" />

  <mc:menu-item id="Meeting" title="MenuItem.Title.Meeting"
    tooltip="MenuItem.Tooltip.Meeting"
    page-id="CalendarMeeting_createMeeting" open-as="modal" />
  <mc:menu-separator id="sep6" />

  <mc:menu-item id="NewCase" title="MenuItem.Title.NewCase"
    tooltip="MenuItem.Tooltip.NewCase" page-id="DefaultIC_selectMember"
    open-as="modal" />
  <mc:menu-separator id="sep7" />

  <mc:menu-item id="Close" title="MenuItem.Title.Close"
    tooltip="MenuItem.Tooltip.Close" page-id="IntegratedCase_close"
    open-as="modal" />

</mc:menu-bar>
