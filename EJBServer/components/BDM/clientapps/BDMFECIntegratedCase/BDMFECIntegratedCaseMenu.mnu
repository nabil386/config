<?xml version="1.0" encoding="UTF-8"?>

<mc:menu-bar
  xmlns:mc="http://www.curamsoftware.com/curam/util/client/menubar-config"
  id="BDMFECIntegratedCaseMenu">

  <mc:loader-registry>
    <mc:loader
      class="curam.core.sl.tab.impl.CaseHomeMenuLoader" />
  </mc:loader-registry>

  <mc:menu-item id="Edit" title="MenuItem.Title.Edit"
    tooltip="MenuItem.Tooltip.Edit"
    page-id="BDMFEC_modifyIntegratedCase" open-as="modal" />

  <mc:menu-separator id="sep1" />

  <!-- Task 102943: DEV: Remove Notes functionality :Set visibility to false JP -->
  <mc:menu-item id="Note" title="MenuItem.Title.Note"
    tooltip="MenuItem.Tooltip.Note" page-id="Case_createNote1"
    open-as="modal" visible="false"/>

  <mc:menu-separator id="sep2" />

  <mc:menu-item id="ApplyEvidenceChanges"
    title="MenuItem.Title.ApplyEvidenceChanges"
    tooltip="MenuItem.Tooltip.ApplyEvidenceChanges"
    page-id="Evidence_applyChanges1" open-as="modal" />

  <mc:menu-separator id="sep3" />

  <mc:menu-item id="Member" title="MenuItem.Title.Member"
    tooltip="MenuItem.Tooltip.Member" page-id="Case_createCaseMember"
    open-as="modal" />

  <mc:menu-separator id="sep4" />

  <mc:menu-item dynamic="true" id="Bookmark"
