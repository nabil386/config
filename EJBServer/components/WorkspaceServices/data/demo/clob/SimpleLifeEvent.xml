<ieg-script
  config-properties="hcr-ieg-config"
  finish-page="cw/FinishLifeEventScript.jspx"
  highlight-validation="false"
  quit-page="cw/QuitLifeEventScript.jspx"
  show-progress-bar="false"
  show-sections="true"
  validate-save-and-exit="true"
>
  <identifier
    id="SimpleLifeEvent"
    scriptversionnumber="V1"
    type="LifeEvent"
  />
  <section>
    <title id="GettingStartedSection.Title"/>
    <icon
      current-image="get_started_enabled.png"
      disabled-image="get_started_disabled.png"
      enabled-image="get_started_enabled.png"
    />
    <question-page
          entity="Person"
          id="AddPhoneNumberPage"
          progress="0"
          set-focus="true"
          show-back-button="false"
          show-exit-button="false"
          show-next-button="true"
          show-save-exit-button="false"
          >
          <title id="AddPhoneNumberPage.Title">Enter Phone Number</title>
          <cluster>
            <layout>
              <label-width>0</label-width>
            </layout>
            <display-text id="AddPhoneNumberPage.PageText"/>
          </cluster>
          <cluster>
            <question
              control-question="false"
              id="phoneNumber"
              mandatory="true"
              >
              <label id="AddPhoneNumberPage.PhoneNumber.Label"/>
            </question>
            <question
              control-question="false"
              id="fromDate"
              mandatory="true"
              >
              <label id="AddPhoneNumberPage.StartDate.Label"/>
            </question>
          </cluster>
      </question-page>
  </section>
</ieg-script>
