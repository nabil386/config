<RuleSet name="LEPhoneNumberRuleSet">
    <Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet" name="LEPhoneNumber">
        <Attribute name="fromDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        
        <Attribute name="phoneNumber">
            <Annotations/>
            <type>
                <javaclass name="java.lang.String"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        
        <Attribute name="toDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        
        <Attribute name="participant">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        
        <Attribute name="related_participant">
            <Annotations/>
            <type>
                <ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet"/>
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator" ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="participant"/>
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>