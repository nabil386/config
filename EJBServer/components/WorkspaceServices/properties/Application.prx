    <type>BOOLEAN</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>CWCONF</category>
    <locales>
      <locale language="en">
        <display-name>curam.citizenworkspace.cache.dsmappings.by.transactionID</display-name>
        <description>Setting that indicates whether Datastore Mappings should be cached by transaction ID.</description>
      </locale>
