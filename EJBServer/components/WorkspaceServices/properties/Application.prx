  <property name="curam.workspaceservices.application.processing.logging.on" dynamic="yes" constant="ENV_WORKSPACESERVICES_APPLICATION_PROCESSING_LOGGING_ON">
    <type>BOOLEAN</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>WSSVCS</category>
    <locales>
      <locale language="en">
        <display-name>Application processing (including data mapping) logging</display-name>
        <description>Determines whether or not additional logging entries should be added to the system logs during application processing.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.workspaceservices.disable.dyn.ev.val.inital.int.app.processing" dynamic="yes" constant="ENV_WORKSPACESERVICES_DISABLE_DYN_EV_VALIDATION_INITIAL_INT_APP_PROCESSING">
    <type>BOOLEAN</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>WSSVCS</category>
    <locales>
      <locale language="en">
        <display-name>Disable dynamic evidence validations for initial intake application processing</display-name>
        <description>Setting this property to true disables all dynamic evidence validations during the inital intake application processing. During this processing the system created the (prospect) person(s) and case(s).</description>
      </locale>
    </locales>
  </property>
  <property name="curam.workspaceservices.disable.dyn.val.ev.intake.ev.mapping" dynamic="yes" constant="ENV_WORKSPACESERVICES_DISABLE_DYN_EV_VALIDATION_INTAKE_APP_EV_MAPPING">
    <type>BOOLEAN</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>WSSVCS</category>
    <locales>
      <locale language="en">
        <display-name>Disable dynamic evidence validations for intake application evidence mapping</display-name>
        <description>Setting this property to true disables all dynamic evidence validations during the evidence mapping phase of intake application processing. During this processing the system creates evidence using the defined mappings.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.creole.fail_on_data_conversion_error" dynamic="yes" constant="ENV_DATA_STORE_RULE_OBJECT_RETRIEVER_FAIL_ON_ERROR">
