  <property name="curam.intake.registerAsProspect" dynamic="yes" constant="ENV_REGISTER_CLIENT_AS_PROSPECT">
    <type>STRING</type>
    <value>YES</value>
    <default-value>YES</default-value>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>Register as Prospect Person</display-name>
        <description>Setting that indicates whether clients registered as part of an intake process should be registered as prospects. If this is set to false then a new client is registered as a person.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.intake.use.resilience" dynamic="yes" constant="ENV_INTAKE_USE_RESILIENT_FLOW">
    <type>STRING</type>
    <value>NO</value>
    <default-value>NO</default-value>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>Intake - use resilient flow</display-name>
        <description>Determines whether the resilient version of the intake flow is used. The resilient flow can route validation errors to a manual task for resolution by a human operator. Note, enabling this flow changes the order in which the IntakeApplicationEvents.preMapDataToCuram() call is made to after case creation but before evidence mapping.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.intake.programNotUsingApplicationCase" dynamic="yes" constant="ENV_PROGRAMS_NOT_USING_APPLICATION_CASE">
    <type>STRING</type>
    <value/>
    <default-value/>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>Programs not using an application case</display-name>
        <description>This property holds a comma separated list of the reference numbers of programs which will not use 6.0.5 application case(intake) processing. These programs will be processed using 6.0 common intake processing.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.holdingcase.creation.enabled" dynamic="yes" constant="ENV_HOLDING_CASE_CREATION_ENABLED">
    <type>STRING</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>curam.holdingcase.creation.enabled</display-name>
        <description>Creates a holding case while creating a linked account if this property is set to true.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.workspaceservices.ssn.treatasprimary" dynamic="yes" constant="ENV_TREAT_SSN_AS_PRIMARY_ALTERNATEID">
    <type>STRING</type>
    <value>YES</value>
    <default-value>YES</default-value>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>Treat SSN as Primary Alternate ID</display-name>
        <description>Setting that indicates whether a client's SSN should be treated as the primary alternate ID during intake.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.intake.pdftemplate.iegsummarypagelayout.scriptexcludelist" dynamic="yes" constant="ENV_INTAKE_APPLICATION_SUMMARY_PDF_TEMPLATE_EXCLUDED_SCRIPT_LIST">
    <type>STRING</type>
    <value/>
    <default-value/>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>IEG script list excluded from Intake Application Summary PDF Template generation</display-name>
        <description>Use this property to exclude IEG script from generating intake application summary PDF template. This is comma-separated list of IEG script identifiers where each identifier is vertical bar ('|') delimited item consisting of script Id, optional version number and optional type. For example "HealthCareCW,IntakeScriptCW|V2,CustomScript|V1|Screening" variable contains 3 script items where first item contains script Id only, second item contains script Id and version number, and third item contains script Id, version number and script type.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.intake.enableCustomConcernRoleMapping" dynamic="yes" constant="ENV_CUSTOM_CONCERN_ROLE_MAPPING_ENABLED">
    <type>BOOLEAN</type>
    <value>false</value>
    <default-value>false</default-value>
    <category>INTAKE</category>
    <locales>
      <locale language="en">
        <display-name>curam.intake.enableCustomConcernRoleMapping</display-name>
        <description>Setting that indicates whether entities registered as part of an intake process should be mapped to a concern role by a custom mapping strategy.</description>
      </locale>
    </locales>
  </property>
