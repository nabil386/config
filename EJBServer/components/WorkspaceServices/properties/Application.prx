    <category>CREOLE</category>
    <locales>
      <locale language="en">
        <display-name>Datastore rule object retriever fail on error</display-name>
        <description>If data conversion errors are detected when Curam Express Rules loads data from the data store into rule classes, whether to halt processing or log the error and carry on.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.creole.log_rule_object_creation" dynamic="yes" constant="ENV_DATA_STORE_LOG_RULE_OBJECT_CREATION">
    <type>STRING</type>
    <value>NO</value>
    <default-value>NO</default-value>
