<codetables package="curam.codetable">
  <codetable
    java_identifier="REVERIFICATIONMODE"
    name="ReverificationMode"
  >
    <displaynames>
      <locale language="en">Reverification Mode</locale>
    </displaynames>
    <code
      default="true"
      java_identifier="REVERIFYALWAYS"
      status="ENABLED"
      value="RV1"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Reverify Always</description>
        <annotation/>
      </locale>
    </code>
    <code
      default="false"
      java_identifier="REVERIFYIFCHANGED"
      status="ENABLED"
      value="RV2"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Reverify If Changed</description>
        <annotation/>
      </locale>
    </code>


    <code
      default="false"
      java_identifier="NEVERREVERIFY"
      status="ENABLED"
      value="RV3"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Never Reverify</description>
        <annotation/>
      </locale>
    </code>
  </codetable>
</codetables>
