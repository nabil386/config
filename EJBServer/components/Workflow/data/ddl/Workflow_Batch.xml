<?xml version="1.0" encoding="UTF-8"?>

<root>
  <!--Batch Declarations-->
  <batches>
    <batch process="ProcessDefinitionImportDirectory" operation="importProcessesFromDir" application="curam">
      <parameter name="fileName" type="WORKFLOW_FILENAME"/>
      <parameter name="overwrite" type="WORKFLOW_IMPORT_OVERWRITE"/>
    </batch>
    <batch process="ProcessDefinitionImportFile" operation="importProcessFromFile" application="curam">
      <parameter name="fileName" type="WORKFLOW_FILENAME"/>
      <parameter name="overwrite" type="WORKFLOW_IMPORT_OVERWRITE"/>
    </batch>
  </batches>
</root>
