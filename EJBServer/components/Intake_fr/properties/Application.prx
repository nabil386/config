  <property name="curam.intake.default.application.reassignment.notification.strategy.enable" dynamic="yes" constant="ENV_DEFAULT_APPLICATION_REASSIGNMENT_NOTIFICATION_STRATEGY"> 
    <type>STRING</type>  
