<!-- The Intake Module State code table contains the set of states that an -->
<!-- Intake Module may transition through during its life cycle. This is a -->
<!-- finite state machine and should not be administered. -->
<!-- TODO: Revisit and decide if the use of a code table is appropriate or -->
<!-- not given the inability to administer the values at runtime -->
<codetables package="curam.intake.codetable"> 
  <codetable java_identifier="INTAKEMODULESTATE" name="IntakeModuleState"> 
    <displaynames> 
      <locale language="fr">Etat du module d'admission</locale> 
    </displaynames>  
    <code default="false" java_identifier="AVAILABLE" status="ENABLED" value="IMS19001"> 
      <locale language="fr" sort_order="0"> 
        <description>Disponible</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="COMPLETE" status="ENABLED" value="IMS19002"> 
      <locale language="fr" sort_order="0"> 
        <description>Terminé</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="INPROGRESS" status="ENABLED" value="IMS19003"> 
      <locale language="fr" sort_order="0"> 
        <description>En cours</description>  
        <annotation/> 
      </locale> 
    </code>  
    <code default="false" java_identifier="CREATED" status="ENABLED" value="IMS19004"> 
      <locale language="fr" sort_order="0"> 
        <description>Créé</description>  
        <annotation/> 
      </locale> 
    </code> 
  </codetable> 
</codetables>
