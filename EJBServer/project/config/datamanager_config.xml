	<entry
      base="basedir"
      name="components/BDM/data/initial/handcraftedscripts/TaxRates/TAXRATES _2022.sql"
      type="sql"
    />
  </target>


  <target name="demo">
    <entry
      base="basedir"
      name="build/datamanager/data/demo"
      type="dmx"
    />
  </target>


  <target name="test">
    <entry
      base="basedir"
      name="build/datamanager/data/test"
      type="dmx"
    />
  </target>


  <target name="constraints">
    <entry
      base="basedir"
      name="build/ddl/Merged_ForeignKeys.xml"
