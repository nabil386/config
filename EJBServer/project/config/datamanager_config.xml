  <target name="indices">
    <entry
      base="basedir"
      name="build/ddl/Merged_PrimaryKeys.xml"
      type="xml"
    />
    <entry
      base="basedir"
      name="build/ddl/Merged_Indices.xml"
      type="xml"
    />
    <entry
      base="basedir"
      name="build/ddl/Merged_UniqueConstraints.xml"
      type="xml"
    />
  </target>


  <target name="extracteddata">
    <entry
      base="basedir"
      name="build/dataextractor"
      type="dmx"
    />
  </target>

  <target name="handcraftedscripts_NONPROD">
   <entry
      base="basedir"
      name="components/BDM/data/initial/handcraftedscripts/localusers.sql"
      type="sql"
    />
	<entry
      base="basedir"
      name="components/BDMOAS/data/initial/handcraftedscripts/BankAccount.sql"
      type="sql"
    />	
  </target>
  
  <compositetarget name="all">
    <subtarget name="tables"/>
    <subtarget name="indices"/>
    <subtarget name="initial"/>
