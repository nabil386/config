<?xml version="1.0" encoding="UTF-8"?>
<datamanager>


  <target name="tables">
    <entry
      base="basedir"
      name="build/ddl/Merged_Tables.xml"
      type="xml"
    />
  </target>


  <target name="initial">
    <entry
