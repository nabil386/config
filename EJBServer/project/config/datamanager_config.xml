    />
    <entry
      base="basedir"
      name="build/ddl/Merged_Batch.xml"
      type="xml"
    />


    <entry
      base="basedir"
      name="build/datamanager/data/initial/"
      type="dmx"
    />
    <entry
      base="basedir"
      name="build/svr/gen/int/rules"
      type="dmx"
    />
    <entry
      base="basedir"
