  <renderers>
    <renderer mime="application/pdf">
       <fonts>
          <directory>fonts</directory>
          <font embed-url="fonts/IBMPlexSans-Regular.ttf">
            <font-triplet name="IBMPlexSans" style="normal" weight="normal"/>
          </font>
          <font embed-url="fonts/IBMPlexSans-SemiBold.ttf">
            <font-triplet name="IBMPlexSans" style="normal" weight="bold"/>
          </font>
       </fonts>
    </renderer>
  </renderers>

 </fop>

