<server description="Server host configuration">
  <cdi12 enableImplicitBeanArchives="false"/>
  <httpSession
    cloneId="${env.HOSTNAME}"
    cookieHttpOnly="true"
    cookieSecure="true"
    idLength="28"
    idReuse="true"
    invalidateOnUnauthorizedSessionRequestException="false" 
    invalidationTimeout="30m"
    maxInMemorySessionCount="1000"
    securityIntegrationEnabled="false"
    storageRef="SessionDB"
   />

  <jndiEntry jndiName="curamejb/ApplicationName" value="Curam" />
  <jndiEntry jndiName="curamejb/ModuleName" value="coreinf-ejb" />
  <jndiEntry jndiName="curamejb/CoreInfModuleName" value="coreinf-ejb" />
  <jndiEntry jndiName="curamejb/ImplementationModuleName" value="implementation" />
</server>