<messages package="curam.message">
  <message name="ERR_PM_BADFACTORYCLASS">
    <locale language="en">Pull mapper: a factory class %1s has been specified that cannot be instantiated.</locale>
  </message>  

  <message name="ERR_PM_BADKEYCLASS">
    <locale language="en">Pull mapper: a key struct class %1s has been specified that cannot be instantiated.</locale>
  </message>  

  <message name="ERR_PM_BADKEYCLASSFIELD">
    <locale language="en">Pull mapper: a key struct class %1s has been specified that does not have an accessible datetime field.</locale>
  </message>  

  <message name="ERR_PM_ENTITYNOSCANMETHOD">
    <locale language="en">Pull mapper: en entity class %1s has been specified that does not have an accessible searchByLastwritten method.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYCLASS">
    <locale language="en">Pull mapper: an entity class %1s has been specified that cannot be instantiated.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYKEYCLASS">
    <locale language="en">Pull mapper: a key class %1s has been specified that cannot be instantiated.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYKEYCLASS_FIELD">
    <locale language="en">Pull mapper: a key class %1s has been specified that contains a field %2s that cannot be set.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYFACTCLASS_NEWINST">
    <locale language="en">Pull mapper: an entity factory class %1s has been specified that cannot be instantiated.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYCLASS_SCANMETHOD">
    <locale language="en">Pull mapper: the searchByLastwritten method on entity %1s cannot be run.</locale>
  </message>  

  <message name="ERR_PM_BADENTITYCLASS_SCANMETHOD_ITE">
    <locale language="en">GSS pull mapper: the searchByLastwritten method on entity %1s threw an error.</locale>
  </message>  

  <message name="ERR_PM_REMOVE">
    <locale language="en">GSS pull mapper: error removing entity %1s - check entity key definitions, etc.</locale>
  </message>  
</messages>