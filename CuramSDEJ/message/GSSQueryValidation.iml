<messages package="curam.message">
<message name="ERR_SS_BAD_QUERY">
    <locale language="en">Invalid query encountered.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_TERMS">
    <locale language="en">Query contains no terms - not a valid query.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_VALID_TERMS">
    <locale language="en">Query contains no valid terms - not a valid query.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_FIELD">
    <locale language="en">Query contains terms referencing a field called \'%1s\' which is unknown or not stored in this search service.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNINDEXED_FIELD">
    <locale language="en">Query contains terms referencing a field called \'%1s\' which is not indexed in this search service.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_NO_FIELDS">
    <locale language="en">Query contains no fields to return - not a valid query.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_RETURN_FIELD">
    <locale language="en">Query attempting to return unknown field \'%1s\'.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_UNTOKENIZED_WILDCARD">
    <locale language="en">Untokenized wildcard queries are not supported.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_EMPTY_TERM">
    <locale language="en">Empty term encountered.</locale>
  </message>  
  <message name="ERR_SS_BAD_QUERY_MULTIPLE_TERM_TOKEN_FIELD">
    <locale language="en">Encountered a prefix query with multiple tokens on a tokenized field.</locale>
  </message>  
  
 </messages>