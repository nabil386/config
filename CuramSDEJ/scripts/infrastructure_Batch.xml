<?xml version="1.0" encoding="UTF-8"?>

<root>
  <!--Batch Declarations-->
  <batches>
    <batch process="ImportConfiguration" operation="importConfig" application="curam.util">
      <parameter name="serverDirectory" type="TAB_STRING"/>
      <parameter name="serverComponentOrder" type="TAB_STRING"/>
      <parameter name="enableValidations" type="BOOLEAN_TYPE"/>
      <parameter name="mergeDirectory" type="TAB_STRING"/>
      <parameter name="extractDirectory" type="TAB_STRING"/>
      <parameter name="inputDirectory" type="TAB_STRING"/>
      <parameter name="serverLocaleList" type="TAB_STRING"/>
    </batch>
    <batch process="ImportDynamicUim" operation="importDuim" application="curam.util">
      <parameter name="inputDirectory" type="TAB_STRING"/>
      <parameter name="validateUim" type="BOOLEAN_TYPE"/>
    </batch>
    <batch process="PropertyLoader" operation="importProperties" application="curam.util">
      <parameter name="filename" type="PROP_FILENAME"/>
      <parameter name="schema" type="PROP_FILENAME"/>
    </batch>
    <batch process="RangeAwareConfiguration" operation="insertRangeAwareConfiguration" application="curam.util">
      <parameter name="configurationFile" type="RANGEAWARE_CONFIG_FILE"/>
    </batch>
    <batch process="RuleSetImport" operation="importRuleSet" application="curam.util">
      <parameter name="fileName" type="RULES_FILENAME"/>
      <parameter name="overwriteInd" type="RULES_OVERWRITE"/>
      <parameter name="importInd" type="RULES_IMPORT_IND"/>
    </batch>
    <batch process="UserPreferenceLoader" operation="insertUserPreferencesToDatabase" application="curam.util">
      <parameter name="serverDirectory" type="USERPREF_SERVER_DIR"/>
      <parameter name="infrastructureDirectory" type="USERPREF_INFRA_DIR"/>
      <parameter name="componentList" type="USERPREF_COMPONENT_LIST"/>
      <parameter name="localeList" type="USERPREF_COMPONENT_LIST"/>
    </batch>
  </batches>
</root>
