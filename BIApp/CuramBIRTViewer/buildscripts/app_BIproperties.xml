<project name="BI Content Properties">


  <property environment="sysenv." />


  <!-- Curam BIRT Viewer properties -->
  <property name="file.DB2JCC_LICENSE_CISUZ_JAR"
            value="${sysenv.DB2JCC_LICENSE_CISUZ_JAR}" />

  <!-- Establish Java 5 as compilation level if not otherwise specified. -->
  <condition property="ant.build.javac.source" value="1.8">
    <not>
      <isset property="ant.build.javac.source" />
    </not>
  </condition>
  <condition property="ant.build.javac.target" value="1.8">
    <not>
      <isset property="ant.build.javac.target" />
    </not>
  </condition>



  <!-- reference all env variables from ant properties -->
  <property name="dir.CURAM_DIR" value="${sysenv.CURAM_DIR}" />


  <property name="dir.DB2DIR" value="${sysenv.DB2DIR}" />
  <property name="dir.CURAMSDEJ" value="${sysenv.CURAMSDEJ}" />
  <!-- Curam BIRT Viewer properties -->
  <property name="dir.bicontent" value="${dir.CURAM_DIR}/BIContent" />

  <!-- Curam BIRT Viewer properties -->
  <property name="dir.biapp" value="${dir.CURAM_DIR}/BIApp/CuramBIRTViewer" />

  <property name="bi.prop.file.location" value="${dir.biapp}/buildscripts" />
  <property name="bi.prop.file"
            value="${bi.prop.file.location}/BIRTthird_party_version.properties" />
  <property file="${bi.prop.file}" />


  <!-- packages up event handlers  -->
  <property name="biaclasses.jar" value="curam-birt.jar" />


  <property name="dir.logs"
            value="${dir.CURAM_DIR}/BIApp/CuramBIRTViewer/logs" />

  <!-- Curam BIRT Viewer properties -->
  <property name="dir.biapp" value="${dir.CURAM_DIR}/BIApp/CuramBIRTViewer" />


  <property name="dir.biapp.webcontent" value="${dir.biapp}/WebContent" />
  <property name="dir.biapp.lib" value="${dir.biapp}/lib" />
  <property name="dir.biapp.doclet.jarname" value="BIBuildAPIDoc.jar" />

  <path id="jar.reporting.doctasks.refid">
    <pathelement path="${dir.biapp.lib}/${dir.biapp.doclet.jarname}" />
  </path>

  <property name="dir.biapp.doc" value="${dir.biapp}/doc" />

  <property name="dir.biapp.webcontent.webinf"
            value="${dir.biapp.webcontent}/WEB-INF" />

  <property name="dir.biapp.webcontent.webinf.platform.plugins"
            value="${dir.biapp.webcontent}/WEB-INF/platform/plugins" />


  <property name="dir.biapp.webcontent.core"
            value="${dir.biapp}/components/core/WebContent" />


  <property name="dir.biapp.webcontent.core.webxml"
            value="${dir.biapp.webcontent.core}/WEB-INF/web.xml" />

  <property name="dir.biapp.birt.languagepacks.source"
            value="${dir.biapp}/components/BIContent/languagepacks" />


  <property name="dir.biapp.birt.languagepacks.target"
            value="${dir.biapp.webcontent.webinf}/lib" />

  <property name="dir.biapp.webcontent.bicontent"
            value="${dir.biapp.webcontent.webinf}/bicontent" />


  <property name="dir.biapp.webcontent.bicontent.resources"
            value="${dir.biapp.webcontent.bicontent}/resources" />

  <property name="dir.biapp.webcontent.bicontent.scriptslib"
            value="${dir.biapp.webcontent.bicontent.resources}/scriptlib" />

  <property name="dir.biapp.components.bicontent.scriptslib"
            value="${dir.biapp}${file.separator}components${file.separator}BIContent${file.separator}resources${file.separator}scriptlib" />




  <!-- copy images to here-->
  <property name="dir.biapp.webcontent.bicontent.resources.images"
            value="${dir.biapp.webcontent.bicontent.resources}/images" />


  <!-- copy javascript to here  -->
  <property name="dir.biapp.webcontent.bicontent.resources.jscript"
            value="${dir.biapp.webcontent.bicontent.resources}/jscript" />

  <!-- copy css to here  -->
  <property name="dir.biapp.webcontent.bicontent.resources.css"
            value="${dir.biapp.webcontent.bicontent.resources}/css" />

  <!-- copy library to here -->
  <property name="dir.biapp.webcontent.bicontent.resources.library"
            value="${dir.biapp.webcontent.bicontent.resources}/library" />
  <!-- copy porperties to here -->
  <property name="dir.biapp.webcontent.bicontent.resources.properties"
            value="${dir.biapp.webcontent.bicontent.resources}/properties" />




  <property name="dir.biapp.webcontent.webinf.plugins"
            value="${dir.biapp.webcontent}/WEB-INF/platform/plugins/" />
  <!-- where in the birt plugin to copy the SDEJ Drivers location  -->
  <property name="dir.biapp.drivers.pluginslocation"
            value="org.eclipse.birt.report.data.oda.jdbc${version-org.eclipse.birt.report.data.oda.jdbc}/drivers/" />


  <!-- where in the curam viewer class files to the eclipse birt plugin location  -->
  <property name="dir.biapp.classes.pluginslocation"
            value="org.eclipse.birt.report.viewer${version-org.eclipse.birt.report.viewer}/birt/WEB-INF/classes/" />


  <property name="curam.jar.source"
            value="${dir.biapp.webcontent.bicontent.scriptslib}/${biaclasses.jar}" />

  <property name="curam.jar.webcontent.scriptlib"
            value="${dir.biapp.webcontent.bicontent.scriptslib}/${biaclasses.jar}" />

  <path id="compile.birt">
    <pathelement path="${pre.classpath}" />
    <!-- include all jar files in the WebContent/Web-inf/lib directory. -->
    <fileset dir="${dir.biapp.webcontent.webinf}/lib">
      <include name="*.jar" />
    </fileset>
    <fileset dir="${dir.CURAMSDEJ}/lib">
      <include name="*.jar" />
    </fileset>
    <pathelement location="${sysenv.J2EE_JAR}" />
    <pathelement path="${post.classpath}" />
  </path>



  <path id="RDBMSlibraries.path">
    <fileset dir="${dir.CURAMSDEJ}/drivers">
      <include name="db2jcc.jar" />
      <include name="db2jcc_license_cu.jar" />
      <include name="h2.jar" />
      <include name="ojdbc.jar" />
      <include name="runtime12.jar" />
      <include name="sqlj.zip" />
      <include name="translator.jar" />
    </fileset>
  </path>

  <!-- END: Curam BIRT Viewer properties -->
  <!-- properties for building EAR files -->
  <property name="dir.biapp.bld" value="${dir.biapp}/biappbuild" />
  <property name="dir.biapp.bld.ear" value="${dir.biapp.bld}/ear" />
  <property name="dir.biapp.bld.ear.was" value="${dir.biapp.bld}/ear/WAS" />
  <property name="dir.biapp.bld.ear.wlp" value="${dir.biapp.bld}/ear/WLP" />
  <property name="dir.biapp.bld.ear.wls" value="${dir.biapp.bld}/ear/WLS" />


  <property name="clientmodule" value="ClientModule.war" />
  <property name="dir.temp" value="temp" />
  <property name="file.config.ini" value="config.ini" />


  <property name="dir.biapp.ear" value="${dir.biapp}/ear" />
  <property name="dir.biapp.ear.was" value="${dir.biapp.ear}/WAS" />
  <property name="filename.web.xml.security" value="websecurity.xml" />

  <property name="dir.biapp.ear.was.web.security"
            value="${dir.biapp.ear}/WAS/ClientModule/WEB-INF/${filename.web.xml.security}" />

  <property name="dir.biapp.ear.wls" value="${dir.biapp.ear}/WLS" />
  <property name="dir.biapp.ear.wls.web.security"
            value="${dir.biapp.ear.wls}/ClientModule/WEB-INF/${filename.web.xml.security}" />
  <property name="dir.biapp.ear.wls.web.weblogic.xml"
            value="${dir.biapp.ear}/WLS/ClientModule/WEB-INF/weblogic.xml" />
  <property name="dir.biapp.ear.wlp" value="${dir.biapp.ear}/WLP" />
  <property name="dir.biapp.ear.wlp.web.security"
            value="${dir.biapp.ear}/WLP/ClientModule/WEB-INF/${filename.web.xml.security}" />

  <property name="dir.biapp.BIRT.config"
            value="WEB-INF/platform/configuration/config.ini" />


  <!-- properties for building  files  -->
  <!-- A setting is necessary on Unix platforms rather than an empty string -->
  <property name="compile.fork" value="true" />

  <property name="compile.debug" value="on" />
  <property name="compile.maxmemory" value="1024m" />
  <property name="compile.nowarn" value="off" />
  <property name="compile.optimize" value="off" />
  <property name="compile.deprecation" value="false" />
  <property name="compile.verbose" value="false" />
  <property name="compile.includeAntRuntime" value="yes" />
  <property name="compile.includeJavaRuntime" value="no" />
  <property name="compile.failonerror" value="true" />
  <property name="compile.listfiles" value="no" />
  <property name="compile.maxwarnings" value="10000" />
  <property name="jar.compression" value="0" />

  <property name="crypto.jar.file" value="CryptoConfig.jar" />

</project>
